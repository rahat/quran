{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "El hakkah(hakkatu)"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Mel hakkah(hakkatu)"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Ve ma edrake mel hakkah(hakkatu)"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kezzebet semudu ve adun bil kariah(kariati)"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fe emma semudu fe uhliku bit tagıyeh(tagıyeti)"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Ve emma adun fe uhliku bi rihın sarsarin atiyeh(atiyetin)"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sehhareha aleyhim seb’a leyalin ve semaniyete eyyamin husumen fe terel kavme fiha sar’a ke ennehum a’cazu nahlin haviyeh(haviyetin)"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fe hel tera lehum min bakıyeh(bakıyetin)"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Ve cae fir’avnu ve men kablehu vel mu’tefikatu bil hatıeh(hatıeti)"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Fe asav resule rabbihim fe ehazehum ahzeten rabiyeh(rabiyeten)"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Inna lemma tagal mau hamelnakum fil cariyeh(cariyeti)"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "Li nec’aleha lekum tezkireten ve teıyeha uzunun vaıyeh(vaıyetun)"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fe iza nufiha fis suri nefhatun vahıdeh(vahıdetun)"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Ve humiletil ardu vel cibalu fe dukketa dekketen vahıdeh(vahıdeten)"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fe yevme izin vekaatil vakıah(vakıatu)"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Ven sakkatis semau fe hiye yevme izin vahiyeh(vahiyetun)"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Vel meleku ala ercaiha, ve yahmilu arse rabbike fevkahum yevme izin semaniyeh(semaniyetun)"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yevme izin tu’radune la tahfa minkum hafiyeh(hafiyetun)"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Fe emma men utiye kitabehu bi yeminihi fe yekulu haumukreu kitabiyeh"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Inni zanentu enniy mulakın hısabiyeh"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fe huve fi isetin radıyeh(radıyetin)"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fi cennetin aliyeh(aliyetin)"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Kutufuha daniyeh(daniyetun)"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kulu vesrebu henien bima esleftum fil eyyamil haliyeh(haliyeti)"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Ve emma men utiye kitabehu bi simalihi fe yekulu ya leyteni lem ute kitabiyeh"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Ve lem edri ma hısabiyeh"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Ya leyteha kanetil kadiyeh(kadiyete)"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Ma agna anni maliyeh"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Heleke anni sultaniyeh"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Huzuhu fe gulluh(gulluhu)"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Summel cahime salluh(salluhu)"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Summe fi silsiletin zer’uha seb’une ziraan feslukuh(feslukuhu)"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Innehu kane la yu’minu billahil azim(azimi)"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Ve la yahuddu ala taamil miskin(miskini)"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Fe leyse lehul yevme hahuna hamim(hamimun)"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Ve la taamun illa min gıslin(gıslinin)"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "La ye’kuluhu illel hatiun(hatiune)"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fe la uksımu bima tubsırun(tubsırune)"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve ma la tubsırun(tubsırune)"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innehu le kavlu resulun kerimin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve ma huve bi kavli sairin, kalilin ma tu’minun(tu’minune)"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ve la bi kavli kahin(kahinin), kalilen ma tezekkerun(tezekkerune)"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tenzilun min rabbil alemin(alemine)"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ve lev tekavvele aleyna ba’dal ekavil(ekavili)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Le ehazna minhu bil yemin(yemini)"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Summe le kata’na minhul vetin(vetine)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fe ma minkum min ehadin anhu hacizin(hacizine)"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ve innehu le tezkiretun lil muttekin(muttekine)"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ve inna le na’lemu enne minkum mukezzibin(mukezzibine)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve innehu le hasretun alel kafirin(kafirine)"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve innehu le hakk´ul yakin(yakini)"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fe sebbıh bismi rabbikel azim(azimi)"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Se ele sailun bi azabin vakı’n(vakıın)"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lil kafirine leyse lehu dafi’(dafiun)"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Minallahi zil mearic(mearici)"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta´rucul melaiketu ver ruhu ileyhi fi yevmin kane mikdaruhu hamsine elfe seneh(senetin)"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir sabren cemila(cemilen)"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Innehum yerevnehu baida(baiden)"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Ve nerahu kariba(kariben)"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yevme tekunus semau kel muhl(muhli)"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Ve tekunul cibalu kel ıhn(ıhni)"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Ve la yes’elu hamimun hamima(hamimen)"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassarunehum yeveddul mucrimu lev yeftedi min azabi yevmi izin bi benih(benihi)"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Ve sahıbetihi ve ahih(ahihi)"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ve fasiletihilleti tu’vih(tu’vihi)"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Ve men fil ardı cemi’an summe yuncih(yuncihi)"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kella, inneha leza"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nezzaaten lis seva"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Ted’u men edbera ve tevella"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ve cemea fe ev’a"
        }
    ]
}