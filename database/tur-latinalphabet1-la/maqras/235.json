{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Izes semaunfetaret"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Ve izel kevakibunteseret"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ve izel biharu fucciret"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ve izel kuburu bu’siret"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Alimet nefsun ma kaddemet ve ahharet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya eyyuhel insanu ma garreke bi rabbikel kerim(kerimi)"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ellezi halakake fe sevvake fe adelek(adeleke)"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fi eyyi suretin ma sae rekkebek(rekkebeke)"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kella bel tukezzibune bid din(dini)"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ve inne aleykum le hafızin(hafızine)"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiramen katibin(katibine)"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ya’lemune ma tef’alun(tef’alune)"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Innel ebrare lefi naim(naimin)"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ve innel fuccare lefi cahim(cahimın)"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslevneha yevmed din(dini)"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ve ma hum anha bi gaibin(gaibine)"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ve ma edrake ma yevmud din(dini)"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Summe ma edrake ma yevmud din(dini)"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yevme la temliku nefsun li nefsin sey’a(sey’en), vel emru yevmeizin lillah(lillahi)"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Veylun lil mutaffifin(mutaffifine)"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Ellezine izektalu alen nasi yestevfun(yestevfune)"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Ve iza kaluhum ev vezenuhum yuhsirun(yuhsirune)"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Ela yezunnu ulaike ennehum meb´usun(meb´usune)"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Li yevmin azim(azimin)"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yevme yekumun nasu li rabbil alemin(alemine)"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kella inne kitabel fuccari le fi siccin(siccinin)"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Ve ma edrake ma siccin(siccinun)"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitabun merkum(merkumun)"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Ellezine yukezzibune bi yevmiddin(yevmiddini)"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Ve ma yukezzıbu bihi illa kullu mu’tedin esim(esimin)"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Iza tutla aleyhi ayatuna kale esatirul evvelin(evveline)"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kella bel rane ala kulubihim ma kanu yeksibun(yeksibune)"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kella innehum an rabbihim yevmeizin le mahcubun(mahcubune)"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Summe innehum le salul cahim(cahimi)"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Summe yukalu hazellezi kuntum bihi tukezzibun(tukezzibune)"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kella inne kitabel ebrari lefi illiyyin(illiyyine)"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Ve ma edrake ma ılliyyun(ılliyyune)"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitabun merkum(merkumun)"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yesheduhul mukarrebun(mukarrebune)"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Innel ebrare le fi naim(naimi)"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Alel eraiki yenzurun(yenzurune)"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Ta’rifu fi vucuhihim nadraten naim(naimi)"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yuskavne min rahikın mahtum(mahtumin)"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Hitamuhu misk(miskun). ve fi zalike fel yetenafesil mutenafisun(mutenafisune)"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Ve mizacuhu min tesnim(tesnimin)"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Aynen yesrebu bihel mukarrabun(mukarrabune)"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Innellezine ecremu kanu minellezine amenu yadhakun(yadhakune)"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Ve iza merru bihim yetegamezune"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Ve izenkalebu ila ehlihimunkalebu fekihin(fekihine)"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Ve iza reevhum kalu inne haulai ledallun(ledallune)"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Ve ma ursilu aleyhim hafızin(hafızine)"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Felyevmellezine amenu minel kuffarı yadhakun(yadhakune)"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Alel eraiki yanzurun(yanzurune)"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Hel suvvibel kuffaru ma kanu yef’alun(yef’alune)"
        }
    ]
}