{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "La uksimu bi hazel beled(beledi)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ve ente hıllun bi hazel beled(beledi)"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ve validin ve ma veled(velede)"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Lekad halaknel insane fi kebed(kebedin)"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "E yahsebu en len yakdira aleyhi ehad(ehadun)"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yekulu ehlektu malen lubeda(lubeden)"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "E yahsebu en lem yerahu ehad(ehadun)"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "E lem nec’al lehu ayneyn(ayneyni)"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ve lisanen ve sefeteyn(sefeteyni)"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ve hedeynahun necdeyn(necdeyni)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fe laktehamel akabete"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ve ma edrake mel akabeh(akabetu)"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fekku rekabetin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ev ıt’amun fi yevmin zi mesgabeh(mesgabetin)"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yetimen za makrabeh(makrabetin)"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ev miskinen za metrabeh(metrabetin)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Summe kane minellezine amenu ve tevasav bis sabri ve tevasav bil merhame(merhameti)"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ulaike ashabul meymeneh(meymeneti)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Vellezine keferu bi ayatina hum ashabul mes’emeh(mes’emeti)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Aleyhim narun mu’sadeh(mu’sadetun)"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ves semsi ve duhaha"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Vel kameri iza telaha"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ven nehari iza cellaha"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Vel leyli iza yagsaha"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Ves semai ve ma benaha"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Vel ardı ve ma tahaha"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ve nefsin ve ma sevvaha"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fe elhemeha fucureha ve takvaha"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Kad efleha men zekkaha"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ve kad habe men dessaha"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kezzebet semudu bi tagvaha"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Izin baase eskaha"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Fe kale lehum resulullahi nakatallahi ve sukyaha"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fe kezzebuhu fe akaruha fe demdeme aleyhim rabbuhum bi zenbihim fe sevvaha"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ve la yehafu ukbaha"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Vel leyli iza yagsa"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ven nehari iza tecella"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ve ma halakaz zekera vel unsa"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inne sa’yekum le setta"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fe emma men a’ta vetteka"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve saddeka bil husna"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fe senuyessiruhu lil yusra"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ve emma men bahıle vestagna"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve kezzebe bil husna"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fe senuyessiruhu lil usra"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Ve ma yugni anhu maluhu iza teredda"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inne aleyna lel huda"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ve inne lena lel ahırete vel ula"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fe enzertukum naren telezza"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "La yaslaha illel eska"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ellezi kezzebe ve tevella"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ve seyucennebuhel etka"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ellezi yu’ti ma lehu yetezekka"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ve ma li ehadin indehu min ni´metin tucza"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Illebtigae vechi rabbihil a’la"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve le sevfe yerda"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ved duha"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Vel leyli iza seca"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ma veddeake rabbuke ve ma kala"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve lel ahıretu hayrun leke minel ula"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve le sevfe yu’tike rabbuke fe terda"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "E lem yecidke yetimen fe ava"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve vecedeke dallen fe heda"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ve vecedeke ailen fe agna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fe emmel yetime fe la takher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve emmes saile fe la tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve emma bi ni’meti rabbike fe haddis"
        }
    ]
}