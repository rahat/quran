{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Izes semaunsakkat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Ve ezinet li rabbiha ve hukkat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Ve izel ardu muddet"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ve elkat ma fiha ve tehallet"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ve ezinet li rabbiha ve hukkat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ya eyyuhel insanu inneke kadihun ila rabbike kedhan fe mulakih(mulakihı)"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Fe emma men utiye kitabehu bi yeminih(yeminihi)"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Fe sevfe yuhasebu hısaben yesira(yesiren)"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ve yenkalibu ila ehlihi mesrura(mesruren)"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ve emma men utiye kitabehu verae zahrih(zahrihi)"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Fe sevfe yed’u subura(suburen)"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ve yasla saira(sairen)"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Innehu kane fi ehlihi mesrura(mesruren)"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Innehu zanne en len yahur(yahura)"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Bela, inne rabbehu kane bihi basira(basiren)"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Fe la uksimu bis sefak(sefakı)"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Vel leyli ve ma vesak(vesaka)"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Vel kameri izet tesak(tesaka)"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Le terkebunne tabakan an tabakın"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Fe ma lehum la yu’minun(yu’minune)"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Ve iza kurıe aleyhimul kur’anu la yescudun(yescudune). (SECDE AYETI)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Belillezine keferu yukezzibun(yukezzibune)"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Vallahu a’lemu bima yuun(yuune)"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Fe bessirhum bi azabin elim(elimin)"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Illellezine amenu ve amilus salihati lehum ecrun gayru memnun(memnunin)"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Ves semai zatil buruc(buruci)"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Vel yevmil mev’ud(mev’udi)"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ve sahidin ve meshud(meshudin)"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kutile ashabul uhdud(uhdudi)"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Ennari zatil vekud(vekudi)"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Izhum aleyha kuud(kuudun)"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ve hum ala ma yef’alune bil mu’minine suhud(suhudun)"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Ve ma nekamu minhum illa en yu’minu billahil azizil hamid(hamidi)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ellezi lehu mulkus semavati vel ard(ardı), vallahu ala kulli sey’in sehid(sehidun)"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Innellezine fetenul mu’minine vel mu’minati summe lem yetubu fe lehum azabu cehenneme ve lehum azabul harik(harikı)"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Innellezine amenu ve amilus salihati lehum cennatun tecri min tahtihel enhar(enharu), zalikel fevzul kebir(kebiru)"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inne batse rabbike le sedid(sedidun)"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innehu huve yubdiu ve yuid(yuidu)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ve huvel gafurul vedud(vedudu)"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Zul arsil mecid(mecidu)"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Fa’alun lima yurid(yuridu)"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hel etake hadisul cunud(cunudi)"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir’avne ve semud(semude)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Belillezine keferu fi tekzib(tekzibin)"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Vallahu min veraihim muhit(muhitun)"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bel huve kur’anun mecid(mecidun)"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fi levhın mahfuz(mahfuzın)"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ves semai vet tarık(tarıkı)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve ma edrake met tarik(tariku)"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "En necmus sakıb(sakıbu)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nefsin lemma aleyha hafız(hafızun)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fel yenzuril insanu mimme hulık(hulıka)"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hulika min main dafik(dafikın)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yahrucu min beynis sulbi vet teraib(teraibi)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innehu ala rec’ıhi le kadir(kadirun)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yevme tubles serair(serairu)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fema lehu min kuvvetin ve la nasır(nasırın)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ves semai zatir rec’(rec’ı)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Vel ardı zatis sad’(sad’ı)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innehu le kavlun fasl(faslun)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve ma huve bil hezl(hezli)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innehum yekidune keyda(keyden)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ekidu keyda(keyden)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Fe mehhilil kafirine emhilhum ruveyda(ruveyden)"
        }
    ]
}