{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "La uksimu bi yevmil kıyameh(kıyameti)"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Ve la uksimu bin nefsil levvameh(levvameti)"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "E yahsebul insanu ellen necmea ıza meh(mehu)"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bela kadirine ala en nusevviye bena neh(nehu)"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bel yuridul insanu li yefcure emameh(emamehu)"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yes’elu eyyane yevmul kıyameh(kıyameti)"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fe iza berikal basar(basaru)"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Ve hasefel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Ve cumias semsu vel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yekulul insanu yevme izin eynel meferr(meferru)"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kella la vezer(vezere)"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila rabbike yevme izinil mustekar(mustekarru)"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunebbeul insanu yevme izin bima kaddeme ve ahhar(ahhare)"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Belil insanu ala nefsihi basireth(basiretun)"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Ve lev elka meazireh(meazirehu)"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La tuharrik bihi lisaneke li ta’cele bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inne aleyna cem’ahu ve kur’anehu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fe iza kara’nahu fettebi’kur’anehu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summe inne aleyna beyanehu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kella bel tuhıbbunel acileh(acilete)"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Ve tezerunel ahıreh(ahirete)"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Vucuhun yevme izin nadıreh(nadıretun)"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ila rabbiha nazıreh(naziretun)"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Ve vucuhun yevme izin basireth(basiretun)"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tezunnu en yuf’ale biha fakıreh(fakıretun)"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kella iza belegatit terakıy(terakıye)"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Ve kile men rak(rakın)"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Ve zanne ennehul firak(firaku)"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Velteffetis saku bis sak(sakı)"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ila rabbike yevme izinil mesak(mesaku)"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fe la saddeka ve la salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Ve lakin kezzebe ve tevella"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Summe zehebe ila ehlihi yetemetta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Evla leke fe evla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Summe evla leke fe evla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "E yahsebul’insanu en yutreke suda(sudan)"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "E lem yeku nutfeten min meni yin yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Summe kane alakaten fe halaka fe sevva"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Fe ceale minhuz zevceyniz zekere vel unsa"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "E leyse zalike bi kadirin ala en yuhyiyel mevta"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hel eta alel insani hinun mined dehri lem yekun sey’en mezkura(mezkuren)"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Inna halaknel insane min nutfetin emsacin nebtelihi fe cealnahu semian basira(basiren)"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Inna hedeynahus sebile imma sakiren ve imma kefura(kefuren)"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Inna a’tedna lil kafirine selasile ve aglalen ve seira(seiren)"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Innel ebrara yesrebune min ke’sin kane mizacuha kafura(kafuren)"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "Aynen yesrebu biha ibadullahi yufecciruneha tefcira(tefciren)"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yufune bin nezri ve yehafune yevmen kane serruhu mustetira(mustetiren)"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "Ve yut’imunet taame ala hubbihi miskinen ve yetimen ve esira(esiren)"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innema nut’imukum li vechillahi la nuridu minkum cezaen ve la sukura(sukuren)"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Inna nehafu min rabbina yevmen abusen kamtarira(kamtariren)"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fe vekahumullahu serra zalikel yevmi ve lakkahum nadreten ve surura(sururen)"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Ve cezahum bima saberu cenneten ve harira(hariren)"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttekiine fiha alel eraik(eraiki), la yeravne fiha semsen ve la zemherira(zemheriren)"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Ve daniyeten aleyhim zılaluha ve zullilet kutufuha tezlila(tezlilen)"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Ve yutafu aleyhim bi aniyetin min fıddatin ve ekvabin kanet kavarira(kavariren)"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Kavarira min fıddatin kadderuha takdira(takdiren)"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Ve yuskavne fiha ke’sen kane mizacuha zencebila(zencebilen)"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "Aynen fiha tusemma selsebila(selsebilen)"
        }
    ]
}