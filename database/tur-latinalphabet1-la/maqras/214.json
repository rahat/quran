{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Iza ve kaatil vakıah(vakıatu)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Leyse li vak’atiha kazibeh(kazibetun)"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Hafidatun rafiah(rafiatun)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Iza ruccetil ardu recca(reccen)"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ve bussetil cibalu bessa(bessen)"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Fe kanet hebaen mun bessa(bessen)"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ve kuntum ezvacen selaseh(selaseten)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Fe ashabul meymeneti ma ashabul meymeneti"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ve ashabul mes´emeti ma ashabul mes’emeti"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ves sabikunes sabikun(sabikune)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ulaikel mukarrebun(mukarrebune)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Fi cennatin naim(naimi)"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Sulletun minel evvelin(evveline)"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ve kalilun minel ahirin(ahirine)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ala sururin mevdunetin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Muttekiine aleyha mutekabilin(mutekabiline)"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yetufu aleyhim vildanun muhalledun(muhalledune)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi ekvabin ve ebarika ve ke’sin min main(mainin)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "La yusaddeune anha ve la yunzifun(yunzifune)"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ve fakihetin mimma yetehayyerun(yetehayyerune)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ve lahmi tayrin mimma yestehun(yestehune)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ve hurun inun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ke emsalil lu’luil meknun(meknuni)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Cezaen bi ma kanu ya’melun(ya’melune)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "La yesmeune fiha lagven ve la te’sima(te’simen)"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Illa kilen selamen selama(selamen)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ve ashabul yemini ma ashabul yemin(yemini)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fi sidrin mahdud(mahdudin)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ve talhın mendud(mendudin)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ve zıllin memdud(memdudin)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ve main meskub(meskubin)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ve fakihetin kesirah(kesiretin)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "La maktuatin ve la memnuah(memnuatin)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve furusin merfuah(merfuatin)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Inna ense’na hunne insaa(insaen)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Fe cealna hunne ebkaran(ebkaren)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Uruben etraba(etraben)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li ashabil yemin(yemini)"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Sulletun minel evvelin(evveline)"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ve sulletun minel ahırin(ahırine)"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ve ashabus simali ma ashabus simal(simali)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fi semumin ve hamim(hamimin)"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Ve zıllin min yahmum(yahmumin)"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "La baridin ve la kerim(kerimin)"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innehum kanu kable zalike mutrefin(mutrefine)"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ve kanu yusirrune alel hınsil azim(azimi)"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ve kanu yekulune e iza mitna ve kunna turaben ve iza men e inna le meb’usun(meb’usune)"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "E ve abaunel evvelun(evvelune)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kul innel evveline vel ahirin(ahirine)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Le mecmuune ila mikati yevmin ma’lum(ma’lumin)"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Summe innekum eyyuhed dallunel mukezzibun(mukezzibune)"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Le akilune min secerin min zakkumin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Fe ma liune minhel butun(butune)"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fe saribune aleyhi minel hamim(hamimi)"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fe saribune surbel him(himi)"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Haza nuzuluhum yevmed din(dini)"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nahnu halaknakum fe lev la tusaddikun(tusaddikune)"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "E fe reeytum ma tumnun(tumnune)"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "E entum tahlukunehu em nahnul halikun(halikune)"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nahnu kadderna beynekumul mevte ve ma nahnu bi mes- bukin(mesbukine)"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Ala en nubeddile emsalekum ve nunsiekum fi ma la ta’lemun(ta’lemune)"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ve lekad alimtumunnes etel ula fe lev la tezekkerun(tezekkerune)"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "E fe reeytum ma tahrusun(tahrusune)"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "E entum tezre unehu em nahnuz zariun(zariune)"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Lev nesau le cealnahu hutamen fe zaltum tefekkehun(tefekkehune)"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Inna le mugremun(mugremune)"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bel nahnu mahrumun(mahrumune)"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "E fe reeytumul maellezi tesrebun(tesrebune)"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "E entum enzeltumuhu minel muzni em nahnul munzilun(munzilune)"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Lev nesau cealnahu ucacen fe levla teskurun(teskurune)"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "E fe reeytumun narelleti turun(turune)"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "E entum ense’tum secereteha em nahnul munsiun(munsiune)"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nahnu cealnaha tezkireten ve metaan lil mukvin(mukvine)"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fe sebbih bismi rabbikel azim(azimi)"
        }
    ]
}