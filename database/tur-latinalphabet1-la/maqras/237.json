{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sebbihısme rabbikel a’la"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ellezi halaka fesevva"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vellezi kaddere fe heda"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vellezi ahrecel mer’a"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Fe cealehu gusaen ahva"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Senukriuke fe la tensa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illa ma saallah(saallahu), innehu ya’lemul cehre ve ma yahfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve nuyessiruke lil yusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fe zekkir in nefeatiz zikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Seyezzekkeru men yahsa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ve yetecennebuhel eska"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ellezi yaslen narel kubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summe la yemutu fiha ve la yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kad efleha men tezekka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ve zekeresme rabbihi fe salla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bel tu’sırunel hayated dunya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Vel ahıretu hayrun ve ebka"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inne haza le fis suhufil ula"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Suhufi ibrahime ve musa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hel etake hadisul gasiyeh(gasiyeti)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Vucuhun yevmeizin hasiah(hasiatun)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Amiletun nasıbeh(nasıbetun)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Tesla naren hamiyeh(hamiyeten)"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tuska min aynin aniyeh(aniyetin)"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Leyse lehum taamun illa min dari’(dariın)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "La yusminu ve la yugni min cu’(cuın)"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Vucuhun yevmeizin naımeh(naımetun)"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Li sa’yiha radiyeh(radiyetun)"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fi cennetin aliyeh(aliyetun)"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "La tesmeu fiha lagıyeh(lagıyeten)"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fiha aynun cariyeh(cariyetun)"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Fiha sururun merfuah(merfuatun)"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ve ekvabun mevduah(mevduatun)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ve nemarıku masfufeh(masfufetun)"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ve zerabiyyu mebsuseh(mebsusetun)"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "E fe la yanzurune ilel ibili keyfe hulikat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ve iles semai keyfe rufiat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ve ilel cibali keyfe nusıbet"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ve ilel ardı keyfe sutıhat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fezekkir innema ente muzekkir(muzekkirun)"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Leste aleyhim bi musaytır(musaytırın)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Illa men tevella ve kefer(kefere)"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fe yuazzibuhullahul azabel ekber(ekbere)"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inne ileyna iyabehum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Summe inne aleyna hisabehum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Vel fecr(fecri)"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ve leyalin asr(asrın)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Ves sef’ı vel vetr(vetri)"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Vel leyli iza yesr(yesri)"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hel fi zalike kasemun lizi hicr(hicrin)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "E lem tere keyfe feale rabbuke bi ad(adin)"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Ireme zatil ımad(ımadi)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Elleti lem yuhlak misluha fil bilad(biladi)"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Ve semudelleziyne cabussahre bil vad(vadi)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ve fir avne zil evtad(evtadi)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ellezine tagav fil bilad(biladi)"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fe ekseru fihel fesad(fesade)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fe sabbe aleyhim rabbuke sevta azab(azabin)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inne rabbeke le bil mirsad(mirsadi)"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fe emmel insanu iza mebtelahu rabbuhu fe ekremehu ve na’amehu fe yekulu rabbi ekremen(ekremeni)"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ve emma iza mebtelahu fe kadere aleyhi rızkahu fe yekulu rabbi ehanen(ehaneni)"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kella bel la tukrimunel yetim(yetime)"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Ve la tehaddune ala taamil miskin(miskini)"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Ve te’kulunet turase eklen lemma(lemmen)"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Ve tuhıbbunel male hubben cemma(cemmen)"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kella iza dukketil ardu dekken dekka(dekken)"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Ve cae rabbuke vel meleku saffen saffa(saffen)"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ve cie yevmeizin bi cehenneme yevmeizin yetezekkerul insanu ve enna lehuz zikra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yekulu ya leyteni kaddemtu li hayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fe yevmeizin la yuazzibu azabehu ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ve la yusiku ve sakahu ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ya eyyetuhen nefsul mutmainneh(mutmainnetu)"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Ircii ila rabbiki radıyeten mardıyyeh(mardıyyeten)"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fedhuli fi ibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Vedhuli cenneti"
        }
    ]
}