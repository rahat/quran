{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "Ve yetufu aleyhim vildanun muhalledun(muhalledune), iza reeytehum hasibtehum lu’luen mensura(mensuren)"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "Ve iza reeyte semme reeyte naimen ve mulken kebira(kebiren)"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "Aliyehum siyabu sundusin hudrun ve istebrakun ve hullu esavira min fıddah(fıddatin), ve sekahum rabbuhum saraben tahura(tahuren)"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "Inne haza kane lekum cezaen ve kane sa’yukum meskura(meskuren)"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "Inna nahnu nezzelna aleykel kur’ane tenzila(tenzilen)"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "Fasbir li hukmi rabbike ve la tutı’minhum asimen ev kefura(kefuren)"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "Vezkurisme rabbike bukreten ve asila(asilen)"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "Ve minel leyli fescud lehu ve sebbihhu leylen tavila(tavilen)"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Inne haulai yuhıbbunel acilete ve yezerune veraehum yevmen sekila(sekilen)"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Nahnu halaknahum ve sededna esrehum, ve iza si’na beddelna emsalehum tebdila(tebdilen)"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Inne hazihi tezkireh(tezkiretun), fe men saettehaze ila rabbihi sebila(sebilen)"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Ve ma tesaune illa en yesaallah(yesaallahu), innallahe kane alimen hakima(hakimen)"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Yudhilu men yesau fi rahmetih(rahmetihi), vez zalimine eadde lehum azaben elima(elimen)"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Vel murselati urfa(urfen)"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Fel asıfati asfa(asfen)"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Vennasirati nesren"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Fel farikati ferka(ferkan)"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Fel mulkıyati zikra(zikren)"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Uzren ev nuzra(nuzren)"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Innema tuadune levakı’(levakıun)"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Fe izen nucumu tumiset"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ve izes semau furicet"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ve izel cibalu nusifet"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ve izer rusulu ukkıtet"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Li eyyi yevmin uccilet"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Li yevmil fasl(fasli)"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ve ma edrake ma yevmul fasl(fasli)"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "E lem nuhlikil evvelin(evveline)"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Summe nutbiuhumul ahırin(ahırine)"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Kezalike nef’alu bil mucrimin(mucrimine)"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "E lem nahlukkum min main mehin(mehinin)"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Fe cealnahu fi kararin mekin(mekinin)"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Ila kaderin ma’lum(ma’lumin)"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Fe kaderna fe ni’mel kadirun(kadirune)"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "E lem nec’alil arda kifata(kifaten)"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Ahyaen ve emvata(emvaten)"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Ve cealna fiha revasiye samihatin ve eskaynakum maen furata(furaten)"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Intaliku ila ma kuntum bihi tukezzibun(tukezzibune)"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Intaliku ila zıllin zi selasi suab(suabin)"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "La zalilin ve la yugni minel leheb(lehebi)"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Inneha termi bi serarin kasr(kasri)"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Ke ennehu cimaletun sufr(sufrun)"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Haza yevmu la yentıkun(yentıkune)"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Ve la yu’zenu lehum fe ya’tezirun(ya’tezirune)"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Haza yevmul fasl(fasli), cema’nakum vel evvelin(evveline)"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Fe in kane lekum keydun fe kidun(kiduni)"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Innel muttekine fi zılalin ve uyun(uyunin)"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Ve fevakihe mimma yestehun(yestehune)"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Kulu vesrebu henien bima kuntum ta’melun(ta’melune)"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Kulu ve temetteu kalilen innekum mucrimun(mucrimune)"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Ve iza kile lehumurkeu la yerkeun(yerkeune)"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Veylun yevmeizin lil mukezzibin(mukezzibine)"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Fe bi eyyi hadisin ba’dehu yu’minun(yu’minune)"
        }
    ]
}