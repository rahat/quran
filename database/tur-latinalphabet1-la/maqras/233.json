{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amme yetesaelun(yetesaelune)"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Anin nebeil azim(azimi)"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Ellezi hum fihi muhtelifun(muhtelifune)"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kella se ya’lemun(ya’lemune)"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Summe kella se ya’lemun(ya’lemune)"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "E lem nec’alil arda mihada(mihaden)"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Vel cibale evtada(evtaden)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ve halaknakum ezvaca(ezvacen)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ve cealna nevmekum subata(subaten)"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Ve cealnel leyle libasa(libasen)"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ve cealnen nehare measa(measen)"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Ve beneyna fevkakum seb´an sidada(sidaden)"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ve cealna siracen vehhaca(vehhacen)"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ve enzelna minel mu’sırati maen seccaca(seccacen)"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Li nuhrice bihi habben ve nebata(nebaten)"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ve cennatin elfafa(elfafen)"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inne yevmel faslı kane mikata(mikaten)"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yevme yunfehu fis suri fe te’tune efvaca(efvacen)"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Ve futihatis semau fe kanet ebvaba(ebvaben)"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Ve suyyiretil cibalu fe kanet seraba(seraben)"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inne cehenneme kanet mirsada(mirsaden)"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lit tagine meaba(meaben)"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Labisine fiha ahkaba(ahkaben)"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "La yezukune fiha berden ve la seraba(seraben)"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illa hamimen ve gassaka(gassakan)"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Cezaen vifaka(vifakan)"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innehum kanu la yercune hısaba(hısaben)"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ve kezzebu bi ayatina kizzaba(kizzaben)"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Ve kulle sey’in ahsaynahu kitaba(kitaben)"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fe zuku felen nezidekum illa azaba(azaben)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inne lil muttekine mefaza(mefazen)"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadaika ve a’naba(a’naben)"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ve kevaıbe etraba(etraben)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ve ke’sen dihaka(dihakan)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "La yes’meune fiha lagven ve la kizzaba(kizzaben)"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Cezaen min rabbike ataen hısaba(hısaben)"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis semavati vel ardı ve ma beynehumer rahmani la yemlikune minhu hitaba(hitaben)"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yevme yekumur ruhu vel melaiketu saffa(saffen), la yetekellemune illa men ezine lehur rahmanu ve kale sevaba(sevaben)"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zalikel yevmul hakk(hakku), femen saettehaze ila rabbihi meaba(meaben)"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Inna enzernakum azaben kariba(kariben), yevme yenzurul mer’u ma kaddemet yedahu ve yekulul kafiru ya leyteni kuntu turaba(turaben)"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ven naziati garka(garkan)"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ven nasitati nesta(nestan)"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ves sabihati sebha(sebhan)"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fes sabikati sebka(sebkan)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fel mudebbirati emra(emren)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yevme tercufur racifeh(racifetu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tetbeuher radifeh(radifetu)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Kulubun yevmeizin vacifeh(vacifetun)"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ebsaruha hasiah(hasiatun)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yekulune e inna le merdudune fil hafireh(hafireti)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "E iza kunna izamen nahıreh(nahıreten)"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Kalu tilke izen kerretun hasireh(hasiretun)"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fe innema hiye zecretun vahıdeh(vahıdetun)"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fe iza hum bis sahireh(sahireti)"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hel etake hadisu musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Iz nadahu rabbuhu bil vadil mukaddesi tuva(tuven)"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Izheb ila fir’avne innehu taga"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Fe kul hel leke ila en tezekka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ve ehdiyeke ila rabbike fe tahsa"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fe erahul ayetel kubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fe kezzebe ve asa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Summe edbere yes’a"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fehasere fe nada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Fe kale ene rabbukumul a’la"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fe ehazehullahu nekalel ahıreti vel ula"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inne fi zalike le ıbreten li men yahsa"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "E entum eseddu halkan emis sema’(semau), benaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Refea semkeha fe sevvaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Ve agtase leyleha ve ahrece duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Vel arda ba’de zalike dehaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ahrece minha maeha ve mer’aha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Vel cibale ersaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Metaan lekum ve li en amikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fe iza caetit tammetul kubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yevme yetezekkerul insanu ma sea"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Ve burrizetil cahimu li men yera"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fe emma men taga"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ve aserel hayated dunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fe innel cahime hiyel me’va"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ve emma men hafe makame rabbihi ve nehennefse anil heva"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fe innel cennete hiyel me’va"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yes’eluneke anis saati eyyane mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fime ente min zikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbike muntehaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innema ente munziru men yahsaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ke ennehum yevme yerevneha lem yelbesu illa asiyyeten ev duhaha"
        }
    ]
}