{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Abese ve tevella"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "En caehul a’ma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ve ma yudrike leallehu yezzekka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ev yezzekkeru fe tenfeahuz zikra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Emma menistagna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Fe ente lehu tesadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ve ma aleyke ella yezzekka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ve emma men caeke yes’a"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ve huve yahsa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Fe ente anhu telehha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kella inneha tezkirah(tezkiratun)"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Fe men sae zekerah(zekerahu)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fi suhufin mukerrameh(mukerrametin)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Merfuatin mutahherah(mutahheratin)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi eydi seferah(seferatin)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kiramin berarah(beraratin)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kutilel insanu ma ekferah(ekferahu)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min eyyi sey’in halakah(halakahu)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min nutfeh(nutfetin), halakahu fe kadderah(kadderahu)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Summes sebile yesserah(yesserahu)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Summe ematehu fe akberah(akberahu)"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Summe iza sae enserah(enserahu)"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kella lemma yakdı ma emerah(emerahu)"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Felyanzuril insanu ila taamih(taamihi)"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Enna sabebnel mae sabba(sabben)"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Summe sekaknel arda sakka(sakkan)"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Fe enbetna fiha habba(habben)"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ve ineben ve kadba(kadben)"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ve zeytunen ve nahla(nahlen)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ve hadaika gulba(gulben)"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ve fakiheten ve ebba(ebben)"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Metaan lekum ve li en’amikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fe iza caetis sahhah(sahhatu)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yevme yefirrul mer’u min ehih(ehihi)"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ve ummihi ve ebih(ebihi)"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ve sahıbetihi ve benih(benihi)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Li kullimriin minhum yevmeizin se’nun yugnih(yugnihi)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Vucuhun yevmeizin musfirah(musfiratun)"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Dahıketun mustebsirah(mustebsiratun)"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ve vucuhun yevmeizin aleyha gaberah(gaberatun)"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Terhekuha katerah(kateratun)"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ulaike humul keferetul fecerah(feceratu)"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Izes semsu kuvviret"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Ve izen nucumun kederet"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Ve izelcibalu suyyiret"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Ve izel ısaru uttılet"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Ve izel vuhusu husiret"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ve izel biharu succiret"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Ve izen nufusu zuvvicet"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Ve izel mev’udetu suilet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi eyyi zenbin kutilet"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ve izes suhufu nusiret"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Ve izes semau kusitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Ve izel cahimu su’ıret"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ve izel cennetu uzlifet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Alimet nefsun ma ahdaret"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Fe la uksimu bil hunnes(hunnesi)"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "El cevaril kunnes(kunnesi)"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Vel leyli iza as’as(as’ase)"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ves subhı iza teneffes(teneffese)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innehu le kavlu resulin kerim(kerimin)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Zi kuvvetin ınde zil arsi mekin(mekinin)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Mutaın semme emin(eminin)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ve ma sahıbukum bi mecnun(mecnunin)"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ve lekad reahu bil ufukıl mubin(mubini)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ve ma huve alel gaybi bi danin(daninin)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ve ma huve bi kavli seytanin recim(recimin)"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Fe eyne tezhebun(tezhebune)"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In huve illa zikrun lil alemin(alemine)"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Li men sae minkum en yestekim(yestekime)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ve ma tesaune illa en yesaallahu rabbul alemin(alemine)"
        }
    ]
}