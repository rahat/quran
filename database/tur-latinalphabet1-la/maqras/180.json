{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min siatihi le ibrahim(ibrahime)"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selim(selimin)"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebihi ve kavmihi maza ta’budun(ta’budune)"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunallahi turidun(turidune)"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemin(alemine)"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nazara nazraten fin nucum(nucumi)"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale inni sakim(sakimun)"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirin(mudbirine)"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e la te’kulun(te’kulune)"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun(tentıkune)"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darben bil yemin(yemini)"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun(yeziffune)"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta’budune ma tenhıtun(tenhıtune)"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halakakum ve ma ta’melun(ta’melune)"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihi keyden fe cealna humul esfelin(esfeline)"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale inni zahibun ila rabbi seyehdin(seyehdini)"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb li mines salihin(salihine)"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halim(halimin)"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Fe lemma belega meahus sa’ye kale ya buneyye inni era fil menami enni ezbehuke fanzur maza tera, kale ya ebetif’al ma tu’meru seteciduni insaallahu mines sabirin(sabirine)"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Fe lemma eslema ve tellehu lil cebin(cebini)"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu en ya ibrahim(ibrahimu)"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru’ya, inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubin(mubinu)"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azim(azimin)"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahim(ibrahime)"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyen mines salihin(salihine)"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barekna aleyhi ve ala ishak(ishaka), ve min zurriyyetihima muhsinun ve zalimun li nefsihi mubin(mubinun)"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve lekad menenna ala musa ve harun(harune)"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceyna huma ve kavme huma minel kerbil azim(azimi)"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibin(galibine)"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateyna humel kitabel mustebin(mustebine)"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeyna humes sıratal mustekim(mustekime)"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terekna aleyhima fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun(harune)"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Inne huma min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihi e la tettekun(tettekune)"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Eted’une ba’len ve tezerune ahsenel halikin(halikine)"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelin(evveline)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe inne hum le muhdarun(muhdarune)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasin(ilyasine)"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutan le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmain(ecmaine)"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirin(gabirine)"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharin(aharine)"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihin(musbihine)"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl(leyli), e fe la ta’kılun(ta’kılune)"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun(meshuni)"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadin(mudhadine)"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulim(mulimun)"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihin(musebbihine)"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fi batnihi ila yevmi yub’asun(yub’asune)"
        }
    ]
}