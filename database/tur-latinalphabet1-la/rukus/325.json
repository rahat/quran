{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabul eyketil murselin(murseline)"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Iz kale lehum suaybun e la tettekun(tettekune)"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Inni lekum resulun emin(eminun)"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es’elukum aleyhi min ecr(ecrin), in ecriye illa ala rabbil alemin(alemine)"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Evful keyle ve la tekunu minel muhsirin(muhsirine)"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Vezinu bil kıstasil mustekim(mustekimi)"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Ve la tebhasun nase esyaehum ve la ta’sev fil ardı mufsidin(mufsidine)"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekullezi halakakum vel cibilletel evvelin(evveline)"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kalu innema ente minel musahharin(musahharine)"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve ma ente illa beserun misluna ve in nazunnuke le minel kazibin(kazibine)"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleyna kisefen mines semai in kunte mines sadıkin(sadıkine)"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kale rabbi a’lemu bi ma ta’melun(ta’melune)"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebuhu fe ehazehum azabu yevmiz zulleh(zulleti), innehu kane azabe yevmin azim(azimin)"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inne fi zalike le ayeh(ayeten), ve ma kane ekseruhum mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le huvel azizur rahim(rahimu)"
        }
    ]
}