{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmu lutınil murselin(murseline)"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Iz kale lehum ehuhum lutun e la tettekun(tettekune)"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Inni lekum resulun emin(eminun)"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve ma es’elukum aleyhi min ecr(ecrin), in ecriye illa ala rabbil alemin(alemine)"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te’tunez zukrane minel alemin(alemine)"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerune ma halaka lekum rabbukum min ezvacikum, bel entum kavmun adun(adune)"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kalu le in lem tentehi ya lutu le tekunenne minel muhracin(muhracine)"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kale inni li amelikum minel kalin(kaline)"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccini ve ehli mimma ya’melun(ya’melune)"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynahu ve ehlehu ecmain(ecmaine)"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa acuzen fil gabirin(gabirine)"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Summe demmernel aharin(aharine)"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarna aleyhim matara(mataran), fe sae matarul munzerin(munzerine)"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inne fi zalike le ayeh(ayeten), ve ma kane ekseruhum mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le huvel azizur rahim(rahimu)"
        }
    ]
}