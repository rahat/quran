{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fe ersele fir’avnu fil medaini hasirin(hasirine)"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inne haulai le sirzimetun kalilun(kalilune)"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve innehum lena le gaizun(gaizune)"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Ve inna le cemiun hazirun(hazirune)"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fe ahracnahum min cennatin ve uyun(uyunin)"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Ve kunuzin ve makamin kerim(kerimin)"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kezalik(kezalike), ve evresnaha beni israil(israile)"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fe etbeuhum musrikin(musrikine)"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Fe lemma terael cem’ani kale ashabu musa inna le mudrakun(mudrakune)"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Kale kella, inne maiye rabbi seyehdin(seyehdini)"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fe evhayna ila musa enıdrib bi asakel bahr(bahra), fenfeleka fe kane kullu firkın ket tavdil azim(azimi)"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Ve ezlefna semmel aharin(aharine)"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Ve enceyna musa ve men meahu ecmain(ecmaine)"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Summe agraknel aharin(aharine)"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inne fi zalike le ayeh(ayeten), ve ma kane ekseruhum mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve inne rabbeke le huvel azizur rahim(rahimu)"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vetlu aleyhim nebee ibrahim(ibrahime)"
        }
    ]
}