{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Izes semaunfetaret"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Ve izel kevakibunteseret"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ve izel biharu fucciret"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ve izel kuburu bu’siret"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Alimet nefsun ma kaddemet ve ahharet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya eyyuhel insanu ma garreke bi rabbikel kerim(kerimi)"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ellezi halakake fe sevvake fe adelek(adeleke)"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fi eyyi suretin ma sae rekkebek(rekkebeke)"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kella bel tukezzibune bid din(dini)"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ve inne aleykum le hafızin(hafızine)"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiramen katibin(katibine)"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ya’lemune ma tef’alun(tef’alune)"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Innel ebrare lefi naim(naimin)"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ve innel fuccare lefi cahim(cahimın)"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslevneha yevmed din(dini)"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ve ma hum anha bi gaibin(gaibine)"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ve ma edrake ma yevmud din(dini)"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Summe ma edrake ma yevmud din(dini)"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yevme la temliku nefsun li nefsin sey’a(sey’en), vel emru yevmeizin lillah(lillahi)"
        }
    ]
}