{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Vez zariyati zerven"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Fel hamilati vırken"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Fel cariyati yusren"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Fel mukassimati"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Innema tuadune le sadikun"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Ve inned dine le vakıu(vakıun)"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Ves semai zatil hubuki"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Innekum le fi kavlin muhtelifin"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Yu’feku anhu men ufik(ufike)"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Kutilel harrasune"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Ellezine hum fi gamretin sahune"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Yes’elune eyyane yevmud din(dini)"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Yevme hum alen nari yuftenune"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Zuku fitnetekum, hazellezi kuntum bihi testa’cilun(testa’cilune)"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Innel muttekine fi cennatin ve uyunin"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Ahizine ma atahum rabbuhum, innehum kanu kable zalike muhsinin(muhsinine)"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Kanu kalilen minel leyli ma yehceun(yehceune)"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Ve bil esharihum yestagfirune"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Ve fi emvalihim hakkun lis saili vel mahrumi"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Ve fil ardı ayatun lil mukınine"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Ve fi enfusikum, e fe la tubsirun(tubsirune)"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Ve fis semai rızkukum ve ma tuadun(tuadune)"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Fe ve rabbis semai vel ardı innehu le hakkun misle ma ennekum tentıkun(tentıkune)"
        }
    ]
}