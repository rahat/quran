{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kezzebet kavmu nuhınil murselin(murseline)"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Iz kale lehum ehuhum nuhun e la tettekun(tettekune)"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Inni lekum resulun emin(eminun)"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Ve ma es’elukum aleyhi min ecr(ecrin), in ecriye illa ala rabbil alemin(alemine)"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Kalu e nu’minu leke vettebeakel erzelun(erzelune)"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Kale ve ma ilmi bima kanu ya’melun(ya’melune)"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hısabuhum illa ala rabbi lev tes’urun(tes’urune)"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ve ma ene bi taridil mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In ene illa nezirun mubin(mubinun)"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Kalu le in lem tentehi ya nuhule tekunenne minel mercumin(mercumine)"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Kale rabbi inne kavmi kezzebun(kezzebuni)"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Feftah beyni ve beynehum fethan ve neccini ve men maiye minel mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fe enceynahu ve men meahu fil fulkil meshun(meshuni)"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Summe agrakna ba’dul bakin(bakine)"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inne fi zalike le ayeh(ayeten), ve ma kane ekseruhum mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ve inne rabbeke le huvel azizur rahim(rahimu)"
        }
    ]
}