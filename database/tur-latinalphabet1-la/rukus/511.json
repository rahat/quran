{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kella vel kamer(kameri)"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Vel leyli iz edber(edbere)"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ves subhı iza esfer(esfere)"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Inneha le ıhdel kuber(kuberi)"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Neziren lil beser(beseri)"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Li men sae minkum en yetekaddeme ev yeteahhar(yeteahhare)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nefsin bima kesebet rehineh(rehinetun)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa ashabel yemin(yemini)"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fi cennat(cennatin), yetesaelun(yetesaelune)"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Anil mucrimin(mucrimine)"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma selekekum fi sekar(sekare)"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Kalu lem neku minel musallin(musalline)"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Ve lem neku nut’ımul miskin(miskine)"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Ve kunna nehudu maal haidin(haidine)"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ve kunna nukezzibu bi yevmid din(dini)"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta etanel yakin(yakinu)"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fe ma tenfeuhum sefaatus safiin(safiine)"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fe ma lehum anit tezkireti mu’rıdin(mu’rıdine)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ke ennehum humurun mustenfireth(mustenfiretun)"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Ferret min kasvereh(kasveretin)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bel yuridu kullumriin minhum en yu’ta suhufen munessereh (munessereten)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kella, bel la yuhafunel ahıreh(ahıreten)"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kella innehu tezkireh(tezkiretun)"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fe men sae zekereh(zekerehu)"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ve ma yezkurune illa en yesaallah(yesaallahu), huve ehlut takva ve ehlul magfireh(magfireti)"
        }
    ]
}