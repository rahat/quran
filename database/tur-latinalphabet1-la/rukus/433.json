{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Inne seceretez zakkum(zakkumi)"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Taamul esim(esimi)"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kel muhl(muhli), yagli fil butun(butuni)"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Ke galyil hamim(hamimi)"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Huzuhu fa’tiluhu ila sevail cahim(cahimi)"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Summe subbu fevka re’sihi min azabil hamim(hamimi)"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Zuk, inneke entel azizul kerim(kerimu)"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Inne haza ma kuntum bihi temterun(temterune)"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Innel muttekine fi makamin emin(eminin)"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fi cennatin ve uyun(uyunin)"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yelbesune min sundusin ve istebrakın mutekabilin(mutekabiline)"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kezalik(kezalike), ve zevvecnahum bi hurin in(inin)"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Yed’une fiha bi kulli fakihetin aminin(aminine)"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "La yezukune fihel mevte illel mevtetel ula, ve vekahum azabel cahim(cahimi)"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fadlen min rabbik(rabbike), zalike huvel fevzul azim(azimu)"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fe innema yessernahu bi lisanike leallehum yetezekkerun(yetezekkerune)"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Fertekib innehum murtekıbun(murtekibune)"
        }
    ]
}