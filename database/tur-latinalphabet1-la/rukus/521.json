{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "E entum eseddu halkan emis sema’(semau), benaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Refea semkeha fe sevvaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Ve agtase leyleha ve ahrece duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Vel arda ba’de zalike dehaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ahrece minha maeha ve mer’aha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Vel cibale ersaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Metaan lekum ve li en amikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fe iza caetit tammetul kubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yevme yetezekkerul insanu ma sea"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Ve burrizetil cahimu li men yera"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fe emma men taga"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ve aserel hayated dunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fe innel cahime hiyel me’va"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ve emma men hafe makame rabbihi ve nehennefse anil heva"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fe innel cennete hiyel me’va"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yes’eluneke anis saati eyyane mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fime ente min zikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbike muntehaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innema ente munziru men yahsaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ke ennehum yevme yerevneha lem yelbesu illa asiyyeten ev duhaha"
        }
    ]
}