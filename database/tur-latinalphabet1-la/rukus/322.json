{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kezzebet adunil murselin(murseline)"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Iz kale lehum ehuhum hudun e la tettekun(tettekune)"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Inni lekum resulun emin(eminun)"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Ve ma es’elukum aleyhi min ecr(ecrin), in ecriye illa ala rabbil alemin(alemine)"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "E tebnune bi kulli riın ayeten ta’besun(ta’besune)"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Ve tettehızune mesania leallekum tahludun(tahludune)"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Ve iza betastum betastum cebbarin(cebbarine)"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fettekullahe ve etiun(etiuni)"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Vettekullezi emeddekum bima ta’lemun(ta’lemune)"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Emeddekum bi en’amin ve benin(benine)"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Ve cennatin ve uyun(uyunin)"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Inni ehafu aleykum azabe yevmin azim(azimin)"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Kalu sevaun aleyna e vaazte em lem tekun minel vaızin(vaızine)"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In haza illa hulukul evvelin(evveline)"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Ve ma nahnu bi muazzebin(muazzebine)"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fe kezzebuhu fe ehleknahum, inne fi zalike le ayeh(ayeten), ve ma kane ekseruhum mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Ve inne rabbeke le huvel azizur rahim(rahimu)"
        }
    ]
}