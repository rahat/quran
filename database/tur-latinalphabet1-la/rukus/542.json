{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Vel adiyati dabha(dabhan)"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fel muriyati kadha(kadhan)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fel mugirati subha(subhan)"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fe eserne bihi nak’a(nak’en)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fe vesatne bihi cem’a(cem’an)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Innel insane li rabbihi le kenud(kenudun)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve innehu ala zalike le sehid(sehidun)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve innehu li hubbil hayri le sedid(sedidun)"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "E fe la ya’lemu iza bu’sirama fil kubur(kuburi)"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve hussıle ma fis sudur(suduri)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inne rabbehum bihim yevme izin le habir(habirun)"
        }
    ]
}