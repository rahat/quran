{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kul euzu bi rabbil felak(felakı)"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Min serri ma halak(halaka)"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ve min serri gasikın iza vekab(vekabe)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve min serrin neffasati fil ukad(ukadi)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve min serri hasidin iza hased(hasede)"
        }
    ]
}