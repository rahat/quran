{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Izes semsu kuvviret"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Ve izen nucumun kederet"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Ve izelcibalu suyyiret"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Ve izel ısaru uttılet"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Ve izel vuhusu husiret"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ve izel biharu succiret"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Ve izen nufusu zuvvicet"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Ve izel mev’udetu suilet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi eyyi zenbin kutilet"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ve izes suhufu nusiret"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Ve izes semau kusitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Ve izel cahimu su’ıret"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ve izel cennetu uzlifet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Alimet nefsun ma ahdaret"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Fe la uksimu bil hunnes(hunnesi)"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "El cevaril kunnes(kunnesi)"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Vel leyli iza as’as(as’ase)"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ves subhı iza teneffes(teneffese)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innehu le kavlu resulin kerim(kerimin)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Zi kuvvetin ınde zil arsi mekin(mekinin)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Mutaın semme emin(eminin)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ve ma sahıbukum bi mecnun(mecnunin)"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ve lekad reahu bil ufukıl mubin(mubini)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ve ma huve alel gaybi bi danin(daninin)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ve ma huve bi kavli seytanin recim(recimin)"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Fe eyne tezhebun(tezhebune)"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In huve illa zikrun lil alemin(alemine)"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Li men sae minkum en yestekim(yestekime)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ve ma tesaune illa en yesaallahu rabbul alemin(alemine)"
        }
    ]
}