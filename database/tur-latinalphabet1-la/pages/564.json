{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "Fe lemma reevhu zulfeten siet vucuhullezine keferu ve kile hazellezi kuntum bihi teddeun(teddeune)"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "Kul ereeytum in ehlekeniyallahu ve men maıye ev rahımena fe men yucirul kafirine min azabin elim(elimin)"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "Kul huver rahmanu amenna bihi ve aleyhi tevekkelna, fe se ta’lemune men huve fi dalalin mubin(mubinin)"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "Kul e re’eytum in asbaha maukum gavren fe men ye’tikum bi main main(mainin)"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "Nun vel kalemi ve ma yesturun(yesturune)"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Ma ente bi ni’meti rabbike bi mecnun(mecnunin)"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Ve inne leke le ecren gayre memnun(memnunin)"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Ve inneke le ala hulukın azim(azimin)"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fe se tubsıru ve yubsırun(yubsırune)"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi eyyikumul meftun(meftunu)"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inne rabbeke huve a’lemu bi men dalle an sebilihi ve huve a’lemu bil muhtedin(muhtedine)"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fe la tutııl mukezzibin(mukezzibine)"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Veddu lev tudhinu fe yudhinun(yudhinune)"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Ve la tutı’ kulle hallafin mehin(mehinin)"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hemmazin messain bi nemim(nemimin)"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "Mennaın lil hayri mu’tedin esim(esimin)"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "Utullin ba’de zalike zenim(zenimin)"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "En kane za malin ve benin(benine)"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Iza tutla aleyhi ayatuna kale esatirul evvelin(evveline)"
        }
    ]
}