{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassarunehum yeveddul mucrimu lev yeftedi min azabi yevmi izin bi benih(benihi)"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Ve sahıbetihi ve ahih(ahihi)"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ve fasiletihilleti tu’vih(tu’vihi)"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Ve men fil ardı cemi’an summe yuncih(yuncihi)"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kella, inneha leza"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nezzaaten lis seva"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Ted’u men edbera ve tevella"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ve cemea fe ev’a"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Innel insane hulika helua(heluan)"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Iza messehus serru cezua(cezuan)"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Ve iza messehul hayru menua(menuan)"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illel musallin(musalline)"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Ellezine hum ala salatihim daimun(daimune)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Vellezine fi emvalihim hakkun ma’lum(ma’lumun)"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lis saili vel mahrum(mahrumi)"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Vellezine yusaddikune bi yevmid din(dini)"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Vellezine hum min azabi rabbihim musfikun(musfikune)"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inne azabe rabbihim gayru me’mun(me’munin)"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Vellezine hum li furucihim hafizun(hafizune)"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illa ala ezvacihim ev ma meleket eymanuhum fe innehum gayru melumin(melumine)"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Fe menibtega verae zalike fe ulaike humul adun(adune)"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Vellezine hum li emanatihim ve ahdihim raun(raune)"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Vellezine hum bi sehadatihim kaimun(kaimune)"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Vellezine hum ala salatihim yuhafizun(yuhafizune)"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ulaike fi cennatin mukremun(mukremune)"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Fe ma lillezine keferu kıbeleke muhtıin(muhtıine)"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Anil yemini ve anis simali ızin(ızine)"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "E yatmeu kullumriin minhum en yudhale cennete naim(naimin)"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kella, inna halaknahum mimma ya’lemun(ya’lemune)"
        }
    ]
}