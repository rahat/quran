{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Kezalike ma etellezine min kablihim min resulin illa kalu sahırun ev mecnun(mecnunun)"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "E tevasav bih(bihi), bel hum kavmun tagun(tagune)"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "Fe tevelle anhum fe ma ente bi melum(melume)"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Ve zekkir fe innez zikra tenfeul mu’minin(mu’minine)"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Ve ma halaktul cinne vel inse illa li ya´budun(ya´buduni)"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Ma uridu minhum min rızkın ve ma uridu en yut’imuni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Innallahe huver rezzaku zul kuvvetil metin(metinu)"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Fe inne lillezine zalemu zenuben misle zenubi ashabihim fe la yesta’ciluni"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Fe veylun lillezine keferu min yevmihimullezi yuadun(yuadune)"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Vet TURi"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Ve kitabin mesTURin"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fi rakkın mensurin"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Vel beytil ma’muri"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Ves sakfil merfui"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Vel bahril mescuri"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inne azabe rabbike le vakı’un"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Ma lehu min dafiin"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yevme temurus semau mevren"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Ve tesirul cibalu seyra(seyren)"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fe veylun yevme izin lil mukezzibine"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Ellezine hum fi havdın yel’abun(yel’abune)"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yevme yude’une ila nari cehenneme de’a(de’an)"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hazihin narulleti kuntum biha tukezzibun(tukezzibune)"
        }
