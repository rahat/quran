{
    "pages": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "Ha mim"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "Vel kitabil mubin(mubini)"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "Inna enzelnahu fi leyletin mubareketin inna kunna munzirin(munzirine)"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "Fiha yufreku kullu emrin hakim(hakimin)"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "Emren min indina inna kunna mursilin(mursiline)"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "Rahmeten min rabbik(rabbike), innehu huves semiul alim(alimu)"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "Rabbis semavati vel ardı ve ma beynehuma, in kuntum mukinin(mukinine)"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "La ilahe illa huve yuhyi ve yumit(yumitu), rabbukumve rabbu abaikumul evvelin(evveline)"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "Bel hum fi sekkin yel’abun(yel’abune)"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "Fertekib yevme te’tis semau bi duhanin mubin(mubinin)"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "Yagsan nas(nase), haza azabun elim(elimun)"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "Rabbeneksif annel azabe inna mu’minun(mu’minune)"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "Enna lehumuz zikra ve kad caehum resulun mubin(mubinun)"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "Summe tevellev anhu ve kalu muallemun mecnun(mecnunun)"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "Inna kasiful azabi kalilen innekum aidun(aidune)"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "Yevme nebtısul batsetel kubra inna muntekimun(muntekimune)"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "Ve lekad fetenna kablehum kavme fir’avne ve caehum resulun kerim(kerimun)"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "En eddu ileyye ibadallah(ibadallahi), inni lekum resulun emin(eminun)"
        }
    ]
}