{
    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma agna anhum ma kanu yumetteun(yumetteune)"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Ve ma ehlekna min karyetin illa leha munzirun(munzirune)"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Zikra, ve ma kunna zalimin(zalimine)"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Ve ma tenezzelet bihis seyatin(seyatinu)"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Ve ma yenbagi lehum ve ma yestetiun(yestetiune)"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innehum anis sem’i le ma’zulun(ma’zulune)"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fe la ted’u meallahi ilahen ahara fe tekune minel muazzebin(muazzebine)"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Ve enzir asiretekel akrebin(akrebine)"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Vahfıd cenahake li menittebeake minel mu’minin(mu’minine)"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fe in asavke fe kul inni beriun mimma ta’melun(ta’melune)"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Ve tevekkel alel azizir rahim(rahimi)"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Ellezi yerake hine tekum(tekumu)"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Ve tekallubeke fis sacidin(sacidine)"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innehu huves semiul alim(alimu)"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hel unebbiukum ala men tenezzelus seyatin(seyatinu)"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tenezzelu ala kulli effakin esim(esimin)"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulkunes sem’a ve ekseruhum kazibun(kazibune)"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Ves suarau yettebiuhumul gavun(gavune)"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "E lem tera ennehum fi kulli vadin yehimun(yehimune)"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Ve ennehum yekulune ma la yef’alun(yef’alune)"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illellezine amenu ve amilus salihati ve zekerullahe kesiran ventesaru min ba’di ma zulimu, ve se ya’lemullezine zalemu eyye munkalebin yenkalibun(yenkalibune)"
        }
    ]
}