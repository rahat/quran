{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fe ma tenfeuhum sefaatus safiin(safiine)"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fe ma lehum anit tezkireti mu’rıdin(mu’rıdine)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ke ennehum humurun mustenfireth(mustenfiretun)"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Ferret min kasvereh(kasveretin)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bel yuridu kullumriin minhum en yu’ta suhufen munessereh (munessereten)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kella, bel la yuhafunel ahıreh(ahıreten)"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kella innehu tezkireh(tezkiretun)"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fe men sae zekereh(zekerehu)"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ve ma yezkurune illa en yesaallah(yesaallahu), huve ehlut takva ve ehlul magfireh(magfireti)"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "La uksimu bi yevmil kıyameh(kıyameti)"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Ve la uksimu bin nefsil levvameh(levvameti)"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "E yahsebul insanu ellen necmea ıza meh(mehu)"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bela kadirine ala en nusevviye bena neh(nehu)"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bel yuridul insanu li yefcure emameh(emamehu)"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yes’elu eyyane yevmul kıyameh(kıyameti)"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fe iza berikal basar(basaru)"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Ve hasefel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Ve cumias semsu vel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yekulul insanu yevme izin eynel meferr(meferru)"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kella la vezer(vezere)"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila rabbike yevme izinil mustekar(mustekarru)"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunebbeul insanu yevme izin bima kaddeme ve ahhar(ahhare)"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Belil insanu ala nefsihi basireth(basiretun)"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Ve lev elka meazireh(meazirehu)"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La tuharrik bihi lisaneke li ta’cele bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inne aleyna cem’ahu ve kur’anehu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fe iza kara’nahu fettebi’kur’anehu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summe inne aleyna beyanehu"
        }
    ]
}