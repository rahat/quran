{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Ve enahtertuke festemi’ li ma yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Inneni enallahu la ilahe illa ene fa’budni ve ekımis salate li zikri"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Innes saate atiyetun ekadu uhfiha li tucza kullu nefsin bima tes’a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fe la yesuddenneke anha men la yu’minu biha vettebea hevahu fe terda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Ve ma tilke bi yeminike ya musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Kale hiye asay(asaye), etevekkeu aleyha ve ehussu biha ala ganemi ve liye fiha mearibu uhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Kale elkıha ya musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fe elkaha fe iza hiye hayyetun tes’a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Kale huzha ve la tehaf se nuiduha siretehel ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Vadmum yedeke ila cenahıke tahruc beydae min gayri suin ayeten uhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Li nuriyeke min ayatinel kubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Izheb ila fir’avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Kale rabbisrah li sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Ve yessir li emri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Vahlul ukdeten min lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yefkahu kavli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Vec’al li veziren min ehli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Harune ahi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Usdud bihi ezri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Ve esrikhu fi emri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Key nusebbihake kesira(kesiren)"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Ve nezkureke kesira(kesiren)"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Inneke kunte bina basira(basiren)"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Kale kad utite su’leke ya musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Ve lekad menenna aleyke merreten uhra"
        }
    ]
}