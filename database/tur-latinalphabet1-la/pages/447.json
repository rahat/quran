{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma lekum la tenasarun(tenasarune)"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimun(musteslimune)"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba’duhum ala ba’dın yetesaelun(yetesaelune)"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kalu innekum kuntum te’tunena anil yemin(yemini)"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kalu bel lem tekunu mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve ma kane lena aleykum min sultan(sultanin), bel kuntum kavmen tagin(tagine)"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleyna kavlu rabbina inna le zaıkun(zaıkune)"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynakum inna kunna gavin(gavine)"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevme izin fil azabi musterikun(musterikune)"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kezalike nef’alu bil mucrimin(mucrimine)"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innehum kanu iza kile lehum la ilahe illallahu yestekbirun(yestekbirune)"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekulune e inna le tariku alihetina li sairin mecnun(mecnunin)"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel cae bil hakkı ve saddakal murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innekum le zaikul azabil elim(elimi)"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve ma tuczevne illa ma kuntum ta’melun(ta’melune)"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa ibadallahil muhlesin(muhlesine)"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulaike lehum rizkun ma’lum(ma’lumun)"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevakih(fevakihu), ve hum mukremun(mukremune)"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fi cennatin naim(naimi)"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala sururin mutekabilin(mutekabiline)"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu aleyhim bi ke’sin min main(mainin)"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydae lezzetin lis saribin(saribine)"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La fiha gavlun ve la hum anha yunzefun(yunzefune)"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve indehum kasıratut tarfı in(inun)"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke enne hunne beydun meknun(meknunun)"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba’duhum ala ba’dın yetesaelun(yetesaelune)"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kale kailun minhum inni kane li karin(karinun)"
        }
    ]
}