{
    "pages": [
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealna zurriyyetehu humul bakin(bakine)"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selamun ala nuhın fil alemin(alemine)"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Summe agraknel aharin(aharine)"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min siatihi le ibrahim(ibrahime)"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selim(selimin)"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebihi ve kavmihi maza ta’budun(ta’budune)"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunallahi turidun(turidune)"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemin(alemine)"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nazara nazraten fin nucum(nucumi)"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale inni sakim(sakimun)"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirin(mudbirine)"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e la te’kulun(te’kulune)"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun(tentıkune)"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darben bil yemin(yemini)"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun(yeziffune)"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta’budune ma tenhıtun(tenhıtune)"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halakakum ve ma ta’melun(ta’melune)"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihi keyden fe cealna humul esfelin(esfeline)"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale inni zahibun ila rabbi seyehdin(seyehdini)"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb li mines salihin(salihine)"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halim(halimin)"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Fe lemma belega meahus sa’ye kale ya buneyye inni era fil menami enni ezbehuke fanzur maza tera, kale ya ebetif’al ma tu’meru seteciduni insaallahu mines sabirin(sabirine)"
        }
    ]
}