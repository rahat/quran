{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Kad eflehal mu’minun(mu’minune)"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Ellezine hum fi salatihim hasiun(hasiune)"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Vellezine hum anil lagvi mu’ridun(mu’ridune)"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Vellezine hum liz zekati failun(failune)"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Vellezine hum li furucihim hafizun(hafizune)"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Illa ala ezvacihim ev ma meleket eymanuhum fe innehum gayru melumin(melumine)"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Fe menibtega verae zalike fe ulaike humul adun(adune)"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Vellezine hum li emanatihim ve ahdihim raun(raune)"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Vellezine hum ala salavatihim yuhafızun(yuhafızune)"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Ulaike humul varisun(varisune)"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Ellezine yerisunel firdevs(firdevse), hum fiha halidun(halidune)"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Ve lekad halaknal insane min sulaletin min tin(tinin)"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Summe cealnahu nutfeten fi kararin mekin(mekinin)"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Summe halaknen nutfete alakaten fe halaknel alakate mudgaten fe halaknel mudgate ızamen fe kesevnel izame lahmen summe ense´nahu halkan ahar(ahara), fe tebarekallahu ahsenul halikin(halikine)"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Summe innekum ba´de zalike le meyyitun(meyyitune)"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Summe innekum yevmel kıyameti tub’asun(tub’asune)"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Ve lekad halakna fevkakum seb´a taraika ve ma kunna anil halkı gafilin(gafiline)"
        }
    ]
}