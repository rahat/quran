{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yetufu aleyhim vildanun muhalledun(muhalledune)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi ekvabin ve ebarika ve ke’sin min main(mainin)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "La yusaddeune anha ve la yunzifun(yunzifune)"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ve fakihetin mimma yetehayyerun(yetehayyerune)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ve lahmi tayrin mimma yestehun(yestehune)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ve hurun inun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ke emsalil lu’luil meknun(meknuni)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Cezaen bi ma kanu ya’melun(ya’melune)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "La yesmeune fiha lagven ve la te’sima(te’simen)"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Illa kilen selamen selama(selamen)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ve ashabul yemini ma ashabul yemin(yemini)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fi sidrin mahdud(mahdudin)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ve talhın mendud(mendudin)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ve zıllin memdud(memdudin)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ve main meskub(meskubin)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ve fakihetin kesirah(kesiretin)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "La maktuatin ve la memnuah(memnuatin)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve furusin merfuah(merfuatin)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Inna ense’na hunne insaa(insaen)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Fe cealna hunne ebkaran(ebkaren)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Uruben etraba(etraben)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li ashabil yemin(yemini)"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Sulletun minel evvelin(evveline)"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ve sulletun minel ahırin(ahırine)"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ve ashabus simali ma ashabus simal(simali)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fi semumin ve hamim(hamimin)"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Ve zıllin min yahmum(yahmumin)"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "La baridin ve la kerim(kerimin)"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innehum kanu kable zalike mutrefin(mutrefine)"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ve kanu yusirrune alel hınsil azim(azimi)"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ve kanu yekulune e iza mitna ve kunna turaben ve iza men e inna le meb’usun(meb’usune)"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "E ve abaunel evvelun(evvelune)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kul innel evveline vel ahirin(ahirine)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Le mecmuune ila mikati yevmin ma’lum(ma’lumin)"
        }
    ]
}