{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lekum, keyfe tahkumun(tahkumune)"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun(tezekkerune)"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultanun mubin(mubinun)"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe’tu bi kitabikum in kuntum sadikin(sadikine)"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba(neseben), ve lekad alimetil cinnetu innehum le muhdarun(muhdarune)"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhanallahi amma yasifun(yasifune)"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve ma ta’budun(ta’budune)"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entum aleyhi bi fatinin(fatinine)"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa men huve salil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehu makamun ma’lum(ma’lumun)"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnus saffun(saffune)"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnul musebbihun(musebbihune)"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun(yekulune)"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne indena zikren minel evvelin(evveline)"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Le kunna ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferu bih(bihi), fe sevfe ya’lemun(ya’lemune)"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve lekad sebekat kelimetuna li ibadinel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innehum le humul mensurun(mensurune)"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundena le humul galibun(galibune)"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hatta hin(hinin)"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsirhum fe sevfe yubsirun(yubsirune)"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe bi azabina yesta’cilun(yesta’cilune)"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe iza nezele bisahatihim fe sae sabahul munzerin(munzerine)"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhum hatta hin(hinin)"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsir fe sevfe yubsirun(yubsırune)"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhane rabbike rabbil izzeti amma yasifun(yasifune)"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selamun alel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdu lillahi rabbil alemin(alemine)"
        }
    ]
}