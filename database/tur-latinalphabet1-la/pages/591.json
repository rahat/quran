{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ves semai vet tarık(tarıkı)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve ma edrake met tarik(tariku)"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "En necmus sakıb(sakıbu)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nefsin lemma aleyha hafız(hafızun)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fel yenzuril insanu mimme hulık(hulıka)"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hulika min main dafik(dafikın)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yahrucu min beynis sulbi vet teraib(teraibi)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innehu ala rec’ıhi le kadir(kadirun)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yevme tubles serair(serairu)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fema lehu min kuvvetin ve la nasır(nasırın)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ves semai zatir rec’(rec’ı)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Vel ardı zatis sad’(sad’ı)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innehu le kavlun fasl(faslun)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve ma huve bil hezl(hezli)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innehum yekidune keyda(keyden)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ekidu keyda(keyden)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Fe mehhilil kafirine emhilhum ruveyda(ruveyden)"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sebbihısme rabbikel a’la"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ellezi halaka fesevva"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vellezi kaddere fe heda"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vellezi ahrecel mer’a"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Fe cealehu gusaen ahva"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Senukriuke fe la tensa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illa ma saallah(saallahu), innehu ya’lemul cehre ve ma yahfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve nuyessiruke lil yusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fe zekkir in nefeatiz zikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Seyezzekkeru men yahsa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ve yetecennebuhel eska"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ellezi yaslen narel kubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summe la yemutu fiha ve la yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kad efleha men tezekka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ve zekeresme rabbihi fe salla"
        }
    ]
}