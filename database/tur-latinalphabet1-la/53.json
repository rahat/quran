{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ven necmi iza heva"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalle sahıbukum ve ma gava"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ve ma yentıku anil heva"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huve illa vahyun yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allemehu sedidul kuva"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Zu mirreh(mirretin), festeva"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Ve huve bil ufukil a’la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Summe dena fe tedella"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fe kane kabe kavseyni ev edna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fe evha ila abdihi ma evha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kezebel fuadu ma rea"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "E fe tumar runehu ala ma yera"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Ve lekad reahu nezleten uhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inde sidretil munteha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indeha cennetul me’va"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Iz yagses sidrete ma yagsa"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagal basaru ve ma tega"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Lekad rea min ayati rabbihil kubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "E fe reeytumul late vel uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Ve menates salisetel uhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "E lekumuz zekeru ve lehul unsa"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilke izen kısmetun diza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiye illa esmaun semmeytumuha entum ve abaukum ma enzelallahu biha min sultan(sultanin), in yettebiune illez zanne ve ma tehvel enfus(enfusu), ve lekad caehum min rabbihimul huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Em lil insani ma temenna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Fe lillahil ahiretu vel ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ve kem min melekin fis semavati la tugni sefaatuhum sey’en illa min ba’di en ye’zenallahu limen yesau ve yerda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Innellezine la yu’minune bil ahireti le yusemmunel melaikete tesmiyetel unsa"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Ve ma lehum bihi min ilm(ilmin), in yettebiune illez zann(zanne), ve innez zanne la yugni minel hakkı sey´a(sey’en)"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Fe a´rıd an men tevella an zikrina ve lem yurid illel hayated dunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Zalike mebleguhum minel ilm(ilmi), inne rabbeke huve a’lemu bi men dalle an sebilihi ve huve a’lemu bi menihteda"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Ve lillahi ma fis semavati ve ma fil ardı li yecziyellezine esau bima amilu ve yecziyellezine ahsenu bil husna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Ellezine yectenibune kebairel ismi vel fevahıse lemem(lememe), inne rabbeke vasiul magfireh(magfireti), huve a´lemu bikum iz enseekum minel ardı ve iz entum e cinnetun fi butuni ummehatikum, fe la tuzekku enfusekum, huve a´lemu bi menitteka"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "E fe re’eytellezi tevella"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Ve a’ta kalilen ve ekda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "E indehu ilmul gaybi fe huve yera"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Em lem yunebbe’ bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve ibrahimellezi veffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ella teziru vaziretun vizre uhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Ve en leyse lil insani illa ma sea"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ve enne sa’yehu sevfe yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Summe yuczahul cezael evfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Ve enne ila rabbikel munteha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ve ennehu huve adhake ve ebka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Ve ennehu huve emate ve ahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ve ennehu halakaz zevceyniz zekere vel unsa"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfetin iza tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Ve enne aleyhin nes’etel uhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Ve ennehu huve agna ve akna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Ve ennehu huve rabbus sı’ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ve ennehu ehleke adenil ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Ve semude fema ebka"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve kavme nuhın min kabl(kablu), innehum kanu hum azleme ve atga"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Vel mu’tefikete ehva"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Fe gassaha ma gassa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fe bi eyyi alai rabbike tetemara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Haza nezirun minen nuzuril ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Ezifetil azifeh(azifetu)"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Leyse leha min dunillahi kasifeh(kasifetun)"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "E fe min hazel hadisi ta’cebun(ta’cebune)"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ve tedhakune ve la tebkun(tebkune)"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve entum samidun(samidune)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fescudu lillahi va’budu. (SECDE AYETI)"
        }
    ]
}