{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Göğe ve Tarık'a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne olduğunu sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlığı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Üzerinde gözetleyici-koruyucu bulunmayan hiçbir nefis (kimse) yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "İnsan bir baksın, hangi şeyden yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Dökülüp atılan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(Bu su,) Bel kemiği ile kaburgalar arasında(ki organlar)dan çıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Şüphesiz (Allah), onu yeniden-döndürmeye güç yetirendir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların orta yere çıkarılacağı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun ne gücü vardır, ne yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Dönüşlü olan göğe andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Yarılan yere de"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Şüphesiz o (Kur'an), ayırdeden bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, bir şaka değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Doğrusu onlar, hileli bir düzen planlayıp kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir düzen kurup hazırlıyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen kafirlere bir mühlet ver, az bir süre tanı"
        }
    ]
}