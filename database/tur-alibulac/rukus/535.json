{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine andolsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Karanlığı iyice çöktüğü' zaman geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terk etmedi ve darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Şüphesiz senin için son olan, ilk olandan (ahiret dünyadan) daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana verecek, böylece sen hoşnut kalacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Bir yetim iken, seni bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni yol bilmez iken, 'doğru yola yöneltip iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Bir yoksul iken seni bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse, sakın yetimi üzüp-kahretme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "İsteyip-dileneni azarlayıp-çıkışma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetini durmaksızın anlat"
        }
    ]
}