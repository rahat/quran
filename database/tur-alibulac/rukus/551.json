            "verse": 1,
            "text": "De ki: \"Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin taptıklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptığıma siz tapacak değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de sizin taptıklarınıza tapacak değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim taptığıma tapacak değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
