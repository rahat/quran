    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Zeg: O ongeloovigen"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ik zal niet aanbidden wat gij aanbidt"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Noch zult gij aanbidden wat ik aanbid"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ik aanbid geenzins wat gij aanbidt"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Gij aanbidt niet wat ik aanbid"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Gij hebt uw Godsdienst, en ik heb mijn godsdienst"
        }
    ]
