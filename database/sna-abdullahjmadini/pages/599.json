{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Mubairo wavo unaTenzi vavo, Paradhiso rine nzizi dzinoerera pasi paro.Vachagara imomo zvachose. Allah vachafadzwa navo uye ivo vachafadzwa navo (Allah). Izvi ndezve uyo anotya Tenzi vake"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kana nyika yodengenyetswa nekudengenyeka kwayo"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Uye kana nyika yoburitsa misengwa yayo (zvayakaviga)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Uye munhu achati: “Chii chiri kunetsa pairi?”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Nezuva iroro ichataura mashoko ayo (zvaiitika pamusoro payo zvakanaka nezvakaipa)"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Nokuda kwekuti Tenzi vako vanenge vaiudza izvozvo"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Nezuva iroro vanhu vachaenda muzvikwata kuitira kuti vagotaridzwa mabasa avo"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Naizvozvo uyo anenge aita chakanaka chakafanana nehuremu hwekasvosve kamwechete achachiona"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Uye uyo anenge aita chakaipa chakafanana nehuremu hwekasvosve kamwechete achachiona"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ndinopika nemabhiza anomhanya achifemereka"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Achiburitsa zvimvari zvemoto (nemakumbo awo)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Uye achimhanyira kunorwa mambakwedza"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Uye achipfumbura huruva nawo"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Uye achipinda nawo mukati (memuvengi)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Zvirokwazvo, munhu haatendi Tenzi vake"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Uye zvirokwazvo iye ichapupu kune izvozvo (nemabasa ake)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Uye zvirokwazvo anosimbirira mukuda zvakanaka (hupfumi)"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Haazivi here kuti apo zviri mumakuva zvichaburitswa"
        }
    ]
}