{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "(Ndinopika) nemuonde, uye nemu orivhi"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Negomo reSinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Neguta iri (Makkah) rakachengetedzeka"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Zvirokwazvo, takasika munhu ane chimiro chakanaka"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Ndokubva tamudzikisira kusvika pakupedzisira"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Kusara kweavo vanotenda (muIslaam) uye vachiita mabasa akanaka. Vachawana mubairo usingaperi (Paradhiso)"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Nokudaro chii chinoita (iwe asingatendi) kuti urambe zuva rekutongwa"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Havasi here Allah mutongi vanoziva pamusoro pevatongi vose"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Verenga! Muzita raTenzi vako avo vakasika (zvinhu zvese)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Vakasika munhu kubva mubumbu reropa"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Verenga! Uye Tenzi vako ndivo mukomboreri mukuru"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Avo vakadzidzisa (kunyora) nechinyoreso"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Vakadzidzisa munhu izvo zvaakanga asingazivi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Asi, zvirokwazvo, munhu anodarika mirairo"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Nokuda kwekuti anozviona sekunge akapfuma"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Zvirokazvo, kuna Tenzi vako ndiko kune kudzokera"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Wakamuona here (Muhammad (SAW)) uyo anodzivirira (anorambidza - Abu Jahl)"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Muranda (Muhammad (SAW)) kana achinamata"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Wakamboona here kana iye (Muhammad (SAW)) ari kutungamirwa (naAllah)"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Kana ari kukomekedza kutya (Allah)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Wakamboona here kana iye (Abu Jahl) achiramba chokwadi (Qurâ€™aan) uye achifuratira"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Haazivi here kuti Allah vanoona (zvose)"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Asi, kana asina kusiya (Abu Jahl), tichamubata nebvudzi rekumberi"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Bvudzi repamberi (pemusoro) rinonyepa nekukanganisa"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Naizvozvo chimusiyai adaidze dare rake (revabetseri)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Isu tichadaidza vachengeti veGehena (kuti vaone chekuita naye)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kwete! (Muhammad (SAW)) usamuteerera (Abu Jahl). Pfugama uye uswedere pedyo (naAllah)"
        }
    ]
}