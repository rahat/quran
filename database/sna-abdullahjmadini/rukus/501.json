{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Uye ndinopika neizvo zvose zvamunoona"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Uyezve neizvo zvose zvamusingaoni"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Kuti zvirokwazvo, ishoko remutumwa anoremekedzwa (Gabriel kana kuti Muhammad(SAW) raaunza kubva kuna Allah"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Haasi mashoko anyanduri. Zvishoma ndizvo zvamunotenda"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Kana kuti mashoko emufemberi. Zvishoma ndizvo zvamunorangarira"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "(Aya mashoko) akadzikiswa kubva kuna Tenzi vezvinhu zvese"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Dai (Muhammad (SAW)) aitaura mashoko enhema maererano nesu (Allah)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Zvirokwazvo taimubata neruoko rwake rwerudyi"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Uye tomudambura tsinga dzake dzeupenyu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Uye hapana mumwechete wenyu aikwanisa kutitadzisa kumuranga"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Uye zvirokwazvo, iri (Qur’aan) murangaridzo wevatsvene (Qur’aan)"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Uye zvirokwazvo, tinoziva kuti pakati penyu pane vamwe vanoriramba"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Uye zvirokwazvo, iri (Qur’aan) richava chirwadzo kune avo vasingatendi (Pazuva rekutongwa)"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Uye zvirokwazvo, iri (Qur’aan) ichokwadi chiri pachena"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Nokudaro rumbidza zita raTenzi vako, mukuru mukuru wezvose"
        }
    ]
}