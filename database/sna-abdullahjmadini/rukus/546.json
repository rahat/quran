{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Matambudziko kune wese anonyepera vamwe uye anonyeya"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Uyo akaunganidza hupfumi uye akahuverenga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Anofunga kuti hupfumi hwake huchamuita kuti agare nokusingaperi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kwete! Achakandwa mumoto unoparadza"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Uye chii chichaita kuti uzive kuiti moto unoparadza chii"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Moto waAllah wakatungidzwa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Uyo unopisa pamusoro pemoyo"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Zvirokwazvo, uchavharwa pamusoro pavo"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Mumitsigo yakatambanudzwa (kuti vacharangwa mumoto une mitsigo)"
        }
    ]
}