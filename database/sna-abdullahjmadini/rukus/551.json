{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Iti (Iwe Muhammad (SAW)): “Imi vasingatendi (muna Allah, muhumwechete wavo, ngirozi dzavo, Mabhuku avo, vatumwa vavo, muzuva rekutongwa, uye kuti zvose zvinoitika nokuda kwavo (Allah)"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ini handinamati izvo zvamunonamata"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Uye imi hamunamati zvandinonamata"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Uye ini handizombonamati zvamunonamata"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Uye imi hamuzombonamati izvo zvandinonamata"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Kwamuri mune chitendero chenyu, uye ini kwandiri ndine chitendero changu.”"
        }
    ]
}