{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Farawo akatumira vadani mumaguta ose"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "(Achiti): “Zvirokwazvo, vanhu ava kaboka kadiki.”"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "“Uye zvirokwazvo, vaita zvinhu zvatigumbura.”"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "“Asi zvirokwazvo, tisu tose takaungana nekuyambirwa.”"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Saka takavadzinga kubva mumapindu uye zvitubu"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Hupfumi, uye nzvimbo dzose dzakarurama"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Nokudaro takaita kuti vana vaIzirairi vave vanogara nhaka yavo"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Saka vakavatevera panguva yekubuda kwezuva"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Mapoka maviri aya paakaonana, vanhu vaMusa vakati: “Zvirokwazvo, tava kuzobatwa zvino.”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(Musa) akati: “Hazviiti. Zvirokwazvo, Tenzi vangu vaneni. Vachava vanonditungamira.”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Takava tinodzikisa kuna Musa (kuti): “Rova nyanza netsvimbo yako.” Uye yakapatsanuka, uye mafasho (emvura) akasiyana-siyana akaita kunge mazikomo mahombe"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Takave tinounza vamwe (vanhu vaFarawo) padyo nenzvimbo iyoyo"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Uye takanunura Musa neavo vaiva naye"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Uye takave tinonyudza vamwe (Farawo nevanhu vake)"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Zvirokwazvo, umu (munyaya yaFarawo naMusa) mune chiratidzo, asi vazhinji vavo havatendi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Uye chokwadi, Tenzi vako (Muhammad (SAW)), zvirokwazvo ndivo vakurusa, uye vane tsitsi zhinji"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Uye vaverengere nyaya yaIbrahim (Abrahamu)"
        }
    ]
}