{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ndinopika nemabhiza anomhanya achifemereka"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Achiburitsa zvimvari zvemoto (nemakumbo awo)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Uye achimhanyira kunorwa mambakwedza"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Uye achipfumbura huruva nawo"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Uye achipinda nawo mukati (memuvengi)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Zvirokwazvo, munhu haatendi Tenzi vake"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Uye zvirokwazvo iye ichapupu kune izvozvo (nemabasa ake)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Uye zvirokwazvo anosimbirira mukuda zvakanaka (hupfumi)"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Haazivi here kuti apo zviri mumakuva zvichaburitswa"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Uye izvo zviri muzvipfuva pazvichaitwa kuti zvizivikanwe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Zvirokwazvo, nezuva iroro Tenzi vavo vachange vachinyatsoziva nezvavo (mabasa avo uye vovapa mubairo wavo)"
        }
    ]
}