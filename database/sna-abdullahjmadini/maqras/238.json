{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ndinopika neguta iri (Makkah)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Uyezve hauna mhosva neguta iri (Wakasununguka muGuta iri)"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Uyezve nemubereki (Adam (AS)) nezvaakabereka (vana vake)"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Zvirokwazvo, tave tinosika munhu mumatambudziko"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Anofunga here kuti hapana anomukurira"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Anoti (mukuzvikudza): “Ndave ndinorasa hupfumi hwakawanda!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Anofunga here kuti hapana anomuona"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Hatina here kumugadzirira maziso maviri"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Uye rurimi nemiromo (wezasi newepamusoro)"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Uye tikamuratidza nzira mbiri (yakanaka neyakaipa)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Asi haana kumboedza kupfuura nenzira yakatevenuka (yakareruka)"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Uye chii chichakuita kuti uzive kuti nzira yakatevenuka chii"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kusunungura muranda (musungwa)"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Kana kupa chikafu muzuva renzara"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Kunherera yepedyo"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Kana kune murombo akarara muvhu (nokuda kweurombo)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Uye akazove umwe weavo vanotenda (muIslaam), uye vakakomekedzana kutsungirira, uye vakakomekedzana kunzwira tsitsi"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ndivo avo veruoko rwerudyi (vanhu veParadhiso)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Asi avo vasina kutenda muzviratidzo zvedu, ndeavo veruoko rweruboshwe (vanhu vekumoto)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Vachave vanovharirwa nemoto (Moto uchavakomberedza kubva kumativi ese)"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "(Ndinopika) nezuva nekubwinya kwaro"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Nemwedzi paunoritevera (zuva)"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "(Ndinopika) nemasikati paanoratidza (kubwinya kwezuva)"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Neusiku paunorivhara (zuva)"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Nedenga neavo vakarivaka"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "(Ndinopika) nepasi neavo vakari tambanudza"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nemoyo (munhu) neavo vakamugadzira mune zvakanaka"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Uye vakamuratidza zvakaipa kwaari nezvakanaka kwaari"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Zvirokwazvo, anobudirira uyo anozvichenesa (anotevedzera Islaam)"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Uye zvirokwazvo anokundikana uyo anozvikanganisira pachake (anoramba Islaam)"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "(Vanhu) veThamud vakaramba kuburikidza nekudarikidza kwavo mitemo (vairamba chitendero chechokwadi, Islaam, uye vachiita mabasa akaipa)"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Apo muipi wavo akaenda mberi (kuti auraye ngamera)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Asi mutumwa waAllah (Swaalih (AS)) akati kwavari: “Iyi ingamera yaAllah (musaikuvadze), uye (musairambidze) kunwa kwayo!”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Naizvozvo vakamuramba uye vakaiuraya. Naizvozvo Tenzi vavo vakavaparadza nekuda kwechitadzo chavo, uye vakavaenzanisa mukuparadzwa (vanhu vose, vapfumi, nevarombo, vane simba nevasina simba)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Uye ivo (Allah) havana kutya zvichaitika mumashure mazvo"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "(Ndinopika) neusiku apo paunosvika"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Nemasikati apo paanobwinya"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Neavo vakasika chirume nechikadzi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Zvirokwazvo, simba nemabasa enyu akasiyana siyana"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kune uyo anopa (zvipo) uye achitya Allah"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Uye achitenda mune zvakanaka (Laa ilaaha illallaah – Hakuna ane kodzero yokunamatwa kunze kwaAllah; kana kuti mubairo kubva kuna Allah)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tichava tinomurerutsira nzira yakanaka"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Asi uyo asingapi (anonyima), uye achifunga kuti mupfumi (haana chaanoda kubva kune umwe munhu)"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Uye achiramba Al-Husna (zvakanaka: Laa ilaaha illallaah, kana kuti mubairo kubva kunaAllah)"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tichava tinomurerutsira nzira yakaipa"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Uye hupfumi hwake huchamubatsira chii paachawira pasi (mumatambudziko)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Zvirokwazvo, kwatiri ndiko kune nhungamiro"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Uye zvirokwazvo kwatiri ndiko kune magumo nemavambo (upenyu hwepano pasi neuchatevera)"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Naizvozvo ndinokuyambirai nezvemoto unopisa (Gehena)"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Hapana achaupinda kunze kweavo vachange vasina mufaro"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Avo vanoramba vofuratira"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Uye Al-Muttaqoon (vatsvene, vanhu vanotya Allah) vachaiswa kure nawo (moto weGehena)"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Uye anopa hupfumi hwake kuti azvichenese"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Uye uyo asiri kuripa chakanaka chaakaitirwa naani zvake"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Kunze kwekuti achitsvaga chiso (kufadza) chaTenzi vake vepamusoro-soro"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Uye zvirokwazvo mushure maizvozvo, achava anofara (paachapinda Paradhiso)"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "(Ndinopika) nenguva yemangwanani (zuva richangobuda)"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "(Ndinopika) nenguva yemanheru apo painoita rima"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Tenzi vako (Iwe Muhammad (SAW)) havana kukurasirira kana kukuvenga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Uye zvirokwazvo hupenyu hwemangwana hwakanaka kwauri kukunda hupenyu hwenyika ino"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Uye zvirokwazvo, Tenzi vako vachakupa (zvakanaka) naizvozvo ugofara"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Havana here kukuwana (Muhammad (SAW)) uri nherera ndokubva vakuchengetedza"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Uye vakakuwana wakarasika ndokubva vakutungamirira"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Uye vakakuwana uri murombo ndokubva vakuita mupfumi (mugutsikani nezvaunazvo)"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Naizvozvo, usachengete nherera nehudzvanyiriri"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Uye usadzinge uyo anokumbira (mupe)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Uyezve taura pamusoro pemakomborero aTenzi vako (awakapiwa, Huporofita nemamwewo)"
        }
    ]
}