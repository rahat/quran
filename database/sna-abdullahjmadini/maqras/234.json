{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Akafinyama (Muhammad (SAW)) uye akafuratira"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Apo bofu (Abdullah Ibn Umm Maktoom) rakanga rauya kwaari (Muhammad (SAW) achiparidzira mukuru kana vakuru vemaQuraish"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Uye unoziva sei pamwe anogona kuzvichenura (kubva kuzvitadzo)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Kana kuti anogona kuyeuka, uyezve yeuchidzo yobva yamubatsira"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Asi kune uyo akapfuma"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Kwaari unoenda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Hapana mhosva kwauri kana iye asina kuzvichenura (kubva mukusatenda)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Asi kune uyo anouya kwauri achimhanya (nechido)"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Uye achitya (Allah)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Kwaari ndiko kwausingaendi (kwausingaratidzi hanya)"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kwete! Zvirokwazvo, iri Qur’aan irangaridzo"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Naizvozvo uyo anoda, ngaarangarire"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Zviri) muMagwaro anoremekedzwa (Al-Lauh Al-Mahfoodh)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Akasimudzirwa, akachenurwa"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Mumaoko evanyori (ngirozi)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Vanoremekedzeka uye vanoteerera"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Ngaashorwe munhu (asingatendi)! Nekusatenda kwake uku"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Kubva pai (munhu) vakamusika"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Kubva kuNutfah (Mvura yemurume nemukadzi) vakamusika uye vakanyatsomugadzira zvakanaka"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Uye vanomuitira kuti nzira imuitire nyore"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Uye vanomuita kuti afe uye vomuisa muguva"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Uye nekuda kwavo, vachamumutsa (zvekare)"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kwete! Asi (munhu) haana kuita zvaakaudzwa (zvaakakomekedzwa)"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Naizvozvo rega munhu atarise chikafu chake"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Tinonaisa mvura yakawanda"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Uye zvirokwazvo tinopatsanura ivhu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Uye tinoita kuti zvirimwa zvikure imomo"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Nemagirepi uye nechikafu chemombe"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Nema Orivhi uye miti yemadheti"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Nemapindu akasvibira akazara nemiti"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Uye michero nehuswa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "(Kuti zvive) rubatsiro (chikafu) kwamuri nezvipfuyo zvenyu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Uye apo zuva rekutongwa parichauya (Hwamanda paicharidzwa kechipiri)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Zuva iri murume achatiza mukoma wake"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Uye amai nababa vake"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Uye mukadzi nevana vake"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Munhu wese zuva iri achange anezvinomukwanira zvichamuita kuti ashaye hanya nevamwe"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Zvimwe zviso zuva iroro zvichave zvakajeka (vatendi vechokwadi muIslaam)"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Zvichiseka, zvichifara nemhaka yenhaurwa yakanaka (yeParadhiso)"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Uye zvimwe zviso zuva iroro zvichange zvakasuwa"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Rima richava kwidibira"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ava vanhu vange vasingatendi (kunaAllah) uye vachiita mabasa akaipa"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Apo zuva parichaputirwa, uye chiedza charo chobva"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Uye apo nyeredzi padzichadonha"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Uye apo makomo paachaitwa kuti afambe (achibva panzvimbo dzawo)"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Uye apo ngamera dzakazvitakura dzichasiiwa dzisina muchengeti"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Uye apo zvikara zvesango pazvichaunganidzwa"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Uye apo makungwa paachatungidzwa ova moto (kana paachapfachukira)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Uye apo mweya ichabatanidzwa nemitumbi yayo (yakanaka neyakanaka uye yakaipa neyakaipa)"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Uye apo mwanasikana akavigwa ari mupenyu (sezvaiitwa nemaArabhu vasati vave maMuslim) paachabvunzwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kuti chitadzo chei chaakaurairwa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Uye apo mabhuku (emabasa akanaka neakaipa) achavhurwa"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Uye denga richasiiwa pachena robviswa panzvimbo paro"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Uye apo Gehena richatungidzwa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Uye apo Paradhiso richaiswa pedyo"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Munhu wese achaziva zvaakatungamidza (mabasa akanaka neakaipa)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Naizvozvo, ndinopika nepasi pose panotaramuka (zvisikwa zvomudenga zvinonyangarika masikati zvobudikira usiku)"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Uyezve (ndinopika) nepasi pose panofamba nekukurumidza nekuzvihwandisa"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Uye neusiku paunoenda"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Uyezve nemambakwedza pakunochena"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Zvirokwazvo, iri (Qur’aan) ishoko (rakaunzwa) nemutumwa anoremekedzeka (Gabriel, kubva kuna Allah richienda kuna Mutumwa Muhammad (SAW)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Anemasimba kuna (Allah), Tenzi vechigaro (chepamusoro)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Anoteererwa (nengirozi kudenga) uye akavimbika"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Uye (Imi vanhu) mumwe wenyu (Muhammad (SAW)) haapengi"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Uye zvirokwazvo iye (Muhammad (SAW)) akamuona (Jibreel – Gabriel (AS)) mudenga makachena (kumabhazuva)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Uye iye (Muhammad (SAW)) haavigi ruzivo rwezvakavanzika"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Uye iri (Qur’aan) harisi shoko raShaitwaan (satani) akarasirirwa"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Naizvozvo murikunanga kupi"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Zvirokwazvo, harisi chimwe chinhu kunze kweyeuchidzo kunyika dzose (Vanhu nemaJinn)"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kune wese kwamuri anoshuvira kutora nzira yakatwasuka"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Uye hapana chamunoshuvira kusara kweizvo zvinoshuvirwa naAllah, Tenzi venyika dzose"
        }
    ]
}