{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Śapata buraūjabiśiṣṭa ākāśara"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "śapata pratiśrautikr̥ta dinara"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "śapata draṣṭā ārau dr̥ṣṭara"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "abhiśapta haichila agnikuṇḍara adhipatisakala"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ya’ta āchila indhanapūrṇa ju'i"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Yētiẏā siham̐ta iẏāra kāṣata upabiṣṭa āchila"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ārau siham̐tē muminasakalara lagata yi karaichila sēẏā pratyakṣa karaichila"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Ārau siham̐tē tē'ōm̐lōkaka niryātana karaichila kērala ē'i kāraṇē yē, tē'ōm̐lōkē īmāna ānichila paraākramaśālī ārau praśansāra yōgya āllāhara ōparata"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "yāra adhikārata āchē ākāśa ārau pr̥thiraīra sārbabhauma kartr̥tba; ārau āllāha sakalō bastura pratyakṣadarśī"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Niścaẏa yiham̐tē mumina purauṣa ārau muminā nāraīka bipadāpanna karaichē tāra pichata tā'ōbā karaā nā'i, siham̐tara bābē āchē jāhānnāmara śāsti, ārau siham̐tara bābē āchē dahana yantraṇā"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Niścaẏa yisakalē īmāna ānichē ārau satkarma karaichē tē'ōm̐lōkara bābē āchē jānnāta, yāra talata nadī prabāhita; ē'iṭōraē mahāsāphalya"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Niścaẏa tōmāra pratipālakara graēptāraī ati kaṭhōra"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Tēraēm̐i astitba pradāna karaē ārau tēraēm̐i punaraārartana ghaṭāẏa"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "niścaẏa tē'ōm̐ kṣamāśīla, atisnēhaśīla"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ārachara adhipati, sanmānita"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Tē'ōm̐ yi bicāraē sē'iṭōraē karaē"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Tōmāra ōcarata sain'yabāhinīra br̥ttānta āhichēnē"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "phira'ā'unara ārau chāmūdara"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Tathāpi'ō kāphirasakala mithyāraōpa karaāta lipta"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ārau āllāhē siham̐taka kē'uphālara paraā paraibēṣṭana karai āchē"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Prakr̥tatē i sanmānita kōra'āna"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "surakṣita phalakata lipibad'dha"
        }
    ]
}