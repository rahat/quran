{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Niścaẏa muttākbīsakalara bābē āchē saphalatā"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Udyānasamūha ārau āṅgurasamūha"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ārau samabaẏasīẏā uda`bhinna nara-yuratī tarauṇī"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ārau paraipūrṇa pānapātra"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tāta tē'ōm̐lōkē kōnō anarthaka kathā ārau michā bākya'ō śunā nāpāba"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Āpōnāra pratipālakara phālara paraā puraṣkāra, yathōcita dānasbaraūpa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Yijana ākāśasamūha ārau pr̥thiraī ārau ē'i duṭāra mājata yi āchē sakalōraē pratipālaka, parama karauṇāmaẏa, tē'ōm̐ra ōcarata ābēdana-nibēdanara śakti siham̐tara kāraō nāthākiba"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Sē'idinā raūha ārau phiraistāsakala śāraībad'dhabhāraē thiẏa ha’ba; rahamānē yāka anumati diba tāra bāhiraē kōnē'ō kathā naka’ba, ārau tē'ōm̐ saṭhika kathā ka’ba"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Ē'i dinaṭō satya, ētēkē yāra icchā siẏē tāra pratipālakara ōcarata āśraẏa grahaṇa karaka"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niścaẏa āmi tōmālōkaka ati nikaṭarartī śāsti samparkē satarka karailō. Yidinā mānuhē nija kr̥takarma dēkhibalai pāba ārau kāphiraē ka’ba, hāẏa! Ma'i yadi māṭi ha'lōhēm̐tēna"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Śapata nirmamabhāraē utpaṭanakāraīsakalara"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ārau mr̥dubhāraē bandhanamūktakāraīsakalara"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ārau tībragatita sām̐turai phuraāsakalara"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Ārau drautagatita āgabāṛhi yōraāsakalara"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Tāra pichata yāratīẏa kāma nirbāhakāraīsakalara"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Sē'idinā kampanakāraīẏē prakampita karaiba"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tāka anusaraṇa karaiba parabartī kampanakāraī"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Sē'idinā kichumāna hr̥daẏa, bhaẏata santrasta ha’ba"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Siham̐tara dr̥ṣṭisamūha ātaṅkita hai nata ha’ba"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Siham̐tē kaẏa, ‘āmi ākau āgara arasthāta ubhati yāma nē"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "yētiẏā āmi cūrṇabicūrṇa asthita paraiṇata hama’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "siham̐tē kaẏa, ‘yadi ē'iṭōraē haẏa tēntē ē'iṭō ēṭā kṣatikara pratyārartana’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Prakr̥tatē ē'iṭō ēṭā biraāṭa śabda"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tatkṣaṇāta bhū-pr̥ṣṭhata siham̐tara ābirbhāra ha’ba"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Mūchāra ghaṭanāṭō tumi pā'ichānē"
        }
    ]
}