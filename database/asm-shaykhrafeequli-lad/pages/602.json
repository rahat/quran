{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Yihētu kburaā'ichasakala abhyasta"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "śīta ārau graīṣmakālara bhramaṇata siham̐tara āsakti āchē"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ētēkē siham̐tē yēna ē'i gharara pratipālakara ibādata karaē"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Yijanē siham̐taka kṣudhā arasthāta khādya pradāna karaichē ārau santraāsara paraā niraāpattā dichē"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi tāka dēkhichānē yiẏē hicāpa pratidānaka asbīkāra karaē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "si sē'ijanē'i, yiẏē anāthaka kaṭhōrabhāraē khēdi paṭhiẏāẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ārau si dukhiẏāka khānā khuraābalai uṯsāha nidiẏē"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ētēkē dhbansa sē'ibidha chālāta ādāẏakāraīsakalara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yisakalē nijara chālātata amanōyōgī"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yisakalē ānaka dēkhuraābalaihē karaē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ārau sarau-suraā gr̥hasāmagraī pradānata bādhā diẏē"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niścaẏa āmi tōmāka āla-kā'uchāra pradāna karaichōm̐"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ētēkē tumi tōmāra pratipālakara uddēśyē chālāta ādāẏa karaā ārau kōrabānī karaā"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Niścaẏa tōmāra prati bidbēṣa pōṣaṇakāraīẏē'i nirbanśa"
        }
    ]
}