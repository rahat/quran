{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Kōraā, ‘tē'ōm̐ āllāha, ēka-adbitīẏa"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "āllāha haichē chāmāda; (tē'ōm̐ kāraō mukhāpēkṣī nahaẏa, sakalō tē'ōm̐raē'i mukhāpēkṣī)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "tē'ōm̐ kākō janma diẏā nā'i ārau tēraōm̐ janma lōraā nā'i"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ārau tē'ōm̐ra samatulya kōnō nā'i’"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kōraā, ma'i āśraẏa bicāraō uṣāra pratipālakara"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Tē'ōm̐ yi sr̥ṣṭi karaichē tāra aniṣṭara paraā"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ārau raātira andhakārara aniṣṭara paraā, yētiẏā i gabhīra haẏa"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ārau sē'isakala nāraīra aniṣṭara paraā yisakalē gām̐ṭhita phum̐ diẏē"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ārau hinsukara aniṣṭara paraā, yētiẏā si hinsā karaē"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kōraā- ma'i mānuhara pratipālakara āśraẏa praārthanā karaichōm̐"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "mānuhara adhipatira"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānuhara ilāhara ōcarata"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ātmagōpanakāraī kumantraṇādātāra aniṣṭara paraā"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "yiẏē mānuhara manata kumantraṇā diẏē"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jinara mājara paraā ārau mānuhara mājara paraā"
        }
    ]
}