{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Śapata ākāśara ārau yi raātita ātmaprakāśa karaē"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ārau kihē tōmāka janāba yē, ‘raātita yiẏē ātmaprakāśa karaē’ sēẏā nō ki"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(sēẏā haichē) ujjbala nakṣatra"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyēka jīrara ōparatē'i tattbābadhāẏaka āchē"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ētēkē mānuhē cintā karaā ucita yē, tāka kihēraē sr̥ṣṭi karaā haichē"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "tāka sr̥ṣṭi karaā haichē drautagatita nirgata hōraā pānīraē"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yiṭō nirgata haẏa mēraudaṇḍa ārau bukura mājara paraā"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niścaẏa tē'ōm̐ tāka ubhatā'i ānibalai sakṣama"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yidinā gōpana biṣaẏasamūha paraīkṣā karaā ha’ba"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "sē'idinā tāra kōnō śakti nāthākiba ārau kōnō sahāẏakāraī'ō nāthākiba"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Śapata ākāśara yiẏē baraṣuṇa dhāraṇa karaē"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "śapata pr̥thiraīra, yiṭō bidīrṇa haẏa"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niścaẏa ē'iṭō (āla-kōra'āna) mīmānsākāraī bāṇī"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ārau ē'iṭō nirarthaka nahaẏa"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Niścaẏa siham̐ta bhīṣaṇa ṣaṛayantra karaē"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ārau maẏō bhīṣaṇa kauśala karaichō"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ētēkē kāphirasakalaka arakāśa diẏā; siham̐taka kichu samaẏara bābē arakāśa diẏā"
        }
    ]
}