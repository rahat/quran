{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yētiẏā kiẏāmata saṅghaṭita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "(tētiẏā) iẏāra saṅghaṭanara asbīkārakāraī kōnō nāthākiba"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "I kāraōbāka adhaḥpatana karaiba, kāraōbāka samunnata karaiba"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "yētiẏā pr̥thiraī prabala prakampanēraē prakampita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "ārau parbatasamūha cūrṇa-bicūrṇa hai paraiba"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "phalata sē'ibōra bikṣipta dhūlikaṇāta paraiṇata ha’ba"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "ārau tōmālōkē bibhakta hai paraibā tiniṭā dalata"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "ētēkē sōm̐phālara dala; ārau sōm̐phālara dalaṭō kimāna yē saubhāgyabāna"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "ārau bā'ōm̐phālara dala; ārau bā'ōm̐phālara dalaṭō kimāna yē durbhagīẏā"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "ārau agragāmīsakalē'i agragāmī"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "tē'ōm̐lōkē'i sānnidhyapraāpta"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "tē'ōm̐lōka thākiba niẏāmatapūrṇa jānnātasamūhata"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "bahusaṅkhyaka ha’ba pūrbarartīsakalara mājara paraā"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "ārau kama saṅkhyaka ha’ba parabartīsakalara mājara paraā"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Sōṇēraē nirmita āsanamūhata"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "siham̐tē parasparaē mukhāmukhikai ā'uji bahiba"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Siham̐tara āśē-pāśē ghuraiphuraiba cira kiśōrasakalē"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "prabāhita nijaraāra suraāpūrṇa pānapātra ārau piẏalābōra lai"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "sē'i suraā pāna karailē tē'ōm̐lōkara mūra nughuraāẏa ārau tē'ōm̐lōka jñānahāraā'ō naha’ba"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "ārau (ghuraiphuraiba) tē'ōm̐lōkara pachandanīẏa phala-mūla lai"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ārau tē'ōm̐lōkara raucī anuyāẏī caraā'ira mānsa lai"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ārau tē'ōm̐lōkara bābē thākiba mr̥ganaẏanī (ḍāṅara cakubiśiṣṭā) hura"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "yēnibā siham̐ta surakṣita mukutāhē"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "siham̐tē yi āmala karaichila tāra puraṣkārasbaraūpa"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tāta siham̐tē kōnō alāgatīẏāla kathā ārau kōnō pāpabākya śunibalai nāpāba"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "śuniba kērala chālāma ārau chālāma"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ārau sōm̐phālara dala; kimāna yē bhāgyabāna sōm̐phālara dalaṭō"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tē'ōm̐lōkē thākiba ēnē udyānata, ya’ta āchē kām̐iṭabihīna bagaraī gacha"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "ārau thōkēraē bharaā kala gacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ārau samprasāraita chām̐"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "ārau sadāẏa prabāhamāna pānī"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ārau pracura phala-mūla"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yibōra śēṣa naha'ba ārau niṣid'dha'ō naha’ba"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ārau (siham̐ta thākiba) su'ucca śayyāsamūhata"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niścaẏa āmi siham̐taka (hurasakalaka) sr̥ṣṭi karaichō biśēṣaraūpata"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "tāra pichata siham̐taka kumāraī banā'ichō"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "nija sbāmīra prati āsakta ārau samabaẏasīẏā"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "sōm̐phālara lōkasakalara bābē"
        }
    ]
}