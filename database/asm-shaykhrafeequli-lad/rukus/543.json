{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bhītiprada mahā bipada"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Bhītiprada mahā bipada ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "bhītiprada mahā bipada samparkē tumi ki jānā"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sē'idinā mānuha ha’ba bikṣipta kīṭa-pataṅgara daraē"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ārau parbatasamūha ha’ba dhūnā raṅgina ūṇara daraē"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ētēkē yāra tarjusamūha gadhura ha’ba"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "tē'ōm̐ thākiba santōṣajanaka jīranata"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "ārau yāra tarjusamūha pātala ha’ba"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tāra ṭhikanā ha’ba ‘hā-raiẏāha’"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ārau tumi ki jānā sē'iṭōnō ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "i ēṭā atyanta uttapta agnikuṇḍa"
        }
    ]
}