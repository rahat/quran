{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o göğe ve Târık'a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Târık nedir, bildin mi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, karanlığı delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hiçbir nefis yoktur ki başında bir denetleyici bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun için insan neden yaratıldığına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O su, erkeğin sulbü ile kadının göğüs kemikleri arasından çıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette Allah'ın onu döndürmeye gücü yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gün bütün sırlar yoklanıp, meydana çıkarılır"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "İnsanın o gün ne bir gücü vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o dönüşlü göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O yarılıp çatlayan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuşkusuz Kur'ân, ayırıcı bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla bir şaka değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, kâfirler hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine karşılık veririm"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun için sen kâfirlere mühlet ver, onlara az bir zaman tanı"
        }
    ]
}