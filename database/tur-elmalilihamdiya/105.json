        {
            "chapter": 105,
            "verse": 1,
            "text": "Görmedin mi Rabb'in fil sahiplerine ne yaptı"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Üzerlerine sürü sürü kuşlar gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara çamurdan sertleşmiş taşlar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ve onları, yenilmiş ekin yaprağı gibi yaptı"
        }
    ]
