    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki; O Allah bir tektir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah eksiksiz, sameddir (Bütün varlıklar O'na muhtaç, fakat O, hiç bir şeye muhtaç değildir)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Doğurmadı ve doğurulmadı"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "O 'na bir denk de olmadı"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Ben, ağaran sabahın Rabbine sığınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlığı çöktüğü zaman gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve düğümlere üfleyen büyücülerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased ettiği zaman hasetçinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sığınırım ben insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların hükümdârına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların ilâhına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi vesvesecinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki, insanların göğüslerine vesveseler fısıldar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan"
        }
    ]
