{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tîn'e ve Zeytun'a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu güvenli beldeye andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da çevirdik aşağıların aşağısına attık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip iyi işler yapanlar başka; onlar için kesintisiz bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde sana dini ne yalanlatır"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi değil mi"
        }
    ]
}