{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Ey Rasûlüm,) de ki: Sığınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların Melik’ine (hükümdarlar Hükümdarına)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların İlâh’ına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi şeytanın şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Öyle bir şeytan ki, insanların kalblerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(O şeytan), cinlerden de olur, insanlardan da"
        }
    ]
}