{
    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "mâ agnâ `anhüm mâ kânû yümette`ûn"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "vemâ ehleknâ min ḳaryetin illâ lehâ münẕirûn"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ẕikrâ. vemâ künnâ żâlimîn"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "vemâ tenezzelet bihi-şşeyâṭîn"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "vemâ yembegî lehüm vemâ yesteṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innehüm `ani-ssem`i lema`zûlûn"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "felâ ted`u me`a-llâhi ilâhen âḫara fetekûne mine-lmü`aẕẕebîn"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "veenẕir `aşîrateke-l'aḳrabîn"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "vaḫfiḍ cenâḥake limeni-ttebe`ake mine-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fein `aṣavke feḳul innî berîüm mimmâ ta`melûn"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "vetevekkel `ale-l`azîzi-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "elleẕî yerâke ḥîne teḳûm"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "veteḳallübeke fi-ssâcidîn"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innehû hüve-ssemî`u-l`alîm"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hel ünebbiüküm `alâ men tenezzelü-şşeyâṭîn"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tenezzelü `alâ külli effâkin eŝîm"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yülḳûne-ssem`a veekŝeruhüm kâẕibûn"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "veşşu`arâü yettebi`uhümü-lgâvûn"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "elem tera ennehüm fî külli vâdiy yehîmûn"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "veennehüm yeḳûlûne mâ lâ yef`alûn"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "ille-lleẕîne âmenû ve`amilu-ṣṣâliḥâti veẕekerü-llâhe keŝîrav venteṣarû mim ba`di mâ żulimû. veseya`lemü-lleẕîne żalemû eyye münḳalebiy yenḳalibûn"
        }
    ]
}