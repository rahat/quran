    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "femâ tenfe`uhüm şefâ`atü-şşâfi`în"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "femâ lehüm `ani-tteẕkirati mü`riḍîn"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "keennehüm ḥumürum müstenfirah"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "ferrat min ḳasverah"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "bel yürîdü küllü-mriim minhüm ey yü'tâ ṣuḥufem müneşşerah"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "kellâ. bel lâ yeḫâfûne-l'âḫirah"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "kellâ innehû teẕkirah"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "femen şâe ẕekerah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "vemâ yeẕkürûne illâ ey yeşâe-llâh. hüve ehlü-ttaḳvâ veehlü-lmagfirah"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "lâ uḳsimü biyevmi-lḳiyâmeh"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "velâ uḳsimü binnefsi-llevvâmeh"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "eyaḥsebü-l'insânü ellen necme`a `iżâmeh"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "belâ ḳâdirîne `alâ en nüsevviye benâneh"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "bel yürîdü-l'insânü liyefcüra emâmeh"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "yes'elü eyyâne yevmü-lḳiyâmeh"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "feiẕâ beriḳa-lbeṣar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "veḫasefe-lḳamer"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "vecümi`a-şşemsü velḳamer"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "yeḳûlü-l'insânü yevmeiẕin eyne-lmeferr"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "kellâ lâ vezer"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "ilâ rabbike yevmeiẕin-lmüsteḳarr"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "yünebbeü-l'insânü yevmeiẕim bimâ ḳaddeme veeḫḫar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "beli-l'insânü `alâ nefsihî beṣîrah"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "velev elḳâ me`âẕîrah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "lâ tüḥarrik bihî lisâneke lita`cele bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "inne `aleynâ cem`ahû veḳur'âneh"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "feiẕâ ḳara'nâhü fettebi` ḳur'âneh"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ŝümme inne `aleynâ beyâneh"
