{
    "pages": [
        {
            "chapter": 15,
            "verse": 71,
            "text": "ḳâle hâülâi benâtî in küntüm fâ`ilîn"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "le`amruke innehüm lefî sekratihim ya`mehûn"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "feeḫaẕethümu-ṣṣayḥatü müşriḳîn"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fece`alnâ `âliyehâ sâfilehâ veemṭarnâ `aleyhim ḥicâratem min siccîl"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inne fî ẕâlike leâyâtil lilmütevessimîn"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "veinnehâ lebisebîlim müḳîm"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inne fî ẕâlike leâyetel lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "vein kâne aṣḥâbü-l'eyketi leżâlimîn"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fenteḳamnâ minhüm. veinnehümâ lebiimâmim mübîn"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "veleḳad keẕẕebe aṣḥâbü-lḥicri-lmürselîn"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "veâteynâhüm âyâtinâ fekânû `anhâ mü`riḍîn"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "vekânû yenḥitûne mine-lcibâli büyûten âminîn"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "feeḫaẕethümu-ṣṣayḥatü muṣbiḥîn"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "femâ agnâ `anhüm mâ kânû yeksibûn"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "vemâ ḫalaḳne-ssemâvâti vel'arḍa vemâ beynehümâ illâ bilḥaḳḳi. veinne-ssâ`ate leâtiyetün faṣfeḥi-ṣṣafḥa-lcemîl"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "inne rabbeke hüve-lḫallâḳu-l`alîm"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "veleḳad âteynâke seb`am mine-lmeŝânî velḳur'âne-l`ażîm"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "lâ temüddenne `ayneyke ilâ mâ metta`nâ bihî ezvâcem minhüm velâ taḥzen `aleyhim vaḫfiḍ cenâḥake lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "veḳul innî ene-nneẕîru-lmübîn"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "kemâ enzelnâ `ale-lmuḳtesimîn"
        }
    ]
}