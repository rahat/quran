{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "felemmâ raevhü zülfeten sîet vucûhü-lleẕîne keferû veḳîle hâẕe-lleẕî küntüm bihî tedde`ûn"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "ḳul era'eytüm in ehlekeniye-llâhü vemem me`iye ev raḥimenâ femey yücîru-lkâfirîne min `aẕâbin elîm"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "ḳul hüve-rraḥmânü âmennâ bihî ve`aleyhi tevekkelnâ. feseta`lemûne men hüve fî ḍalâlim mübîn"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "ḳul era'eytüm in aṣbeḥa mâüküm gavran femey ye'tîküm bimâim me`în"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "nûn. velḳalemi vemâ yesṭurûn"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "mâ ente bini`meti rabbike bimecnûn"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "veinne leke leecran gayra memnûn"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "veinneke le`alâ ḫulüḳin `ażîm"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "fesetübṣiru veyübṣirûn"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "bieyyikümü-lmeftûn"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "inne rabbeke hüve a`lemü bimen ḍalle `an sebîlih. vehüve a`lemü bilmühtedîn"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "felâ tüṭi`i-lmükeẕẕibîn"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "veddû lev tüdhinü feyüdhinûn"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "velâ tüṭi` külle ḥallâfim mehîn"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "hemmâzim meşşâim binemîm"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "mennâ`il lilḫayri mü`tedin eŝîm"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "utüllim ba`de ẕâlike zenîm"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "en kâne ẕâ mâliv vebenîn"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "iẕâ tütlâ `aleyhi âyâtünâ ḳâle esâṭîru-l'evvelîn"
        }
    ]
}