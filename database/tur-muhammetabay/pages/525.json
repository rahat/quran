{
    "pages": [
        {
            "chapter": 52,
            "verse": 32,
            "text": "em te'müruhüm aḥlâmühüm bihâẕâ em hüm ḳavmün ṭâgûn"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "em yeḳûlûne teḳavveleh. bel lâ yü'minûn"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "felye'tû biḥadîŝim miŝlihî in kânû ṣâdiḳîn"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "em ḫuliḳû min gayri şey'in em hümü-lḫâliḳûn"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "em ḫaleḳu-ssemâvâti vel'arḍ. bel lâ yûḳinûn"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "em `indehüm ḫazâinü rabbike em hümü-lmüṣayṭirûn"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "em lehüm süllemüy yestemi`ûne fîh. felye'ti müstemi`uhüm bisülṭânim mübîn"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "em lehü-lbenâtü velekümü-lbenûn"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "em tes'elühüm ecran fehüm mim magramim müŝḳalûn"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "em `indehümü-lgaybü fehüm yektübûn"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "em yürîdûne keydâ. felleẕîne keferû hümü-lmekîdûn"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "em lehüm ilâhün gayru-llâh. sübḥâne-llâhi `ammâ yüşrikûn"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "veiy yerav kisfem mine-ssemâi sâḳiṭay yeḳûlû seḥâbüm merkûm"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "feẕerhüm ḥattâ yülâḳû yevmehümü-lleẕî fîhi yuṣ`aḳûn"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yevme lâ yugnî `anhüm keydühüm şey'ev velâ hüm yünṣarûn"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "veinne lilleẕîne żalemû `aẕâben dûne ẕâlike velâkinne ekŝerahüm lâ ya`lemûn"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "vaṣbir liḥukmi rabbike feinneke bia`yüninâ vesebbiḥ biḥamdi rabbike ḥîne teḳûm"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "vemine-lleyli fesebbiḥhü veidbâra-nnücûm"
        }
    ]
}