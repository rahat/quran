{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "vennecmi iẕâ hevâ"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "mâ ḍalle ṣâḥibüküm vemâ gavâ"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "vemâ yenṭiḳu `ani-lhevâ"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in hüve illâ vaḥyüy yûḥâ"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "allemehû şedîdü-lḳuvâ"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ẕû mirrah. festevâ"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "vehüve bil'üfüḳi-l'a`lâ"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ŝümme denâ fetedellâ"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fekâne ḳâbe ḳavseyni ev ednâ"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "feevḥâ ilâ `abdihî mâ evḥâ"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "mâ keẕebe-lfüâdü mâ raâ"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "efetümârûnehû `alâ mâ yerâ"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "veleḳad raâhü nezleten uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "inde sidrati-lmüntehâ"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "indehâ cennetü-lme'vâ"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "iẕ yagşe-ssidrate mâ yagşâ"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "mâ zâga-lbeṣaru vemâ ṭagâ"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "leḳad raâ min âyâti rabbihi-lkübrâ"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "eferaeytümü-llâte vel`uzzâ"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "vemenâte-ŝŝâliŝete-l'uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "elekümü-ẕẕekeru velehü-l'ünŝâ"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "tilke iẕen ḳismetün ḍîzâ"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiye illâ esmâün semmeytümûhâ entüm veâbâüküm mâ enzele-llâhü bihâ min sülṭân. iy yettebi`ûne ille-żżanne vemâ tehve-l'enfüs. veleḳad câehüm mir rabbihimü-lhüdâ"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "em lil'insâni mâ temennâ"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "felillâhi-l'âḫiratü vel'ûlâ"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "vekem mim melekin fi-ssemâvâti lâ tugnî şefâ`atühüm şey'en illâ mim ba`di ey ye'ẕene-llâhü limey yeşâü veyerḍâ"
        }
    ]
}