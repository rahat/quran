{
    "pages": [
        {
            "chapter": 37,
            "verse": 77,
            "text": "vece`alnâ ẕürriyyetehû hümü-lbâḳîn"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "selâmün `alâ nûḥin fi-l`âlemîn"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "ŝümme agraḳne-l'âḫarîn"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "veinne min şî`atihî leibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "iẕ câe rabbehû biḳalbin selîm"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "iẕ ḳâle liebîhi veḳavmihî mâẕâ ta`büdûn"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "eifken âliheten dûne-llâhi türîdûn"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "femâ żannüküm birabbi-l`âlemîn"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "feneżara nażraten fi-nnücûm"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "feḳâle innî seḳîm"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "fetevellev `anhü müdbirîn"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "ferâga ilâ âlihetihim feḳâle elâ te'külûn"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "mâ leküm lâ tenṭiḳûn"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "ferâga `aleyhim ḍarbem bilyemîn"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "feaḳbelû ileyhi yeziffûn"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "ḳâle eta`büdûne mâ tenḥitûn"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "vellâhü ḫaleḳaküm vemâ ta`melûn"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "ḳâlü-bnû lehû bünyânen feelḳûhü fi-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "feerâdû bihî keyden fece`alnâhümü-l'esfelîn"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "veḳâle innî ẕâhibün ilâ rabbî seyehdîn"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "rabbi heb lî mine-ṣṣâliḥîn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "febeşşernâhü bigulâmin ḥalîm"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "felemmâ belega me`ahü-ssa`ye ḳâle yâ büneyye innî erâ fi-lmenâmi ennî eẕbeḥuke fenżur mâẕâ terâ. ḳâle yâ ebeti-f`al mâ tü'mer. setecidünî in şâe-llâhü mine-ṣṣâbirîn"
        }
    ]
}