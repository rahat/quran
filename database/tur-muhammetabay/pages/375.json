{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "vetteḳu-lleẕî ḫaleḳaküm velcibillete-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "ḳâlû innemâ ente mine-lmüseḥḥarîn"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "vemâ ente illâ beşer miŝlünâ vein neżunnüke lemine-lkâẕibîn"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "feesḳiṭ `aleynâ kisefem mine-ssemâi in künte mine-ṣṣâdiḳîn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "ḳâle rabbî a`lemü bimâ ta`melûn"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fekeẕẕebûhü feeḫaẕehüm `aẕâbü yevmi-żżulleh. innehû kâne `aẕâbe yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "veinnehû letenzîlü rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nezele bihi-rrûḥu-l'emîn"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "alâ ḳalbike litekûne mine-lmünẕirîn"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisânin `arabiyyim mübîn"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "veinnehû lefî zübüri-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "evelem yekül lehüm âyeten ey ya`lemehû `ulemâü benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "velev nezzelnâhü `alâ ba`ḍi-l'a`cemîn"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "feḳara'ehû `aleyhim mâ kânû bihî mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "keẕâlike seleknâhü fî ḳulûbi-lmücrimîn"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "lâ yü'minûne bihî ḥattâ yeravu-l`aẕâbe-l'elîm"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "feye'tiyehüm bagtetev vehüm lâ yeş`urûn"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "feyeḳûlû hel naḥnü münżarûn"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "efebi`aẕâbinâ yesta`cilûn"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "eferaeyte im metta`nâhüm sinîn"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ŝümme câehüm mâ kânû yû`adûn"
        }
    ]
}