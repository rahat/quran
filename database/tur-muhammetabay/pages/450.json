{
    "pages": [
        {
            "chapter": 37,
            "verse": 103,
            "text": "felemmâ eslemâ vetellehû lilcebîn"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "venâdeynâhü ey yâ ibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ḳad ṣaddaḳte-rru'yâ. innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "inne hâẕâ lehüve-lbelâü-lmübîn"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "vefedeynâhü biẕibḥin `ażîm"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selâmün `alâ ibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "vebeşşernâhü biisḥâḳa nebiyyem mine-ṣṣâliḥîn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "vebâraknâ `aleyhi ve`alâ isḥâḳ. vemin ẕürriyyetihimâ muḥsinüv veżâlimül linefsihî mübîn"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "veleḳad menennâ `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "venecceynâhümâ veḳavmehümâ mine-lkerbi-l`ażîm"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "veneṣarnâhüm fekânû hümü-lgâlibîn"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "veâteynâhüme-lkitâbe-lmüstebîn"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "vehedeynâhüme-ṣṣirâṭa-lmüsteḳîm"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "veteraknâ `aleyhimâ fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selâmün `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innehümâ min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "veinne ilyâse lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "iẕ ḳâle liḳavmihî elâ tetteḳûn"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "eted`ûne ba`lev veteẕerûne aḥsene-lḫâliḳîn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "allâhe rabbeküm verabbe âbâikümü-l'evvelîn"
        }
    ]
