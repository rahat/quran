{
    "pages": [
        {
            "chapter": 26,
            "verse": 61,
            "text": "felemmâ terâe-lcem`âni ḳâle aṣḥâbü mûsâ innâ lemüdrakûn"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "ḳâle kellâ. inne me`iye rabbî seyehdîn"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "feevḥaynâ ilâ mûsâ eni-ḍrib bi`aṣâke-lbaḥr. fenfeleḳa fekâne küllü firḳin keṭṭavdi-l`ażîm"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "veezlefnâ ŝemme-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "veenceynâ mûsâ vemem me`ahû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ŝümme agraḳne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "vetlü `aleyhim nebee ibrâhîm"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "iẕ ḳâle liebîhi veḳavmihî mâ ta`büdûn"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "ḳâlû na`büdü aṣnâmen feneżallü lehâ `âkifîn"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "ḳâle hel yesme`ûneküm iẕ ted`ûn"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "ev yenfe`ûneküm ev yeḍurrûn"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "ḳâlû bel vecednâ âbâenâ keẕâlike yef`alûn"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "ḳâle eferaeytüm mâ küntüm ta`büdûn"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "entüm veâbâükümü-l'aḳdemûn"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "feinnehüm `adüvvül lî illâ rabbe-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "elleẕî ḫaleḳanî fehüve yehdîn"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "velleẕî hüve yuṭ`imünî veyesḳîn"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "veiẕâ meriḍtü fehüve yeşfîn"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "velleẕî yümîtünî ŝümme yuḥyîn"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "velleẕî aṭme`u ey yagfira lî ḫaṭîetî yevme-ddîn"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi heb lî ḥukmev veelḥiḳnî biṣṣâliḥîn"
        }
    ]
}