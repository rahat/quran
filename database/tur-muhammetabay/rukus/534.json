{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "velleyli iẕâ yagşâ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "vennehâri iẕâ tecellâ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "vemâ ḫaleḳa-ẕẕekera vel'ünŝâ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "inne sa`yeküm leşettâ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "feemmâ men a`ṭâ vetteḳâ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "veṣaddeḳa bilḥusnâ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "fesenüyessiruhû lilyüsrâ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "veemmâ mem beḫile vestagnâ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "vekeẕẕebe bilḥusnâ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "fesenüyessiruhû lil`usrâ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "vemâ yugnî `anhü mâlühû iẕâ teraddâ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "inne `aleynâ lelhüdâ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "veinne lenâ lel'âḫirate vel'ûlâ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "feenẕertüküm nâran teleżżâ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "lâ yaṣlâhâ ille-l'eşḳâ"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "elleẕî keẕẕebe vetevellâ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "veseyücennebühe-l'etḳâ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "elleẕî yü'tî mâlehû yetezekkâ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "vemâ lieḥadin `indehû min ni`metin tüczâ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "ille-btigâe vechi rabbihi-l'a`lâ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "velesevfe yerḍâ"
        }
    ]
}