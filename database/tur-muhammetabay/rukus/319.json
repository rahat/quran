{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "feersele fir`avnü fi-lmedâini ḥâşirîn"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inne hâülâi leşirẕimetün ḳalîlûn"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "veinnehüm lenâ legâiżûn"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "veinnâ lecemî`un ḥâẕirûn"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "feaḫracnâhüm min cennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "vekünûziv vemeḳâmin kerîm"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "keẕâlik. veevraŝnâhâ benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "feetbe`ûhüm müşriḳîn"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "felemmâ terâe-lcem`âni ḳâle aṣḥâbü mûsâ innâ lemüdrakûn"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "ḳâle kellâ. inne me`iye rabbî seyehdîn"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "feevḥaynâ ilâ mûsâ eni-ḍrib bi`aṣâke-lbaḥr. fenfeleḳa fekâne küllü firḳin keṭṭavdi-l`ażîm"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "veezlefnâ ŝemme-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "veenceynâ mûsâ vemem me`ahû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ŝümme agraḳne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "vetlü `aleyhim nebee ibrâhîm"
        }
    ]
}