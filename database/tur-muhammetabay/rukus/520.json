{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "vennâzi`âti garḳâ"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "vennâşiṭâti neşṭâ"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "vessâbiḥâti sebḥâ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fessâbiḳâti sebḳâ"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "felmüdebbirâti emrâ"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yevme tercüfü-rrâcifeh"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tetbe`uhe-rrâdifeh"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "ḳulûbüy yevmeiẕiv vâcifeh"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "ebṣâruhâ ḫâşi`ah"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yeḳûlûne einnâ lemerdûdûne fi-lḥâfirah"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "eiẕâ künnâ `iżâmen neḫirah"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "ḳâlû tilke iẕen kerratün ḫâsirah"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "feinnemâ hiye zecratüv vâḥideh"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "feiẕâ hüm bissâhirah"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hel etâke ḥadîŝü mûsâ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "iẕ nâdâhü rabbühû bilvâdi-lmüḳaddesi ṭuvâ"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "iẕheb ilâ fir`avne innehû ṭagâ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "feḳul hel leke ilâ en tezekkâ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "veehdiyeke ilâ rabbike fetaḫşâ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "feerâhü-l'âyete-lkübrâ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fekeẕẕebe ve`aṣâ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ŝümme edbera yes`â"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "feḥaşera fenâdâ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "feḳâle ene rabbükümü-l'a`lâ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "feeḫaẕehü-llâhü nekâle-l'âḫirati vel'ûlâ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inne fî ẕâlike le`ibratel limey yaḫşâ"
        }
    ]
}