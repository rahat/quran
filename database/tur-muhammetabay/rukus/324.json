{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "keẕẕebet ḳavmü lûṭini-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "iẕ ḳâle lehüm eḫûhüm lûṭun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "ete'tûne-ẕẕükrâne mine-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "veteẕerûne mâ ḫaleḳa leküm rabbüküm min ezvâciküm. bel entüm ḳavmün `âdûn"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "ḳâlû leil lem tentehi yâ lûṭu letekûnenne mine-lmuḫracîn"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "ḳâle innî li`ameliküm mine-lḳâlîn"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "rabbi neccinî veehlî mimmâ ya`melûn"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "fenecceynâhü veehlehû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "illâ `acûzen fi-lgâbirîn"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ŝümme demmerne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "veemṭarnâ `aleyhim meṭarâ. fesâe meṭaru-lmünẕerîn"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        }
    ]
}