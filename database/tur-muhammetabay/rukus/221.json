{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "inne-lmütteḳîne fî cennâtiv ve`uyûn"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "üdḫulûhâ biselâmin âminîn"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "veneza`nâ mâ fî ṣudûrihim min gillin iḫvânen `alâ sürurim müteḳâbilîn"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "lâ yemessühüm fîhâ neṣabüv vemâ hüm minhâ bimuḫracîn"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "nebbi' `ibâdî ennî ene-lgafûru-rraḥîm"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "veenne `aẕâbî hüve-l`aẕâbü-l'elîm"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "venebbi'hüm `an ḍayfi ibrâhîm"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "iẕ deḫalû `aleyhi feḳâlû selâmâ. ḳâle innâ minküm vecilûn"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "ḳâlû lâ tevcel innâ nübeşşiruke bigulâmin `alîm"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "ḳâle ebeşşertümûnî `alâ em messeniye-lkiberu febime tübeşşirûn"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "ḳâlû beşşernâke bilḥaḳḳi felâ teküm mine-lḳâniṭîn"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "ḳâle vemey yaḳneṭu mir raḥmeti rabbihî ille-ḍḍâllûn"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "illâ âle lûṭ. innâ lemüneccûhüm ecme`în"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "ille-mraetehû ḳaddernâ innehâ lemine-lgâbirîn"
        }
    ]
}