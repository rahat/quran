{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "veleḳad menennâ `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "venecceynâhümâ veḳavmehümâ mine-lkerbi-l`ażîm"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "veneṣarnâhüm fekânû hümü-lgâlibîn"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "veâteynâhüme-lkitâbe-lmüstebîn"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "vehedeynâhüme-ṣṣirâṭa-lmüsteḳîm"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "veteraknâ `aleyhimâ fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selâmün `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innehümâ min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "veinne ilyâse lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "iẕ ḳâle liḳavmihî elâ tetteḳûn"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "eted`ûne ba`lev veteẕerûne aḥsene-lḫâliḳîn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "allâhe rabbeküm verabbe âbâikümü-l'evvelîn"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "fekeẕẕebûhü feinnehüm lemuḥḍarûn"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selâmün `alâ ilyâsîn"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "veinne lûṭal lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "iẕ necceynâhü veehlehû ecme`în"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illâ `acûzen fi-lgâbirîn"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ŝümme demmerne-l'âḫarîn"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "veinneküm letemürrûne `aleyhim muṣbiḥîn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vebilleyl. efelâ ta`ḳilûn"
        }
    ]
}