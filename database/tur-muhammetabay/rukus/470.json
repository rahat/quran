{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "ŝülletüm mine-l'evvelîn"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "veŝülletüm mine-l'âḫirîn"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "veaṣḥâbü-şşimâli mâ aṣḥâbü-şşimâl"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fî semûmiv veḥamîm"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "veżillim miy yaḥmûm"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "lâ bâridiv velâ kerîm"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innehüm kânû ḳable ẕâlike mütrafîn"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "vekânû yüṣirrûne `ale-lḥinŝi-l`ażîm"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "vekânû yeḳûlûne eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemeb`ûŝûn"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "eveâbâüne-l'evvelûn"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "ḳul inne-l'evvelîne vel'âḫirîn"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lemecmû`ûne ilâ mîḳâti yevmim ma`lûm"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ŝümme inneküm eyyühe-ḍḍâllûne-lmükeẕẕibûn"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "leâkilûne min şecerim min zeḳḳûm"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "femâliûne minhe-lbüṭûn"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "feşâribûne `aleyhi mine-lḥamîm"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "feşâribûne şürbe-lhîm"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "hâẕâ nüzülühüm yevme-ddîn"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naḥnü ḫalaḳnâküm felevlâ tüṣaddiḳûn"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "eferaeytüm mâ tümnûn"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "eentüm taḫlüḳûnehû em naḥnü-lḫâliḳûn"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "naḥnü ḳaddernâ beynekümü-lmevte vemâ naḥnü bimesbûḳîn"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "alâ en nübeddile emŝâleküm venünşieküm fî mâ lâ ta`lemûn"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "veleḳad `alimtümü-nneş'ete-l'ûlâ felevlâ teẕekkerûn"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "eferaeytüm mâ taḥruŝûn"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "eentüm tezra`ûnehû em naḥnü-zzâri`ûn"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "lev neşâü lece`alnâhü ḥuṭâmen feżaltüm tefekkehûn"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "innâ lemugramûn"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bel naḥnü maḥrûmûn"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "eferaeytümü-lmâe-lleẕî teşrabûn"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "eentüm enzeltümûhü mine-lmüzni em naḥnü-lmünzilûn"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "lev neşâü ce`alnâhü ücâcen felevlâ teşkürûn"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "eferaeytümü-nnâra-lletî tûrûn"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "eentüm enşe'tüm şeceratehâ em naḥnü-lmünşiûn"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "naḥnü ce`alnâhâ teẕkiratev vemetâ`al lilmuḳvîn"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fesebbiḥ bismi rabbike-l`ażîm"
        }
    ]
}