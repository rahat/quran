{
    "rukus": [
        {
            "chapter": 50,
            "verse": 16,
            "text": "veleḳad ḫalaḳne-l'insâne vena`lemü mâ tüvesvisü bihî nefsüh. venaḥnü aḳrabü ileyhi min ḥabli-lverîd"
        },
        {
            "chapter": 50,
            "verse": 17,
            "text": "iẕ yeteleḳḳe-lmüteleḳḳiyâni `ani-lyemîni ve`ani-şşimâli ḳa`îd"
        },
        {
            "chapter": 50,
            "verse": 18,
            "text": "mâ yelfiżu min ḳavlin illâ ledeyhi raḳîbün `atîd"
        },
        {
            "chapter": 50,
            "verse": 19,
            "text": "vecâet sekratü-lmevt bilḥaḳḳ. ẕâlike mâ künte minhü teḥîd"
        },
        {
            "chapter": 50,
            "verse": 20,
            "text": "venüfiḫa fi-ṣṣûr. ẕâlike yevmü-lve`îd"
        },
        {
            "chapter": 50,
            "verse": 21,
            "text": "vecâet küllü nefsim me`ahâ sâiḳuv veşehîd"
        },
        {
            "chapter": 50,
            "verse": 22,
            "text": "leḳad künte fî gafletim min hâẕâ fekeşefnâ `anke giṭâeke febeṣaruke-lyevme ḥadîd"
        },
        {
            "chapter": 50,
            "verse": 23,
            "text": "veḳâle ḳarînühû hâẕâ mâ ledeyye `atîd"
        },
        {
            "chapter": 50,
            "verse": 24,
            "text": "elḳiyâ fî cehenneme külle keffârin `anîd"
        },
        {
            "chapter": 50,
            "verse": 25,
            "text": "mennâ`il lilḫayri mü`tedim mürîbün"
        },
        {
            "chapter": 50,
            "verse": 26,
            "text": "elleẕî ce`ale me`a-llâhi ilâhen âḫara feelḳiyâhü fi-l`aẕâbi-şşedîd"
        },
        {
            "chapter": 50,
            "verse": 27,
            "text": "ḳâle ḳarînühû rabbenâ mâ aṭgaytühû velâkin kâne fî ḍalâlim be`îd"
        },
        {
            "chapter": 50,
            "verse": 28,
            "text": "ḳâle lâ taḫteṣimû ledeyye veḳad ḳaddemtü ileyküm bilve`îd"
        },
        {
            "chapter": 50,
            "verse": 29,
            "text": "mâ yübeddelü-lḳavlü ledeyye vemâ ene biżallâmil lil`abîd"
        }
    ]
}