{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "veḍḍuḥâ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "velleyli iẕâ secâ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "mâ vedde`ake rabbüke vemâ ḳalâ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "velel'âḫiratü ḫayrul leke mine-l'ûlâ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "velesevfe yü`ṭîke rabbüke feterḍâ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "elem yecidke yetîmen feâvâ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "vevecedeke ḍâllen fehedâ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "vevecedeke `âilen feagnâ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "feemme-lyetîme felâ taḳher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "veemme-ssâile felâ tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "veemmâ bini`meti rabbike feḥaddiŝ"
        }
    ]
}