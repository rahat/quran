{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "iẕâ zülzileti-l'arḍu zilzâlehâ"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "veaḫraceti-l'arḍu eŝḳâlehâ"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "veḳâle-l'insânü mâ lehâ"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "yevmeiẕin tüḥaddiŝü aḫbârahâ"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "bienne rabbeke evḥâ lehâ"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "yevmeiẕiy yaṣdüru-nnâsü eştâtel liyürav a`mâlehüm"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "femey ya`mel miŝḳâle ẕerratin ḫayray yerah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "vemey ya`mel miŝḳâle ẕerratin şerray yerah"
        }
    ]
