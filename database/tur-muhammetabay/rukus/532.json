{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "lâ uḳsimü bihâẕe-lbeled"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "veente ḥillüm bihâẕe-lbeled"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "vevâlidiv vemâ veled"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "leḳad ḫalaḳne-l'insâne fî kebed"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "eyaḥsebü el ley yaḳdira `aleyhi eḥad"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "yeḳûlü ehlektü mâlel lübedâ"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "eyaḥsebü el lem yerahû eḥad"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "elem nec`al lehû `ayneyn"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "velisânev veşefeteyn"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "vehedeynâhü-nnecdeyn"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "fele-ḳteḥame-l`aḳabeh"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "vemâ edrâke me-l`aḳabeh"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "fekkü raḳabeh"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ev iṭ`âmün fî yevmin ẕî mesgabeh"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yetîmen ẕâ maḳrabeh"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ev miskînen ẕâ metrabeh"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ŝümme kâne mine-lleẕîne âmenû vetevâṣav biṣṣabri vetevâṣav bilmerḥameh"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ülâike aṣḥâbü-lmeymeneh"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "velleẕîne keferû biâyâtinâ hüm aṣḥâbü-lmeş'emeh"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "aleyhim nârum mü'ṣadeh"
        }
    ]
}