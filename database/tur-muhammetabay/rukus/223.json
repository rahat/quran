{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "veleḳad keẕẕebe aṣḥâbü-lḥicri-lmürselîn"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "veâteynâhüm âyâtinâ fekânû `anhâ mü`riḍîn"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "vekânû yenḥitûne mine-lcibâli büyûten âminîn"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "feeḫaẕethümu-ṣṣayḥatü muṣbiḥîn"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "femâ agnâ `anhüm mâ kânû yeksibûn"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "vemâ ḫalaḳne-ssemâvâti vel'arḍa vemâ beynehümâ illâ bilḥaḳḳi. veinne-ssâ`ate leâtiyetün faṣfeḥi-ṣṣafḥa-lcemîl"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "inne rabbeke hüve-lḫallâḳu-l`alîm"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "veleḳad âteynâke seb`am mine-lmeŝânî velḳur'âne-l`ażîm"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "lâ temüddenne `ayneyke ilâ mâ metta`nâ bihî ezvâcem minhüm velâ taḥzen `aleyhim vaḫfiḍ cenâḥake lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "veḳul innî ene-nneẕîru-lmübîn"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "kemâ enzelnâ `ale-lmuḳtesimîn"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "elleẕîne ce`alü-lḳur'âne `iḍîn"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "feverabbike lenes'elennehüm ecme`în"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "ammâ kânû ya`melûn"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "faṣda` bimâ tü'meru vea`riḍ `ani-lmüşrikîn"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "innâ kefeynâke-lmüstehziîn"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "elleẕîne yec`alûne me`a-llâhi ilâhen âḫar. fesevfe ya`lemûn"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "veleḳad na`lemü enneke yeḍîḳu ṣadruke bimâ yeḳûlûn"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "fesebbiḥ biḥamdi rabbike veküm mine-ssâcidîn"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "va`büd rabbeke ḥattâ ye'tiyeke-lyeḳîn"
        }
    ]
}