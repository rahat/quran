{
    "rukus": [
        {
            "chapter": 70,
            "verse": 36,
            "text": "femâ lilleẕîne keferû ḳibeleke mühti`în"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "ani-lyemîni ve`ani-şşimâli `izîn"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "eyaṭme`u küllü-mriim minhüm ey yüdḫale cennete ne`îm"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "kellâ. innâ ḫalaḳnâhüm mimmâ ya`lemûn"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "felâ uḳsimü birabbi-lmeşâriḳi velmegâribi innâ leḳâdirûn"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "alâ en nübeddile ḫayram minhüm vemâ naḥnü bimesbûḳîn"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "feẕerhüm yeḫûḍû veyel`abû ḥattâ yülâḳû yevmehümü-lleẕî yû`adûn"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "yevme yaḫrucûne mine-l'ecdâŝi sirâ`an keennehüm ilâ nüṣubiy yûfiḍûn"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "ḫâşi`aten ebṣâruhüm terheḳuhüm ẕilleh. ẕâlike-lyevmü-lleẕî kânû yû`adûn"
        }
    ]
}