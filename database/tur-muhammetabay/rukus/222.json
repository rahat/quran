{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "felemmâ câe âle lûṭini-lmürselûn"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "ḳâle inneküm ḳavmüm münkerûn"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "ḳâlû bel ci'nâke bimâ kânû fîhi yemterûn"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "veeteynâke bilḥaḳḳi veinnâ leṣâdiḳûn"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "feesri biehlike biḳiṭ`im mine-lleyli vettebi` edbârahüm velâ yeltefit minküm eḥadüv vemḍû ḥayŝü tü'merûn"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "veḳaḍaynâ ileyhi ẕâlike-l'emra enne dâbira hâülâi maḳṭû`um muṣbiḥîn"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "vecâe ehlü-lmedîneti yestebşirûn"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "ḳâle inne hâülâi ḍayfî felâ tefḍaḥûn"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "vetteḳu-llâhe velâ tuḫzûn"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "ḳâlû evelem nenheke `ani-l`âlemîn"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "ḳâle hâülâi benâtî in küntüm fâ`ilîn"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "le`amruke innehüm lefî sekratihim ya`mehûn"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "feeḫaẕethümu-ṣṣayḥatü müşriḳîn"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fece`alnâ `âliyehâ sâfilehâ veemṭarnâ `aleyhim ḥicâratem min siccîl"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inne fî ẕâlike leâyâtil lilmütevessimîn"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "veinnehâ lebisebîlim müḳîm"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inne fî ẕâlike leâyetel lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "vein kâne aṣḥâbü-l'eyketi leżâlimîn"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fenteḳamnâ minhüm. veinnehümâ lebiimâmim mübîn"
        }
    ]
}