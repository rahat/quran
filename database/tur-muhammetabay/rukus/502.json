{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "seele sâilüm bi`aẕâbiv vâḳi"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "lilkâfirîne leyse lehû dâfi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "mine-llâhi ẕi-lme`âric"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "ta`rucü-lmelâiketü verrûḥu ileyhi fî yevmin kâne miḳdâruhû ḫamsîne elfe seneh"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "faṣbir ṣabran cemîlâ"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "innehüm yeravnehû be`îdâ"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "venerâhü ḳarîbâ"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "yevme tekûnü-ssemâü kelmühl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "vetekûnü-lcibâlü kel`ihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "velâ yes'elü ḥamîmün ḥamîmâ"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "yübeṣṣarûnehüm. yeveddü-lmücrimü lev yeftedî min `aẕâbi yevmiiẕim bibenîh"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "veṣâḥibetihî veeḫîh"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "vefeṣîletihi-lletî tü'vîh"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "vemen fi-l'arḍi cemî`an ŝümme yüncîh"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "kellâ. innehâ leżâ"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "nezzâ`atel lişşevâ"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "ted`û men edbera vetevellâ"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "veceme`a feev`â"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "inne-l'insâne ḫuliḳa helû`â"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "iẕâ messehü-şşerru cezû`â"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "veiẕâ messehü-lḫayru menû`â"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "ille-lmüṣallîn"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "elleẕîne hüm `alâ ṣalâtihim dâimûn"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "velleẕîne fî emvâlihim ḥaḳḳum ma`lûm"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "lissâili velmaḥrûm"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "velleẕîne yüṣaddiḳûne biyevmi-ddîn"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "velleẕîne hüm min `aẕâbi rabbihim müşfiḳûn"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "inne `aẕâbe rabbihim gayru me'mûn"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "velleẕîne hüm lifürûcihim ḥâfiżûn"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "illâ `alâ ezvâcihim ev mâ meleket eymânühüm feinnehüm gayru melûmîn"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "femeni-btegâ verâe ẕâlike feülâike hümü-l`âdûn"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "velleẕîne hüm liemânâtihim ve`ahdihim râ`ûn"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "velleẕîne hüm bişehâdetihim ḳâimûn"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "velleẕîne hüm `alâ ṣalâtihim yüḥâfiżûn"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "ülâike fî cennâtim mükramûn"
        }
    ]
}