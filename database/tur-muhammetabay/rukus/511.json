{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "kellâ velḳamer"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "velleyli iẕ edbera"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "veṣṣubḥi iẕâ esfera"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "innehâ leiḥde-lküber"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "neẕîral lilbeşer"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "limen şâe minküm ey yeteḳaddeme ev yeteeḫḫar"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "küllü nefsim bimâ kesebet rahîneh"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "illâ aṣḥâbe-lyemîn"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "fî cennâtin. yetesâelûn"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ani-lmücrimîn"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "mâ selekeküm fî seḳara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "ḳâlû lem nekü mine-lmüṣallîn"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "velem nekü nuṭ`imü-lmiskîn"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "vekünnâ neḫûḍu me`a-lḫâiḍîn"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "vekünnâ nükeẕẕibü biyevmi-ddîn"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "ḥattâ etâne-lyeḳîn"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "femâ tenfe`uhüm şefâ`atü-şşâfi`în"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "femâ lehüm `ani-tteẕkirati mü`riḍîn"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "keennehüm ḥumürum müstenfirah"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "ferrat min ḳasverah"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "bel yürîdü küllü-mriim minhüm ey yü'tâ ṣuḥufem müneşşerah"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "kellâ. bel lâ yeḫâfûne-l'âḫirah"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "kellâ innehû teẕkirah"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "femen şâe ẕekerah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "vemâ yeẕkürûne illâ ey yeşâe-llâh. hüve ehlü-ttaḳvâ veehlü-lmagfirah"
        }
    ]
}