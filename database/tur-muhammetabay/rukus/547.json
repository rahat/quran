{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "elem tera keyfe fe`ale rabbüke biaṣḥâbi-lfîl"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "elem yec`al keydehüm fî taḍlîl"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "veersele `aleyhim ṭayran ebâbîl"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "termîhim biḥicâratim min siccîl"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "fece`alehüm ke`aṣfim me'kûl"
        }
    ]
}