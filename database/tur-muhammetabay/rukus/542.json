{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "vel`âdiyâti ḍabḥâ"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "felmûriyâti ḳadḥâ"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "felmügîrâti ṣubḥâ"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "feeŝerne bihî naḳ`â"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "fevesaṭne bihî cem`â"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "inne-l'insâne lirabbihî lekenûd"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "veinnehû `alâ ẕâlike leşehîd"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "veinnehû liḥubbi-lḫayri leşedîd"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "efelâ ya`lemü iẕâ bü`ŝira mâ fi-lḳubûr"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "veḥuṣṣile mâ fi-ṣṣudûr"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "inne rabbehüm bihim yevmeiẕil leḫabîr"
        }
    ]
}