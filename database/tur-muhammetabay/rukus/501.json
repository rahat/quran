{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "felâ uḳsimü bimâ tübṣirûn"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "vemâ lâ tübṣirûn"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "innehû leḳavlü rasûlin kerîm"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "vemâ hüve biḳavli şâ`ir. ḳalîlem mâ tü'minûn"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "velâ biḳavli kâhin. ḳalîlem mâ teẕekkerûn"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "tenzîlüm mir rabbi-l`âlemîn"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "velev teḳavvele `aleynâ ba`ḍa-l'eḳâvîl"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "leeḫaẕnâ minhü bilyemîn"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "ŝümme leḳaṭa`nâ minhü-lvetîn"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "femâ minküm min eḥadin `anhü ḥâcizîn"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "veinnehû leteẕkiratül lilmütteḳîn"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "veinnâ lena`lemü enne minküm mükeẕẕibîn"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "veinnehû leḥasratün `ale-lkâfirîn"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "veinnehû leḥaḳḳu-lyeḳîn"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "fesebbiḥ bismi rabbike-l`ażîm"
        }
    ]
}