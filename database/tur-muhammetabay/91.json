{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "veşşemsi veḍuḥâhâ"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "velḳameri iẕâ telâhâ"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "vennehâri iẕâ cellâhâ"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "velleyli iẕâ yagşâhâ"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "vessemâi vemâ benâhâ"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "vel'arḍi vemâ ṭaḥâhâ"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "venefsiv vemâ sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "feelhemehâ fücûrahâ vetaḳvâhâ"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "ḳad efleḥa men zekkâhâ"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "veḳad ḫâbe men dessâhâ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "keẕẕebet ŝemûdü biṭagvâhâ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "iẕi-mbe`aŝe eşḳâhâ"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "feḳâle lehüm rasûlü-llâhi nâḳate-llâhi vesuḳyâhâ"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "fekeẕẕebûhü fe`aḳarûhâ. fedemdeme `aleyhim rabbühüm biẕembihim fesevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "velâ yeḫâfü `uḳbâhâ"
        }
    ]
