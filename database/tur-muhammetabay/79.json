{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "vennâzi`âti garḳâ"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "vennâşiṭâti neşṭâ"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "vessâbiḥâti sebḥâ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fessâbiḳâti sebḳâ"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "felmüdebbirâti emrâ"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yevme tercüfü-rrâcifeh"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tetbe`uhe-rrâdifeh"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "ḳulûbüy yevmeiẕiv vâcifeh"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "ebṣâruhâ ḫâşi`ah"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yeḳûlûne einnâ lemerdûdûne fi-lḥâfirah"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "eiẕâ künnâ `iżâmen neḫirah"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "ḳâlû tilke iẕen kerratün ḫâsirah"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "feinnemâ hiye zecratüv vâḥideh"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "feiẕâ hüm bissâhirah"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hel etâke ḥadîŝü mûsâ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "iẕ nâdâhü rabbühû bilvâdi-lmüḳaddesi ṭuvâ"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "iẕheb ilâ fir`avne innehû ṭagâ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "feḳul hel leke ilâ en tezekkâ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "veehdiyeke ilâ rabbike fetaḫşâ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "feerâhü-l'âyete-lkübrâ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fekeẕẕebe ve`aṣâ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ŝümme edbera yes`â"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "feḥaşera fenâdâ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "feḳâle ene rabbükümü-l'a`lâ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "feeḫaẕehü-llâhü nekâle-l'âḫirati vel'ûlâ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inne fî ẕâlike le`ibratel limey yaḫşâ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "eentüm eşeddü ḫalḳan emi-ssemâü. benâhâ"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafe`a semkehâ fesevvâhâ"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "veagṭaşe leylehâ veaḫrace ḍuḥâhâ"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "vel'arḍa ba`de ẕâlike deḥâhâ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "aḫrace minhâ mâehâ vemer`âhâ"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "velcibâle ersâhâ"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "metâ`al leküm velien`âmiküm"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "feiẕâ câeti-ṭṭâmmetü-lkübrâ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yevme yeteẕekkeru-l'insânü mâ se`â"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "vebürrizeti-lceḥîmü limey yerâ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "feemmâ men ṭagâ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "veâŝera-lḥayâte-ddünyâ"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "feinne-lceḥîme hiye-lme'vâ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "veemmâ men ḫâfe meḳâme rabbihî venehe-nnefse `ani-lhevâ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "feinne-lcennete hiye-lme'vâ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yes'elûneke `ani-ssâ`ati eyyâne mürsâhâ"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fîme ente min ẕikrâhâ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ilâ rabbike müntehâhâ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innemâ ente münẕiru mey yaḫşâhâ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "keennehüm yevme yeravnehâ lem yelbeŝû illâ `aşiyyeten ev ḍuḥâhâ"
        }
    ]
}