{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "abese vetevellâ"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "en câehü-l'a`mâ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "vemâ yüdrîke le`allehû yezzekkâ"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ev yeẕẕekkeru fetenfe`ahü-ẕẕikrâ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "emmâ meni-stagnâ"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "feente lehû teṣaddâ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "vemâ `aleyke ellâ yezzekkâ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "veemmâ men câeke yes`â"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "vehüve yaḫşâ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "feente `anhü telehhâ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "kellâ innehâ teẕkirah"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "femen şâe ẕekerah"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "fî ṣuḥufim mükerrameh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "merfû`atim müṭahherah"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bieydî seferah"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kirâmim berarah"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "ḳutile-l'insânü mâ ekferah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "min eyyi şey'in ḫaleḳah"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "min nuṭfeh. ḫaleḳahû feḳadderah"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ŝümme-ssebîle yesserah"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ŝümme emâtehû feaḳberah"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ŝümme iẕâ şâe enşerah"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "kellâ lemmâ yaḳḍi mâ emerah"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "felyenżuri-l'insânü ilâ ṭa`âmih"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "ennâ ṣabebne-lmâe ṣabbâ"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ŝümme şaḳaḳne-l'arḍa şeḳḳâ"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "feembetnâ fîhâ ḥabbâ"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ve`inebev veḳaḍbâ"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "vezeytûnev venaḫlâ"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "veḥadâiḳa gulbâ"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "vefâkihetev veebbâ"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "metâ`al leküm velien`âmiküm"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "feiẕâ câeti-ṣṣâḫḫah"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "yevme yefirru-lmerü min eḫîh"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "veümmihî veebîh"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "veṣâḥibetihî vebenîh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "likülli-mriim minhüm yevmeiẕin şe'nüy yugnîh"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "vucûhüy yevmeiẕim müsfirah"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ḍâḥiketüm müstebşirah"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "vevucûhüy yevmeiẕin `aleyhâ gaberah"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "terheḳuhâ ḳaterah"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ülâike hümü-lkeferatü-lfecerah"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "iẕe-şşemsü küvvirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "veiẕe-nnücûmü-nkederat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "veiẕe-lcibâlü süyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "veiẕe-l`işâru `uṭṭilet"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "veiẕe-lvuḥûşü ḥuşirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "veiẕe-lbiḥâru süccirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "veiẕe-nnüfûsü züvvicet"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "veiẕe-lmev'ûdetü süilet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bieyyi ẕembin ḳutilet"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "veiẕe-ṣṣuḥufü nüşirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "veiẕe-ssemâü küşiṭat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "veiẕe-lceḥîmü sü``irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "veiẕe-lcennetü üzlifet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "alimet nefsüm mâ aḥḍarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "felâ uḳsimü bilḫunnes"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "elcevâri-lkünnes"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "velleyli iẕâ `as`as"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "veṣṣubḥi iẕâ teneffes"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "innehû leḳavlü rasûlin kerîm"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ẕî ḳuvvetin `inde ẕi-l`arşi mekîn"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "müṭâ`in ŝemme emîn"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "vemâ ṣâḥibüküm bimecnûn"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "veleḳad raâhü bil'üfüḳi-lmübîn"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "vemâ hüve `ale-lgaybi biḍanîn"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "vemâ hüve biḳavli şeyṭânir racîm"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "feeyne teẕhebûn"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "in hüve illâ ẕikrul lil`âlemîn"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "limen şâe minküm ey yesteḳîm"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "vemâ teşâûne illâ ey yeşâe-llâhü rabbü-l`âlemîn"
        }
    ]
}