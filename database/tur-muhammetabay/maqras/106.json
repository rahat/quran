{
    "maqras": [
        {
            "chapter": 15,
            "verse": 50,
            "text": "veenne `aẕâbî hüve-l`aẕâbü-l'elîm"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "venebbi'hüm `an ḍayfi ibrâhîm"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "iẕ deḫalû `aleyhi feḳâlû selâmâ. ḳâle innâ minküm vecilûn"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "ḳâlû lâ tevcel innâ nübeşşiruke bigulâmin `alîm"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "ḳâle ebeşşertümûnî `alâ em messeniye-lkiberu febime tübeşşirûn"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "ḳâlû beşşernâke bilḥaḳḳi felâ teküm mine-lḳâniṭîn"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "ḳâle vemey yaḳneṭu mir raḥmeti rabbihî ille-ḍḍâllûn"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "illâ âle lûṭ. innâ lemüneccûhüm ecme`în"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "ille-mraetehû ḳaddernâ innehâ lemine-lgâbirîn"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "felemmâ câe âle lûṭini-lmürselûn"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "ḳâle inneküm ḳavmüm münkerûn"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "ḳâlû bel ci'nâke bimâ kânû fîhi yemterûn"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "veeteynâke bilḥaḳḳi veinnâ leṣâdiḳûn"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "feesri biehlike biḳiṭ`im mine-lleyli vettebi` edbârahüm velâ yeltefit minküm eḥadüv vemḍû ḥayŝü tü'merûn"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "veḳaḍaynâ ileyhi ẕâlike-l'emra enne dâbira hâülâi maḳṭû`um muṣbiḥîn"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "vecâe ehlü-lmedîneti yestebşirûn"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "ḳâle inne hâülâi ḍayfî felâ tefḍaḥûn"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "vetteḳu-llâhe velâ tuḫzûn"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "ḳâlû evelem nenheke `ani-l`âlemîn"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "ḳâle hâülâi benâtî in küntüm fâ`ilîn"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "le`amruke innehüm lefî sekratihim ya`mehûn"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "feeḫaẕethümu-ṣṣayḥatü müşriḳîn"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fece`alnâ `âliyehâ sâfilehâ veemṭarnâ `aleyhim ḥicâratem min siccîl"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inne fî ẕâlike leâyâtil lilmütevessimîn"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "veinnehâ lebisebîlim müḳîm"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inne fî ẕâlike leâyetel lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "vein kâne aṣḥâbü-l'eyketi leżâlimîn"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fenteḳamnâ minhüm. veinnehümâ lebiimâmim mübîn"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "veleḳad keẕẕebe aṣḥâbü-lḥicri-lmürselîn"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "veâteynâhüm âyâtinâ fekânû `anhâ mü`riḍîn"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "vekânû yenḥitûne mine-lcibâli büyûten âminîn"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "feeḫaẕethümu-ṣṣayḥatü muṣbiḥîn"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "femâ agnâ `anhüm mâ kânû yeksibûn"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "vemâ ḫalaḳne-ssemâvâti vel'arḍa vemâ beynehümâ illâ bilḥaḳḳi. veinne-ssâ`ate leâtiyetün faṣfeḥi-ṣṣafḥa-lcemîl"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "inne rabbeke hüve-lḫallâḳu-l`alîm"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "veleḳad âteynâke seb`am mine-lmeŝânî velḳur'âne-l`ażîm"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "lâ temüddenne `ayneyke ilâ mâ metta`nâ bihî ezvâcem minhüm velâ taḥzen `aleyhim vaḫfiḍ cenâḥake lilmü'minîn"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "veḳul innî ene-nneẕîru-lmübîn"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "kemâ enzelnâ `ale-lmuḳtesimîn"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "elleẕîne ce`alü-lḳur'âne `iḍîn"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "feverabbike lenes'elennehüm ecme`în"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "ammâ kânû ya`melûn"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "faṣda` bimâ tü'meru vea`riḍ `ani-lmüşrikîn"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "innâ kefeynâke-lmüstehziîn"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "elleẕîne yec`alûne me`a-llâhi ilâhen âḫar. fesevfe ya`lemûn"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "veleḳad na`lemü enneke yeḍîḳu ṣadruke bimâ yeḳûlûn"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "fesebbiḥ biḥamdi rabbike veküm mine-ssâcidîn"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "va`büd rabbeke ḥattâ ye'tiyeke-lyeḳîn"
        }
    ]
}