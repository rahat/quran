{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sebbiḥi-sme rabbike-l'a`lâ"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "elleẕî ḫaleḳa fesevvâ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "velleẕî ḳaddera fehedâ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "velleẕî aḫrace-lmer`â"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fece`alehû guŝâen aḥvâ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "senuḳriüke felâ tensâ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illâ mâ şâe-llâh. innehû ya`lemü-lcehra vemâ yaḫfâ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "venüyessiruke lilyüsrâ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "feẕekkir in nefe`ati-ẕẕikrâ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "seyeẕẕekkeru mey yaḫşâ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "veyetecennebühe-l'eşḳâ"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "elleẕî yaṣle-nnâra-lkübrâ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ŝümme lâ yemûtü fîhâ velâ yaḥyâ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "ḳad efleḥa men tezekkâ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "veẕekera-sme rabbihî feṣallâ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bel tü'ŝirûne-lḥayâte-ddünyâ"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "vel'âḫiratü ḫayruv veebḳâ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inne hâẕâ lefi-ṣṣuḥufi-l'ûlâ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ṣuḥufi ibrâhîme vemûsâ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "hel etâke ḥadîŝü-lgâşiyeh"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "vucûhüy yevmeiẕin ḫâşi`ah"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "âmiletün nâṣibeh"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "taṣlâ nâran ḥâmiyeh"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tüsḳâ min `aynin âniyeh"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "leyse lehüm ṭa`âmün illâ min ḍarî"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "lâ yüsminü velâ yugnî min cû"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "vucûhüy yevmeiẕin nâ`imeh"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lisa`yihâ râḍiyeh"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fî cennetin `âliyeh"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "lâ tesme`u fîhâ lâgiyeh"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "fîhâ `aynün câriyeh"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fîhâ sürurum merfû`ah"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "veekvâbüm mevḍû`ah"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "venemâriḳu maṣfûfeh"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "vezerâbiyyü mebŝûŝeh"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "efelâ yenżurûne ile-l'ibili keyfe ḫuliḳat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "veile-ssemâi keyfe rufi`at"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "veile-lcibâli keyfe nüṣibet"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "veile-l'arḍi keyfe süṭiḥat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "feẕekkir innemâ ente müẕekkir"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "leste `aleyhim bimüṣayṭir"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "illâ men tevellâ vekefera"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "feyü`aẕẕibühü-llâhü-l`aẕâbe-l'ekber"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "inne ileynâ iyâbehüm"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ŝümme inne `aleynâ ḥisâbehüm"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "velfecr"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "veleyâlin `aşr"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "veşşef`i velvetr"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "velleyli iẕâ yesr"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hel fî ẕâlike ḳasemül liẕî ḥicr"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "elem tera keyfe fe`ale rabbüke bi`âd"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irame ẕâti-l`imâd"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "elletî lem yuḫlaḳ miŝlühâ fi-lbilâd"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "veŝemûde-lleẕîne câbu-ṣṣaḫra bilvâd"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "vefir`avne ẕi-l'evtâd"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "elleẕîne ṭagav fi-lbilâd"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "feekŝerû fîhe-lfesâd"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "feṣabbe `aleyhim rabbüke sevṭa `aẕâb"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inne rabbeke lebilmirṣâd"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "feemme-l'insânü iẕâ me-btelâhü rabbühû feekramehû vene``amehû feyeḳûlü rabbî ekramen"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "veemmâ iẕâ me-btelâhü feḳadera `aleyhi rizḳahû feyeḳûlü rabbî ehânen"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kellâ bel lâ tükrimûne-lyetîm"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "velâ teḥâḍḍûne `alâ ṭa`âmi-lmiskîn"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "vete'külûne-ttürâŝe eklel lemmâ"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "vetüḥibbûne-lmâle ḥubben cemmâ"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kellâ iẕâ dükketi-l'arḍu dekken dekkâ"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "vecâe rabbüke velmelekü ṣaffen ṣaffâ"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "vecîe yevmeiẕim bicehenneme yevmeiẕiy yeteẕekkeru-l'insânü veennâ lehü-ẕẕikrâ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yeḳûlü yâ leytenî ḳaddemtü liḥayâtî"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "feyevmeiẕil lâ yü`aẕẕibü `aẕâbehû eḥad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "velâ yûŝiḳu veŝâḳahû eḥad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "yâ eyyetühe-nnefsü-lmuṭmeinneh"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "irci`î ilâ rabbiki râḍiyetem merḍiyyeh"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fedḫulî fî `ibâdî"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "vedḫulî cennetî"
        }
    ]
}