{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "iẕe-ssemâü-nşeḳḳat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "veeẕinet lirabbihâ veḥuḳḳat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "veiẕe-l'arḍu müddet"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "veelḳat mâ fîhâ veteḫallet"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "veeẕinet lirabbihâ veḥuḳḳat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "yâ eyyühe-l'insânü inneke kâdiḥun ilâ rabbike kedḥan femülâḳîh"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "feemmâ men ûtiye kitâbehû biyemînih"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "fesevfe yüḥâsebü ḥisâbey yesîrâ"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "veyenḳalibü ilâ ehlihî mesrûrâ"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "veemmâ men ûtiye kitâbehû verâe żahrih"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "fesevfe yed`û ŝübûrâ"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "veyaṣlâ se`îrâ"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "innehû kâne fî ehlihî mesrûrâ"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "innehû żanne el ley yeḥûr"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "belâ. inne rabbehû kâne bihî beṣîrâ"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "felâ uḳsimü bişşefeḳ"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "velleyli vemâ veseḳa"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "velḳameri iẕe-tteseḳa"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "leterkebünne ṭabeḳan `an ṭabaḳ"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "femâ lehüm lâ yü'minûn"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "veiẕâ ḳurie `aleyhimü-lḳur'ânü lâ yescüdûn"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "beli-lleẕîne keferû yükeẕẕibûn"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "vellâhü a`lemü bimâ yû`ûn"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "febeşşirhüm bi`aẕâbin elîm"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "ille-lleẕîne âmenû ve`amilu-ṣṣâliḥâti lehüm ecrun gayru memnûn"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "vessemâi ẕâti-lbürûc"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "velyevmi-lmev`ûd"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "veşâhidiv vemeşhûd"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "ḳutile aṣḥâbü-l'uḫdûd"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ennâri ẕâti-lveḳûd"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "iẕ hüm `aleyhâ ḳu`ûd"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "vehüm `alâ mâ yef`alûne bilmü'minîne şühûd"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "vemâ neḳamû minhüm illâ ey yü'minû billâhi-l`azîzi-lḥamîd"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "elleẕî lehû mülkü-ssemâvâti vel'arḍ. vellâhü `alâ külli şey'in şehîd"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inne-lleẕîne fetenü-lmü'minîne velmü'minâti ŝümme lem yetûbû felehüm `aẕâbü cehenneme velehüm `aẕâbü-lḥarîḳ"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inne-lleẕîne âmenû ve`amilu-ṣṣâliḥâti lehüm cennâtün tecrî min taḥtihe-l'enhâr. ẕâlike-lfevzü-lkebîr"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "inne baṭşe rabbike leşedîd"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "innehû hüve yübdiü veyü`îd"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "vehüve-lgafûru-lvedûd"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "ẕü-l`arşi-lmecîd"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "fa``âlül limâ yürîd"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "hel etâke ḥadîŝü-lcünûd"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "fir`avne veŝemûd"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "beli-lleẕîne keferû fî tekẕîb"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "vellâhü miv verâihim müḥîṭ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "bel hüve ḳur'ânüm mecîd"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "fî levḥim maḥfûż"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "vessemâi veṭṭâriḳ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "vemâ edrâke me-ṭṭâriḳ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ennecmü-ŝŝâḳib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "in küllü nefsil lemmâ `aleyhâ ḥâfiż"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "felyenżuri-l'insânü mimme ḫuliḳ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ḫuliḳa mim mâin dâfiḳ"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yaḫrucü mim beyni-ṣṣulbi vetterâib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "innehû `alâ rac`ihî leḳâdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "yevme tüble-sserâir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "femâ lehû min ḳuvvetiv velâ nâṣir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "vessemâi ẕâti-rrac"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "vel'arḍi ẕâti-ṣṣad"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "innehû leḳavlün faṣl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "vemâ hüve bilhezl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "innehüm yekîdûne keydâ"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "veekîdü keydâ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "femehhili-lkâfirîne emhilhüm ruveydâ"
        }
    ]
}