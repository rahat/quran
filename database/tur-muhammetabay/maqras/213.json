{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "erraḥmân"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "alleme-lḳur'ân"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "ḫaleḳa-l'insân"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "allemehü-lbeyân"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "eşşemsü velḳameru biḥusbân"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "vennecmü veşşeceru yescüdân"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "vessemâe rafe`ahâ veveḍa`a-lmîzân"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "ellâ taṭgav fi-lmîzân"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "veeḳîmü-lvezne bilḳisṭi velâ tuḫsirü-lmîzân"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "vel'arḍa veḍa`ahâ lil'enâm"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "fîhâ fâkiheh. vennaḫlü ẕâtü-l'ekmâm"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "velḥabbü ẕü-l`aṣfi verrayḥân"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "ḫaleḳa-l'insâne min ṣalṣâlin kelfeḫḫâr"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "veḫaleḳa-lcânne mim mâricim min nâr"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "rabbü-lmeşriḳayni verabbü-lmagribeyn"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "merace-lbaḥrayni yelteḳiyân"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "beynehümâ berzeḫul lâ yebgiyân"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "yaḫrucü minhüme-llü'lüü velmercân"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "velehü-lcevâri-lmünşeâtü fi-lbaḥri kel'a`lâm"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "küllü men `aleyhâ fân"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "veyebḳâ vechü rabbike ẕü-lcelâli vel'ikrâm"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "yes'elühû men fi-ssemâvâti vel'arḍ. külle yevmin hüve fî şe'n"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "senefrugu leküm eyyühe-ŝŝeḳalân"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "yâ ma`şera-lcinni vel'insi ini-steṭa`tüm en tenfüẕû min aḳṭâri-ssemâvâti vel'arḍi fenfüẕû. lâ tenfüẕûne illâ bisülṭân"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "yürselü `aleykümâ şüvâżum min nâriv venüḥâsün felâ tenteṣirân"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "feiẕe-nşeḳḳati-ssemâü fekânet verdeten keldihân"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "feyevmeiẕil lâ yüs'elü `an ẕembihî insüv velâ cânn"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "yü`rafü-lmücrimûne bisîmâhüm feyü'ḫaẕü binnevâṣî vel'aḳdâm"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "hâẕihî cehennemü-lletî yükeẕẕibü bihe-lmücrimûn"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "yeṭûfûne beynehâ vebeyne ḥamîmin ân"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "velimen ḫâfe meḳâme rabbihî cennetân"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ẕevâtâ efnân"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "fîhimâ `aynâni tecriyân"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "fîhimâ min külli fâkihetin zevcân"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "müttekiîne `alâ füruşim beṭâinühâ min istebraḳ. vecene-lcenneteyni dân"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "fîhinne ḳâṣirâtu-ṭṭarfi lem yaṭmiŝhünne insün ḳablehüm velâ cânn"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "keennehünne-lyâḳûtü velmercân"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "hel cezâü-l'iḥsâni ille-l'iḥsân"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "vemin dûnihimâ cennetân"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "müẕâmmetân"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "fîhimâ `aynâni neḍḍâḫatân"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "fîhimâ fâkihetüv venaḫlüv verummân"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "fîhinne ḫayrâtün ḥisân"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "ḥûrum maḳṣûrâtün fi-lḫiyâm"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "lem yaṭmiŝhünne insün ḳablehüm velâ cânn"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "müttekiîne `alâ rafrafin ḫuḍriv ve`abḳariyyin ḥisân"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "febieyyi âlâi rabbikümâ tükeẕẕibân"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "tebârake-smü rabbike ẕi-lcelâli vel'ikrâm"
        }
    ]
}