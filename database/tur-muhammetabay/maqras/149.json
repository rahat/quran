{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "ḳâlû enü'minü leke vettebe`ake-l'erẕelûn"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "ḳâle vemâ `ilmî bimâ kânû ya`melûn"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "in ḥisâbühüm illâ `alâ rabbî lev teş`urûn"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "vemâ ene biṭâridi-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "in ene illâ neẕîrum mübîn"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "ḳâlû leil lem tentehi yâ nûḥu letekûnenne mine-lmercûmîn"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "ḳâle rabbi inne ḳavmî keẕẕebûn"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "feftaḥ beynî vebeynehüm fetḥav veneccinî vemem me`iye mine-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "feenceynâhü vemem me`ahû fi-lfülki-lmeşḥûn"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ŝümme agraḳnâ ba`dü-lbâḳîn"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "keẕẕebet `âdün-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "iẕ ḳâle lehüm eḫûhüm hûdün elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "etebnûne bikülli rî`in âyeten ta`beŝûn"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "vetetteḫiẕûne meṣâni`a le`alleküm taḫlüdûn"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "veiẕâ beṭaştüm beṭaştüm cebbârîn"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "vetteḳu-lleẕî emeddeküm bimâ ta`lemûn"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "emeddeküm bien`âmiv vebenîn"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "vecennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "innî eḫâfü `aleyküm `aẕâbe yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "ḳâlû sevâün `aleynâ eve`ażte em lem teküm mine-lvâ`iżîn"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "in hâẕâ illâ ḫulüḳu-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "vemâ naḥnü bimü`aẕẕebîn"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "fekeẕẕebûhü feehleknâhüm. inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "keẕẕebet ŝemûdü-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "iẕ ḳâle lehüm eḫûhüm ṣâliḥun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "etütrakûne fî mâ hâhünâ âminîn"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "fî cennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "vezürû`iv venaḫlin ṭal`uhâ heḍîm"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "vetenḥitûne mine-lcibâli büyûten fârihîn"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "velâ tüṭî`û emra-lmüsrifîn"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "elleẕîne yüfsidûne fi-l'arḍi velâ yuṣliḥûn"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "ḳâlû innemâ ente mine-lmüseḥḥarîn"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "mâ ente illâ beşerum miŝlünâ. fe'ti biâyetin in künte mine-ṣṣâdiḳîn"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "ḳâle hâẕihî nâḳatül lehâ şirbüv veleküm şirbü yevmim ma`lûm"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "velâ temessûhâ bisûin feye'ḫuẕeküm `aẕâbü yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "fe`aḳarûhâ feaṣbeḥû nâdimîn"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "feeḫaẕehümü-l`aẕâb. inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "keẕẕebet ḳavmü lûṭini-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "iẕ ḳâle lehüm eḫûhüm lûṭun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "ete'tûne-ẕẕükrâne mine-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "veteẕerûne mâ ḫaleḳa leküm rabbüküm min ezvâciküm. bel entüm ḳavmün `âdûn"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "ḳâlû leil lem tentehi yâ lûṭu letekûnenne mine-lmuḫracîn"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "ḳâle innî li`ameliküm mine-lḳâlîn"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "rabbi neccinî veehlî mimmâ ya`melûn"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "fenecceynâhü veehlehû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "illâ `acûzen fi-lgâbirîn"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ŝümme demmerne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "veemṭarnâ `aleyhim meṭarâ. fesâe meṭaru-lmünẕerîn"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "keẕẕebe aṣḥâbü-l'eyketi-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "iẕ ḳâle lehüm şu`aybün elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        }
    ]
}