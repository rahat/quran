{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "veyeṭûfü `aleyhim gilmânül lehüm keennehüm lü'lüüm meknûn"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "veaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "ḳâlû innâ künnâ ḳablü fî ehlinâ müşfiḳîn"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "femenne-llâhü `aleynâ veveḳânâ `aẕâbe-ssemûm"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "innâ künnâ min ḳablü ned`ûh. innehû hüve-lberru-rraḥîm"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "feẕekkir femâ ente bini`meti rabbike bikâhiniv velâ mecnûn"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "em yeḳûlûne şâ`irun neterabbeṣu bihî raybe-lmenûn"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "ḳul terabbeṣû feinnî me`aküm mine-lmüterabbiṣîn"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "em te'müruhüm aḥlâmühüm bihâẕâ em hüm ḳavmün ṭâgûn"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "em yeḳûlûne teḳavveleh. bel lâ yü'minûn"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "felye'tû biḥadîŝim miŝlihî in kânû ṣâdiḳîn"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "em ḫuliḳû min gayri şey'in em hümü-lḫâliḳûn"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "em ḫaleḳu-ssemâvâti vel'arḍ. bel lâ yûḳinûn"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "em `indehüm ḫazâinü rabbike em hümü-lmüṣayṭirûn"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "em lehüm süllemüy yestemi`ûne fîh. felye'ti müstemi`uhüm bisülṭânim mübîn"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "em lehü-lbenâtü velekümü-lbenûn"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "em tes'elühüm ecran fehüm mim magramim müŝḳalûn"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "em `indehümü-lgaybü fehüm yektübûn"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "em yürîdûne keydâ. felleẕîne keferû hümü-lmekîdûn"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "em lehüm ilâhün gayru-llâh. sübḥâne-llâhi `ammâ yüşrikûn"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "veiy yerav kisfem mine-ssemâi sâḳiṭay yeḳûlû seḥâbüm merkûm"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "feẕerhüm ḥattâ yülâḳû yevmehümü-lleẕî fîhi yuṣ`aḳûn"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yevme lâ yugnî `anhüm keydühüm şey'ev velâ hüm yünṣarûn"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "veinne lilleẕîne żalemû `aẕâben dûne ẕâlike velâkinne ekŝerahüm lâ ya`lemûn"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "vaṣbir liḥukmi rabbike feinneke bia`yüninâ vesebbiḥ biḥamdi rabbike ḥîne teḳûm"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "vemine-lleyli fesebbiḥhü veidbâra-nnücûm"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "vennecmi iẕâ hevâ"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "mâ ḍalle ṣâḥibüküm vemâ gavâ"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "vemâ yenṭiḳu `ani-lhevâ"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in hüve illâ vaḥyüy yûḥâ"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "allemehû şedîdü-lḳuvâ"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ẕû mirrah. festevâ"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "vehüve bil'üfüḳi-l'a`lâ"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ŝümme denâ fetedellâ"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fekâne ḳâbe ḳavseyni ev ednâ"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "feevḥâ ilâ `abdihî mâ evḥâ"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "mâ keẕebe-lfüâdü mâ raâ"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "efetümârûnehû `alâ mâ yerâ"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "veleḳad raâhü nezleten uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "inde sidrati-lmüntehâ"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "indehâ cennetü-lme'vâ"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "iẕ yagşe-ssidrate mâ yagşâ"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "mâ zâga-lbeṣaru vemâ ṭagâ"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "leḳad raâ min âyâti rabbihi-lkübrâ"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "eferaeytümü-llâte vel`uzzâ"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "vemenâte-ŝŝâliŝete-l'uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "elekümü-ẕẕekeru velehü-l'ünŝâ"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "tilke iẕen ḳismetün ḍîzâ"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiye illâ esmâün semmeytümûhâ entüm veâbâüküm mâ enzele-llâhü bihâ min sülṭân. iy yettebi`ûne ille-żżanne vemâ tehve-l'enfüs. veleḳad câehüm mir rabbihimü-lhüdâ"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "em lil'insâni mâ temennâ"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "felillâhi-l'âḫiratü vel'ûlâ"
        }
    ]
}