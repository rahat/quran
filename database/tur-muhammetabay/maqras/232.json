{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "veyeṭûfü `aleyhim vildânüm müḫalledûn. iẕâ raeytehüm ḥasibtehüm lü'lüem menŝûrâ"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "veiẕâ raeyte ŝemme raeyte ne`îmev vemülken kebîrâ"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "âliyehüm ŝiyâbü sündüsin ḫuḍruv veistebraḳ. veḥullû esâvira min fiḍḍah. veseḳâhüm rabbühüm şerâben ṭahûrâ"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "inne hâẕâ kâne leküm cezâev vekâne sa`yüküm meşkûrâ"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "innâ naḥnü nezzelnâ `aleyke-lḳur'âne tenzîlâ"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "faṣbir liḥukmi rabbike velâ tüṭi` minhüm âŝimen ev kefûrâ"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "veẕküri-sme rabbike bükratev veeṣîlâ"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "vemine-lleyli fescüd lehû vesebbiḥhü leylen ṭavîlâ"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "inne hâülâi yüḥibbûne-l`âcilete veyeẕerûne verâehüm yevmen ŝeḳîlâ"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "naḥnü ḫalaḳnâhüm veşedednâ esrahüm. veiẕâ şi'nâ beddelnâ emŝâlehüm tebdîlâ"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "inne hâẕihî teẕkirah. femen şâe-tteḫaẕe ilâ rabbihî sebîlâ"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "vemâ teşâûne illâ ey yeşâe-llâh. inne-llâhe kâne `alîmen ḥakîmâ"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "yüdḫilü mey yeşâü fî raḥmetih. veżżâlimîne e`adde lehüm `aẕâben elîmâ"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "velmürselâti `urfâ"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "fel`âṣifâti `aṣfâ"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "vennâşirâti neşrâ"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "felfâriḳâti ferḳâ"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "felmülḳiyâti ẕikrâ"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "uẕran ev nüẕrâ"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "innemâ tû`adûne levâḳi"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "feiẕe-nnücûmü ṭumiset"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "veiẕe-ssemâü füricet"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "veiẕe-lcibâlü nüsifet"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "veiẕe-rrusülü üḳḳitet"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "lieyyi yevmin üccilet"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "liyevmi-lfaṣl"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "vemâ edrâke mâ yevmü-lfaṣl"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "elem nühliki-l'evvelîn"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ŝümme nütbi`uhümü-l'âḫirîn"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "keẕâlike nef`alü bilmücrimîn"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "elem naḫlukküm mim mâim mehîn"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "fece`alnâhü fî ḳarârim mekîn"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "ilâ ḳaderim ma`lûm"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "feḳadernâ. feni`me-lḳâdirûn"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "elem nec`ali-l'arḍa kifâtâ"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "aḥyâev veemvâtâ"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "vece`alnâ fîhâ ravâsiye şâmiḫâtiv veesḳaynâküm mâen fürâtâ"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "inṭaliḳû ilâ mâ küntüm bihî tükeẕẕibûn"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "inṭaliḳû ilâ żillin ẕî ŝelâŝi şu`ab"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "lâ żalîliv velâ yugnî mine-lleheb"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "innehâ termî bişerarin kelḳaṣr"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "keennehû cimâlâtün ṣufr"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "hâẕâ yevmü lâ yenṭiḳûn"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "velâ yü'ẕenü lehüm feya`teẕirûn"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "hâẕâ yevmü-lfaṣl. cema`nâküm vel'evvelîn"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "fein kâne leküm keydün fekîdûn"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "inne-lmütteḳîne fî żilâliv ve`uyûn"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "vefevâkihe mimmâ yeştehûn"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "külû veşrabû henîem bimâ küntüm ta`melûn"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "külû vetemette`û ḳalîlen inneküm mücrimûn"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "veiẕâ ḳîle lehümü-rke`û lâ yerke`ûn"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "febieyyi ḥadîŝim ba`dehû yü'minûn"
        }
    ]
}