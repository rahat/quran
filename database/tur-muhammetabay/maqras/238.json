{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "lâ uḳsimü bihâẕe-lbeled"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "veente ḥillüm bihâẕe-lbeled"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "vevâlidiv vemâ veled"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "leḳad ḫalaḳne-l'insâne fî kebed"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "eyaḥsebü el ley yaḳdira `aleyhi eḥad"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "yeḳûlü ehlektü mâlel lübedâ"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "eyaḥsebü el lem yerahû eḥad"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "elem nec`al lehû `ayneyn"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "velisânev veşefeteyn"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "vehedeynâhü-nnecdeyn"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "fele-ḳteḥame-l`aḳabeh"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "vemâ edrâke me-l`aḳabeh"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "fekkü raḳabeh"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ev iṭ`âmün fî yevmin ẕî mesgabeh"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yetîmen ẕâ maḳrabeh"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ev miskînen ẕâ metrabeh"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ŝümme kâne mine-lleẕîne âmenû vetevâṣav biṣṣabri vetevâṣav bilmerḥameh"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ülâike aṣḥâbü-lmeymeneh"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "velleẕîne keferû biâyâtinâ hüm aṣḥâbü-lmeş'emeh"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "aleyhim nârum mü'ṣadeh"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "veşşemsi veḍuḥâhâ"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "velḳameri iẕâ telâhâ"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "vennehâri iẕâ cellâhâ"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "velleyli iẕâ yagşâhâ"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "vessemâi vemâ benâhâ"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "vel'arḍi vemâ ṭaḥâhâ"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "venefsiv vemâ sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "feelhemehâ fücûrahâ vetaḳvâhâ"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "ḳad efleḥa men zekkâhâ"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "veḳad ḫâbe men dessâhâ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "keẕẕebet ŝemûdü biṭagvâhâ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "iẕi-mbe`aŝe eşḳâhâ"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "feḳâle lehüm rasûlü-llâhi nâḳate-llâhi vesuḳyâhâ"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "fekeẕẕebûhü fe`aḳarûhâ. fedemdeme `aleyhim rabbühüm biẕembihim fesevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "velâ yeḫâfü `uḳbâhâ"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "velleyli iẕâ yagşâ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "vennehâri iẕâ tecellâ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "vemâ ḫaleḳa-ẕẕekera vel'ünŝâ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "inne sa`yeküm leşettâ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "feemmâ men a`ṭâ vetteḳâ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "veṣaddeḳa bilḥusnâ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "fesenüyessiruhû lilyüsrâ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "veemmâ mem beḫile vestagnâ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "vekeẕẕebe bilḥusnâ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "fesenüyessiruhû lil`usrâ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "vemâ yugnî `anhü mâlühû iẕâ teraddâ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "inne `aleynâ lelhüdâ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "veinne lenâ lel'âḫirate vel'ûlâ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "feenẕertüküm nâran teleżżâ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "lâ yaṣlâhâ ille-l'eşḳâ"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "elleẕî keẕẕebe vetevellâ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "veseyücennebühe-l'etḳâ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "elleẕî yü'tî mâlehû yetezekkâ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "vemâ lieḥadin `indehû min ni`metin tüczâ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "ille-btigâe vechi rabbihi-l'a`lâ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "velesevfe yerḍâ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "veḍḍuḥâ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "velleyli iẕâ secâ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "mâ vedde`ake rabbüke vemâ ḳalâ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "velel'âḫiratü ḫayrul leke mine-l'ûlâ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "velesevfe yü`ṭîke rabbüke feterḍâ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "elem yecidke yetîmen feâvâ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "vevecedeke ḍâllen fehedâ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "vevecedeke `âilen feagnâ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "feemme-lyetîme felâ taḳher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "veemme-ssâile felâ tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "veemmâ bini`meti rabbike feḥaddiŝ"
        }
    ]
}