{
    "maqras": [
        {
            "chapter": 70,
            "verse": 19,
            "text": "inne-l'insâne ḫuliḳa helû`â"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "iẕâ messehü-şşerru cezû`â"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "veiẕâ messehü-lḫayru menû`â"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "ille-lmüṣallîn"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "elleẕîne hüm `alâ ṣalâtihim dâimûn"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "velleẕîne fî emvâlihim ḥaḳḳum ma`lûm"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "lissâili velmaḥrûm"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "velleẕîne yüṣaddiḳûne biyevmi-ddîn"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "velleẕîne hüm min `aẕâbi rabbihim müşfiḳûn"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "inne `aẕâbe rabbihim gayru me'mûn"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "velleẕîne hüm lifürûcihim ḥâfiżûn"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "illâ `alâ ezvâcihim ev mâ meleket eymânühüm feinnehüm gayru melûmîn"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "femeni-btegâ verâe ẕâlike feülâike hümü-l`âdûn"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "velleẕîne hüm liemânâtihim ve`ahdihim râ`ûn"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "velleẕîne hüm bişehâdetihim ḳâimûn"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "velleẕîne hüm `alâ ṣalâtihim yüḥâfiżûn"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "ülâike fî cennâtim mükramûn"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "femâ lilleẕîne keferû ḳibeleke mühti`în"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "ani-lyemîni ve`ani-şşimâli `izîn"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "eyaṭme`u küllü-mriim minhüm ey yüdḫale cennete ne`îm"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "kellâ. innâ ḫalaḳnâhüm mimmâ ya`lemûn"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "felâ uḳsimü birabbi-lmeşâriḳi velmegâribi innâ leḳâdirûn"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "alâ en nübeddile ḫayram minhüm vemâ naḥnü bimesbûḳîn"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "feẕerhüm yeḫûḍû veyel`abû ḥattâ yülâḳû yevmehümü-lleẕî yû`adûn"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "yevme yaḫrucûne mine-l'ecdâŝi sirâ`an keennehüm ilâ nüṣubiy yûfiḍûn"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "ḫâşi`aten ebṣâruhüm terheḳuhüm ẕilleh. ẕâlike-lyevmü-lleẕî kânû yû`adûn"
        },
        {
            "chapter": 71,
            "verse": 1,
            "text": "innâ erselnâ nûḥan ilâ ḳavmihî en enẕir ḳavmeke min ḳabli ey ye'tiyehüm `aẕâbün elîm"
        },
        {
            "chapter": 71,
            "verse": 2,
            "text": "ḳâle yâ ḳavmi innî leküm neẕîrum mübîn"
        },
        {
            "chapter": 71,
            "verse": 3,
            "text": "eni-`büdü-llâhe vetteḳûhü veeṭî`ûn"
        },
        {
            "chapter": 71,
            "verse": 4,
            "text": "yagfir leküm min ẕünûbiküm veyüeḫḫirküm ilâ ecelim müsemmâ. inne ecele-llâhi iẕâ câe lâ yü'eḫḫar. lev küntüm ta`lemûn"
        },
        {
            "chapter": 71,
            "verse": 5,
            "text": "ḳâle rabbi innî de`avtü ḳavmî leylev venehârâ"
        },
        {
            "chapter": 71,
            "verse": 6,
            "text": "felem yezidhüm dü`âî illâ firârâ"
        },
        {
            "chapter": 71,
            "verse": 7,
            "text": "veinnî küllemâ de`avtühüm litagfira lehüm ce`alû eṣâbi`ahüm fî âẕânihim vestagşev ŝiyâbehüm veeṣarru vestekberü-stikbârâ"
        },
        {
            "chapter": 71,
            "verse": 8,
            "text": "ŝümme innî de`avtühüm cihârâ"
        },
        {
            "chapter": 71,
            "verse": 9,
            "text": "ŝümme innî a`lentü lehüm veesrartü lehüm isrârâ"
        },
        {
            "chapter": 71,
            "verse": 10,
            "text": "feḳultü-stagfirû rabbeküm innehû kâne gaffârâ"
        },
        {
            "chapter": 71,
            "verse": 11,
            "text": "yürsili-ssemâe `aleyküm midrârâ"
        },
        {
            "chapter": 71,
            "verse": 12,
            "text": "veyümdidküm biemvâliv vebenîne veyec`al leküm cennâtiv veyec`al leküm enhârâ"
        },
        {
            "chapter": 71,
            "verse": 13,
            "text": "mâ leküm lâ tercûne lillâhi veḳârâ"
        },
        {
            "chapter": 71,
            "verse": 14,
            "text": "veḳad ḫaleḳaküm aṭvârâ"
        },
        {
            "chapter": 71,
            "verse": 15,
            "text": "elem terav keyfe ḫaleḳa-llâhü seb`a semâvâtin ṭibâḳâ"
        },
        {
            "chapter": 71,
            "verse": 16,
            "text": "vece`ale-lḳamera fîhinne nûrav vece`ale-şşemse sirâcâ"
        },
        {
            "chapter": 71,
            "verse": 17,
            "text": "vellâhü embeteküm mine-l'arḍi nebâtâ"
        },
        {
            "chapter": 71,
            "verse": 18,
            "text": "ŝümme yü`îdüküm fîhâ veyuḫricüküm iḫrâcâ"
        },
        {
            "chapter": 71,
            "verse": 19,
            "text": "vellâhü ce`ale lekümü-l'arḍa bisâṭâ"
        },
        {
            "chapter": 71,
            "verse": 20,
            "text": "liteslükû minhâ sübülen ficâcâ"
        },
        {
            "chapter": 71,
            "verse": 21,
            "text": "ḳâle nûḥur rabbi innehüm `aṣavnî vettebe`û mel lem yezidhü mâlühû veveledühû illâ ḫasârâ"
        },
        {
            "chapter": 71,
            "verse": 22,
            "text": "vemekerû mekran kübbârâ"
        },
        {
            "chapter": 71,
            "verse": 23,
            "text": "veḳâlû lâ teẕerunne âliheteküm velâ teẕerunne veddev velâ süvâ`av velâ yegûŝe veye`ûḳa venesrâ"
        },
        {
            "chapter": 71,
            "verse": 24,
            "text": "veḳad eḍallû keŝîrâ. velâ tezidi-żżâlimîne illâ ḍalâlâ"
        },
        {
            "chapter": 71,
            "verse": 25,
            "text": "mimmâ ḫaṭîâtihim ugriḳû feüdḫilû nâran felem yecidû lehüm min dûni-llâhi enṣârâ"
        },
        {
            "chapter": 71,
            "verse": 26,
            "text": "veḳâle nûḥur rabbi lâ teẕer `ale-l'arḍi mine-lkâfirîne deyyârâ"
        },
        {
            "chapter": 71,
            "verse": 27,
            "text": "inneke in teẕerhüm yüḍillû `ibâdeke velâ yelidû illâ fâciran keffârâ"
        },
        {
            "chapter": 71,
            "verse": 28,
            "text": "rabbi-gfir lî velivâlideyye velimen deḫale beytiye mü'minev velilmü'minîne velmü'minât. velâ tezidi-żżâlimîne illâ tebârâ"
        }
    ]
}