        {
            "chapter": 95,
            "verse": 1,
            "text": "vettîni vezzeytûn"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "veṭûri sînîn"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "vehâẕe-lbeledi-l'emîn"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "leḳad ḫalaḳne-l'insâne fî aḥseni taḳvîm"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ŝümme radednâhü esfele sâfilîn"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "ille-lleẕîne âmenû ve`amilu-ṣṣâliḥâti felehüm ecrun gayru memnûn"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "femâ yükeẕẕibüke ba`dü biddîn"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "eleyse-llâhü biaḥkemi-lḥâkimîn"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "iḳra' bismi rabbike-lleẕî ḫaleḳ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ḫaleḳa-l'insâne min `alaḳ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "iḳra' verabbüke-l'ekram"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "elleẕî `alleme bilḳalem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "alleme-l'insâne mâ lem ya`lem"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "kellâ inne-l'insâne leyaṭgâ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "er raâhü-stagnâ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "inne ilâ rabbike-rruc`â"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "era'eyte-lleẕî yenhâ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "abden iẕâ ṣallâ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "era'eyte in kâne `ale-lhüdâ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ev emera bittaḳvâ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "era'eyte in keẕẕebe vetevellâ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "elem ya`lem bienne-llâhe yerâ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "kellâ leil lem yentehi lenesfe`am binnâṣiyeh"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "nâṣiyetin kâẕibetin ḫâṭieh"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "felyed`u nâdiyeh"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "sened`u-zzebâniyeh"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "kellâ. lâ tüṭi`hü vescüd vaḳterib"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "innâ enzelnâhü fî leyleti-lḳadr"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "vemâ edrâke mâ leyletü-lḳadr"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "leyletü-lḳadri ḫayrum min elfi şehr"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "tenezzelü-lmelâiketü verrûḥu fîhâ biiẕni rabbihim. min külli emr"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "selâmün. hiye ḥattâ maṭle`i-lfecr"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "lem yeküni-lleẕîne keferû min ehli-lkitâbi velmüşrikîne münfekkîne ḥattâ te'tiyehümü-lbeyyineh"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "rasûlüm mine-llâhi yetlû ṣuḥufem müṭahherah"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "fîhâ kütübün ḳayyimeh"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "vemâ teferraḳa-lleẕîne ûtü-lkitâbe illâ mim ba`di mâ câethümü-lbeyyineh"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "vemâ ümirû illâ liya`büdü-llâhe muḫliṣîne lehü-ddîne ḥunefâe veyüḳîmu-ṣṣalâte veyü'tü-zzekâte veẕâlike dînü-lḳayyimeh"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "inne-lleẕîne keferû min ehli-lkitâbi velmüşrikîne fî nâri cehenneme ḫâlidîne fîhâ. ülâike hüm şerru-lberiyyeh"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "inne-lleẕîne âmenû ve`amilu-ṣṣâliḥâti ülâike hüm ḫayru-lberiyyeh"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "cezâühüm `inde rabbihim cennâtü `adnin tecrî min taḥtihe-l'enhâru ḫâlidîne fîhâ ebedâ. raḍiye-llâhü `anhüm veraḍû `anh. ẕâlike limen ḫaşiye rabbeh"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "iẕâ zülzileti-l'arḍu zilzâlehâ"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "veaḫraceti-l'arḍu eŝḳâlehâ"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "veḳâle-l'insânü mâ lehâ"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "yevmeiẕin tüḥaddiŝü aḫbârahâ"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "bienne rabbeke evḥâ lehâ"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "yevmeiẕiy yaṣdüru-nnâsü eştâtel liyürav a`mâlehüm"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "femey ya`mel miŝḳâle ẕerratin ḫayray yerah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "vemey ya`mel miŝḳâle ẕerratin şerray yerah"
        },
