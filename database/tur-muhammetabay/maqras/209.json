{
    "maqras": [
        {
            "chapter": 51,
            "verse": 31,
            "text": "ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "linürsile `aleyhim ḥicâratem min ṭîn"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "müsevvemeten `inde rabbike lilmüsrifîn"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "feaḫracnâ men kâne fîhâ mine-lmü'minîn"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "femâ vecednâ fîhâ gayra beytim mine-lmüslimîn"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "veteraknâ fîhâ âyetel lilleẕîne yeḫâfûne-l`aẕâbe-l'elîm"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "vefî mûsâ iẕ erselnâhü ilâ fir`avne bisülṭânim mübîn"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "fetevellâ biruknihî veḳâle sâḥirun ev mecnûn"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "feeḫaẕnâhü vecünûdehû fenebeẕnâhüm fi-lyemmi vehüve mülîm"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "vefî `âdin iẕ erselnâ `aleyhimü-rrîḥa-l`aḳîm"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "mâ teẕeru min şey'in etet `aleyhi illâ ce`alethü kelramîm"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "vefî ŝemûde iẕ ḳîle lehüm temette`û ḥattâ ḥîn"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "fe`atev `an emri rabbihim feeḫaẕethümu-ṣṣâ`iḳatü vehüm yenżurûn"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "feme-steṭâ`û min ḳiyâmiv vemâ kânû münteṣirîn"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "veḳavme nûḥim min ḳabl. innehüm kânû ḳavmen fâsiḳîn"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "vessemâe beneynâhâ bieydiv veinnâ lemûsi`ûn"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "vel'arḍa feraşnâhâ feni`me-lmâhidûn"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "vemin külli şey'in ḫalaḳnâ zevceyni le`alleküm teẕekkerûn"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "fefirrû ile-llâh. innî leküm minhü neẕîrum mübîn"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "velâ tec`alû me`a-llâhi ilâhen âḫar. innî leküm minhü neẕîrum mübîn"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "keẕâlike mâ ete-lleẕîne min ḳablihim mir rasûlin illâ ḳâlû sâḥirun ev mecnûn"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "etevâṣav bih. bel hüm ḳavmün ṭâgûn"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "fetevelle `anhüm femâ ente bimelûm"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "veẕekkir feinne-ẕẕikrâ tenfe`u-lmü'minîn"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "vemâ ḫalaḳtü-lcinne vel'inse illâ liya`büdûn"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "mâ ürîdü minhüm mir rizḳiv vemâ ürîdü ey yuṭ`imûn"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "inne-llâhe hüve-rrazzâḳu ẕü-lḳuvveti-lmetîn"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "feinne lilleẕîne żalemû ẕenûbem miŝle ẕenûbi aṣḥâbihim felâ yesta`cilûn"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "feveylül lilleẕîne keferû miy yevmihimü-lleẕî yû`adûn"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "veṭṭûr"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "vekitâbim mesṭûr"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "fî raḳḳim menşûr"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "velbeyti-lma`mûr"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "vessaḳfi-lmerfû"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "velbaḥri-lmescûr"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "inne `aẕâbe rabbike levâḳi"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "mâ lehû min dâfi"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "yevme temûru-ssemâü mevrâ"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "vetesîru-lcibâlü seyrâ"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "feveylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "elleẕîne hüm fî ḫavḍiy yel`abûn"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "yevme yüde``ûne ilâ nâri cehenneme da``â"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "hâẕihi-nnâru-lletî küntüm bihâ tükeẕẕibûn"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "efesiḥrun hâẕâ em entüm lâ tübṣirûn"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "iṣlevhâ faṣbirû ev lâ taṣbirû. sevâün `aleyküm. innemâ tüczevne mâ küntüm ta`melûn"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "inne-lmütteḳîne fî cennâtiv vene`îm"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "fâkihîne bimâ âtâhüm rabbühüm. veveḳâhüm rabbühüm `aẕâbe-lceḥîm"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "külû veşrabû henîem bimâ küntüm ta`melûn"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "müttekiîne `alâ sürurim maṣfûfeh. vezevvecnâhüm biḥûrin `în"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "velleẕîne âmenû vettebe`athüm ẕürriyyetühüm biîmânin elḥaḳnâ bihim ẕürriyyetehüm vemâ eletnâhüm min `amelihim min şey'. küllü-mriim bimâ kesebe rahîn"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "veemdednâhüm bifâkihetiv velaḥmim mimmâ yeştehûn"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "yetenâza`ûne fîhâ ke'sel lâ lagvun fîhâ velâ te'ŝîm"
        }
    ]
}