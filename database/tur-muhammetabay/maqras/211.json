{
    "maqras": [
        {
            "chapter": 53,
            "verse": 26,
            "text": "vekem mim melekin fi-ssemâvâti lâ tugnî şefâ`atühüm şey'en illâ mim ba`di ey ye'ẕene-llâhü limey yeşâü veyerḍâ"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "inne-lleẕîne lâ yü'minûne bil'âḫirati leyüsemmûne-lmelâikete tesmiyete-l'ünŝâ"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "vemâ lehüm bihî min `ilm. iy yettebi`ûne ille-żżanne. veinne-żżanne lâ yugnî mine-lḥaḳḳi şey'â"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fea`riḍ `am men tevellâ `an ẕikrinâ velem yürid ille-lḥayâte-ddünyâ"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "ẕâlike mebleguhüm mine-l`ilm. inne rabbeke hüve a`lemü bimen ḍalle `an sebîlihî vehüve a`lemü bimeni-htedâ"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "velillâhi mâ fi-ssemâvâti vemâ fi-l'arḍi liyecziye-lleẕîne esâü bimâ `amilû veyecziye-lleẕîne aḥsenû bilḥusnâ"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "elleẕîne yectenibûne kebâira-l'iŝmi velfevâḥişe ille-llemem. inne rabbeke vâsi`u-lmagfirah. hüve a`lemü biküm iẕ enşeeküm mine-l'arḍi veiẕ entüm ecinnetün fî büṭûni ümmehâtiküm. felâ tüzekkû enfüseküm. hüve a`lemü bimeni-tteḳâ"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "eferaeyte-lleẕî tevellâ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "vea`ṭâ ḳalîlev veekdâ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "e`indehû `ilmü-lgaybi fehüve yerâ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "em lem yünebbe' bimâ fî ṣuḥufi mûsâ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "veibrâhime-lleẕî veffâ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ellâ teziru vâziratüv vizra uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "veel leyse lil'insâni illâ mâ se`â"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "veenne sa`yehû sevfe yürâ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ŝümme yüczâhü-lcezâe-l'evfâ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "veenne ilâ rabbike-lmüntehâ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "veennehû hüve aḍḥake veebkâ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "veennehû hüve emâte veaḥyâ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "veennehû ḫaleḳa-zzevceyni-ẕẕekera vel'ünŝâ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nuṭfetin iẕâ tümnâ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "veenne `aleyhi-nneş'ete-l'uḫrâ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "veennehû hüve agnâ veaḳnâ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "veennehû hüve rabbü-şşi`râ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "veennehû ehleke `âden-l'ûlâ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "veŝemûde femâ ebḳâ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "veḳavme nûḥim min ḳabl. innehüm kânû hüm ażleme veaṭgâ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "velmü'tefikete ehvâ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "fegaşşâhâ mâ gaşşâ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "febieyyi âlâi rabbike tetemârâ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "hâẕâ neẕîrum mine-nnüẕüri-l'ûlâ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "ezifeti-l'âzifeh"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "leyse lehâ min dûni-llâhi kâşifeh"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "efemin hâẕe-lḥadîŝi ta`cebûn"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "vetaḍḥakûne velâ tebkûn"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "veentüm sâmidûn"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fescüdû lillâhi va`büdû"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "iḳterabeti-ssâ`atü venşeḳḳa-lḳamer"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "veiy yerav âyetey yü`riḍû veyeḳûlû siḥrum müstemirr"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "vekeẕẕebû vettebe`û ehvâehüm veküllü emrim müsteḳirr"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "veleḳad câehüm mine-l'embâi mâ fîhi müzdecer"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "ḥikmetüm bâligatün femâ tugni-nnüẕür"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "fetevelle `anhüm. yevme yed`u-ddâ`i ilâ şey'in nükür"
        },
        {
            "chapter": 54,
            "verse": 7,
            "text": "ḫuşşe`an ebṣâruhüm yaḫrucûne mine-l'ecdâŝi keennehüm cerâdüm münteşir"
        },
        {
            "chapter": 54,
            "verse": 8,
            "text": "mühti`îne ile-ddâ`. yeḳûlü-lkâfirûne hâẕâ yevmün `asir"
        }
    ]
}