{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "lâ uḳsimü biyevmi-lḳiyâmeh"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "velâ uḳsimü binnefsi-llevvâmeh"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "eyaḥsebü-l'insânü ellen necme`a `iżâmeh"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "belâ ḳâdirîne `alâ en nüsevviye benâneh"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "bel yürîdü-l'insânü liyefcüra emâmeh"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "yes'elü eyyâne yevmü-lḳiyâmeh"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "feiẕâ beriḳa-lbeṣar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "veḫasefe-lḳamer"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "vecümi`a-şşemsü velḳamer"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "yeḳûlü-l'insânü yevmeiẕin eyne-lmeferr"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "kellâ lâ vezer"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "ilâ rabbike yevmeiẕin-lmüsteḳarr"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "yünebbeü-l'insânü yevmeiẕim bimâ ḳaddeme veeḫḫar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "beli-l'insânü `alâ nefsihî beṣîrah"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "velev elḳâ me`âẕîrah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "lâ tüḥarrik bihî lisâneke lita`cele bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "inne `aleynâ cem`ahû veḳur'âneh"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "feiẕâ ḳara'nâhü fettebi` ḳur'âneh"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ŝümme inne `aleynâ beyâneh"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "kellâ bel tüḥibbûne-l`âcileh"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "veteẕerûne-l'âḫirah"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "vucûhüy yevmeiẕin nâḍirah"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "ilâ rabbihâ nâżirah"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "vevucûhüy yevmeiẕim bâsirah"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "teżunnü ey yüf`ale bihâ fâḳirah"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "kellâ iẕâ belegati-tterâḳiy"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "veḳîle men râḳ"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "veżanne ennehü-lfirâḳ"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "velteffeti-ssâḳu bissâḳ"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "ilâ rabbike yevmeiẕin-lmesâḳ"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "felâ ṣaddeḳa velâ ṣallâ"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "velâkin keẕẕebe vetevellâ"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ŝümme ẕehebe ilâ ehlihî yetemeṭṭâ"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "evlâ leke feevlâ"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ŝümme evlâ leke feevlâ"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "eyaḥsebü-l'insânü ey yütrake südâ"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "elem yekü nuṭfetem mim meniyyiy yümnâ"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "ŝümme kâne `aleḳaten feḫaleḳa fesevvâ"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "fece`ale minhü-zzevceyni-ẕẕekera vel'ünŝâ"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "eleyse ẕâlike biḳâdirin `alâ ey yuḥyiye-lmevtâ"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "hel etâ `ale-l'insâni ḥînüm mine-ddehri lem yekün şey'em meẕkûrâ"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "innâ ḫalaḳne-l'insâne min nuṭfetin emşâc. nebtelîhi fece`alnâhü semî`am beṣîrâ"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "innâ hedeynâhü-ssebîle immâ şâkirav veimmâ kefûrâ"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "innâ a`tednâ lilkâfirîne selâsile veaglâlev vese`îrâ"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "inne-l'ebrâra yeşrabûne min ke'sin kâne mizâcühâ kâfûrâ"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "ayney yeşrabü bihâ `ibâdü-llâhi yüfeccirûnehâ tefcîrâ"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "yûfûne binneẕri veyeḫâfûne yevmen kâne şerruhû müsteṭîrâ"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "veyuṭ`imûne-ṭṭa`âme `alâ ḥubbihî miskînev veyetîmev veesîrâ"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "innemâ nuṭ`imüküm livechi-llâhi lâ nürîdü minküm cezâev velâ şükûrâ"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "innâ neḫâfü mir rabbinâ yevmen `abûsen ḳamṭarîrâ"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "feveḳâhümü-llâhü şerra ẕâlike-lyevmi veleḳḳâhüm naḍratev vesürûrâ"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "vecezâhüm bimâ ṣaberû cennetev veḥarîrâ"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "müttekiîne fîhâ `ale-l'erâik. lâ yeravne fîhâ şemsev velâ zemherîrâ"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "vedâniyeten `aleyhim żilâlühâ veẕüllilet ḳuṭûfühâ teẕlîlâ"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "veyüṭâfü `aleyhim biâniyetim min fiḍḍativ veekvâbin kânet ḳavârîrâ"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "ḳavârîrae min fiḍḍatin ḳadderûhâ taḳdîrâ"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "veyüsḳavne fîhâ ke'sen kâne mizâcühâ zencebîlâ"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "aynen fîhâ tüsemmâ selsebîlâ"
        }
    ]
}