{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "veṣṣâffâti ṣaffâ"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "fezzâcirâti zecrâ"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "fettâliyâti ẕikrâ"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "inne ilâheküm levâḥid"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "rabbü-ssemâvâti vel'arḍi vemâ beynehümâ verabbü-lmeşâriḳ"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "innâ zeyyenne-ssemâe-ddünyâ bizînetini-lkevâkib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "veḥifżam min külli şeyṭânim mârid"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "lâ yessemme`ûne ile-lmelei-l'a`lâ veyuḳẕefûne min külli cânib"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "düḥûrav velehüm `aẕâbüv vâṣib"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "illâ men ḫaṭife-lḫaṭfete feetbe`ahû şihâbün ŝâḳib"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "festeftihim ehüm eşeddü ḫalḳan em men ḫalaḳnâ. innâ ḫalaḳnâhüm min ṭînil lâzib"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "bel `acibte veyesḫarûn"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "veiẕâ ẕükkirû lâ yeẕkürûn"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "veiẕâ raev âyetey yestesḫirûn"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "veḳâlû in hâẕâ illâ siḥrum mübîn"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemeb`ûŝûn"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "eveâbâüne-l'evvelûn"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "ḳul ne`am veentüm dâḫirûn"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "feinnemâ hiye zecratüv vâḥidetün feiẕâ hüm yenżurûn"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "veḳâlû yâ veylenâ hâẕâ yevmü-ddîn"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "hâẕâ yevmü-lfaṣli-lleẕî küntüm bihî tükeẕẕibûn"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "uḥşürü-lleẕîne żalemû veezvâcehüm vemâ kânû ya`büdûn"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "min dûni-llâhi fehdûhüm ilâ ṣirâṭi-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "veḳifûhüm innehüm mes'ûlûn"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "mâ leküm lâ tenâṣarûn"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "bel hümü-lyevme müsteslimûn"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "veaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "ḳâlû inneküm küntüm te'tûnenâ `ani-lyemîn"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "ḳâlû bel lem tekûnû mü'minîn"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "vemâ kâne lenâ `aleyküm min sülṭân. bel küntüm ḳavmen ṭâgîn"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "feḥaḳḳa `aleynâ ḳavlü rabbinâ. innâ leẕâiḳûn"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "feagveynâküm innâ künnâ gâvîn"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "feinnehüm yevmeiẕin fi-l`aẕâbi müşterikûn"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "innâ keẕâlike nef`alü bilmücrimîn"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "innehüm kânû iẕâ ḳîle lehüm lâ ilâhe ille-llâhü yestekbirûn"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "veyeḳûlûne einnâ letârikû âlihetinâ lişâ`irim mecnûn"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "bel câe bilḥaḳḳi veṣaddeḳa-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "inneküm leẕâiḳu-l`aẕâbi-l'elîm"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "vemâ tüczevne illâ mâ küntüm ta`melûn"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "illâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "ülâike lehüm rizḳum ma`lûm"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "fevâkih. vehüm mükramûn"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "fî cennâti-nne`îm"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "alâ sürurim müteḳâbilîn"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "yüṭâfü `aleyhim bike'sim mim me`în"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "beyḍâe leẕẕetil lişşâribîn"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "lâ fîhâ gavlüv velâ hüm `anhâ yünzefûn"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "ve`indehüm ḳâṣirâtu-ṭṭarfi `în"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "keennehünne beyḍum meknûn"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "feaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "ḳâle ḳâilüm minhüm innî kâne lî ḳarîn"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "yeḳûlü einneke lemine-lmüṣaddiḳîn"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemedînûn"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "ḳâle hel entüm müṭṭali`ûn"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "feṭṭale`a feraâhü fî sevâi-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "ḳâle tellâhi in kitte letürdîn"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "velevlâ ni`metü rabbî leküntü mine-lmuḥḍarîn"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "efemâ naḥnü bimeyyitîn"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "illâ mevtetene-l'ûlâ vemâ naḥnü bimü`aẕẕebîn"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "inne hâẕâ lehüve-lfevzü-l`ażîm"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "limiŝli hâẕâ felya`meli-l`âmilûn"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "eẕâlike ḫayrun nüzülen em şeceratü-zzeḳḳûm"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "innâ ce`alnâhâ fitnetel liżżâlimîn"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "innehâ şeceratün taḫrucü fî aṣli-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "ṭal`uhâ keennehû ruûsü-şşeyâṭîn"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "feinnehüm leâkilûne minhâ femâliûne minhe-lbüṭûn"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "ŝümme inne lehüm `aleyhâ leşevbem min ḥamîm"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "ŝümme inne merci`ahüm leile-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "innehüm elfev âbâehüm ḍâllîn"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "fehüm `alâ âŝârihim yühra`ûn"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "veleḳad ḍalle ḳablehüm ekŝeru-l'evvelîn"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "veleḳad erselnâ fîhim münẕirîn"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "fenżur keyfe kâne `âḳibetü-lmünẕerîn"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "illâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "veleḳad nâdânâ nûḥun feleni`me-lmücîbûn"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "venecceynâhü veehlehû mine-lkerbi-l`ażîm"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "vece`alnâ ẕürriyyetehû hümü-lbâḳîn"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "selâmün `alâ nûḥin fi-l`âlemîn"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "ŝümme agraḳne-l'âḫarîn"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "veinne min şî`atihî leibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "iẕ câe rabbehû biḳalbin selîm"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "iẕ ḳâle liebîhi veḳavmihî mâẕâ ta`büdûn"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "eifken âliheten dûne-llâhi türîdûn"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "femâ żannüküm birabbi-l`âlemîn"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "feneżara nażraten fi-nnücûm"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "feḳâle innî seḳîm"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "fetevellev `anhü müdbirîn"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "ferâga ilâ âlihetihim feḳâle elâ te'külûn"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "mâ leküm lâ tenṭiḳûn"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "ferâga `aleyhim ḍarbem bilyemîn"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "feaḳbelû ileyhi yeziffûn"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "ḳâle eta`büdûne mâ tenḥitûn"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "vellâhü ḫaleḳaküm vemâ ta`melûn"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "ḳâlü-bnû lehû bünyânen feelḳûhü fi-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "feerâdû bihî keyden fece`alnâhümü-l'esfelîn"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "veḳâle innî ẕâhibün ilâ rabbî seyehdîn"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "rabbi heb lî mine-ṣṣâliḥîn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "febeşşernâhü bigulâmin ḥalîm"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "felemmâ belega me`ahü-ssa`ye ḳâle yâ büneyye innî erâ fi-lmenâmi ennî eẕbeḥuke fenżur mâẕâ terâ. ḳâle yâ ebeti-f`al mâ tü'mer. setecidünî in şâe-llâhü mine-ṣṣâbirîn"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "felemmâ eslemâ vetellehû lilcebîn"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "venâdeynâhü ey yâ ibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ḳad ṣaddaḳte-rru'yâ. innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "inne hâẕâ lehüve-lbelâü-lmübîn"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "vefedeynâhü biẕibḥin `ażîm"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selâmün `alâ ibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "vebeşşernâhü biisḥâḳa nebiyyem mine-ṣṣâliḥîn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "vebâraknâ `aleyhi ve`alâ isḥâḳ. vemin ẕürriyyetihimâ muḥsinüv veżâlimül linefsihî mübîn"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "veleḳad menennâ `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "venecceynâhümâ veḳavmehümâ mine-lkerbi-l`ażîm"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "veneṣarnâhüm fekânû hümü-lgâlibîn"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "veâteynâhüme-lkitâbe-lmüstebîn"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "vehedeynâhüme-ṣṣirâṭa-lmüsteḳîm"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "veteraknâ `aleyhimâ fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selâmün `alâ mûsâ vehârûn"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innehümâ min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "veinne ilyâse lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "iẕ ḳâle liḳavmihî elâ tetteḳûn"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "eted`ûne ba`lev veteẕerûne aḥsene-lḫâliḳîn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "allâhe rabbeküm verabbe âbâikümü-l'evvelîn"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "fekeẕẕebûhü feinnehüm lemuḥḍarûn"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "veteraknâ `aleyhi fi-l'âḫirîn"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selâmün `alâ ilyâsîn"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "innâ keẕâlike neczi-lmuḥsinîn"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innehû min `ibâdine-lmü'minîn"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "veinne lûṭal lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "iẕ necceynâhü veehlehû ecme`în"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illâ `acûzen fi-lgâbirîn"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ŝümme demmerne-l'âḫarîn"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "veinneküm letemürrûne `aleyhim muṣbiḥîn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vebilleyl. efelâ ta`ḳilûn"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "veinne yûnüse lemine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "iẕ ebeḳa ile-lfülki-lmeşḥûn"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "fesâheme fekâne mine-lmüdḥaḍîn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "felteḳamehü-lḥûtü vehüve mülîm"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "felevlâ ennehû kâne mine-lmüsebbiḥîn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "lelebiŝe fî baṭnih ilâ yevmi yüb`aŝûn"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "fenebeẕnâhü bil`arâi vehüve seḳîm"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "veembetnâ `aleyhi şeceratem miy yaḳṭîn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "veerselnâhü ilâ mieti elfin ev yezîdûn"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "feâmenû femetta`nâhüm ilâ ḥîn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "festeftihim elirabbike-lbenâtü velehümü-lbenûn"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "em ḫalaḳne-lmelâikete inâŝev vehüm şâhidûn"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "elâ innehüm min ifkihim leyeḳûlûn"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "velede-llâhü veinnehüm lekâẕibûn"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "aṣṭafe-lbenâti `ale-lbenîn"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "mâ leküm. keyfe taḥkümûn"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "efelâ teẕekkerûn"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "em leküm sülṭânüm mübîn"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "fe'tû bikitâbiküm in küntüm ṣâdiḳîn"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "vece`alû beynehû vebeyne-lcinneti nesebâ. veleḳad `alimeti-lcinnetü innehüm lemuḥḍarûn"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "sübḥâne-llâhi `ammâ yeṣifûn"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "illâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "feinneküm vemâ ta`büdûn"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "mâ entüm `aleyhi bifâtinîn"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "illâ men hüve ṣâli-lceḥîm"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "vemâ minnâ illâ lehû meḳâmüm ma`lûm"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "veinnâ lenaḥnu-ṣṣâffûn"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "veinnâ lenaḥnü-lmüsebbiḥûn"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "vein kânû leyeḳûlûn"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "lev enne `indenâ ẕikram mine-l'evvelîn"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "lekünnâ `ibâde-llâhi-lmuḫleṣîn"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "fekeferû bih. fesevfe ya`lemûn"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "veleḳad sebeḳat kelimetünâ li`ibâdine-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "innehüm lehümü-lmenṣûrûn"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "veinne cündenâ lehümü-lgâlibûn"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "fetevelle `anhüm ḥattâ ḥîn"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "veebṣirhüm fesevfe yübṣirûn"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "efebi`aẕâbinâ yesta`cilûn"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "feiẕâ nezele bisâḥatihim fesâe ṣabâḥu-lmünẕerîn"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "vetevelle `anhüm ḥattâ ḥîn"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "veebṣir fesevfe yübṣirûn"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "sübḥâne rabbike rabbi-l`izzeti `ammâ yeṣifûn"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "veselâmün `ale-lmürselîn"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "velḥamdü lillâhi rabbi-l`âlemîn"
        }
    ]
}