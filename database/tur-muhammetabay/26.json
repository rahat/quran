{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "ṭâ-sîn-mîm"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "tilke âyâtü-lkitâbi-lmübîn"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "le`alleke bâḫi`un nefseke ellâ yekûnû mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "in neşe' nünezzil `aleyhim mine-ssemâi âyeten feżallet a`nâḳuhüm lehâ ḫâḍi`în"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "vemâ ye'tîhim min ẕikrim mine-rraḥmâni muḥdeŝin illâ kânû `anhü mü`riḍîn"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "feḳad keẕẕebû feseye'tîhim embâü mâ kânû bihî yestehziûn"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "evelem yerav ile-l'arḍi kem embetnâ fîhâ min külli zevcin kerîm"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "veiẕ nâdâ rabbüke mûsâ eni-'ti-lḳavme-żżâlimîn"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "ḳavme fir`avn. elâ yetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "ḳâle rabbi innî eḫâfü ey yükeẕẕibûn"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "veyeḍîḳu ṣadrî velâ yenṭaliḳu lisânî feersil ilâ hârûn"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "velehüm `aleyye ẕembün feeḫâfü ey yaḳtülûn"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "ḳâle kellâ. feẕhebâ biâyâtinâ innâ me`aküm müstemi`ûn"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "fe'tiyâ fir`avne feḳûlâ innâ rasûlü rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "en ersil me`anâ benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "ḳâle elem nürabbike fînâ velîdev velebiŝte fînâ min `umürike sinîn"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "vefe`alte fa`leteke-lletî fe`alte veente mine-lkâfirîn"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "ḳâle fe`altühâ iẕev veenâ mine-ḍḍâllîn"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "feferartü minküm lemmâ ḫiftüküm fevehebe lî rabbî ḥukmev vece`alenî mine-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "vetilke ni`metün temünnühâ `aleyye en `abbette benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "ḳâle fir`avnü vemâ rabbü-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "ḳâle rabbü-ssemâvâti vel'arḍi vemâ beynehümâ. in küntüm mûḳinîn"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "ḳâle limen ḥavlehû elâ testemi`ûn"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "ḳâle rabbüküm verabbü âbâikümü-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "ḳâle inne rasûlekümü-lleẕî ürsile ileyküm lemecnûn"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "ḳâle rabbü-lmeşriḳi velmagribi vemâ beynehümâ. in küntüm ta`ḳilûn"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "ḳâle leini-tteḫaẕte ilâhen gayrî leec`alenneke mine-lmescûnîn"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "ḳâle evelev ci'tüke bişey'im mübîn"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "ḳâle fe'ti bihî in künte mine-ṣṣâdiḳîn"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "feelḳâ `aṣâhü feiẕâ hiye ŝü`bânüm mübîn"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "veneza`a yedehû feiẕâ hiye beyḍâü linnâżirîn"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "ḳâle lilmelei ḥavlehû inne hâẕâ lesâḥirun `alîm"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "yürîdü ey yuḫriceküm min arḍiküm bisiḥrih. femâẕâ te'mürûn"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "ḳâlû ercih veeḫâhü veb`aŝ fi-lmedâini ḥâşirîn"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "ye'tûke bikülli seḥḥârin `alîm"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "fecümi`a-sseḥaratü limîḳâti yevmim ma`lûm"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "veḳîle linnâsi hel entüm müctemi`ûn"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "le`allenâ nettebi`u-sseḥarate in kânû hümü-lgâlibîn"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "felemmâ câe-sseḥaratü ḳâlû lifir`avne einne lenâ leecran in künnâ naḥnü-lgâlibîn"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "ḳâle ne`am veinneküm iẕel lemine-lmüḳarrabîn"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "ḳâle lehüm mûsâ elḳû mâ entüm mülḳûn"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "feelḳav ḥibâlehüm ve`iṣiyyehüm veḳâlû bi`izzeti fir`avne innâ lenaḥnü-lgâlibûn"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "feelḳâ mûsâ `aṣâhü feiẕâ hiye telḳafü mâ ye'fikûn"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "feülḳiye-sseḥaratü sâcidîn"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "ḳâlû âmennâ birabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "rabbi mûsâ vehârûn"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "ḳâle âmentüm lehû ḳable en âẕene leküm. innehû lekebîrukümü-lleẕî `allemekümü-ssiḥr. felesevfe ta`lemûn. leüḳaṭṭi`anne eydiyeküm veercüleküm min ḫilâfiv veleüṣallibenneküm ecme`în"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "ḳâlû lâ ḍayr. innâ ilâ rabbinâ münḳalibûn"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "innâ naṭme`u ey yagfira lenâ rabbünâ ḫaṭâyânâ en künnâ evvele-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "veevḥaynâ ilâ mûsâ en esri bi`ibâdî inneküm müttebe`ûn"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "feersele fir`avnü fi-lmedâini ḥâşirîn"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inne hâülâi leşirẕimetün ḳalîlûn"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "veinnehüm lenâ legâiżûn"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "veinnâ lecemî`un ḥâẕirûn"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "feaḫracnâhüm min cennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "vekünûziv vemeḳâmin kerîm"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "keẕâlik. veevraŝnâhâ benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "feetbe`ûhüm müşriḳîn"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "felemmâ terâe-lcem`âni ḳâle aṣḥâbü mûsâ innâ lemüdrakûn"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "ḳâle kellâ. inne me`iye rabbî seyehdîn"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "feevḥaynâ ilâ mûsâ eni-ḍrib bi`aṣâke-lbaḥr. fenfeleḳa fekâne küllü firḳin keṭṭavdi-l`ażîm"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "veezlefnâ ŝemme-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "veenceynâ mûsâ vemem me`ahû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ŝümme agraḳne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "vetlü `aleyhim nebee ibrâhîm"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "iẕ ḳâle liebîhi veḳavmihî mâ ta`büdûn"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "ḳâlû na`büdü aṣnâmen feneżallü lehâ `âkifîn"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "ḳâle hel yesme`ûneküm iẕ ted`ûn"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "ev yenfe`ûneküm ev yeḍurrûn"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "ḳâlû bel vecednâ âbâenâ keẕâlike yef`alûn"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "ḳâle eferaeytüm mâ küntüm ta`büdûn"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "entüm veâbâükümü-l'aḳdemûn"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "feinnehüm `adüvvül lî illâ rabbe-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "elleẕî ḫaleḳanî fehüve yehdîn"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "velleẕî hüve yuṭ`imünî veyesḳîn"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "veiẕâ meriḍtü fehüve yeşfîn"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "velleẕî yümîtünî ŝümme yuḥyîn"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "velleẕî aṭme`u ey yagfira lî ḫaṭîetî yevme-ddîn"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi heb lî ḥukmev veelḥiḳnî biṣṣâliḥîn"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "vec`al lî lisâne ṣidḳin fi-l'âḫirîn"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "vec`alnî miv veraŝeti cenneti-nne`îm"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "vagfir liebî innehû kâne mine-ḍḍâllîn"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "velâ tuḫzinî yevme yüb`aŝûn"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "yevme lâ yenfe`u mâlüv velâ benûn"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "illâ men ete-llâhe biḳalbin selîm"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "veüzlifeti-lcennetü lilmütteḳîn"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "vebürrizeti-lceḥîmü lilgâvîn"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "veḳîle lehüm eyne mâ küntüm ta`büdûn"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "min dûni-llâh. hel yenṣurûneküm ev yenteṣirûn"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "fekübkibû fîhâ hüm velgâvûn"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "vecünûdü iblîse ecme`ûn"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "ḳâlû vehüm fîhâ yaḫteṣimûn"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "tellâhi in künnâ lefî ḍalâlim mübîn"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "iẕ nüsevvîküm birabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "vemâ eḍallenâ ille-lmücrimûn"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "femâ lenâ min şâfi`în"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "velâ ṣadîḳin ḥamîm"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "felev enne lenâ kerraten fenekûne mine-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "keẕẕebet ḳavmü nûḥin-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "iẕ ḳâle lehüm eḫûhüm nûḥun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "ḳâlû enü'minü leke vettebe`ake-l'erẕelûn"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "ḳâle vemâ `ilmî bimâ kânû ya`melûn"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "in ḥisâbühüm illâ `alâ rabbî lev teş`urûn"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "vemâ ene biṭâridi-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "in ene illâ neẕîrum mübîn"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "ḳâlû leil lem tentehi yâ nûḥu letekûnenne mine-lmercûmîn"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "ḳâle rabbi inne ḳavmî keẕẕebûn"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "feftaḥ beynî vebeynehüm fetḥav veneccinî vemem me`iye mine-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "feenceynâhü vemem me`ahû fi-lfülki-lmeşḥûn"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ŝümme agraḳnâ ba`dü-lbâḳîn"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "keẕẕebet `âdün-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "iẕ ḳâle lehüm eḫûhüm hûdün elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "etebnûne bikülli rî`in âyeten ta`beŝûn"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "vetetteḫiẕûne meṣâni`a le`alleküm taḫlüdûn"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "veiẕâ beṭaştüm beṭaştüm cebbârîn"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "vetteḳu-lleẕî emeddeküm bimâ ta`lemûn"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "emeddeküm bien`âmiv vebenîn"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "vecennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "innî eḫâfü `aleyküm `aẕâbe yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "ḳâlû sevâün `aleynâ eve`ażte em lem teküm mine-lvâ`iżîn"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "in hâẕâ illâ ḫulüḳu-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "vemâ naḥnü bimü`aẕẕebîn"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "fekeẕẕebûhü feehleknâhüm. inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "keẕẕebet ŝemûdü-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "iẕ ḳâle lehüm eḫûhüm ṣâliḥun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "etütrakûne fî mâ hâhünâ âminîn"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "fî cennâtiv ve`uyûn"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "vezürû`iv venaḫlin ṭal`uhâ heḍîm"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "vetenḥitûne mine-lcibâli büyûten fârihîn"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "velâ tüṭî`û emra-lmüsrifîn"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "elleẕîne yüfsidûne fi-l'arḍi velâ yuṣliḥûn"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "ḳâlû innemâ ente mine-lmüseḥḥarîn"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "mâ ente illâ beşerum miŝlünâ. fe'ti biâyetin in künte mine-ṣṣâdiḳîn"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "ḳâle hâẕihî nâḳatül lehâ şirbüv veleküm şirbü yevmim ma`lûm"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "velâ temessûhâ bisûin feye'ḫuẕeküm `aẕâbü yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "fe`aḳarûhâ feaṣbeḥû nâdimîn"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "feeḫaẕehümü-l`aẕâb. inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "keẕẕebet ḳavmü lûṭini-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "iẕ ḳâle lehüm eḫûhüm lûṭun elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "ete'tûne-ẕẕükrâne mine-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "veteẕerûne mâ ḫaleḳa leküm rabbüküm min ezvâciküm. bel entüm ḳavmün `âdûn"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "ḳâlû leil lem tentehi yâ lûṭu letekûnenne mine-lmuḫracîn"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "ḳâle innî li`ameliküm mine-lḳâlîn"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "rabbi neccinî veehlî mimmâ ya`melûn"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "fenecceynâhü veehlehû ecme`în"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "illâ `acûzen fi-lgâbirîn"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ŝümme demmerne-l'âḫarîn"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "veemṭarnâ `aleyhim meṭarâ. fesâe meṭaru-lmünẕerîn"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "keẕẕebe aṣḥâbü-l'eyketi-lmürselîn"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "iẕ ḳâle lehüm şu`aybün elâ tetteḳûn"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "innî leküm rasûlün emîn"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "fetteḳu-llâhe veeṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "vemâ es'elüküm `aleyhi min ecr. in ecriye illâ `alâ rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "evfü-lkeyle velâ tekûnû mine-lmuḫsirîn"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "vezinû bilḳisṭâsi-lmüsteḳîm"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "velâ tebḫasü-nnâse eşyâehüm velâ ta`ŝev fi-l'arḍi müfsidîn"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "vetteḳu-lleẕî ḫaleḳaküm velcibillete-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "ḳâlû innemâ ente mine-lmüseḥḥarîn"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "vemâ ente illâ beşer miŝlünâ vein neżunnüke lemine-lkâẕibîn"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "feesḳiṭ `aleynâ kisefem mine-ssemâi in künte mine-ṣṣâdiḳîn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "ḳâle rabbî a`lemü bimâ ta`melûn"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fekeẕẕebûhü feeḫaẕehüm `aẕâbü yevmi-żżulleh. innehû kâne `aẕâbe yevmin `ażîm"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "veinne rabbeke lehüve-l`azîzü-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "veinnehû letenzîlü rabbi-l`âlemîn"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nezele bihi-rrûḥu-l'emîn"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "alâ ḳalbike litekûne mine-lmünẕirîn"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisânin `arabiyyim mübîn"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "veinnehû lefî zübüri-l'evvelîn"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "evelem yekül lehüm âyeten ey ya`lemehû `ulemâü benî isrâîl"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "velev nezzelnâhü `alâ ba`ḍi-l'a`cemîn"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "feḳara'ehû `aleyhim mâ kânû bihî mü'minîn"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "keẕâlike seleknâhü fî ḳulûbi-lmücrimîn"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "lâ yü'minûne bihî ḥattâ yeravu-l`aẕâbe-l'elîm"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "feye'tiyehüm bagtetev vehüm lâ yeş`urûn"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "feyeḳûlû hel naḥnü münżarûn"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "efebi`aẕâbinâ yesta`cilûn"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "eferaeyte im metta`nâhüm sinîn"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ŝümme câehüm mâ kânû yû`adûn"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "mâ agnâ `anhüm mâ kânû yümette`ûn"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "vemâ ehleknâ min ḳaryetin illâ lehâ münẕirûn"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ẕikrâ. vemâ künnâ żâlimîn"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "vemâ tenezzelet bihi-şşeyâṭîn"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "vemâ yembegî lehüm vemâ yesteṭî`ûn"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innehüm `ani-ssem`i lema`zûlûn"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "felâ ted`u me`a-llâhi ilâhen âḫara fetekûne mine-lmü`aẕẕebîn"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "veenẕir `aşîrateke-l'aḳrabîn"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "vaḫfiḍ cenâḥake limeni-ttebe`ake mine-lmü'minîn"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fein `aṣavke feḳul innî berîüm mimmâ ta`melûn"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "vetevekkel `ale-l`azîzi-rraḥîm"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "elleẕî yerâke ḥîne teḳûm"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "veteḳallübeke fi-ssâcidîn"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innehû hüve-ssemî`u-l`alîm"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hel ünebbiüküm `alâ men tenezzelü-şşeyâṭîn"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tenezzelü `alâ külli effâkin eŝîm"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yülḳûne-ssem`a veekŝeruhüm kâẕibûn"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "veşşu`arâü yettebi`uhümü-lgâvûn"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "elem tera ennehüm fî külli vâdiy yehîmûn"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "veennehüm yeḳûlûne mâ lâ yef`alûn"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "ille-lleẕîne âmenû ve`amilu-ṣṣâliḥâti veẕekerü-llâhe keŝîrav venteṣarû mim ba`di mâ żulimû. veseya`lemü-lleẕîne żalemû eyye münḳalebiy yenḳalibûn"
        }
    ]
}