{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Скажи: \"Он - Аллах - един"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллах, вечный"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "не родил и не был рожден"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "и не был Ему равным ни один"
        }
    ]
}