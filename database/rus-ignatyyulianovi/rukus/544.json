{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Увлекла вас страсть к умножению"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "пока не навестили вы могилы"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Так нет же, вы узнаете"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Потом нет же, вы узнаете"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Нет же, если бы вы знали знанием достоверности"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Вы непременно увидите огонь"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Потом непременно вы увидите его оком достоверности"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Потом вы будете спрошены в тот день о наслаждении"
        }
    ]
}