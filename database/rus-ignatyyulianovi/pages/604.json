{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Скажи: \"Он - Аллах - един"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллах, вечный"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "не родил и не был рожден"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "и не был Ему равным ни один"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Скажи: \"Прибегаю я к Господу рассвета"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "от зла того, что он сотворил"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "от зла мрака, когда он покрыл"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "от зла дующих на узлы"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "от зла завистника, когда он завидовал"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Скажи: \"Прибегаю к Господу людей"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "царю людей"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Богу людей"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "от зла наущателя скрывающегося"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "который наущает груди людей"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "от джиннов и людей"
        }
    ]
}