{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Alfahari da yawan dukiya da dangi ya shagaltar da ku (dagaibada mai amfaninku)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Har kuka ziyarci kaburbura"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A'aha! (Nan gaba) za ku sani"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sa'an nan, tabbas, za ku sani"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Haƙiƙa, da kuna da sani sani na yaƙini"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Lalle ne da kuna ganin Jahim"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sa'an nan lalle ne za ku gan ta, da idanu, bayyane"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sa'an nan lalle ne za a tambaye ku, a ranar nan labarin ni'imar (da aka yi muku)"
        }
    ]
}