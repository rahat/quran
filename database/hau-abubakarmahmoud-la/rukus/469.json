{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Idan mai aukuwa ta auku"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Babu wani (rai) mai ƙaryatawa ga aukuwarta"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ita) mai ƙasƙantawa ce, mai ɗaukakawa"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Idan aka girgiza ƙasa girgizwa"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Kuma aka niƙe duwatsu, niƙewa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Sai suka kasance ƙura da ake watsarwa"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Kuma kun kasance nau'i uku"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Watau mazowa dama. Mene ne mazowa dama"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Da mazowa hagu. Mene ne mazowa hagu"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Da waɗanda suka tsere. Su wɗanda suka tseren nan"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Waɗancan, su ne waɗanda aka kusantar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "A ckin Aljannar ni'ima"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Jama'a ne daga mutanen farko"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Da kaɗan daga mutanen ƙarshe"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Suna) a kan wasu gadaje saƙaƙƙuu"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Suna gincire a kansu, suna masu kallon juna"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Wasu yara samari na dindindin gewaya a kansu"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Da wasu kofuna da shantula da hinjalai daga (giya) mai ɓuɓɓuga"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ba a sanya musu ciwon jiri saboda ita, kuma ba su buguwa"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Da wasu 'ya'yan itacen marmari daga irin waɗanda suke zaɓe"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Da naman tsuntsaye daga wanda suke ganin sha'awa"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Da wasu mata masu fararen idanu da girmansu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kamar misalan lu'ulu'u wanda aka ɓoye"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "A kan sakamakon, domin abin da suka kasance suna aikatawa"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ba su jin wata yasassar magana a cikinta, kuma ba su jin sun yi laifi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Sai dai wata magana (mai daɗi): Salamun, Salamun"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Da mazowa dama. Mene ne mazowa dama"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Suna) a cikin itacen magarya maras ƙaya"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Da wata ayaba mai yawan 'ya'ya"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Da wata inuwa miƙaƙƙiya"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Da wani ruwa mai gudana"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Da wasu 'ya'yan itacen marmari masu yawa"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ba su yankewa kuma ba a hana su"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Da wasu shimfiɗu maɗaukaka"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Lalle Mu, Mun ƙaga halittarsu ƙagawa"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Sa'an nan Muka sanya su budurwai"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Masu son mazansu, a cikin tsara ɗaya"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ga mazowa dama"
        }
    ]
}