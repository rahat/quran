{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Shin, ka ga wanda ke ƙaryatawa game da sakamako"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To, wannan shi ne ke tunkue maraya (daga haƙƙinsa)"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Kuma ba ya kwaaitarwa bisa bayar da abinci ga matalauci"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "To, bone ya tabbata ga masallata"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Waɗanda suke masu shagala daga sallarsu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Waɗanda suke yin riya (ga ayyukansu)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Kuma suna hana taimako"
        }
    ]
}