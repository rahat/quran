{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ka yi karatu da sunan Ubangijinka, wanda Ya yi halitta"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ya hahitta mutum daga gudan jini"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Ka yi karatu, kuma UbangiJinka shi ne Mafi karimci"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Wanda Ya sanar (da mutum) game da alƙalami"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ya sanar da mutum abin da bai sani ba"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "A'aha! Lalle, ne mutum haƙiƙa, yana girman kai (ya ƙi karatu)"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Domin ya ga kansa, ya wadata"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Lalle ne zuwa ga Ubangijinka makoma take"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Shin, ka ga wanda ke hana"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bawa idan ya yi salla"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ashe, ka gani, idan (shi bawan) ya kasance a kan shiriya"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ko ya yi umurni da taƙawa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Ashe, ka gani, idan (shi mai hanin) ya ƙaryata, kuma ya juya baya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ashe, bai sani ba cewa Allah Yana gani"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "A'aha! Lalle ne, idan bai hanu ba, lalle ne za Mu ja gashin makwarkwaɗa"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Makwarkwaɗa maƙaryaciya, mai laifi"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sai ya kirayi ƙungiyarsa"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Za Mu kirayi zabaniyawa (mala'iku masu girma)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "A'aha kada ka bi shi. Kuma ka yi tawali'u, kuma ka nemi kusanta (zuwa ga Ubangijinka)"
        }
    ]
}