{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bone ya tabbata ga duk mai nune, mai zunɗe (mai raɗa)"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Wanda ya tara dukiya, kuma ya mayar da ita abar tattalinsa"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yana zaton cewa dukiyarsa za ta dawwamar da shi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A'aha! Lalle ne za a jefa shi a cikin Huɗama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Kuma me ya sanar da kai abin da ake ce wa Huɗama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Wutar Allah ce wadda ake hurawa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Wadda take leƙawa a kan zukata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Lalle ne ita abar kullewa cea kansu"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "A cikin wasu ginshiƙai miƙaƙƙu"
        }
    ]
}