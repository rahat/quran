{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve tarık´a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık´ın ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlıgı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetleyici koruyucu bulunmayan hicbir nefis (kimse) yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan bir baksın, hangi seyden yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Dokulup atılan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(Bu su,) Bel kemigi ile kaburgalar arasında(ki organlar)dan cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Hic suphesiz (Allah,) onu yeniden dondurmege guc yetirendir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların orta yere cıkarılacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun ne gucu vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Donuslu olan goge andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Yarılan yere de"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Hic suphesiz o (Kur´an), ayırdeden bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, bir saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Dogrusu onlar, hileli bir duzen planlayıp kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir duzen kurup hazırlamaktayım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen simdi kufretmekte olanlara bir muhlet ver, kendilerine az bir sure tanı"
        }
    ]
}