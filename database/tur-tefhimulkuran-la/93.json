{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine andolsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "´Karanlıgı iyice coktugu´ zaman geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terketmedi ve darılmadı da"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Suphesiz senin icin son olan, ilk olandan (ahiret dunyadan) daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana verecek, boylece sen hosnut kalacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Bir yetim iken, seni bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni yol bilmez iken, dogru yola yoneltip iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Bir yoksul iken seni bulup da zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse, sakın yetimi uzup kahretme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Isteyip dileneni de azarlayıp cıkısma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetini durmaksızın anlat"
        }
    ]
}