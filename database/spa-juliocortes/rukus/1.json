{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "¡En el nombre de Alá, el Compasivo, el Misericordioso"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Alabado sea Alá, Señor del universo"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "el Compasivo, el Misericordioso"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Dueño del día del Juicio"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "A Ti solo servimos y a Ti solo imploramos ayuda"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Dirígenos por la vía recta"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "la vía de los que Tú has agraciado, no de los que han incurrido en la ira, ni de los extraviados"
        }
    ]
