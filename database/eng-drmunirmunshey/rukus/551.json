{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say, \"Oh you unbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "That which you worship is not what I worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "And you are not going to worship the One that I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I shall never worship that which you serve so devotedly"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nor will you worship Whom I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "You have your faith, and I have mine"
        }
    ]
}