{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Koly nebo rozirvetʹsya"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "i slukhatymetʹsya Hospoda svoho yak slid"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "i koly zemlya roztyahnetʹsya"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "i vyverne z sebe te, shcho v niy ye, ta y sporozhniye"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "i slukhatymetʹsya Hospoda svoho yak slid"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O lyudyno! Voistynu, ty napolehlyvo prahnesh do Hospoda svoho, tozh zustrinesh Yoho"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "I toy, komu dana bude knyha yoho v pravu ruku"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "otrymaye rozrakhunok lehkyy"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Povernetʹsya vin do rodyny svoyeyi, radiyuchy"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "A toy zhe, komu dana bude knyha yoho z-za spyny"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "bude naklykaty sobi pohybelʹ"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "i vviyde u vohnyane polumʺya"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Vin radiv, buduchy z rodynoyu svoyeyu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "i, voistynu, vvazhav, shcho ne povernetʹsya"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Ale zh, voistynu, Hospodʹ yoho sposterihav za nym"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Ni! Klyanusya vechirnʹoyu zoreyu"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "i nichchyu, i tym, shcho obiymaye vona"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "i misyatsem, koly povnyy vin"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "shcho vy budete perekhodyty z odnoho stanu v inshyy"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "To chomu zh vony ne uviruyutʹ"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "I koly chytayutʹ yim Koran, ne vklonyayutʹsya nyzʹko"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Ta zh ni! Ti, yaki ne viruyutʹ, vvazhayutʹ yoho brekhneyu"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Ta Allah krashche znaye te, shcho prykhovuyutʹ vony"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Tozh poraduy yikh karoyu bolisnoyu"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Okrim tykh, yaki uviruvaly ta chynyly dobri spravy, yim — nahoroda nevycherpna"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanusya nebom, volodarem suzirʺyiv"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "klyanusya Dnem obitsyanym"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "klyanusya svidkom i zasvidchenym"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Nekhay z·hynutʹ zibrani bilya rovu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "vohnyanoho, rozpechenoho palyvom"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "koly vsilysya vony bilya nʹoho"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "buduchy svidkamy toho, yak vony chynyatʹ z viruyuchymy"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "I vony mstylysya yim lyshe za te, shcho ti uviruvaly v Allaha, Velykoho, Khvalymoho"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Yakomu nalezhytʹ vlada nad nebesamy y zemleyu, y Allah — usʹomu sushchomu Svidok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Voistynu, tym, yaki siyaly smutu proty viruyuchykh, cholovikiv i zhinok, a potim ne pokayalysʹ, yim kara heyenoyu, yim kara vohnyana"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Voistynu, tym, yaki uviruvaly ta chynyly dobri spravy, yim sady, de techutʹ riky. Tse — uspikh velykyy"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Voistynu, kara Hospoda tvoho suvora"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Voistynu, Vin pochynaye ta povtoryuye"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "I Vin — Proshchayuchyy, Lyublyachyy"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Volodar tronu, Preslavnyy"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Chynytʹ Vin tak, yak voliye toho"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Chy diyshla do tebe rozpovidʹ pro viysʹka"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firauna ta samudytiv"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Khocha neviruyuchi y zaperechuyutʹ tse"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ta Allah okhoplyuye yikh zvidusilʹ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "A tse — preslavnyy Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "yakyy mistytʹsya u Skryzhali Zakhyshcheniy"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanusya nebom i podorozhnim"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Zvidky tobi znaty, khto takyy podorozhniy"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Tse — zirka syayucha"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "I nemaye dushi, poryad z yakoyu ne bulo by storozha"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Khay pohlyane lyudyna, z choho vona stvorena"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvorena z ridyny, shcho llyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yaka vykhodytʹ z-pomizh khrebta y hrudey"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voistynu, Vin zdatnyy povernuty yiyi"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "U toy Denʹ budutʹ vyprobuvani tayemnytsi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "tozh ne bude v lyudyny ni syly, ni pomichnyka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanusya nebom, yake ponovlyuye doshchi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "klyanusya zemleyu, yaka rozkolyuyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "shcho tse — slovo rozriznyuvalʹne"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Vono ne ye rozvahoyu"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Vony zamyslyuyutʹ khytroshchi"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ta YA tezh zamyslyuyu khytroshchi"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "tozh day neviruyuchym vidstrochku nedovhu"
        }
    ]
}