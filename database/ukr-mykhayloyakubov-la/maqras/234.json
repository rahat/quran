{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Nasupyvsya ta vidvernuvsya vin"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "bo pidiyshov do nʹoho slipyy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A zvidky tobi znaty, mozhlyvo, ochystyvsya b vin"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Chy zapamʺyatav by nastavnytstvo, i vono bulo b korysne yomu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A tomu, khto bahatyy"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ty yomu prydilyayesh uvahu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Khocha ne tobi vidpovidaty za te, shcho ne ochystytʹsya vin"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A khto prykhodytʹ do tebe, pospishayuchy"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ta shche y z ostrakhom"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "to ty nekhtuyesh nym"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ta zh ni! Voistynu, tse — nahaduvannya"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "I khto bazhaye, toy z·hadaye yoho"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "U suvoyakh shanovanykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "pidnesenykh i prechystykh"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "u rukakh pysariv"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "shlyakhetnykh i pravednykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Khay z·hyne lyudyna! Yaka zh nevdyachna vona"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z choho stvoryv Vin yiyi"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z krapli simʺya stvoryv Vin yiyi ta rozmiryv"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Potim polehshyv Vin shlyakh yiyi"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "z·hodom zrobyv yiyi mertvoyu i pokhovav yiyi"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "potim, koly pobazhaye, voskresytʹ yiyi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ta zh ni! Ne vykonuye vona toho, shcho nakazav Vin yiy"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Khay pohlyane lyudyna na yizhu svoyu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Prolyvayemo My doshchi zlyvamy"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "potim rozkolyuyemo My zemlyu trishchynamy"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i proroshchuyemo na niy zerno"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vynohrad i travy"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i olyvy, i palʹmy"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i sady husti"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i plody, i pasovysʹka –"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na korystʹ vam i khudobi vashiy"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "I koly prolunaye hlas"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "u Denʹ toy cholovik pokyne svoho brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i matir, i batʹka"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i druzhynu svoyu, i ditey svoyikh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "u kozhnoyi lyudyny v toy Denʹ svoya sprava, yaka tsilkom zakhopytʹ yiyi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Oblychchya odnykh u toy Denʹ budutʹ syayaty"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smiyuchysʹ ta radiyuchy"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a na oblychchi inshykh u Denʹ toy bude pyl"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "yakyy vkryye yikh chornotoyu"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Otse vony — neviruyuchi nechestyvtsi"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Koly sontse z·hornetʹsya"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "koly zirky posyplyutʹsya"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i koly hory zrushatʹsya"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i koly verblyudytsi na desyatomu misyatsi vahitnosti zalyshatʹsya bez nahlyadu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i koly dyki zviri zberutʹsya"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i koly morya zapalayutʹ"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i koly dushi zʺyednayutʹsya"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "koly pokhovanu zhyvtsem zapytayutʹ"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za yakyy hrikh yiyi vbyly"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i koly suvoyi roz·hornutʹsya"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i koly nebo bude zirvane"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i koly peklo zapalaye"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i koly ray nablyzytʹsya –"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "diznayetʹsya dusha, shcho pryhotuvala sobi vona"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ta zh ni! Klyanusya svitylamy vidstupayuchymy"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "shcho shvydko plynutʹ i znykayutʹ"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "I nichchyu, shcho rozsiyuyetʹsya"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i svitankom, koly zaymayetʹsya vin"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Voistynu, tse — slova poslantsya shlyakhetnoho"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Volodarya syly bilya Volodarya tronu, mohutnʹoho"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "yakomu koryatʹsya, virnomu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I shcho vash tovarysh ne bozhevilʹnyy"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "istynno, bachyv vin yoho na neboskhyli yasnomu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I shcho vin ne skupytʹsya na rozpovidi pro potayemne"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "i shcho tse — ne slova proklyatoho shaytana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ta kudy zh vy ydete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Voistynu, tse lyshe nahaduvannya svitam"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tym z-posered vas, khto bazhaye yty pryamym shlyakhom"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ale ne pobazhayete vy tsʹoho, yakshcho ne pobazhaye Allah, Hospodʹ svitiv"
        }
    ]
}