{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Tam rizni plody — palʹmy, hranaty"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Tam dobri ta prekrasni"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Chornooki, zakhovani v nametakh"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Ne torkalasya yikh ranishe ni lyudyna, ni dzhyn"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Lezhatymutʹ vony na zelenykh podushkakh i bahatykh kylymakh"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Khay zhe bude blahoslovenne imʺya Hospoda tvoho, Slavnoho ta Nayshchedrishoho"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Koly nastane nevidvorotna Podiya"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "to nikhto ne zaperechuvatyme prykhodu yiyi"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Vona prynyzhuye ta pidnosytʹ"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Koly zdryhnetʹsya zemlya"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "koly hory rozkolyutʹsya na shmatky"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanutʹ rozsypanym pylom"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "to vy stanete trʹoma hrupamy"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Odni lyudy pravoruch — khto ti, shcho pravoruch"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Odni lyudy livoruch — khto ti, shcho livoruch"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A ti, shcho poperedu — budutʹ poperedu"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Vony i ye nablyzhenymy"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "yaki perebuvayutʹ v sadakh nasolody"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bahato khto z nykh — pershi pokolinnya"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a menshistʹ — nastupni"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na lozhakh ozdoblenykh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "vony lezhatymutʹ odne navproty odnoho"
        }
    ]
}