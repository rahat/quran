{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanusya nebom i podorozhnim"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Zvidky tobi znaty, khto takyy podorozhniy"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Tse — zirka syayucha"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "I nemaye dushi, poryad z yakoyu ne bulo by storozha"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Khay pohlyane lyudyna, z choho vona stvorena"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvorena z ridyny, shcho llyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yaka vykhodytʹ z-pomizh khrebta y hrudey"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voistynu, Vin zdatnyy povernuty yiyi"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "U toy Denʹ budutʹ vyprobuvani tayemnytsi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "tozh ne bude v lyudyny ni syly, ni pomichnyka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanusya nebom, yake ponovlyuye doshchi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "klyanusya zemleyu, yaka rozkolyuyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "shcho tse — slovo rozriznyuvalʹne"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Vono ne ye rozvahoyu"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Vony zamyslyuyutʹ khytroshchi"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ta YA tezh zamyslyuyu khytroshchi"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "tozh day neviruyuchym vidstrochku nedovhu"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Proslavlyay imʺya Hospoda tvoho Vsevyshnʹoho"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Yakyy stvoryv ta rozmiryv"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Yakyy peredvyznachyv, vkazav shlyakh"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Yakyy zrostyv pasovysʹka"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "zrobyvshy yikh potim sukhymy ta pochornilymy"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My damo tobi prochytaty Koran, i ty ne zabudesh nichoho"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "okrim toho, shcho zabazhaye Allah. Vin znaye vidkryte y tayemne"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My polehshymo tobi shlyakh do naylehshoho"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "tozh nahaduy lyudyam, yakshcho nahaduvannya dopomozhe"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Z·hadaye zh toy, khto bohoboyazlyvyy"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A vidvernetʹsya zh toy nayneshchasnishyy"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "yakyy uviyde v vohonʹ naybilʹshyy"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "I pislya toho y ne pomre vin tam, i ne bude zhyty"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Vyhrav toy, khto ochystyvsya"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "z·haduvav svoho Hospoda y molyvsya"
        }
    ]
}