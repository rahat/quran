{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Klyanusya chasom"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Voistynu, lyudyna zaznaye zbytkiv"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Okrim tykh, yaki uviruvaly, tvoryly dobri spravy, zapovidaly odne odnomu istynu, zapovidaly odne odnomu terpinnya"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hore kozhnomu nasmishnyku, naklepnyku"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Yakyy zibrav bahat·stvo ta pidrakhuvav yoho"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dumaye vin, shcho bahat·stvo zrobytʹ yoho bezsmertnym"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Ale zh ni! Istynno, bude vin vkynutyy u roztroshchuyuche"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A zvidky tobi znaty, shcho tse — roztroshchuyuche"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tse — vohonʹ Allaha rozpalenyy"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yakyy zdiymayetʹsya nad sertsyamy"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Voistynu, vin zimknetʹsya nad nymy"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na lantsyuhakh prostyahnutykh"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Chy bachyv ty, yak vchynyv Hospodʹ tviy z vlasnykamy slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Chy Vin ne zrobyv yikhni khytroshchi nevdalymy"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I naslav na nykh ptakhiv z·hrayamy"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Kydaly vony v nykh kaminnya z obpalenoyi hlyny"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "I zrobyv Vin yikh podibnymy do nyvy stochenoyi"
        }
    ]
}