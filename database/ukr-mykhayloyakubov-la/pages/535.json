{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "a obkhodytymutʹ yikh vichno molodi yunaky"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "iz chashamy, hlekamy ta kubkamy z dzherelʹnym napoyem"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "vid yakoho ne bolytʹ holova ta vid yakoho ne khmeliyutʹ"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tam plody, yaki vony vybyrayutʹ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ptashyne mʺyaso, yakoho vony pobazhayutʹ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "ta chornooki"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "skhozhi na prykhovanu perlynu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Osʹ vynahoroda za te, shcho chynyly vony"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "I ne pochuyutʹ vony tam ni marnoslivʺya, ni hrikhovnykh rozmov"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "a lyshe slova: «Myr! Myr!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Lyudy pravoruch — khto ti, shcho pravoruch"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Vony budutʹ sered lotosiv, pozbavlenykh shypiv"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "pid bananamy, na yakykh ryadamy vysyatʹ plody"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "u shyrokykh zatinkakh"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "sered rozlytykh vod"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i bahatʹokh plodiv"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yakykh zavzhdy vdostalʹ, i vony pid rukoyu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Yim — pidneseni kylymy"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "voistynu, My stvoryly yikh shche raz"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i zrobyly yikh tsnotlyvymy"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lyublyachymy odnolitkamy"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Tse dlya tykh, khto pravoruch"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bahato khto z nykh — iz pershykh pokolinʹ"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a bahato khto — iz nastupnykh"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Lyudy livoruch — khto ti, shcho livoruch"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Pid palayuchym vitrom, u okropi"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "U zatinku chornoho dymu"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ni prokholody yim, ni blaha"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ranishe, pered tsym, vony rozkoshuvaly"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "buly vpertymy u svoyemu hrikhu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i hovoryly odne odnomu: «Nevzhe, koly my pomremo ta peretvorymosya na prakh i kistky, my spravdi voskresnemo"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "abo nashi batʹky?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Skazhy: «Voistynu — i pershi, i ostanni pokolinnya"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "spravdi budutʹ zibrani v pevnomu mistsi, u vyznachenyy denʹ!»"
        }
    ]
}