{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zarady yednannya kurayshytiv —"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yednannya pid chas podorozhey, vlitku ta vzymku"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Nekhay zhe vony poklonyayutʹsya Hospodu tsʹoho Domu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Adzhe Vin nahoduvav yikh u holodnyy chas i pozbavyv strakhu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Chy bachyv ty toho, khto zaperechuye Sud"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ta tse zh toy, khto prohanyaye syrotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne zaklykaye nahoduvaty bidnoho"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Lykho zh tym, yaki molyatʹsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ale stavlyatʹsya do molytov svoyikh nedbalo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yaki roblyatʹ vse lyshe napokaz"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i vidmovlyayutʹsya davaty mylostynyu"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Voistynu, daruvaly My tobi dostatok"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Tozh molysya Hospodu svoyemu ta zhertvuy"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Voistynu, toy, khto nenavydytʹ tebe — sam bezplidnyy"
        }
    ]
}