{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Dosyahayutʹ uspikhu viruyuchi"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "yaki smyrenni u svoyikh molytvakh"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "yaki unykayutʹ usʹoho marnoho"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "yaki dayutʹ zakyat"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "yaki zakryvayutʹ svoyu nahotu vid usikh"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "okrim druzhyn, i tykh, kym ovolodily yikhni pravytsi, — yim ne dokoryatymutʹ za tse"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "a khto prahne bilʹshoho, ti — porushnyky"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "yaki zberihayutʹ dovirene yim ta dohovory"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "yaki nalezhno zvershuyutʹ svoyi molytvy"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "same vony — spadkoyemtsi"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "yaki otrymayutʹ u spadok Firdaus, de budutʹ doviku"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Spravdi, My stvoryly lyudynu z hlynyanoho zamisu"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "a potim u nadiynomu mistsi peretvoryly na kraplyu"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "potim stvoryly z krapli krovʺyanyy z·hustok, a z krovʺyanoho z·hustku stvoryly shmatochok ploti. Zi shmatochku ploti My stvoryly kistky, a potim pokryly yikh mʺyazamy. Potim My vyrostyly tse yak nove tvorinnya! Blahoslovennyy Allah, Naykrashchyy iz tvortsiv"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Z·hodom vy neodminno pomrete"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "a potim, u Denʹ Voskresinnya, vas znovu pidnimutʹ"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Spravdi, My stvoryly nad vamy sim shlyakhiv. I My ne buly neuvazhni do Svoyikh tvorinʹ"
        }
    ]
}