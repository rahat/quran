{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Koly nastane nevidvorotna Podiya"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "to nikhto ne zaperechuvatyme prykhodu yiyi"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Vona prynyzhuye ta pidnosytʹ"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Koly zdryhnetʹsya zemlya"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "koly hory rozkolyutʹsya na shmatky"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanutʹ rozsypanym pylom"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "to vy stanete trʹoma hrupamy"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Odni lyudy pravoruch — khto ti, shcho pravoruch"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Odni lyudy livoruch — khto ti, shcho livoruch"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A ti, shcho poperedu — budutʹ poperedu"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Vony i ye nablyzhenymy"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "yaki perebuvayutʹ v sadakh nasolody"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bahato khto z nykh — pershi pokolinnya"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a menshistʹ — nastupni"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na lozhakh ozdoblenykh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "vony lezhatymutʹ odne navproty odnoho"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "a obkhodytymutʹ yikh vichno molodi yunaky"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "iz chashamy, hlekamy ta kubkamy z dzherelʹnym napoyem"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "vid yakoho ne bolytʹ holova ta vid yakoho ne khmeliyutʹ"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tam plody, yaki vony vybyrayutʹ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ptashyne mʺyaso, yakoho vony pobazhayutʹ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "ta chornooki"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "skhozhi na prykhovanu perlynu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Osʹ vynahoroda za te, shcho chynyly vony"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "I ne pochuyutʹ vony tam ni marnoslivʺya, ni hrikhovnykh rozmov"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "a lyshe slova: «Myr! Myr!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Lyudy pravoruch — khto ti, shcho pravoruch"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Vony budutʹ sered lotosiv, pozbavlenykh shypiv"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "pid bananamy, na yakykh ryadamy vysyatʹ plody"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "u shyrokykh zatinkakh"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "sered rozlytykh vod"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i bahatʹokh plodiv"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yakykh zavzhdy vdostalʹ, i vony pid rukoyu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Yim — pidneseni kylymy"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "voistynu, My stvoryly yikh shche raz"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i zrobyly yikh tsnotlyvymy"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lyublyachymy odnolitkamy"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Tse dlya tykh, khto pravoruch"
        }
    ]
}