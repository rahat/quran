{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Voistynu, derevo zakkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "bude yizheyu dlya hrishnyka"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "i, nache oliynyy osad, kypityme v cherevakh"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "tak, yak kypytʹ okrip"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Skhopitʹ yoho! Tyahnitʹ do seredyny pekla"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Skarayte yoho — lyyte na holovu okrip"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Skushtuy zhe tsʹoho! Adzhe ty — vsemohutniy, blahorodnyy"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Osʹ te, v chomu vy sumnivalysʹ"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Voistynu, bohoboyazlyvi opynyatʹsya u bezpechnomu mistsi"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Sered sadiv ta dzherel"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "odyahnuvshysʹ u atlas i parchu, vony syditymutʹ odne navproty odnoho"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Osʹ tak! My poyednayemo yikh z chornookymy"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Vony prosytymutʹ kozhen plid, perebuvayuchy v bezpetsi"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Tam vony vzhe ne zaznayutʹ smerti — pislya toho, yak stalasya persha smertʹ. Vin vryatuye yikh vid pekelʹnoyi kary"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "za Svoyeyu laskoyu! Tse i ye velykyy uspikh"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "My polehshyly yoho na tvoyiy movi, mozhlyvo, zamyslyatʹsya vony"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Zachekay zhe, adzhe chekayutʹ i vony"
        }
    ]
}