{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Skazhy: «O vy, neviruyuchi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "YA ne poklonyayusya tomu, chomu poklonyayetesya vy"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a vy ne poklonyayetesʹ tomu, chomu ya poklonyayusya"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "YA ne poklonyayusya tak, yak poklonyayetesya vy"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "a vy ne budete poklonyatysya tak, yak budu poklonyatysʹ ya"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "vam — vasha vira, a meni — moya!»"
        }
    ]
}