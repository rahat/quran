{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Lykho"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Shcho zh take lykho"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I zvidky znaty tobi, shcho take lykho"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Tse — Denʹ, koly lyudy rozsiyutʹsya, nache metelyky"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "A hory budutʹ skhozhi na vovnu rozchesanu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Todi toy, chyya chasha na terezakh bude vazhkoyu –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "otrymaye vin zhyttya, spovnene zadovolennya"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A toy, chyya chasha na terezakh bude lehkoyu –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "prytulkom yoho bude prirva"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A zvidky znaty tobi, shcho tse take"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Tse — vohonʹ palayuchyy"
        }
    ]
}