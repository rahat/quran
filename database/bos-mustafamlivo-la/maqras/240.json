{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Pa zar ne zna, kad se rasturi sta je u kaburima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "I otkrije sta je u prsima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Uistinu ce Gospodar njihov o njima Tog dana biti Obavijesteni"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Sta je udar"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I sta znas ti sta je udar"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Dan kad budu ljudi kao leptiri rastrkani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "I budu brda kao vuna rascupana"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Pa onaj cije mjere budu teske"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Pa on ce u zivotu zadovoljstva biti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A onaj cije mjere budu lahke"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Pa majka njegova bice hawija"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A sta znas ti sta je ona"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Vatra zestoka"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Odvraca vas uvecavanje"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Dok greblja ne posjetite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikako! Saznacete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ponovo ne, saznacete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikako! Kad biste znali znanjem sigurnim"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Zaista cete vidjeti dzehim"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ponovo, zaista cete ga vidjeti okom sigurnim"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim cete zaista Tog dana biti pitani o uzivanju"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Tako mi vremena"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Uistinu, covjek je u gubitku"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Izuzev onih koji vjeruju i cine dobra djela, i preporucuju jedni drugima Istinu i preporucuju jedni drugima strpljenje"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Tesko svakom klevetniku, podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Koji bogatstvo skuplja i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Misli da ce ga bogatstvo njegovo besmrtnim uciniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikako! Sigurno ce bacen biti u hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A sta znas ti sta je hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova raspaljena"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Koja ce na srca dosezati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Uistinu, ona ce na njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "U stupovima izduzenim"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Zar nisi vidio kako je Gospodar tvoj sa vlasnicima slona postupio"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Zar nije osujetio (ratno) lukavstvo njihovo"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I poslao na njih jata ptica"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Gađale su ih kamenjem od gline specene"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Te ih ucinio kao vlat izjedenu"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog navike Kurejsija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Navike putovanja njihovog, zimi i ljeti"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pa neka obozavaju Gospodara ove Kuce"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Koji ih za gladi hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Jesi li vidio onog koji porice Sud"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji odbija siroce"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "I ne podstice hranjenju siromaha"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa tesko musallijama"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onima koji su prema salatu svom nemarni"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onima koji bi da budu viđeni"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odbijaju el-ma'un"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Uistinu, Mi smo tebi dali Kewser"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Zato cini salat Gospodaru svom i kolji (kurban)"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Uistinu, mrzitelj tvoj - on je ebter"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: \"O nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ne obozavam sta obozavate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "I niste vi obozavatelji onog sta obozavam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I nisam ja obozavalac onog sta obozavate"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Niti ste vi obozavaoci onog sta obozavam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vama vjera vasa, a meni vjera moja"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kad dođe pomoc Allahova i pobjeda"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "I vidis ljude ulaze u vjeru Allahovu u grupama"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Tad slavi sa hvalom Gospodara svog i trazi oprost od Njega. Uistinu! On je Primalac pokajanja"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Propale ruke Ebu Lehebove - i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nece mu koristiti bogatstvo njegovo i sta je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Przice se vatrom rasplamsalom"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "I zena njegova, nosacica drva"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Na vratu njenom bice konop od palmina lika"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Reci: \"On, Allah, je Jedan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah je Apsolutni"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ne rađa i nije rođen"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "I ravan Mu nije niko"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: \"Trazim zastitu u Gospodara svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Od zla onog sta stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "I od zla tame kad zamraci"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "I od zla onih koje pusu u cvorove"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "I od zla zavidnika kad zavidi"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Trazim zastitu u Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Od zla dosaptavanja hannasa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Koji dosaptava u grudi ljudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Od dzinna i ljudi"
        }
    ]
}