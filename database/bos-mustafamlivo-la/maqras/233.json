{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O cemu se raspituju"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O vijesti krupnoj"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Onoj o kojoj se oni razilaze"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Nikako! Saznace"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Ponovo, nikako, saznace"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Zar nismo ucinili Zemlju posteljom"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "A brda stubovima"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "I stvorili vas u parovima"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "I ucinili san vas odmorom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "I ucinili noc pokrivkom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "I nacinili dan za privređivanje"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "I sazdali iznad vas sedam jakih"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "I nacinili svjetiljku plamtecu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "I spustili iz oblaka vodu obilnu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Da izvedemo njome zrno i bilje"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "I basce guste"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Uistinu, Dan odluke je vrijeme određeno"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Na Dan kad se puhne u sur pa dođete u grupama"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "I otvori nebo, pa bude kapijama"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "I pokrenu brda, pa bude fatamorgana"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Uistinu, Dzehennem ce biti busija"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Za prevrsitelje povratiste"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Oni ce ostati u njemu godinama"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Nece okusiti u njemu hladnocu, niti pice"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Izuzev vrele vode i ledene"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Place odgovarajuce"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Uistinu, oni se nisu nadali obracunu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "I poricali su ajete Nase laganjem"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "A svaku stvar: pobrojali smo je Knjigom"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zato kusajte. Pa necemo vam povecati - sem kaznu"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Uistinu, bogobojazni ce imati uspjeh"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Basce i vinograde"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "I jedrih grudi vrsnjakinje"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "I casu punu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Nece u njemu cuti besmislicu, niti laganje"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Placa od Gospodara tvog, dar obracunat"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gospodara nebesa i Zemlje i sta je između njih, Milostivog. Nece moci govoriti s Njim"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Na Dan kad bude stajao Duh i meleci posafani, nece govoriti, izuzev onaj kome dopusti Milostivi - a govorice tacno"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Taj dan je istinit, pa ko hoce, uzece ka Gospodaru svom povratiste"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Uistinu, Mi vas upozoravamo kaznom bliskom, na Dan kad vidi covjek sta su unaprijed poslale ruke njegove i rekne nevjernik: \"O da sam prasina"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji cupaju zestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeci"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad uzdrma potres"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijedice ga naredni"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca ce Tog dana biti uzdrhtala"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi bice oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: \"Da li cemo uistinu mi biti vraceni u prvobitno stanje"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: \"To je onda ponavljanje pogubno"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa uistinu, to ce krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je dosao hadis o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kad ga zovnu Gospodar njegov u dolini svetoj Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idi faraonu, zaista je on prevrsio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Pa reci: 'Da li bi ti da se ocistis"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "I: 'Uputicu te Gospodaru tvom, pa da se bojis"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Tad mu je pokazao znak najveci"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Pa - porekao je i nije poslusao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Zatim se okrenuo zureci"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Pa sakupio (ljude), te povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Pa rekao: \"Ja sam gospodar vas najuzviseniji"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Zato ga je Allah dograbio primjernom kaznom drugog i prvog"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Zaista je u tome ibret za onog ko se boji"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Da li je vas teze stvoriti ili nebo? Sazdao ga je"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Uzdigao visinu njegovu, pa ga sredio"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "I zatamnio noc njegovu, i izveo svjetlo njegovo"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "I Zemlju poslije toga - rasprostro je nju"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Izveo iz nje vodu njenu i pasnjake njene"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "I brda: ustabilio ih je"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Uzivanje za vas i stoku vasu"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Pa kad dođe tammet najveci"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Na Dan kad se sjeti covjek sta se trudio"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "I pokaze se dzehim onom ko vidi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Pa sto se tice onog ko je pretjerivao"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "I vise volio zivot Dunjaa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Pa uistinu, dzehim - to ce skloniste biti"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "A sto se tice onog ko se plasio stajanja pred Gospodarom svojim, i suzdrzao dusu od strasti"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Pa uistinu, Dzennet - to ce skloniste biti"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Pitaju te o Casu: \"Kad ce dolazak njegov"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Cemu? Ti si za podsjecanje na njega"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Do Gospodara tvoga je vrhunac njegov"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Ti si samo opominjac onom ko ga se plasi"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Na dan kad ga vide, bice kao da su oni ostali samo vecer ili jutro njegovo"
        }
    ]
}