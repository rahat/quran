{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kad se nebo rascijepi"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "I kad se planete rasprse"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "I kad mora uzavriju"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "I kad se kaburi ispreturaju"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Znace dusa sta je unaprijed poslala i (sta) zapostavila"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O covjece! Sta te je obmanulo o Gospodaru tvom plemenitom"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Koji te je stvorio, pa te uredio, te te simetricnim ucinio"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "U kojem obliku, kako je htio, sastavio te"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Nikako! Naprotiv, poricete Sud"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A uistinu, nad vama su cuvari"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Plemeniti pisari"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Znaju sta cinite"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Uistinu, pravedni ce u zadovoljstvo"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "I uistinu, razvratnici ce u dzehim"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Przice se njime Dana sudnjeg"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I nece oni iz njega biti odsutni"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "A sta znas ti sta je Dan sudnji"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ponovo, sta znas ti sta je Dan sudnji"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Dan kad nece vladati dusa za dusu nimalo, a komanda ce Tog dana biti Allahova"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Tesko zakidateljima"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Koji kad uzimaju od svijeta, traze potpuno"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "A kad njima mjere ili im vagaju, zakidaju"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Zar ne misle takvi da ce oni biti podignuti"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Za Dan strasni"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Dan kad ce stati ljudi pred Gospodara svjetova"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Nikako! Zaista je knjiga razvratnika u Sidzdzinu"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "A sta znas ti sta je Sidzdzin"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Knjiga ispisana"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Koji nijecu Dan sudnji"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "A ne nijece ga, izuzev svaki prestupnik, grjesnik"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Kad mu se uce ajeti Nasi, rekne: \"Price ranijih"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Nikako! Naprotiv, ono sto su stekli je hrđa na srcima njihovim"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Nikako! Zaista ce oni od Gospodara svoga Tog dana biti zaklonjeni"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Zatim ce se uistinu oni prziti dzehimom"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Potom ce biti receno: \"Ovo je to sto ste poricali"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Nikako! Uistinu je Knjiga cestitih u Illijjunu"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "A sta znas ti sta je Illijjun"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Knjiga ispisana"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Svjedocice je priblizeni"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Uistinu ce cestiti u zadovoljstvu biti"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Na divanima ce gledati"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Spoznaces na licima njihovim sjaj zadovoljstva"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Napajani ce biti nektarom zapecacenim"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Pecat njegov bice misk; a za to neka se natjecu takmicari"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "A mjesavina njegova od Tesnima"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Izvora s kojeg ce piti priblizeni"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Uistinu! Oni koji su skrivili ismijavali su se onima koji su vjerovali"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "I kad su pored njih prolazili, namigivali"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "A kad su se vracali porodici svojoj, vracali su se saleci se"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "I kad bi ih vidjeli, rekli bi: \"Zaista su ovi zalutali"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "A nisu poslani nad njima cuvarima"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Pa, Danas ce se oni koji su vjerovali, nevjernicima ismijavati"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Na divanima ce gledati"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Zar nisu nevjernici nagrađeni kako su radili"
        }
    ]
}