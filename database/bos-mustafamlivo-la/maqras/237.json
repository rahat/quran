{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi ime Gospodara svog Svevisnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji stvara, pa usklađuje"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "I Koji odmjerava, pa upucuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "I Koji izvodi pasnjak"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pa ga ucini otpadom tamnozelnim"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Poucicemo te ucenju, pa neces zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Izuzev sta htjedne Allah. Uistinu! On zna otkriveno i sta je skriveno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "A olaksacemo ti za lahkocu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Zato opominji, ako koristila opomena"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Opomenuce se ko se boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A izbjegava je najnesrecniji"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Koji ce se prziti vatrom velikom"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Potom nece u njoj umrijeti, niti zivjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Zaista ce uspjeti ko se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "I sjeti imena Gospodara svog, pa salat obavi"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Naprotiv, vise volite zivot Dunjaa"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "A Ahiret je bolji i trajniji"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Uistinu! Ovo je u listovima ranijim"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Listovima Ibrahimovim i Musaovim"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li ti je dosao hadis o prekricu"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Lica ce Tog dana biti ponizena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Izmorena, napacena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Vatrom uzarenom przena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Sa izvora uzavrelog pojena"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Nece biti hrane za njih, izuzev od trna"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Nece ugojiti, niti protiv gladi koristiti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Lica ce Tog dana biti radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "U Dzennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Nece cuti u njemu besmislicu"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu je izvor tekuci"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "U njemu su sofe uzdignute"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "I case postavljene"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "I jastucici poredani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "I cilimi razastrti"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa zar nece pogledati kamilu, kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I nebo, kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I brda, kako su postavljena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I Zemlju, kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pa opominji, ti si samo opominjac"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nisi nad njima pazitelj"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A ko se okrene i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pa kaznice ga Allah kaznom najvecom"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Uistinu, Nama je povratak njihov"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Zatim, uistinu, na Nama je obracun njihov"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Tako mi zore"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "I deset noci"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "I parnog i neparnog"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "I noci kad odlazi"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Da li je u tome zakletva za posjednika razuma"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Zar nisi vidio kako je postupio Gospodar tvoj sa Adom"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iremom punim stubista"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Kojima nisu stvoreni slicni u zemljama"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "I Semudom, koji su klesali stijene u dolini"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "I faraonom, posjednikom kolceva"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Koji su prevrsili u zemljama"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Te namnozili u njima fesad"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Pa je Gospodar tvoj pustio na njih bic kazne"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Uistinu, Gospodar tvoj je u zasjedi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Pa sto se tice covjeka, kad ga iskusava Gospodar njegov, pa mu ukaze cast i podari mu blagodat, tad kaze: \"Gospodar moj mi ukaza cast"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kad ga iskusava, pa mu uskrati opskrbu njegovu, tad kaze: \"Gospodar moj me prezreo"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Nikako! Naprotiv! Ne ukazujete cast sirocetu"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "I ne podsticete hranjenju siromaha"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "I nasljedstvo jedete zderackom pohlepom"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "I volite bogatstvo ljubavlju neizmjernom"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Nikako! Kad se zdrobi Zemlja, u mrvice, komadice"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "I dođe Gospodar tvoj i meleci red do reda"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "I prinese se Tog dana Dzehennem -Tog dana sjecat ce se covjek - a cemu mu sjecanje"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Govorit ce: \"O da sam (sto) unaprijed poslao za zivot svoj"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Pa Tog dana nece kazniti kaznom Njegovom niko"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Niti ce vezati vezom Njegovom iko (kao On)"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O duso smirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Vrati se Gospodaru svom zadovoljna, zadovoljavajuca"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Pa uđi među robove Moje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "I uđi u Dzennet Moj"
        }
    ]
}