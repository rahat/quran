{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Trazim zastitu u Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Od zla dosaptavanja hannasa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Koji dosaptava u grudi ljudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Od dzinna i ljudi"
        }
    ]
}