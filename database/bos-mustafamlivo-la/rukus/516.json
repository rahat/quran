{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Tako mi izaslanika uzastopnih"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Te onih koji bjesne uraganski"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "I onih koji rasprostiru prostiranjem"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Te onih koji razdvajaju dijeljenjem"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Te onih koji dostavljaju Opomenu"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Opravdanje ili upozorenje"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Uistinu, ono cime vam se prijeti dogodice se"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Pa kad se zvijezde pogase"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "I kad se nebo rastvori"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "I kad se brda zdrobe (u prasinu)"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "I kad se poslanicima vrijeme odredi"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Za koji dan je odgođeno"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Za Dan razdvajanja"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A sta znas ti sta je Dan razdvajanja"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Zar nismo unistili ranije"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Zatim dali da ih slijede kasniji"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Tako postupamo sa prestupnicima"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Zar vas ne stvaramo od tekucine prezrene"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Pa je stavljamo u boraviste postojano"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Do roka poznatog"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pa uređujemo, ta divni su Uređivaci"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Zar nismo ucinili Zemlju sabiralistem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Zivih i mrtvih"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "I nacinili na njoj gore stabilne, visoke, i napojili vas vodom pitkom"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Idite onom sta ste poricali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Idite prema sjeni sa tri grane"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Bez hlada ce biti, a nece koristiti protiv plamena"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Uistinu! Ona ce izbacivati iskre kao dvorac"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Kao da su one kamile zute"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ovo je Dan kad nece govoriti"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Niti ce im se dopustiti, pa da se opravdavaju"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ovo je Dan odluke! Sabrali smo vas i prve"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pa ako vi imate varku - ta prevarite Me"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Tesko Tog dana poricateljima"
        }
    ]
}