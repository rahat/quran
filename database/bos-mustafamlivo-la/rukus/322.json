{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Ad je poricao izaslanike"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Kad im rece brat njihov Hud: \"Zar se ne bojite"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Uistinu! Ja sam vam poslanik pouzdani"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Zato se bojte Allaha i poslusajte mene"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "A ne trazim od vas za to nikakvu nagradu, nagrada moja je jedino do Gospodara svjetova"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Gradite li na svakoj uzvisici obiljezje, poigravate se"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "I gradite tvrÄ‘ave, da biste se vi ovjekovjecili"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "I kad napadate, napadate (kao) tirani"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Zato se bojte Allaha i poslusajte mene"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "I bojte se Onog koji vas je pomogao onim sta znate"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Pomogao vas je stokom i sinovima"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "I bascama i izvorima"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Uistinu! Ja se plasim nad vama kazne Dana strahovitog"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Rekose: \"Isto nam je da li ti wazio ili ne bio od waiza"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "Ovo je samo obicaj ranijih"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "I necemo mi biti kaznjeni"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Tad su ga porekli, pa smo ih unistili. Uistinu! U tome je znak, a vecina njih nisu vjernici"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "A uistinu! Gospodar tvoj! On je Mocni, Milosrdni"
        }
    ]
}