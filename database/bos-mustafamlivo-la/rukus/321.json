{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Narod Nuhov je poricao izaslanike"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Kad im rece brat njihov Nuh: \"Zar se ne bojite"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Uistinu! Ja sam vama poslanik pouzdan"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Zato se bojte Allaha i mene poslusajte"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "A ne trazim od vas za to nikakvu nagradu, nagrada moja je jedino do Gospodara svjetova"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Zato se bojte Allaha i mene poslusajte"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Rekose: \"Zar da tebi vjerujemo, a slijede te najnizi"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Rece: \"A nemam ja znanje o onom sta rade"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "Obracun njihov je jedino do Gospodara mog, kad biste zapazili"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "I nisam ja progonitelj vjernika"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "Ja sam samo opominjac jasni"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Rekose: \"Ako ne prestanes, o Nuhu, sigurno ces biti od kamenovanih"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Rece: \"Gospodaru moj! Uistinu, moj narod me porice"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Zato presudi između mene i između njih presudom, i spasi mene i onog ko je sa mnom od vjernika"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Pa spasismo njega i onog ko je bio s njim, u lađi natovarenoj"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Zatim smo poslije potopili preostale"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Uistinu, u tome je znak, a vecina njih nisu vjernici"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "I uistinu, Gospodar tvoj! On je Mocni, Milosrdni"
        }
    ]
}