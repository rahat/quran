{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "I doista smo obdarili Musaa i Haruna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "I spasili njih dvojicu i narod njihov od zala strasnog"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "I pomogli ih, pa su oni bili pobjednici"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "I dali smo njima dvojici Knjigu jasnih (stvari)"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "I uputili ih putem pravim"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "I ostavili njima dvojici među kasnijima"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selam Musau i Harunu"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Uistinu, Mi tako nagrađujemo dobrocinitelje"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Uistinu, njih dvojica su bili od robova Nasih vjernika"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "I uistinu, Il'jas je bio od izaslanika"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Kad rece narodu svom: \"Zar se ne bojite"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Zar prizivate Ba'la, a ostavljate Najboljeg od stvoritelja"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha, Gospodara vaseg i Gospodara oceva vasih ranijih"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Pa su ga porekli, pa uistinu ce oni biti privedeni"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Izuzev robova Allahovih cistih"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "I ostavili smo njemu među kasnijima"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selam Il'jasu"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Uistinu, Mi tako nagrađujemo dobrocinitelje"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Uistinu, on je bio od robova Nasih vjernika"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "I uistinu, Lut je bio od izaslanika"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Kad spasismo njega i porodicu njegovu - sve"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Izuzev starice među izostalima"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Zatim smo unistili druge"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "A uistinu, vi prolazite pored njih jutrom"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "I nocu; pa zar ne shvatate"
        }
    ]
}