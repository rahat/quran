{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Ne! Kunem se Danom kijameta"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "I ne! Kunem se dusom samooptuzujucom"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Misli li covjek da necemo sastaviti kosti njegove"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Svakako! Kadri smo da uredimo jagodice njegove"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Naprotiv! Zeli covjek da grijesi pred sobom"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Pita: \"Kad ce Dan kijameta"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Pa kad se ukoci pogled"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "I pomraci Mjesec"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "I spoji Sunce i Mjesec"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Tog dana reci ce covjek: \"Gdje je pribjeziste"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Nikako! Nema utocista"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Gospodaru tvom je Tog dana staniste"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Obavijestit ce se covjek Tog dana sta je unaprijed poslao i (sta) zapostavio"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Stavise, covjek ce protiv duse svoje dokaz biti"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Makar davao opravdanja svoja"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Ne pokreci njime jezik svoj, da bi ga ubrzao"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Uistinu, na Nama je sastavljanje njegovo i ucenje njegovo"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Pa kad ga ucimo, slijedi ucenje njegovo"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Zatim, uistinu na Nama je objasnjavanje njegovo"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Nikako! Naprotiv! Volite ubrzano"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "A ostavljate Ahiret"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Lica ce Tog dana biti blistava"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Prema Gospodaru svome gledajuci"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "I lica ce Tog dana biti smrknuta"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Znat ce da ce im se uciniti nesreca"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Nikako! Kad dospije kljucnjacama"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "I bude receno: \"Ko je vrac"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "I znadne da je to rastanak"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "I savije potkoljenica uz potkoljenicu"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Gospodaru tvom bice Tog dana sprovoÄ‘enje"
        }
    ]
}