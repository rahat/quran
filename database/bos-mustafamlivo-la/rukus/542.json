{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Tako mi onih koji dahcuci jure"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Pa onih koji iskre kresu"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Te onih koji napadaju jutrom"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Pa dizu time prasinu"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Pa prodiru time u sred skupine"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Uistinu! Covjek je Gospodaru svome nezahvalan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "I uistinu, on je tome svjedok"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "I uistinu, on je u ljubavi prema dobru zestok"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Pa zar ne zna, kad se rasturi sta je u kaburima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "I otkrije sta je u prsima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Uistinu ce Gospodar njihov o njima Tog dana biti Obavijesteni"
        }
    ]
}