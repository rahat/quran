{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kad se dogodi događaj"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Nece biti poricanja događanja njegovog"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ponizavajuci, uzdizuci"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se zatrese Zemlja potresom"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "I smrve brda drobljenjem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "I budu prasina rasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "I budete vrste tri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Pa drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I drugovi lijeve strane - sta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "I prednjaci, prednjaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Takvi ce biti priblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "U basce uzivanja"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnostvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "A malo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na sofama (ukrasom) oblozenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Naslonjeni na njima, naspramni"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obilazice oko njih mladici vjecni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "S peharima i ibricima i casom s izvorista"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Nece dobiti glavobolju od njega, niti se zamoriti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I vocem od kojeg ce birati"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I mesom pticjim od cega zazele"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I hurije lijepih krupnih ociju"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kao primjerci bisera skrivenog"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Placa za ono sta su radili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nece cuti u njemu besmislicu, niti grijesnost"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Izuzev govora: \"Selam! Selam"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U lotosovom drvecu bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I bananama nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I hladu rasprostrtom"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I vodi tekucoj"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I vocu mnogobrojnom"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Nije odsjeceno, niti zabranjeno"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Uistinu! Mi smo ih stvorili stvaranjem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I ucinili ih djevicama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Umilnim vrsnjakinjama"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Drugovima desne strane"
        }
    ]
}