{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "Sta vam je? Kako sudite"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Pa zar se necete pouciti"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Ili vi imate dokaz ocit"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Pa donesite knjigu svoju, ako istinu govorite"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "I uspostavljaju izmeÄ‘u Njega i dzinna srodstvo, a doista dzinni znaju da ce oni uistinu biti privedeni"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Slavljen neka je Allah od onog sta pripisuju"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Izuzev robova Allahovih cistih"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Pa uistinu, vi i ono sta obozavate"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Niste vi protiv Njega zavodnici"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Izuzev onog koji ce biti przen dzehimom"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "A nema od nas nijednog, a da njegovo mjesto nije poznato"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "I uistinu! Mi, Mi smo poredani"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "I uistinu, Mi, Mi smo slavitelji"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "A oni su sigurno govorili"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Da je kod nas Opomena od ranijih"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Sigurno bismo bili robovi Allahovi cisti"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Pa nisu u njega vjerovali, pa saznace"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "A vec je prethodila Rijec Nasa robovima Nasim poslanim"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Uistinu, oni - oni ce biti pomognuti"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "I uistinu, vojska Nasa - oni ce biti pobjednici"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Zato se okreni od njih izvjesno vrijeme"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "I posmatraj ih, pa vidjece"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Pa zar kaznu Nasu pozuruju"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Pa kad se spusti u avliju njihovu, tad ce biti zlo jutro upozoravanih"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "I okreni se od njih izvjesno vrijeme"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "I posmatraj, pa vidjet ce"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Slavljen neka je Gospodar tvoj, Gospodar moci, od onog sta pripisuju"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "I selam izaslanicima"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "I hvala Allahu, Gospodaru svjetova"
        }
    ]
}