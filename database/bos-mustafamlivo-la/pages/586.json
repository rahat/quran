{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada se Sunce smota"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "I kad se zvijezde razlete"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "I kad se brda pokrenu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "I kad se steone kamile napuste"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "I kad se zvijeri sakupe"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "I kad se mora zapale"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "I kad se duse spare"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "I kad zivo zakopana bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Zbog kojeg grijeha je ubijena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "I kad se listovi rasire"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "I kad nebo bude svuceno"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "I kad se dzehim raspali"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "I kad se Dzennet priblizi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Znace dusa sta je pripremila"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Pa ne! Kunem se onima koje odstupaju"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Koje se krecu, koje se skrivaju"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "I nocu, kad odlazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "I jutrom, kad dise"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Uistinu, to je rijec izaslanika plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Posjednika moci, kod Vlasnika Arsa cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Slusanog, tamo pouzdanog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I nije drug vas lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "A uistinu, vidio ga je na horizontu jasnom"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I nije on nad nevidljivim skrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "I nije to govor sejtana prokletog"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Pa kamo idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Zaista je on samo Opomena svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Za onog od vas ko hoce da ide pravo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "A necete, izuzev sta hoce Allah, Gospodar svjetova"
        }
    ]
}