{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tako mi smokve i masline"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "I Gore Sinajske"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "I grada ovog sigurnog"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Zaista smo covjeka stvorili u skladu najboljem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Zatim cemo ga vratiti najnizem od niskih"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Izuzev onih koji vjeruju i cine dobra djela - pa imace oni nagradu neprekidnu"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pa sta te navodi da porices Sud nakon (ovog)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Zar nije Allah Najmudriji od sudaca"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Citaj u ime Gospodara svoga koji stvara"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Stvara covjeka od zakvacka"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Citaj! A Gospodar tvoj je Najplemenitiji"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Onaj koji poucava perom"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Poucava covjeka ono sta ne zna"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nikako! Zaista covjek prevrsi"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Cim se neovisnim vidi"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Uistinu, Gospodaru tvom je povratak"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Jesi li vidio onog koji sprjecava"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Roba kad salat obavlja"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Jesi li vidio - ako je na Uputi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ili nareÄ‘uje bogobojaznost"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Jesi li vidio - ako porice i okrece se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Zar ne zna da Allah vidi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nikako! Ako ne prestane, zaista cemo ga scepati za kiku"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Kiku laznu, grjesnu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pa neka pozove skup svoj"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Pozvacemo zebanije"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nikako! Ne poslusaj ga, a ucini sedzdu i priblizi se"
        }
    ]
}