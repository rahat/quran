{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obilazice oko njih mladici vjecni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "S peharima i ibricima i casom s izvorista"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Nece dobiti glavobolju od njega, niti se zamoriti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I vocem od kojeg ce birati"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I mesom pticjim od cega zazele"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I hurije lijepih krupnih ociju"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kao primjerci bisera skrivenog"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Placa za ono sta su radili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nece cuti u njemu besmislicu, niti grijesnost"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Izuzev govora: \"Selam! Selam"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U lotosovom drvecu bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I bananama nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I hladu rasprostrtom"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I vodi tekucoj"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I vocu mnogobrojnom"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Nije odsjeceno, niti zabranjeno"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Uistinu! Mi smo ih stvorili stvaranjem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I ucinili ih djevicama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Umilnim vrsnjakinjama"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Drugovima desne strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Mnostvu od prvih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "I mnostvu od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A drugovi lijeve strane - sta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "U przecem vjetru i vrijucoj vodi ce biti"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "I sjenci crnog dima"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ni hladnoj, ni oplemenjujucoj"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Uistinu, oni su prije toga bili raskosnici"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I ustrajali na prekrsaju strasnom"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I govorili: \"Hocemo li kad pomremo i budemo prah i kosti, uistinu mi biti podignuti"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ili ocevi nasi raniji"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: \"Uistinu, raniji i kasniji"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Doista ce biti sakupljeni za odreÄ‘eno vrijeme Dana poznatog"
        }
    ]
}