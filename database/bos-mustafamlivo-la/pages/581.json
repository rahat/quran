{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "Zar vas ne stvaramo od tekucine prezrene"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Pa je stavljamo u boraviste postojano"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Do roka poznatog"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pa uređujemo, ta divni su Uređivaci"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Zar nismo ucinili Zemlju sabiralistem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Zivih i mrtvih"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "I nacinili na njoj gore stabilne, visoke, i napojili vas vodom pitkom"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Idite onom sta ste poricali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Idite prema sjeni sa tri grane"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Bez hlada ce biti, a nece koristiti protiv plamena"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Uistinu! Ona ce izbacivati iskre kao dvorac"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Kao da su one kamile zute"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ovo je Dan kad nece govoriti"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Niti ce im se dopustiti, pa da se opravdavaju"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ovo je Dan odluke! Sabrali smo vas i prve"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pa ako vi imate varku - ta prevarite Me"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Uistinu, bogobojazni ce biti u hladovima i izvorima"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "I vocu od cega zele"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Jedite i pijte prijatno, za ono sto ste radili"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Uistinu, Mi tako nagrađujemo dobrocinitelje"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Jedite i uzivajte malo, uistinu, vi ste prestupnici"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "A kad im se kaze: \"Cinite ruk'u!\" - ne cine ruk'u"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Tesko Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Pa u koji hadis ce poslije njega vjerovati"
        }
    ]
}