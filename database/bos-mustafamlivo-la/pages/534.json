    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "U njima je voce i hurme i nar"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Pa koju od blagodati Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "U njima su dobre, lijepe"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Pa koju od blagodati Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Hurije zadrzane u paviljonima"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Pa koju od blagodati Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Nije ih prije njih razdjevicio covjek, niti dzinn"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Pa koju od blagodati Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Naslonjeni na jastuku zelenom i cilimima prekrasnim"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Pa koju od blagodati Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Blagoslovljeno neka je ime Gospodara tvog, Vlasnika uzvisenosti i plemenitosti"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kad se dogodi događaj"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Nece biti poricanja događanja njegovog"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ponizavajuci, uzdizuci"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se zatrese Zemlja potresom"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "I smrve brda drobljenjem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "I budu prasina rasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "I budete vrste tri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Pa drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I drugovi lijeve strane - sta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "I prednjaci, prednjaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Takvi ce biti priblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "U basce uzivanja"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnostvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "A malo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na sofama (ukrasom) oblozenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Naslonjeni na njima, naspramni"
        }
    ]
}