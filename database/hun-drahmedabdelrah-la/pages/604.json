{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Mondd: O, Allah az Egyeduli, az Egyetlen"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, as-Samadu (az Ur, Akihez minden teremtmeny fordul)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nem nemzett es nem nemzetet"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Es senki es semmi nem hasonlo Ohozza"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Mondd: A hajnal Uranal keresek menedeket"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Annak a rossza elol, amit teremtett"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "A mindent elborito sotetseg rossza elol"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "A csomokba fujo boszorkanyok rossza elol"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Es az irigykedo rossza elol, ha irigykedik"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Mondd: , Az emberek Urahoz menekulok"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Az emberek Kiralyahoz"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Az emberek Istenehez"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "A titokban sugalmazok gonoszsaga elol"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Aki bunos gondolatokat sugall az emberek kebleibe"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Akar dzsinn, akar ember"
        }
    ]
}