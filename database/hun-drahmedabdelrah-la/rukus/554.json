{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Mondd: O, Allah az Egyeduli, az Egyetlen"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, as-Samadu (az Ur, Akihez minden teremtmeny fordul)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nem nemzett es nem nemzetet"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Es senki es semmi nem hasonlo Ohozza"
        }
    ]
}