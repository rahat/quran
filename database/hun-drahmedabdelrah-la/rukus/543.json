{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "AI-Oarra (A csapas, a lesujto Ora, vagyis a Feltamadas Napja)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mi a Csapas"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Honnan tudhatod, mi a Csapas"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Azon a Napon az emberek szetszort lepkekhez lesznek hasonloak"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "A hegyek pedig olyanok lesznek, mint a kartolt, bolyholt gyapju"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Az pedig, akinek merlege sulyos lesz"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Annak oromteli elete lesz"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Az pedig, akinek merlege konnyunek mutatkozik"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Annak az anyja (azaz a lakhelye) hawiya (a Pokol lakoja lesz)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Honnan tudhatod, mi is az"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Langolo Tuz"
        }
    ]
}