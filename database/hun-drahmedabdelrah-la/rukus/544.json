{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Orommel tolt el benneteket a gyarapodas"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Egeszen addig, amig fel nem keresitek a sirotokat (halalotokig)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Denem! Meg fogjatok tudni"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Es meg egyszer: nem! Meg fogjatok majd tudni"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nem! Barcsak biztos tudasotok lenne rola"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Latjatok a Poklot"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Meg egyszer, bizton latjatok azt"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Azon a Napon bizonyosan kerdore Vonattok a foldi gyonyorokrol"
        }
    ]
}