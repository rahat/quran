{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai[393] u vrenjt dhe ktheu kurrizin"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "kur i erdhi prane i verbri"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E ku e di ti, ndoshta ai deshironte te pastrohej (nga gjynahet)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ose te merrte keshille, e keshilla t’i sillte dobi atij"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Sa per ate qe eshte i pasur"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti atij po i kushton vemendje"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "megjithese nuk ke pergjegjesi, nese ai nuk deshiron te pastrohet"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Sa per ate qe te vjen duke nxituar"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "dhe ndien frike (nga Allahu)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti atij nuk po ia ve veshin"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Mos vepro ashtu! Ne te vertete, ky (Kuran) eshte keshille"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pra, kush te doje, le te marre keshille"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ai ndodhet ne Faqe te nderuara"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "te lartesuara, te pastra"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ne duart e shkruesve (engjej)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "te ndershem dhe te drejte"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mallkuar qofte njeriu! Sa mosmirenjohes qe eshte ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Prej cfare gjeje e krijon Allahu ate"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Prej pikes se fares e krijon dhe i jep permasa te rregullta"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Me pas ia lehteson rrugen (e lindjes)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "e pastaj e vdes dhe i cakton nje varr"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "dhe, kur te doje, Ai e ringjall"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "E megjithate, njeriu ende nuk i ka plotesuar ato qe i ka urdheruar Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Le te mendoje njeriu per ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Jemi Ne qe leshojme uje me bollek"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pastaj e cajme token ne thellesi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "dhe nxjerrim prej saj dritherat"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "rrushin e perimet"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "ullirin e palmat"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "kopshtet e dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "pemet dhe gjithe barerat"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "qe t’i gezoni ju dhe bagetite tuaja"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Por, kur te vije Ushtima"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ate Dite njeriu do te ike nga i vellai"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "nga e ema dhe nga i ati"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "nga e shoqja dhe nga femijet e vet"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ate Dite, secili prej tyre do te kete aq shqetesim per veten, sa do t’i mjaftoje"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ate Dite do te kete fytyra rrezellore"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "te qeshura e te gezuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ate Dite do te kete (edhe) fytyra te pluhurosura"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "qe do t’i mbuloje erresira"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Keta jane jobesimtaret, gjynahqaret"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kur Dielli te mblidhet e te humbe shkelqimin"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kur yjet te rrezohen"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "dhe malet te levizin tutje"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "kur devete barrse te braktisen"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "dhe egersirat te tubohen"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kur detet te vlojne"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "dhe shpirtrat te bashkohen (me trupat)"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kur foshnja e varrosur per se gjalli te pyetet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "per cfare faji ishte vrare"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kur fletet te shperndahen"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "dhe qielli te zhvishet"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "kur Xhehenemi te flakeroje"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "dhe Xheneti te afrohet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "atehere cdokush do te marre vesh cfare kishte pergatitur (per kete Dite)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Betohem per ata (planete) qe terhiqen"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "qe lindin e perendojne"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "per naten kur bie terri"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "e per agimin kur nis te zbardhe"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "se ky (Kuran) eshte Fjale e (kumtuar prej) nje te Derguari fisnik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "qe zoteron fuqi dhe pozite te larte te Zoti i Fronit"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "e qe eshte i respektuar atje dhe besnik"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Shoku juaj[397] nuk eshte i marre"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ai e ka pare ate[398] ne horizontin e qarte"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "dhe nuk mbulon asnje te fshehte prej se Padukshmes"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "As ky (Kuran) nuk eshte fjale e djallit te mallkuar"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Prandaj, nga po ia mbani"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ai eshte vecse keshille per botet"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "per secilin nga ju qe deshiron te ndjeke rrugen e drejte"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Por ju nuk mund te deshironi, pervecse me vullnetin e Allahut, Zotit te boteve"
        }
    ]
}