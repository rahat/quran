{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Betohem ne shkelqimin e paradites"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "dhe ne naten kur erret e qetesohet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "se Zoti yt nuk te ka braktisur e as nuk te urren (o Muhamed)"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Natyrisht, jeta e ardhshme per ty eshte me e mire se kjo jete"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "e, sigurisht qe Zoti yt do te te jape ty, sa te jesh i kenaqur"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk ishe jetim e Ai te dha strehe"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "A nuk te gjeti te paditur (per Islamin) e te udhezoi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "A nuk ishe nevojtar e te beri te mos ia kesh nevojen askujt"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Prandaj, mos e shtyp jetimin"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "lypesin mos e shtyj me ashpersi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "dhe shpalli dhuntite e Zotit tend"
        }
    ]
}