{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Per qiellin dhe per vizitorin e nates"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "E kush mund te ta shpjegoje ty se câ€™eshte vizitori i nates"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ai eshte ylli qe shpon terrin"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Per cdo njeri ka mbikeqyres"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Le te meditoje njeriu se prej cfare eshte krijuar"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "eshte krijuar prej nje lengu qe hidhet"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "e qe del ndermjet shtylles kurrizore dhe brinjeve"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Me te vertete, Ai eshte i Fuqishem per ta kthyer (ne jete) ate"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "diten, kur do te zbulohen te fshehtat"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kur njeriu nuk do te kete as fuqi, as ndihmes"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Per qiellin qe kthehet pareshtur"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "dhe per Token qe plasaritet"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "qe kjo eshte nje Fjale ndarese"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "dhe nuk eshte aspak shaka"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Ne te vertete, ata ngrene kurthe"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "por dhe Une ua pres me kurth"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Prandaj, jepu pak afat jobesimtareve, leri te qete edhe per pak"
        }
    ]
}