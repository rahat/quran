{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Mjere per cdo perqeshes - shpifes"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "qe grumbullon pasuri dhe e ruan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "duke menduar se ajo pasuri do ta beje te pavdekshem"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jo, aspak! Ne te vertete, ai do te hidhet ne torturen shkaterruese"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "E kush mund te ta shpjegoje ty se cfare eshte tortura shkaterruese"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ajo eshte zjarri i Allahut i ndezur fort"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "i cili deperton deri ne zemren e njeriut"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ai do tâ€™i rrethoje ata nga cdo ane"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "ne shtylla te gjata flakeruese"
        }
    ]
}