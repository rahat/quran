{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Valle, a nuk ta hapem ty (Muhamed) kraharorin (per te pranuar besimin)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "dhe ta hoqem barren"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "e cila ta rendonte teper kurrizin"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "dhe ta ngritem lart emrin"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Me te vertete, pas veshtiresise - vjen lehtesimi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Me te vertete, pas veshtiresise - vjen lehtesimi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Prandaj, kur te clirohesh (nga punet e ndryshme), perpiqu fort (ne adhurim)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "dhe vetem ndaj Zotit tend perkushtohu"
        }
    ]
}