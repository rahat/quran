{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Per cfare e pyesin ata njeri-tjetrin"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Per Lajmin e madh"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "rreth te cilit ata kane kundershtime"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Ani, shpejt do ta marrin vesh"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Sigurisht qe shpejt do ta marrin vesh"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "A nuk jua kemi bere Token si shtrat"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "e malet si shtylla"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ne ju kemi krijuar ne cifte"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "dhe gjumin jua kemi bere per prehje"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Naten jua kemi bere mbulese"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "ndersa diten e kemi caktuar per te punuar"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Mbi ju kemi ndertuar shtate qiej te forte"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "dhe kemi vene kandil (diell) te shndritshem"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Nga rete Ne ju leshojme ujera te bollshme"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "per rritjen e dritherave, bimeve"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "dhe te kopshteve te dendura"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Sigurisht, Dita e Ndarjes vendimtare eshte caktuar"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Ate Dite do te fryhet ne Sur dhe ju do te vini grupe-grupe"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Dyert e qiellit do te hapen"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "dhe malet do te shtyhen tutje e do te behen si mirazh"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Me te vertete, Xhehenemi do te behet pusi"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "vendbanim i caktuar per keqberesit"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "qe aty te mbeten pergjithmone"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Aty nuk do te shijojne as freski, as pije"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "perpos ujit te valuar dhe qelbesirave"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "si shperblim perkates (te veprave te tyre)"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Ne te vertete, ata as qe e conin neper mend se do te jepnin llogari"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "dhe shpalljet Tona i mohonin prerazi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Por Ne cdo gje e kemi shenuar ne nje liber"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Prandaj do t’u themi: “Shijojeni kete! Ne nuk do t’ju shtojme gje tjeter pervecse denim!”"
        }
    ]
}