{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Betohem per ata (kuaj) qe vrapojne duke dihatur"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "qe nxjerrin xixa nga thundrat"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "dhe trokojne per sulm ne mengjes"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "duke cuar re pluhuri"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ndersa cajne armikun ne dysh"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "se njeriu eshte vertet mosmirenjohes ndaj Zotit te vet"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "dhe ai, pa dyshim, kete e deshmon"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Vertet, ai eshte i babezitur per pasuri"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Valle, a nuk e di ai se kur te nxirren jashte ata qe gjenden ne varre"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "dhe, kur te dale ne shesh ajo qe eshte ne gjokse"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Zoti i tyre, me siguri, ate Dite di cdo gje per ta"
        }
    ]
}