{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tin´e ve Zeytun´a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu guvenli beldeye andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da cevirdik asagıların asagısına attık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip iyi isler yapanlar baska; onlar icin kesintisiz bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde sana dini ne yalanlatır"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi degil mi"
        }
    ]
}