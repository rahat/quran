{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "O harıl harıl (savasa) kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tırnaklarıyla yerden) ates cıkaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabahleyin akın edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana karıstıranlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir toplulugun ortasına dalanlara yemin ederim ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Suphesiz insan, Rabbine karsı cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gercekten o dunya malını cok sevdigi icin katıdır"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirlerin icindekiler fırlatılacak"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve sinelerin icindekiler derlenecek"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gun Rableri onların butun yaptıklarından haberdardır"
        }
    ]
}