{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Burclar sahibi gokyuzune"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Vaad olunan o gune"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Sahitlik edene ve edilene andolsun ki"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kahroldu o hendegin sahipleri"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "O cıralı atesin"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Hani o atesin basına oturmuslar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Muminlere yaptıklarını seyrediyorlardı"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Muminlere kızmalarının sebebi de, onların yalnız cok guclu ve ovguye layık olan Allah´a iman etmeleri idi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "O Allah ki, goklerin ve yerin hukumranlıgı O´nundur ve Allah her seye sahittir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inanan erkek ve kadınlara iskence yapıp sonra da tevbe etmeyenlere cehennem azabı ve yangın azabı vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inanan ve iyi amel yapanlar icin de altından ırmaklar akan cennetler vardır. Iste buyuk kurtulus odur"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Kuskusuz Rabbinin yakalaması serttir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Yoktan o yaratır ve tekrar o diriltir"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Bununla beraber cok bagıslayandır, cok sevendir"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ars´ın sahibidir, yucedir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Diledigini yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "O orduların kıssası sana geldi mi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Yani Firavun ve Semud´un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Fakat o inkarcılar hala bir yalanlama icinde"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Oysa Allah onları arkalarından kusatmıstır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Hayır o serefli bir Kur´an´dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Levh-i Mahfuz´dadır"
        }
    ]
}