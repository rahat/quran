{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sıgınırım ben insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların hukumdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki, insanların goguslerine vesveseler fısıldar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan"
        }
    ]
}