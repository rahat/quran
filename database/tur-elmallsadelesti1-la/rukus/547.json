{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Gormedin mi Rabb´in fil sahiplerine ne yaptı"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Uzerlerine suru suru kuslar gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara camurdan sertlesmis taslar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ve onları, yenilmis ekin yapragı gibi yaptı"
        }
    ]
}