{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(maranayata pat vuvanva) tigæssimata salasvana siddhiyayi"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "(evæni) tigæssimata salasvana siddhiya kumakda"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "(nabiye!) ema siddhiya asaval eka yayi oba dannehida"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "edinadi minisun ævissi visiri tikhena meru men bavata pat vanu æta"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "kandu visiri dæmu pulun men pavi yanu æta"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "kavurunge pin vala bara vædi vuveda"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ohu trptiyata pat jivitayehi (sæpase) jivatva sitinu æta"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "kavurunge (pin vala) bara pahasu vi (papayanhi bara vædi) vuveda"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ohu ræendana sthanaya haviyaya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "(ema) haviya asaval eka yayi (nabiye!) oba dannehida"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(eya) idi tikhena (nira) ginna vanneya"
        }
    ]
}