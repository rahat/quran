{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Osore ononoku hi (Saigonoshinpan)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "osore ononoku hi to wa nani ka"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Osore ononoku hi ga, nanidearu ka o anata ni rikai sa seru mono wa nani ka"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "(Soreha) ningen ga hisan suru ga no yō ni naru hi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Mata yamayama ga, suka reta yōmō no yō ni naru (Ni~Tsudearu)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sore de, kare no hakari ga (zenkō de) omoi mono wa"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "kōfuku de michitarite kurasudearou"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Daga hakari no karui mono wa"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "naraku ga, kare no satodearou"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Sore ga nandearu ka o, anata ni rikai sa seru mono wa nani ka"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Soreha) shōnetsu (jigoku) no hi"
        }
    ]
}