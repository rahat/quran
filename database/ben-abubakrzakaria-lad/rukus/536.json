{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Āmarā ki āpanāra bakṣa āpanāra kalyāṇē praśasta karē dē'ini"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Āra āmarā apasāraṇa karēchi āpanāra bhāra"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yā āpanāra piṭha bhēṅgē dicchila"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Āra āmarā āpanāra (maryādā br̥d'dhira) jan'ya āpanāra smaraṇakē samunnata karēchi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Sutarāṁ kaṣṭēra sāthē'i tō sbasti āchē"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niścaẏa kaṣṭēra sāthē'i sbasti āchē"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ata'ēba āpani yakhana'i abasara pāna takhana'i kaṭhōra ibādātē rata hōna"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "āra āpanāra rabēra prati gabhīra manōyōgī hōna"
        }
    ]
}