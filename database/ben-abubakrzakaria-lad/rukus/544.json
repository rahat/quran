{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Tōmādērakē mōhācchanna rākhē [1] prācuryēra pratiyōgitā"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "yatakṣaṇa nā tōmarā kabarē upanīta ha'ō"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kakhanō naẏa, tōmarā śīghra'i jānatē pārabē"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "tārapara, kakhanō naẏa, tōmarā śīghra'i jānatē pārabē"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "kakhanō naẏa! Yadi tōmarā niścita jñānē jñānī hatē"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "abaśya'i tōmarā jāhānnāma dēkhabē"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "tārapara abaśya'i tōmarā tā dēkhabē cākṣuṣa pratyaẏē"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "tārapara abaśya'i sēdina tōmādērakē nēẏāmata samparkē praśna karā habē"
        }
    ]
}