{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kurā'iśē[1]ra āsaktira kāraṇē"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "tādēra āsakti āchē śīta ō grīṣmē sapharēra"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ata'ēba, tārā ‘ibādāta karuka ē gharēra rabēra"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yini tādērakē kṣudhāẏa khādya diẏēchēna [1] ēbaṁ bhīti thēkē tādērakē nirāpada karēchēna"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Āpani ki dēkhēchēna [1] tākē, yē dīnakē [2] asbīkāra karē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "sē tō sē-i, yē iẏātīmakē rūṛhabhābē tāṛiẏē dēẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "āra sē ud'dhud'dha karē nā [1] misakīnadēra khādya dānē"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Kājē'i durbhōga sē sālāta ādāẏakārīdēra"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yārā tādēra sālāta sambandhē udāsīna"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yārā lōka dēkhānōra jan'ya tā karē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ēbaṁ mā‘una [1] pradāna karatē birata thākē"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niścaẏa āmarā āpanākē kā'uchāra [1] dāna karēchi"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Kājē'i āpani āpanāra rabēra uddēśyē sālāta ādāẏa karuna ēbaṁ kurabānī karuna"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Niścaẏa āpanāra prati bidbēṣa pōṣaṇakārī'i tō nirbanśa"
        }
    ]
}