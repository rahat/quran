{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Niścaẏa muttākīdēra jan'ya āchē sāphalya"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "udyānasamūha, āṅgurasamūha"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "āra samabaẏaskā [1] udbhinna yaubanā taruṇī"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "ēbaṁ paripūrṇa pānipātra"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Sēkhānē tārā śunabē nā kōnō asāra ō mithyā bākya"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "āpanāra rabēra pakṣa thēkē puraskāra, yathōcita dānasbarūpa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yini āsamānasamūha, yamīna ō ē du’ẏēra madhyabartī samasta kichura raba, daẏāmaẏa; tām̐ra kāchē ābēdana-nibēdanēra śakti tādēra thākabē nā"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Sēdina rūh ō phērēśatāgaṇa sāribad'dhabhābē dām̐rābē [1]; sēdina kē'u kathā balabē nā, tabē ‘rahamāna’ yākē anumati dēbēna sē chāṛā ēbaṁ sē saṭhika kathā balabē"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Ē dinaṭi satya; ata'ēba, yāra icchē sē tāra rabēra nikaṭa āśraẏa grahaṇa karuka"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niścaẏa āmarā tōmādērakē āsanna śāsti samparkē satarka karalāma; yēdina mānuṣa tāra kr̥takarma dēkhatē pābē ēbaṁ kāphira balabē, ‘hāẏa! Āmi yadi māṭi hatāma [1]!’"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Śapatha [1] nirmamabhābē uṯpāṭanakārīdēra"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "āra mr̥dubhābē bandhanamuktakārīdēra"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "āra tībra gatitē santaraṇakārīdēra"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "āra drutabēgē agrasaramānadēra"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "ataḥpara saba kāja nirbāhakārīdēra"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Sēdina prakampitakārī prakampita karabē"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tākē anusaraṇa karabē parabartī kampanakārī"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "anēka hr̥daẏa sēdina santrasta habē"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "tādēra dr̥ṣṭisamūha bhīti-bihbalatāẏa nata habē"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Tārā balē, ‘āmarā ki āgēra abasthāẏa phirē yāba'i"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "cūrṇabicūrṇa asthitē pariṇata ha'ōẏāra para'ō?’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Tārā balē, ‘tā'i yadi haẏa tabē tō ēṭā ēka sarbanāśā pratyābartana.’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Ē tō śudhu ēka bikaṭa ā'ōẏāja"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "takhana'i maẏadānē [1] tādēra ābirbhāba habē"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Āpanāra kāchē mūsāra br̥ttānta paum̐chēchē ki"
        }
    ]
}