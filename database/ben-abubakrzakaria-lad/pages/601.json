{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Samaẏēra śapatha"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "niścaẏa mānuṣa [1] kṣatira mājhē nipatita"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Kintu tārā naẏa [1], yārā īmāna ēnēchē [2] ēbaṁ saṯkāja karēchē [3] āra parasparakē upadēśa diẏēchē hakēra [4] ēbaṁ upadēśa diẏēchē dhairyēra"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Durbhōga pratyēkēra, yē pichanē ō sāmanē lōkēra nindā karē"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "yē sampada jamāẏa ō tā bāra bāra gaṇanā karē"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "sē dhāraṇā karē yē, tāra artha tākē amara karē rākhabē"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "kakhana'ō nā, sē abaśya'i nikṣipta habē [1] hutāmāẏa"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "āra āpanākē kisē jānābē hutāmā kī"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ēṭā āllāhara prajbalita āguna"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yā hr̥daẏakē graāsa karabē"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "niścaẏa ēṭā tādērakē paribēṣṭana karē rākhabē"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dīrghāẏita stambhasamūhē"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Āpani ki dēkhēna ni [1] āpanāra raba hātira adhipatidēra prati kī karēchilēna"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tini ki tādēra kauśala byarthatāẏa paryabasita karē dēnani"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "āra [1] tādēra birud'dhē tini jhām̐kē jhām̐kē pākhi pāṭhāna"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "yārā tādēra upara śakta pōṛāmāṭira kaṅkara nikṣēpa karē"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ataḥpara tini tādērakē bhakṣita tr̥ṇa-sadr̥śa karēna"
        }
    ]
