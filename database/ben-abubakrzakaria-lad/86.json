{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Śapatha āsamānēra ēbaṁ rātē yā ābirbhūta haẏa tāra"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "āra kisē āpanākē jānābē ‘rātē yā ābirbhūta haẏa’ tā kī"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ujjbala nakṣatra"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyēka jībēra upara'i tattbābadhāẏaka raẏēchē"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ata'ēba, mānuṣa yēna cintā karē dēkhē tākē kī thēkē sr̥ṣṭi karā haẏēchē"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "tākē sr̥ṣṭi karā haẏēchē sabēgē sthalita pāni hatē"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ēṭā nirgata haẏa mērudaṇḍa ō pañjarāsthira madhya thēkē"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niścaẏa tini tākē phiriẏē ānatē sakṣama"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yēdina gōpana biṣaẏa parīkṣita habē"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "sēdina tāra kōnō sāmarthya thākabē nā ēbaṁ sāhāyyakārī'ō naẏa"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Śapatha āsamānēra, yā dhāraṇa karē br̥ṣṭi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ēbaṁ śapatha yamīnēra, yā bidīrṇa haẏa"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niścaẏa āla-kura'āna mīmānsākārī bāṇī"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ēbaṁ ēṭā nirarthaka naẏa"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tārā bhīṣaṇa ṣaṛayantra karē"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ēbaṁ āmi'ō bhīṣaṇa kauśala kari"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ata'ēba, kāphiradērakē abakāśa dina; tādērakē abakāśa dina kichu kālēra jan'ya"
        }
    ]
}