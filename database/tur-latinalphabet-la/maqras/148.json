{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "Ve evhayna ila musa en esri bi ıbadı innekum muttebeun"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fe ersele fir´avnu fil medaini hasirın"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inne haulai le sirzimetun kalılun"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve innehum lena le gaizun"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Ve inna le cemıun hazirun"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fe ahracnahum min cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Ve kunuziv ve mekamin kerım"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kezalik ve evrasnaha benı israıl"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fe etbeuhum musrikıyn"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Felemma terael cem´ani kale ashabu musa inna le mudrakun"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Kale kella inne meıye rabbı seyehdın"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fe evhayna ila masa enıdrib bi asakel bahr fenfeleka fe kane kullu firkın ket tavdil azıym"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Ve ezlefna semmel aharın"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Ve enceyna musa ve mem meahu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Summe agraknel aharın"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vetlu aleyhim nebee ibrahım"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Iz kale li ebıhi ve kavmihı ma ta´budun"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Kalu na´budu asnamen fe nezallu leha akifın"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Kale hel yesmeunekum iz ted´un"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Ev yenfeunekum ev yedurrun"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Kalu bel vecedna abaena kezalike yef´alun"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Kale e feraeytum ma kuntum ta´budun"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Entum ve abaukumul akdemun"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fe innehum aduvvul lı illa rabbel alemın"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Ellezı halekanı fe huve yehdın"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Vellezı huve yut´ımunı ve yeskıyn"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Ve iza merıdtu fe huve yesfın"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Vellezı yumıtunı summe yuhyın"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Vellezı at´meu ey yagfira lı hatıy´etı yevmeddın"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi heb lı hukmev ve elhıknı bis salihıyn"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Vec´al lı lisane sıdkın fil ahırın"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Vec´alnı miv veraseti cennetin neıym"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Vagfir li ebı innehu kane mined dallın"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Ve la tuhzinı yevme yub´asun"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yevme la yenfeu maluv ve la benun"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa men etellahe bi kalbin selim"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Ve uzlifetil cennetu lil muttekıyn"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Ve burrizetil cehıymu li gavın"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Ve kıyle lehum eyne ma kuntum ta´budun"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min dunillah hel yensurunekum ev yentesırun"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fe kubkibu fıhahum vel gavun"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Ve cunudu iblıse ecmeun"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Kalu ve hum fıha yahtesımun"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Tellahi in kunna le fı dalalim mubın"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Iz nusevvıkum bi rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Ve ma edalleha illel mucrimun"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fe ma lena min safiıyn"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Ve la sadıkın hamım"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Fe lev enne lena kerraten fe nekune minel mu´minın"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kezzebet kavmu nuhınil murselın"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Iz kale lehum ehuhum nuhun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Inni lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Ve ma es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fettekullahe ve etıy´un"
        }
    ]
