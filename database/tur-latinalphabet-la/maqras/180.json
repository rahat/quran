{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min sıatihı le ibrahım"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selım"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebıhi ve kavmihı maza ta´budun"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunellahi turıdun"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemın"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nezara nazraten fin nucum"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale innı sekıym"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirın"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e ela te´kulun"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darbem bil yemın"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta´budune ma tenhıtun"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halekkkakum ve ma ta´melun"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cehıym"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihı keyden fe cealnahumul esfelın"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale innı zahibun ila rabbı seyehdın"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lı mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halım"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Felemma belega meahus sa´ye kale ya buneyye innı era fil menami ennı ezbehuke fenzur maza tera kale ya ebetif´al ma tu´meru setecidunı in saellahu mines sabirın"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Felemma eslema ve tellehu lil cebın"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu ey ya ibrahım"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru´ya inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubın"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azıym"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahım"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyem mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barakna aleyhi ve ala ishak ve min zurriyyetihima muhsinuv ve zalimul li nefsihı mubın"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve le kad menenna ala musa ve haun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynahuma va kavmehuma minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibın"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateynahumel kitabel mustebın"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynahumes sıratal mustekıym"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terakna aleyhima fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalik enczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innehuma min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihı ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "E ted´une ba´lev ve tezerune ahsenel halikıyn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelın"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasın"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutal le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihıyn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl e fe la ta´kılun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yub´asun"
        }
    ]
}