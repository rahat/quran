{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "Kalu enu´minu leke vettebeakel erzelun"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Kale vema ılmı bima kanu ya´melun"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hısabuhum illa ala rabbı lev tes´urun"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ve ma ene bi taridil mu´minın"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In ene illa nezırum mubın"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Kalu le il lem tentehi ya nuhu le tekunenne minel mercumın"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Kale rabbi inne kavmı kezzebun"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Fettah beynı ve beynehum fethav ve neccinı ve mem meıye minel mu´minın"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fe enceynahu ve mem meahu fil fulkil meshun"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Summe agrakna ba´dul bakıyn"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inne fı zalik le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kezzebet adunil murselın"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Iz kale lehum ehuhum hudun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Ve ma es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "E tebnune bi kulli riy´ın ayeten ta´besun"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Ve tettehızune mesanıa leallekum tahludun"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Ve iza betastum betastum cebbarın"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Vettekullezı emeddekum bima ta´lemun"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Emeddekum bi en´amiv ve benın"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Ve cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Innı ehafu aleykum azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Kalu sevaun aleyna e veazte em lem tekum minel vaızıyn"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In haza illa hulukul evvelın"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Ve ma nahnu bi muazzebın"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fe kezzebuhu fe ehleknahum inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kezzebet semudul murselın"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Iz kale lehum ehuhum salihun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Ve ma es´elukum aleyhi men ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "E tutrakune fı ma hahuna aminın"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fı cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Ve zuruıv ve nahlin tal´uha hedıym"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Ve tenhıtune minel cibali buyuten farihın"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Ve la tutıy´u emral musrifın"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Ellezıne yufsidune fil erdı ve la yuslihun"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Kalu innema ente minel musahharın"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ma ente illa beserum misluna fe´ti bi ayetin in kunte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Kale hazihı nakatul leha sirbuv ve lekum sirbu yevmim ma´lum"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Ve la temessuha bi suin fe ye´huzekum azabu yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fe akaruha fe asbehu nadimın"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fe ehazehumul azab inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmu lutınil murselun"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Iz kale lehum ehuhum lutun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te´tunez zukrane minel alemın"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerune ma haleka lekum rabbukum min ezvacikum bel entum kavmun adun"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kalu leil lem tentehi ya lutu le tekunenne minel muhracın"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kale innı li amelikum minel kalın"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccinı ve ehlı mimma ya´melun"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarna aleyhim metara fe sae metarul munzerın"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabul eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Iz kale lehum suaybun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        }
    ]
}