{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve le kad menenna ala musa ve haun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynahuma va kavmehuma minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibın"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateynahumel kitabel mustebın"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynahumes sıratal mustekıym"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terakna aleyhima fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalik enczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innehuma min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihı ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "E ted´une ba´lev ve tezerune ahsenel halikıyn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelın"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasın"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutal le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihıyn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl e fe la ta´kılun"
        }
    ]
}