{
    "rukus": [
        {
            "chapter": 37,
            "verse": 22,
            "text": "Uhsurullezıne zalemu ve ezvacehum ve ma kanu ya´budun"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dunillahi fehduhum ila sıratıl cehıym"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Ve kıfuhum innehum mes´ulun"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Me lekum la tenasarun"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimun"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba´duhum ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kalu innekum kuntum te´tunena anil yemın"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kalu bel lem tekunu mu´minın"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve ma kane lena aleykum min sultan bel kuntum kavmen tagıyn"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleyna kavlu rabbina inna le zaikun"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynakum inna kunna gavın"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevmeizin fil azabi musterikun"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kezalike nef´alu bil mucrimın"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innehum kanu iza kıyle lehum la ilahe illellahu yestekbirun"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekulune e inna letariku alihetina li saırim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel cae bil hakkı ve saddekal murselın"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innekum lezaikul azabil elım"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve ma tuczevne illa ma kuntum ta´melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulaike lehum rizkum ma´lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevakih ve hum mukramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fı cennatin neıym"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala sururim mutekabilın"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu alyhim bi ke´sim mim meıyn"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydae lezzetil lissaribın"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La fıha gavluv ve la hum anha yunzefun"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve ındehum kasıratut tarfi ıyn"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke ennehunne beydum meknun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba´duhum ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kle kailum minhum innı kane lı karın"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekulu e inneke le minel musaddikıyn"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E iza mitna ve kunna turabev ve ızamen e inna le medınun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kale hel entum muttaliun"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattalea fe raahu fı sevail cehıym"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kale tellahi in kidte le turdın"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev la nı´metu rabbı lekuntu minel muhdarın"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe ma nahnu bi meyyitın"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mevtetenel ula ve ma nahnu bi muazzebın"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inne haza le huvel fevzul azıym"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli haza felya´melil amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zalike hayrun nuzulen em seceratuzc zekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna cealnaha fitnetel liz zalimın"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Inneha seceratun tahrucu fı aslil cehıym"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal´uha ke ennehu ruusus seyatıyn"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehum le akilune minha fe maliune minhel butun"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Summe inne lehum aleyha le sevbem min hamum"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Summe inne merciahum le ilel cehıym"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innehum elfev abaehum dallın"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hum ala asarihim yuhraun"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve le kad dalhle kablehum ekserul evvelın"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve le kad erselna fıhim munzirın"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fenzur keyfe kane akıbetul munzerın"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa ıbadellahil muhlesıyn"
        }
    ]
}