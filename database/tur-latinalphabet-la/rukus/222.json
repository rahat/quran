{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "Felemma cae ale lutnil murselun"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Kale innekum kavmumu munkerun"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Kalu bel ci´nake bima kanu fıhi yemterun"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Ve eteynake bil hakkı ve inna le sadikun"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Fe esri bi ehlike bi kıt´ım minel leyli vettebı´ edbarahum ve la yeltefit minkum ehaduv vemdu haysu tu´merun"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Ve kadayna ileyhi zalikel emra enne dabira haulai maktuum musbihıyn"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Ve cae ehlul medıneti yestebsirun"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Kale inne haulai dayfı fe la tefdahun"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Vettekullahe ve la tuhzun"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Kalu e ve lem nenheke anil alemın"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Kale haulai benatı in kuntum faılın"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "Le amruke innehum le fı sekratihim ya´mehun"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fe ehazethumus sayhatu musrikıyn"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Fe cealna aliyeha safileha ve emtarna aleyhim hıcaratem min siccıl"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inne fı zalike le ayatil lil mutevessimın"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Ve inneha le bisebılim mukıyn"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inne fı zalike le ayatel lil mu´minın"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Ve in kane ashabul eyketi le zalimın"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fentekamna minhum ve innehuma le bi imamim mubın"
        }
    ]
