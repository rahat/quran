{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Ve le kad halaknel insane min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Vel canne halaknahu min kablu min naris semum"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Ve iz kale rabbuke lil melaiketi innı halikum beseram min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fe iza sevveytuhu ve nefahtu fıhi mir ruhıy fekau lehu sacidın"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fe secedel melaiketu kulluhum ecmeun"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illa iblıs eba ey yekune meas sacidın"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Kale ya iblısu ma leke ella tekune meas sacidın"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Kale lem ekul li escude li beserin halaktehu min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Kale fahruc minha fe inneke racım"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Ve inne aleykel la´nete ila yevmid dın"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Kale rabbi fe enzırnı ila yevmi yub´asun"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Kale fe inneke minel munzarın"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila yevmil vaktil ma´lum"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Kale rabbi bima agveytenı le uzeyyinenne lehum fil erdı ve le ugviyennehum ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa ıbadeke minhumul muhlesıyn"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Kale haza sıratun aleyye mustekıym"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inne ıbadı leyse leke aleyhim sultanun illa menittebeake minel gavın"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Ve inne cehenneme le mev´ıduhum ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Leha seb´atu ebvab likulli babim minhum cuz´um maksum"
        }
    ]
}