        {
            "chapter": 99,
            "verse": 1,
            "text": "Iza zulziletil erdu zilzaleha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ve ahracetilerdu eskaleha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ve kalel insanu ma leha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yevmeizin tuhaddisu ahbaraha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bienne rabbeke evha leha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yevmeiziy yasdurun nasu estatel li yurav a´malehum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Fe mey ya´mel miskale zerratin hayray yerah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ve mey ya´mel miskale zerratin serray yerah"
        }
    ]
