{
    "rukus": [
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve le kad nadana nuhun fe le nı´mel muccıbun"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynahu ve ehlehu minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealna zurriyyetehu humul bakıyn"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selamun ala nuhın fil alemın"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Summe agraknel aharın"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min sıatihı le ibrahım"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selım"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebıhi ve kavmihı maza ta´budun"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunellahi turıdun"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemın"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nezara nazraten fin nucum"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale innı sekıym"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirın"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e ela te´kulun"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darbem bil yemın"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta´budune ma tenhıtun"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halekkkakum ve ma ta´melun"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cehıym"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihı keyden fe cealnahumul esfelın"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale innı zahibun ila rabbı seyehdın"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lı mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halım"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Felemma belega meahus sa´ye kale ya buneyye innı era fil menami ennı ezbehuke fenzur maza tera kale ya ebetif´al ma tu´meru setecidunı in saellahu mines sabirın"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Felemma eslema ve tellehu lil cebın"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu ey ya ibrahım"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru´ya inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubın"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azıym"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahım"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyem mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barakna aleyhi ve ala ishak ve min zurriyyetihima muhsinuv ve zalimul li nefsihı mubın"
        }
    ]
}