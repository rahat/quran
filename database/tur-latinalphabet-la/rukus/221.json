{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "Innel muttekıyne fı cennativ ve uyun"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Udhuluha bi selamin aminın"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "Ve neza´na ma fı sudurihim min gıllin ıhvanen ala sururim mutekabilın"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La yemessuhum fıha nesabuv ve ma hum minha bi muhracın"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nebbi´ ıbadı ennı enel gafurur rahıym"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Ve enne azabı huvel azabul elım"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Ve nebbi´hum an dayfi ibrahım"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Iz dehalu aleyhi fe kalu selama kale inna minkum vecilun"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Kalu la tevcel inna nubessiruke bi gulamin alım"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Kale e bessertumunı ala em messeniyel kiberu fe bime tubessirun"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Kalu bessernake bil hakkı fe la tekum minel kanitıyn"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Kale ve mey yaknetu mir rahmeti rabbihı illed dallun"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Kale fe ma hatbukum eyyuhel murselun"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Kalu inna ursilna ila kavmim mucrimın"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ale lut inna le muneccuhum ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illemraetehu kadderna inneha le minel gabirın"
        }
    ]
}