    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Vel adiyati dabha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fel muriyati kadha"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fel mugırati subha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fe eserne bihı nak´a"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fe vesatne bihı cem´a"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Innelinsane li rabbihı le kenud"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve innehu ala zalike le sehıd"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve innehu li hubbil hayri le sedıd"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "E fe la ya´lemu iza bu´sira ma fil kubur"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve hussıle ma fis sudur"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inne rabbehum bihim yevmeizin le habir"
        }
    ]
