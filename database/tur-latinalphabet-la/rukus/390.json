{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yub´asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahu bil arai ve huve sekıym"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve embenta aleyhi seceratem miy yaktıyn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahu ila mieti elfin ev yezıdun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta´nahum ila hıyn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatu ve lehumul benun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaiket inasev ve hum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehum min ifkihim le yekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledellahu ve innehum le kazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benın"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lekum keyfe tahkumun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultanum mubın"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe´tu bi kitabikum in kuntum sadikıyn"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba ve le kad alimetil cinnetu innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhanellahi amma yesıun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve ma ta´budun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entum aleyhi bi fatinın"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa men huve salil cehıym"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehu mekamum ma´lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnus saffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnul musebbihun"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne ındena zikram minel evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lekunna ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferu bih fe sevfe ya´lemun"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve le kad sebekat kelimetuna li ıbadinel murselın"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innehum le humul mensurun"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundena lehumul galibun"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hatta hıyn"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsırhum fe sevfe yubsırun"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe biazabina yesta´cilun"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe iza nezele bi sahatihim fe sae sabahul munzerın"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhum hatta hıyn"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsır fe sevfe yubsırun"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhane rabbike rabbil ızzeti amma yesfun"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selamun alel murselın"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdu lillahi rabbil alemın"
        }
    ]
}