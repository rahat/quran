{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabul eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Iz kale lehum suaybun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Evful keyle ve la tekunu minel muhsirın"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Vezinu bil kıstasil mustekıym"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Ve la tebhasun nase esyaehum ve la ta´sev fil erdı musidın"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekullezı halekakum vel cibilletel evvelın"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kalu innema ente minel musahharın"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve ma ente illa beserum misluna ve in nezunnuke le minel kazibın"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleyna kisefem mines semai in kunte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kale rabbı a´lemu bi ma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebuhu fe ehazehum azabu yevmiz zulleh innehu kane azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        }
    ]
}