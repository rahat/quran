{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Vessaffati saffa"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Fezzacirati zecra"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Fettaliyati zikra"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "Inne ilahekum le vahıd"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbus semavati vel erdı ve ma beynehuma ve rabbul mesarık"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "Inna zeyyennes semaed dunya bi zınetinil kevakib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Ve hıfzam min kulli seytanim marid"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "La yessemmeune ilel meleil a´la ve yukzefune min kulli canib"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duhurav ve lehum azabuv vasıb"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "Illa men hatfel hatfete fe etbeahu sihabun sakıb"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Festeftihim ehum esddu halkan em men halakna inna halaknahum min tıynil lazib"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bel acibte ve yesharun"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Ve iza zukkiru la yezkurun"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Ve iza raev ayetey yesteshırun"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Ve kalu in haza illa sıhrum mubın"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "E iza mitna ve kunna turabev ve ızamen e inna le meb´usun"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "E ve abaunel evvelun"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Kul neam ve entum dahırun"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fe innema hiye zecratuv vahıdetun fe izahum yenzurun"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Ve kalu ya veylena haza yevmud dın"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Haza yevmul faslillezı kuntum bihı tukezzibun"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Uhsurullezıne zalemu ve ezvacehum ve ma kanu ya´budun"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dunillahi fehduhum ila sıratıl cehıym"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Ve kıfuhum innehum mes´ulun"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Me lekum la tenasarun"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimun"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba´duhum ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kalu innekum kuntum te´tunena anil yemın"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kalu bel lem tekunu mu´minın"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve ma kane lena aleykum min sultan bel kuntum kavmen tagıyn"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleyna kavlu rabbina inna le zaikun"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynakum inna kunna gavın"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevmeizin fil azabi musterikun"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kezalike nef´alu bil mucrimın"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innehum kanu iza kıyle lehum la ilahe illellahu yestekbirun"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekulune e inna letariku alihetina li saırim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel cae bil hakkı ve saddekal murselın"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innekum lezaikul azabil elım"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve ma tuczevne illa ma kuntum ta´melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulaike lehum rizkum ma´lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevakih ve hum mukramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fı cennatin neıym"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala sururim mutekabilın"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu alyhim bi ke´sim mim meıyn"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydae lezzetil lissaribın"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La fıha gavluv ve la hum anha yunzefun"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve ındehum kasıratut tarfi ıyn"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke ennehunne beydum meknun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba´duhum ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kle kailum minhum innı kane lı karın"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekulu e inneke le minel musaddikıyn"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E iza mitna ve kunna turabev ve ızamen e inna le medınun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kale hel entum muttaliun"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattalea fe raahu fı sevail cehıym"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kale tellahi in kidte le turdın"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev la nı´metu rabbı lekuntu minel muhdarın"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe ma nahnu bi meyyitın"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mevtetenel ula ve ma nahnu bi muazzebın"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inne haza le huvel fevzul azıym"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli haza felya´melil amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zalike hayrun nuzulen em seceratuzc zekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna cealnaha fitnetel liz zalimın"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Inneha seceratun tahrucu fı aslil cehıym"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal´uha ke ennehu ruusus seyatıyn"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehum le akilune minha fe maliune minhel butun"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Summe inne lehum aleyha le sevbem min hamum"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Summe inne merciahum le ilel cehıym"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innehum elfev abaehum dallın"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hum ala asarihim yuhraun"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve le kad dalhle kablehum ekserul evvelın"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve le kad erselna fıhim munzirın"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fenzur keyfe kane akıbetul munzerın"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve le kad nadana nuhun fe le nı´mel muccıbun"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynahu ve ehlehu minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealna zurriyyetehu humul bakıyn"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selamun ala nuhın fil alemın"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Summe agraknel aharın"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min sıatihı le ibrahım"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selım"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebıhi ve kavmihı maza ta´budun"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunellahi turıdun"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemın"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nezara nazraten fin nucum"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale innı sekıym"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirın"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e ela te´kulun"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darbem bil yemın"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta´budune ma tenhıtun"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halekkkakum ve ma ta´melun"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cehıym"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihı keyden fe cealnahumul esfelın"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale innı zahibun ila rabbı seyehdın"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lı mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halım"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Felemma belega meahus sa´ye kale ya buneyye innı era fil menami ennı ezbehuke fenzur maza tera kale ya ebetif´al ma tu´meru setecidunı in saellahu mines sabirın"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Felemma eslema ve tellehu lil cebın"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu ey ya ibrahım"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru´ya inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubın"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azıym"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahım"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyem mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barakna aleyhi ve ala ishak ve min zurriyyetihima muhsinuv ve zalimul li nefsihı mubın"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve le kad menenna ala musa ve haun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynahuma va kavmehuma minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibın"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateynahumel kitabel mustebın"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynahumes sıratal mustekıym"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terakna aleyhima fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalik enczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innehuma min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihı ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "E ted´une ba´lev ve tezerune ahsenel halikıyn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelın"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasın"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutal le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihıyn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl e fe la ta´kılun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yub´asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahu bil arai ve huve sekıym"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve embenta aleyhi seceratem miy yaktıyn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahu ila mieti elfin ev yezıdun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta´nahum ila hıyn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatu ve lehumul benun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaiket inasev ve hum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehum min ifkihim le yekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledellahu ve innehum le kazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benın"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lekum keyfe tahkumun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultanum mubın"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe´tu bi kitabikum in kuntum sadikıyn"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba ve le kad alimetil cinnetu innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhanellahi amma yesıun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve ma ta´budun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entum aleyhi bi fatinın"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa men huve salil cehıym"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehu mekamum ma´lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnus saffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnul musebbihun"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne ındena zikram minel evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lekunna ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferu bih fe sevfe ya´lemun"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve le kad sebekat kelimetuna li ıbadinel murselın"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innehum le humul mensurun"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundena lehumul galibun"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hatta hıyn"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsırhum fe sevfe yubsırun"
        },
        {
            "chapter": 37,
            "verse": 176,
