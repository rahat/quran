{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "El kariah"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mel kariah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ve ma edrake mel kariah"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Yevme yekunun nasu kelferasil mebsus"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ve tekunul cibalu kelıhnil menfus"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Fe emma men sekulet mevazınuh"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Fe huve fi ıysetir radıyeh"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ve emma men haffet mevazınuh"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Fe ummuhu havıyeh"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ve ma edrake mahiyeh"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Narun hamiyeh"
        }
    ]
}