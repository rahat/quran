        {
            "chapter": 37,
            "verse": 103,
            "text": "Felemma eslema ve tellehu lil cebın"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu ey ya ibrahım"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru´ya inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubın"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azıym"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahım"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyem mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barakna aleyhi ve ala ishak ve min zurriyyetihima muhsinuv ve zalimul li nefsihı mubın"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve le kad menenna ala musa ve haun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynahuma va kavmehuma minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibın"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateynahumel kitabel mustebın"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynahumes sıratal mustekıym"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terakna aleyhima fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalik enczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innehuma min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihı ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "E ted´une ba´lev ve tezerune ahsenel halikıyn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelın"
        }
