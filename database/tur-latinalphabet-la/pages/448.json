{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekulu e inneke le minel musaddikıyn"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E iza mitna ve kunna turabev ve ızamen e inna le medınun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kale hel entum muttaliun"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattalea fe raahu fı sevail cehıym"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kale tellahi in kidte le turdın"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev la nı´metu rabbı lekuntu minel muhdarın"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe ma nahnu bi meyyitın"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mevtetenel ula ve ma nahnu bi muazzebın"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inne haza le huvel fevzul azıym"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli haza felya´melil amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zalike hayrun nuzulen em seceratuzc zekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna cealnaha fitnetel liz zalimın"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Inneha seceratun tahrucu fı aslil cehıym"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal´uha ke ennehu ruusus seyatıyn"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehum le akilune minha fe maliune minhel butun"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Summe inne lehum aleyha le sevbem min hamum"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Summe inne merciahum le ilel cehıym"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innehum elfev abaehum dallın"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hum ala asarihim yuhraun"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve le kad dalhle kablehum ekserul evvelın"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve le kad erselna fıhim munzirın"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fenzur keyfe kane akıbetul munzerın"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve le kad nadana nuhun fe le nı´mel muccıbun"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynahu ve ehlehu minel kerbil azıym"
        }
    ]
}