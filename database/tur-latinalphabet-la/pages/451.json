{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe innehum le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasın"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ıbadinel mu´minın"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutal le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihıyn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl e fe la ta´kılun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yub´asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahu bil arai ve huve sekıym"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve embenta aleyhi seceratem miy yaktıyn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahu ila mieti elfin ev yezıdun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta´nahum ila hıyn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatu ve lehumul benun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaiket inasev ve hum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehum min ifkihim le yekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledellahu ve innehum le kazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benın"
        }
    ]
}