{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekullezı halekakum vel cibilletel evvelın"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kalu innema ente minel musahharın"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve ma ente illa beserum misluna ve in nezunnuke le minel kazibın"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleyna kisefem mines semai in kunte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kale rabbı a´lemu bi ma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebuhu fe ehazehum azabu yevmiz zulleh innehu kane azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Ve innehu le tenzılu rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nezele bihir ruhul emın"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala kalbike li tekune minel munzirın"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bi lisanin arabiyyim mubın"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Ve innehu lefı zuburil evvelın"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "E ve lem yekul lehum ayeten ey ya´lemehu ulemau benı israıl"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Ve lev nezzelnahu ala ba´dıl a´cemın"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Fe karaehu aleyhim ma kanu bihı mu´minın"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kezalike seleknahu fı kulubil mucrimın"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yu´minune bihı hatta yeravul azabel elım"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Fe ye´tiyehum bagtetev ve hum la yes´urun"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fe yekul hel nahnu munzarun"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "E fe bi azabina yesta´cilun"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "E feraeyte im metta´nahum sinın"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Summe caehum ma kun yuadun"
        }
    ]
}