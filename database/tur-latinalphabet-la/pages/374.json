    "pages": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmu lutınil murselun"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Iz kale lehum ehuhum lutun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te´tunez zukrane minel alemın"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerune ma haleka lekum rabbukum min ezvacikum bel entum kavmun adun"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kalu leil lem tentehi ya lutu le tekunenne minel muhracın"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kale innı li amelikum minel kalın"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccinı ve ehlı mimma ya´melun"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynahu ve ehlehu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa acuzen fil gabirın"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Summe demmernel aharın"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarna aleyhim metara fe sae metarul munzerın"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inne fı zalike le ayeh ve ma kane ekseruhum mu´minın"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le huvel azızur rahıym"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabul eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Iz kale lehum suaybun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Innı lekum rasulun emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elukum aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Evful keyle ve la tekunu minel muhsirın"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Vezinu bil kıstasil mustekıym"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Ve la tebhasun nase esyaehum ve la ta´sev fil erdı musidın"
        }
    ]
}