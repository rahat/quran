    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Ve enahtertuke festemı´ lima yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innenı enallahu la ilahe illa ene fa´budnı ve ekımıs salate li zikrı"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Innes saate atiyetun ekadu uhfıha li tucza kullu nefsim bi ma tes´a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fe la yesuddenneke anha mel la yu´minu biha vettebea hevahu fe terda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Ve ma tilke bi yemınike ya musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Kale hiye asay etevekkeu aleyha ve ehussu biha ala ganemı ve liye fıha mearibu uhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Kale elkıha ya musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fe elkaha fe iza hiye hayyetun tes´a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Kale huzha ve la tehaf se nuıyduha sıratehel ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Vadmun yedeke ila cenahıke tahruc beydae min gayri suin ayeten uhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Li nuriyeke min ayatinel kubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Izheb ila fir´avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Kale rabbisrah lı sadrı"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Ve yessir lı emrı"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Vahlul ukdetem mil lisanı"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yefkahu kavlı"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Vec´al li vezıram min ehlı"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Harune ehıy"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Usdud bihı ezrı"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Ve esrikhu fı emrı"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Key nusebbihake kesıra"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Ve nezkurake kesıra"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Inneke kunte bina besıyra"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Kale kad utiyte su´leke ya musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Ve lekad menenna aleyke merraten uhra"
        }
