    "pages": [
        {
            "chapter": 15,
            "verse": 52,
            "text": "Iz dehalu aleyhi fe kalu selama kale inna minkum vecilun"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Kalu la tevcel inna nubessiruke bi gulamin alım"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Kale e bessertumunı ala em messeniyel kiberu fe bime tubessirun"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Kalu bessernake bil hakkı fe la tekum minel kanitıyn"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Kale ve mey yaknetu mir rahmeti rabbihı illed dallun"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Kale fe ma hatbukum eyyuhel murselun"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Kalu inna ursilna ila kavmim mucrimın"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ale lut inna le muneccuhum ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illemraetehu kadderna inneha le minel gabirın"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "Felemma cae ale lutnil murselun"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Kale innekum kavmumu munkerun"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Kalu bel ci´nake bima kanu fıhi yemterun"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Ve eteynake bil hakkı ve inna le sadikun"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Fe esri bi ehlike bi kıt´ım minel leyli vettebı´ edbarahum ve la yeltefit minkum ehaduv vemdu haysu tu´merun"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Ve kadayna ileyhi zalikel emra enne dabira haulai maktuum musbihıyn"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Ve cae ehlul medıneti yestebsirun"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Kale inne haulai dayfı fe la tefdahun"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Vettekullahe ve la tuhzun"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Kalu e ve lem nenheke anil alemın"
        }
    ]
