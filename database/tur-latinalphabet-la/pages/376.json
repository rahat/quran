    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma agna anhum ma kanu yumetteun"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Ve ma ehlekna min karyetin illa leha munzirun"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Zikra ve ma kunna zalimın"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Ve ma tenezzelet bihis seyatıyn"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Ve ma yembegıy lehum ve ma yestetıy´un"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innehum anis sem´ı le ma´zulun"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fe la ted´u meallahi ilahen ahara fe tekune minel muazzebın"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Ve enzir asiratekel akrabın"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Vahfıd cenahake li menit tebeake minel mu´minın"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fe in asavke fe kul innı berıum mimma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Ve tevekkel alel azızir rahıym"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Ellezı yerake hıyne tekum"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Ve tekallubeke fis sacidın"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innehu huves semıul alım"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hel unebbiukum ala men tenezzelus seyatıyn"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tenezzelu ala kulli effakin esım"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulkunes sem´a ve ekseruhum kazibun"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Ves suarau yettebiuhumul gavun"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "E lem tera ennehum fı kulli vadiy yehımun"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Ve ennehum yekulune ma la yef´alun"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illellezıne amenu ve amilus salihati ve zekerullahe kesırav ventesaru mim ba´di ma zulimu ve seya´lemullezıne zalemu eyye munkalebiy yenkalibun"
        }
    ]
