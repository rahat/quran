{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Voi sunteti legati de Viata de Acum"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "insa Viata de Apoi este mai buna si mai trainica"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Asa este si in sulurile cele dintai"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "sulurile lui Abraham si ale lui Moise"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "A venit la tine spusa despre cea care invaluie"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "In Ziua aceea, chipuri care se umilesc"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "care se trudesc si se sleiesc"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "vor fi aruncate intr-un foc dogoritor"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Se vor adapa la un izvor clocotit"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "si vor avea drept ospat maracini"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "care nici nu-i vor ingrasa, insa nici foamea nu le-o va astampara"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "In Ziua aceea, chipuri voioase"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "de zelul lor multumite"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "vor fi intr-o Gradina inalta"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "unde nu vor auzi nici o vorba desarta"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Acolo va fi un izvor curgator"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "paturi inaltate"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "pocale aranjate"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "perne randuite"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "covoare asternute"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Oare ei nu privesc la camile cum au fost create"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Si nici la cer cum a fost ridicat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Si nici la munti cum au fost inaltati"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Si nici la pamant cum a fost intins"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Adu-le aminte, caci asupra ta sta aducerea aminte"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "insa asupra lor nu ai nici o putere"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Celui care intoarce spatele si tagaduieste"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Dumnezeu ii va da osanda cea mare"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "La Noi este intoarcerea lor"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "si asupra Noastra sta judecata lor"
        }
    ]
}