{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "in care va fi aruncat cel prea nemernic"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "cel care huleste si apoi intoarce spatele"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Cel mult temator va fi tinut insa deoparte"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ca si cel care da din averea sa intru mantuire"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Nimeni nu-si va afla rasplata la El"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "in afara celui care cauta la fata Domnului sau, Preainaltul"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "si care va fi multumit"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pe lumina zilei"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Pe noaptea ce invaluie pamantul"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Domnul tau nici nu te-a parasit si nici nu te-a urat"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Pentru tine, mai buna va fi Viata de Apoi decat Viata de Acum"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Din darurile Sale Domnul tau iti va darui, iar tu multumit vei fi"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nu te-a aflat El orfan si te-a adapostit"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ratacitor te-a aflat si te-a calauzit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Sarac te-a aflat si te-a imbogatit"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Orfanul, tu sa nu-l oropsesti"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Cersetorul, tu sa nu-l alungi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "De harul Domnului tau, necurmat sa povestesti"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Nu ti-am deschis Noi oare pieptul"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Nu ti-am luat povara"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "care-ti frangea spatele"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "N-am inaltat Noi oare amintirea ta"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Cu greul vine usorul"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Da, cu greul vine usorul"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cand ai ispravit munca, ridica-te"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "si catre Domnul tau cauta cu dor"
        }
    ]
}