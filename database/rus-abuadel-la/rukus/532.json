{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Nu, eu jur pe aceasta cetate"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Si tu esti locuitor al acestei cetati"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Pe un tata si pe ceea ce a odraslit"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Noi am creat omul in neajuns"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Oare el socoate ca nimeni nu-l poate dovedi"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "El spune: “Am ispravit averi cu toptanul!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Oare el socoate ca nimeni nu l-a vazut"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Oare nu Noi i-am facut ochi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "limba si buze"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Oare nu Noi i-am aratat cele doua cai"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "El nu o apuca insa pe calea suitoare"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Cum vei sti tu oare care este calea suitoare"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Aceasta inseamna sa rascumperi un prins"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "sa ospatezi, intr-o zi de foamete"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "o ruda orfana"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ori un sarman lipit pamantului"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Aceasta inseamna sa fii in randul celor care cred, al celor care se indeamna la rabdare, al celor care se indeamna la indurare"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Acestia sunt cei de-a dreapta"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Cei care tagaduiesc insa semnele Noastre sunt cei de-a stanga"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "asupra lor un foc se va bolti"
        }
    ]
}