{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Nu!... Jur pe asfintitul stelelor"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Si este un juramant, daca stiti, greu"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Acesta este un Coran Prea-cinstit"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "dintr-o Carte ascunsa"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Sa nu-l atinga decat cei curati"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "caci este o Pogorare de la Domnul lumilor"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Va indoiti de aceasta spusa"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Vreti sa va castigati traiul hulind"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Cand sufletul da sa iasa"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "si voi, in clipa aceea il priviti"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Noi suntem mult mai aproape de el decat voi cei ce-l inconjurati, insa nu Ne puteti zari"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Daca nu ati fi printre cei care vor fi judecati"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "de ce nu intoarceti acest suflet, daca spuneti adevarul"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Daca acest om este dintre cei apropiati de Dumnezeu"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "va gasi tihna, parfumurile si Gradinile Placerii"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Daca este dintre cei de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "“Pace tie!... Tu esti cu cei de-a dreapta!”"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Daca este insa dintre hulitorii rataciti"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "va fi gazduit in apa clocotinda"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "si apoi aruncat in Iadul cu flacari"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Acesta este Adevarul sigur"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Preamareste Numele Domnului tau cel Mare"
        }
    ]
}