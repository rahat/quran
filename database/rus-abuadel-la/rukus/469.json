{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Cand faptul se va implini"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nimeni nu-l va mai socoti minciuna"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "El coboara, El ridica"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Cand pamantul va fi zgaltait din tatani"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "cand muntii vor fi facuti farame"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "si vor fi precum praful spulberat"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "atunci veti fi voi de trei feluri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Cei de-a dreapta! Cine vor fi oare cei de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Cei de-a stanga! Cine vor fi oare cei de-a stanga"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Cei inainte mergatori. Inainte mergatorii"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ei vor fi cei apropiati"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "In Gradinile Placerii"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "vor fi multime dintre primii"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "si putinii dintre ultimii"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "pe paturi impletite"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "intinsi pe ele fata in fata"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Flacaiandri nemuritori vor umbla printre ei"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "cu potire si ibrice, si un pocal de la izvor"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "de care nu-i va durea capul si nici nu se vor imbata"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "fructele vor fi pe alese"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "si carnea de pasare dupa pofta"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Vor avea acolo hurii cu ochi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "asemenea margaritarelor ascunse"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "drept rasplata a faptelor lor"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Si nu vor auzi acolo vorbe desarte si nici invinuiri de pacate"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "ci numai un singur cuvant: “Pace!... Pace!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Cei de-a dreapta! Ce va fi cu cei de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Ei vor sta sub lotusi despinati"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "sub salcami frumos insirati"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "sub umbra intinsa"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "langa apa curgatoare"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "cu fructe din belsug"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "neculese dinainte si neoprite"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "pe asternuturi inaltate"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Noi le-am facut pe ele in chip deosebit"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Noi le-am facut fecioare"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "iubitoare, insotitoare"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "celor de-a dreapta"
        }
    ]
}