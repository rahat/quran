{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Pe smochin si pe maslin"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Pe muntele Sinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Pe aceasta cetate tihnita"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Noi am creat omul dandu-i chip desavarsit"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "si l-am trimis apoi in catul cel mai de jos"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "afara de cei care au crezut si au facut fapte bune pe care o rasplata necurmata ii asteapta"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ce te face sa socoti apoi Legea minciuna"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Nu este oare Dumnezeu cel mai drept dintre judecatori"
        }
    ]
}