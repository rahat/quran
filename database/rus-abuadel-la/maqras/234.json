{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "El s-a incruntat si a intors spatele"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "cand orbul a venit la el"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ce stii tu? Poate s-ar fi mantuit"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ori si-ar fi amintit, iar amintirea i-ar fi folosit"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "In schimb pe cel bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "tu il primesti cu cinste"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "fara sa-ti pese daca se va mantui"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "iar pe cel care vine la tine, plin de ravna"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "plin de teama"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "tu, cu nepasare, il primesti"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Nu! Aceasta este o amintire"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "oricui vrea sa-si aminteasca"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ea este de pe sulurile prea cinstite"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "inaltate, curatate"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "de mainile scribilor"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "cinstiti si nevinovati"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Sa fie ucis omul! Cat este de tagaduitor"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Din ce l-a creat"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Dintr-un strop de samanta! L-a creat si i-a dat o soarta"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "i-a facut calea usoara"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "apoi l-a lasat sa moara si a pus sa fie inmormantat"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "iar cand va voi, il va scula"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Nu! Omul nu implineste ceea ce El ii porunceste"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Omul, sa se uite la hrana sa"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Noi am varsat apa, si ce varsare"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Noi am crapat pamantul, si ce crapare"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Noi am facut sa rasara grane"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vii si legume"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "maslini si curmali"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "si gradini dese"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "fructe si pasuni"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "zestre voua si turmelor voastre"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Cand va veni insa Vuietul"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "in ziua cand omul va fugi de fratele sau"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "de mama sa, de tatal sau"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "de sotia sa, de fiii sai"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "in Ziua aceea fiece om isi va primi rasplata"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "In Ziua aceea"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "vor fi chipuri stralucitoare, surazatoare si bucuroase"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "insa in Ziua aceea"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "vor fi si chipuri colbuite"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Acestia vor fi tagaduitorii, desfranatii"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Cand soarele se va chirci"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "cand stelele vor cadea"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "cand muntii vor fi pusi in miscare"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "cand camilele in zece luni lasate in uitare"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "cand fiarele vor fi adunate"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "cand marile se vor revarsa"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "cand sufletele vor fi imperecheate"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "cand copila de vie ingropata va fi intrebata"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "pentru care pacat a fost omorata"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "cand sulurile vor fi imprastiate"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "cand cerul va fi dat la o parte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "cand Iadul va fi incins"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "cand Raiul va fi aproape"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "atunci fiece suflet va sti ceea ce a adus"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ba nu! Jur pe planetele"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ce aluneca si trec"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Pe noapte cand se intuneca"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Pe zori cand lin adie"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Acesta este cuvantul unui prea-cinstit trimis"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "cu putere de la Stapanul Tronului Nezdruncinat"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "supus si credincios"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Prietenul vostru nu este indracit"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "El l-a vazut in zare deslusit"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "si El cu Taina nu este zgarcit"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Acesta nu este cuvantul unui diavol la pietre luat"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Unde plecati"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Aceasta este doar o amintire pentru lumi"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "pentru aceia dintre voi care vor sa calce drept"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Voi nu voiti insa decat ceea ce Dumnezeu, Stapanul lumilor, voieste"
        }
    ]
}