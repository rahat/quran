{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Nu ti-am deschis Noi oare pieptul"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Nu ti-am luat povara"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "care-ti frangea spatele"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "N-am inaltat Noi oare amintirea ta"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Cu greul vine usorul"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Da, cu greul vine usorul"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cand ai ispravit munca, ridica-te"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "si catre Domnul tau cauta cu dor"
        }
    ]
}