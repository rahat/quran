{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "وَالسَّمَآءِ وَالطَّارِقِ ١ﶫ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا الطَّارِقُ ٢ﶫ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "النَّجۡمُ الثَّاقِبُ ٣ﶫ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "اِنۡ كُلُّ نَفۡسٍ لَّمَّا عَلَيۡهَا حَافِظٌ ٤ﶠ"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ مِمَّ خُلِقَ ٥ﶠ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خُلِقَ مِنۡ مَّآءٍ دَافِقٍ ٦ﶫ"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "يَّخۡرُجُ مِنۣۡ بَيۡنِ الصُّلۡبِ وَالتَّرَآئِبِ ٧ﶠ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "اِنَّهٗ عَلٰي رَجۡعِهٖ لَقَادِرٌ ٨ﶠ"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "يَوۡمَ تُبۡلَي السَّرَآئِرُ ٩ﶫ"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فَمَا لَهٗ مِنۡ قُوَّةٍ وَّلَا نَاصِرٍ ١٠ﶠ"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "وَالسَّمَآءِ ذَاتِ الرَّجۡعِ ١١ﶫ"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "وَالۡاَرۡضِ ذَاتِ الصَّدۡعِ ١٢ﶫ"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "اِنَّهٗ لَقَوۡلٌ فَصۡلٌ ١٣ﶫ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وَّمَا هُوَ بِالۡهَزۡلِ ١٤ﶠ"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "اِنَّهُمۡ يَكِيۡدُوۡنَ كَيۡدًا ١٥ﶫ"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "وَّاَكِيۡدُ كَيۡدًا ١٦ﶗ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فَمَهِّلِ الۡكٰفِرِيۡنَ اَمۡهِلۡهُمۡ رُوَيۡدًا ١٧ﶒ"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَي ١ﶫ"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "الَّذِيۡ خَلَقَ فَسَوّٰي ٢ﶟ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَالَّذِيۡ قَدَّرَ فَهَدٰي ٣ﶟ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَالَّذِيۡ٘ اَخۡرَجَ الۡمَرۡعٰي ٤ﶟ"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهٗ غُثَآءً اَحۡوٰي ٥ﶠ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنۡسٰ٘ي ٦ﶫ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "اِلَّا مَا شَآءَ اللّٰهُﵧ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰي ٧ﶠ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡرٰي ٨ﶗ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰي ٩ﶠ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَيَذَّكَّرُ مَنۡ يَّخۡشٰي ١٠ﶫ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَيَتَجَنَّبُهَا الۡاَشۡقَي ١١ﶫ"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "الَّذِيۡ يَصۡلَي النَّارَ الۡكُبۡرٰي ١٢ﶔ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰي ١٣ﶠ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰي ١٤ﶫ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰي ١٥ﶠ"
        }
