{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "اِذَا وَقَعَتِ الۡوَاقِعَةُ ١ﶫ"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ ٢ﶭ"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "خَافِضَةٌ رَّافِعَةٌ ٣ﶫ"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "اِذَا رُجَّتِ الۡاَرۡضُ رَجًّا ٤ﶫ"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "وَّبُسَّتِ الۡجِبَالُ بَسًّا ٥ﶫ"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "فَكَانَتۡ هَبَآءً مُّنۣۡبَثًّا ٦ﶫ"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "وَّكُنۡتُمۡ اَزۡوَاجًا ثَلٰثَةً ٧ﶠ"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "فَاَصۡحٰبُ الۡمَيۡمَنَةِﵿ مَا٘ اَصۡحٰبُ الۡمَيۡمَنَةِ ٨ﶠ"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "وَاَصۡحٰبُ الۡمَشۡـَٔمَةِﵿ مَا٘ اَصۡحٰبُ الۡمَشۡـَٔمَةِ ٩ﶠ"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "وَالسّٰبِقُوۡنَ السّٰبِقُوۡنَ ١٠ﶫ"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "اُولٰٓئِكَ الۡمُقَرَّبُوۡنَ ١١ﶔ"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "فِيۡ جَنّٰتِ النَّعِيۡمِ ١٢"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَ ١٣ﶫ"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "وَقَلِيۡلٌ مِّنَ الۡاٰخِرِيۡنَ ١٤ﶠ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "عَلٰي سُرُرٍ مَّوۡضُوۡنَةٍ ١٥ﶫ"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "مُّتَّكِـِٕيۡنَ عَلَيۡهَا مُتَقٰبِلِيۡنَ ١٦"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "يَطُوۡفُ عَلَيۡهِمۡ وِلۡدَانٌ مُّخَلَّدُوۡنَ ١٧ﶫ"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "بِاَكۡوَابٍ وَّاَبَارِيۡقَﵿ وَكَاۡسٍ مِّنۡ مَّعِيۡنٍ ١٨ﶫ"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "لَّا يُصَدَّعُوۡنَ عَنۡهَا وَلَا يُنۡزِفُوۡنَ ١٩ﶫ"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوۡنَ ٢٠ﶫ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "وَلَحۡمِ طَيۡرٍ مِّمَّا يَشۡتَهُوۡنَ ٢١ﶠ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "وَحُوۡرٌ عِيۡنٌ ٢٢ﶫ"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "كَاَمۡثَالِ اللُّؤۡلُؤِ الۡمَكۡنُوۡنِ ٢٣ﶔ"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "جَزَآءًۣ بِمَا كَانُوۡا يَعۡمَلُوۡنَ ٢٤"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَغۡوًا وَّلَا تَاۡثِيۡمًا ٢٥ﶫ"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "اِلَّا قِيۡلًا سَلٰمًا سَلٰمًا ٢٦"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "وَاَصۡحٰبُ الۡيَمِيۡنِﵿ مَا٘ اَصۡحٰبُ الۡيَمِيۡنِ ٢٧ﶠ"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "فِيۡ سِدۡرٍ مَّخۡضُوۡدٍ ٢٨ﶫ"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "وَّطَلۡحٍ مَّنۡضُوۡدٍ ٢٩ﶫ"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "وَّظِلٍّ مَّمۡدُوۡدٍ ٣٠ﶫ"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "وَّمَآءٍ مَّسۡكُوۡبٍ ٣١ﶫ"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "وَّفَاكِهَةٍ كَثِيۡرَةٍ ٣٢ﶫ"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "لَّا مَقۡطُوۡعَةٍ وَّلَا مَمۡنُوۡعَةٍ ٣٣ﶫ"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "وَّفُرُشٍ مَّرۡفُوۡعَةٍ ٣٤ﶠ"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "اِنَّا٘ اَنۡشَاۡنٰهُنَّ اِنۡشَآءً ٣٥ﶫ"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "فَجَعَلۡنٰهُنَّ اَبۡكَارًا ٣٦ﶫ"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "عُرُبًا اَتۡرَابًا ٣٧ﶫ"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "لِّاَصۡحٰبِ الۡيَمِيۡنِ ٣٨ﷰ"
        }
    ]
}