{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "هَلۡ اَتٰىكَ حَدِيۡثُ الۡغَاشِيَةِ ١ﶠ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ خَاشِعَةٌ ٢ﶫ"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عَامِلَةٌ نَّاصِبَةٌ ٣ﶫ"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تَصۡلٰي نَارًا حَامِيَةً ٤ﶫ"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تُسۡقٰي مِنۡ عَيۡنٍ اٰنِيَةٍ ٥ﶠ"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "لَيۡسَ لَهُمۡ طَعَامٌ اِلَّا مِنۡ ضَرِيۡعٍ ٦ﶫ"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِيۡ مِنۡ جُوۡعٍ ٧ﶠ"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ نَّاعِمَةٌ ٨ﶫ"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لِّسَعۡيِهَا رَاضِيَةٌ ٩ﶫ"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "فِيۡ جَنَّةٍ عَالِيَةٍ ١٠ﶫ"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لَّا تَسۡمَعُ فِيۡهَا لَاغِيَةً ١١ﶠ"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فِيۡهَا عَيۡنٌ جَارِيَةٌ ١٢ﶭ"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فِيۡهَا سُرُرٌ مَّرۡفُوۡعَةٌ ١٣ﶫ"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "وَّاَكۡوَابٌ مَّوۡضُوۡعَةٌ ١٤ﶫ"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "وَّنَمَارِقُ مَصۡفُوۡفَةٌ ١٥ﶫ"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وَّزَرَابِيُّ مَبۡثُوۡثَةٌ ١٦ﶠ"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "اَفَلَا يَنۡظُرُوۡنَ اِلَي الۡاِبِلِ كَيۡفَ خُلِقَتۡ ١٧ﷃ"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "وَاِلَي السَّمَآءِ كَيۡفَ رُفِعَتۡ ١٨ﷃ"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "وَاِلَي الۡجِبَالِ كَيۡفَ نُصِبَتۡ ١٩ﷃ"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "وَاِلَي الۡاَرۡضِ كَيۡفَ سُطِحَتۡ ٢٠"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فَذَكِّرۡﵴ اِنَّمَا٘ اَنۡتَ مُذَكِّرٌ ٢١ﶠ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لَسۡتَ عَلَيۡهِمۡ بِمُصَۜيۡطِرٍ ٢٢ﶫ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "اِلَّا مَنۡ تَوَلّٰي وَكَفَرَ ٢٣ﶫ"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فَيُعَذِّبُهُ اللّٰهُ الۡعَذَابَ الۡاَكۡبَرَ ٢٤ﶠ"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "اِنَّ اِلَيۡنَا٘ اِيَابَهُمۡ ٢٥ﶫ"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثُمَّ اِنَّ عَلَيۡنَا حِسَابَهُمۡ ٢٦ﶒ"
        }
    ]
}