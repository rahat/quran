{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "اَفَرَءَيۡتَ الَّذِيۡ تَوَلّٰي ٣٣ﶫ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَاَعۡطٰي قَلِيۡلًا وَّاَكۡدٰي ٣٤"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "اَعِنۡدَهٗ عِلۡمُ الۡغَيۡبِ فَهُوَ يَرٰي ٣٥"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "اَمۡ لَمۡ يُنَبَّاۡ بِمَا فِيۡ صُحُفِ مُوۡسٰي ٣٦ﶫ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَاِبۡرٰهِيۡمَ الَّذِيۡ وَفّٰ٘ي ٣٧ﶫ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "اَلَّا تَزِرُ وَازِرَةٌ وِّزۡرَ اُخۡرٰي ٣٨ﶫ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَاَنۡ لَّيۡسَ لِلۡاِنۡسَانِ اِلَّا مَا سَعٰي ٣٩ﶫ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَاَنَّ سَعۡيَهٗ سَوۡفَ يُرٰي ٤٠"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزٰىهُ الۡجَزَآءَ الۡاَوۡفٰي ٤١ﶫ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَاَنَّ اِلٰي رَبِّكَ الۡمُنۡتَهٰي ٤٢ﶫ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "وَاَنَّهٗ هُوَ اَضۡحَكَ وَاَبۡكٰي ٤٣ﶫ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَاَنَّهٗ هُوَ اَمَاتَ وَاَحۡيَا ٤٤ﶫ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَاَنَّهٗ خَلَقَ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰي ٤٥ﶫ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِنۡ نُّطۡفَةٍ اِذَا تُمۡنٰي ٤٦ﶝ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وَاَنَّ عَلَيۡهِ النَّشۡاَةَ الۡاُخۡرٰي ٤٧ﶫ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَاَنَّهٗ هُوَ اَغۡنٰي وَاَقۡنٰي ٤٨ﶫ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَاَنَّهٗ هُوَ رَبُّ الشِّعۡرٰي ٤٩ﶫ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَاَنَّهٗ٘ اَهۡلَكَ عَادَا اِۨلۡاُوۡلٰي ٥٠ﶫ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُوۡدَا۠ فَمَا٘ اَبۡقٰي ٥١ﶫ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوۡمَ نُوۡحٍ مِّنۡ قَبۡلُﵧ اِنَّهُمۡ كَانُوۡا هُمۡ اَظۡلَمَ وَاَطۡغٰي ٥٢ﶠ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَالۡمُؤۡتَفِكَةَ اَهۡوٰي ٥٣ﶫ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشّٰىهَا مَا غَشّٰي ٥٤ﶔ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكَ تَتَمَارٰي ٥٥"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هٰذَا نَذِيۡرٌ مِّنَ النُّذُرِ الۡاُوۡلٰي ٥٦"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "اَزِفَتِ الۡاٰزِفَةُ ٥٧ﶔ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيۡسَ لَهَا مِنۡ دُوۡنِ اللّٰهِ كَاشِفَةٌ ٥٨ﶠ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "اَفَمِنۡ هٰذَا الۡحَدِيۡثِ تَعۡجَبُوۡنَ ٥٩ﶫ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضۡحَكُوۡنَ وَلَا تَبۡكُوۡنَ ٦٠ﶫ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَاَنۡتُمۡ سٰمِدُوۡنَ ٦١"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاسۡجُدُوۡا لِلّٰهِ وَاعۡبُدُوۡا ٦٢ﶶ"
        }
    ]
