{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "اِنَّا٘ اَنۡزَلۡنٰهُ فِيۡ لَيۡلَةِ الۡقَدۡرِ ١ﶖ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا لَيۡلَةُ الۡقَدۡرِ ٢ﶠ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَيۡلَةُ الۡقَدۡرِﵿ خَيۡرٌ مِّنۡ اَلۡفِ شَهۡرٍ ٣ﶣ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ الۡمَلٰٓئِكَةُ وَالرُّوۡحُ فِيۡهَا بِاِذۡنِ رَبِّهِمۡ مِّنۡ كُلِّ اَمۡرٍ ٤ﶬ"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلٰمٌﵵ هِيَ حَتّٰي مَطۡلَعِ الۡفَجۡرِ ٥ﶒ"
        }
    ]
}