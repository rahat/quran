{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "وَالذّٰرِيٰتِ ذَرۡوًا ١ﶫ"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "فَالۡحٰمِلٰتِ وِقۡرًا ٢ﶫ"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "فَالۡجٰرِيٰتِ يُسۡرًا ٣ﶫ"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "فَالۡمُقَسِّمٰتِ اَمۡرًا ٤ﶫ"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "اِنَّمَا تُوۡعَدُوۡنَ لَصَادِقٌ ٥ﶫ"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "وَّاِنَّ الدِّيۡنَ لَوَاقِعٌ ٦ﶠ"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "وَالسَّمَآءِ ذَاتِ الۡحُبُكِ ٧ﶫ"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "اِنَّكُمۡ لَفِيۡ قَوۡلٍ مُّخۡتَلِفٍ ٨ﶫ"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "يُّؤۡفَكُ عَنۡهُ مَنۡ اُفِكَ ٩ﶠ"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "قُتِلَ الۡخَرّٰصُوۡنَ ١٠ﶫ"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "الَّذِيۡنَ هُمۡ فِيۡ غَمۡرَةٍ سَاهُوۡنَ ١١ﶫ"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "يَسۡـَٔلُوۡنَ اَيَّانَ يَوۡمُ الدِّيۡنِ ١٢ﶠ"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "يَوۡمَ هُمۡ عَلَي النَّارِ يُفۡتَنُوۡنَ ١٣"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ذُوۡقُوۡا فِتۡنَتَكُمۡﵧ هٰذَا الَّذِيۡ كُنۡتُمۡ بِهٖ تَسۡتَعۡجِلُوۡنَ ١٤"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "اِنَّ الۡمُتَّقِيۡنَ فِيۡ جَنّٰتٍ وَّعُيُوۡنٍ ١٥ﶫ"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "اٰخِذِيۡنَ مَا٘ اٰتٰىهُمۡ رَبُّهُمۡﵧ اِنَّهُمۡ كَانُوۡا قَبۡلَ ذٰلِكَ مُحۡسِنِيۡنَ ١٦ﶠ"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "كَانُوۡا قَلِيۡلًا مِّنَ الَّيۡلِ مَا يَهۡجَعُوۡنَ ١٧"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "وَبِالۡاَسۡحَارِ هُمۡ يَسۡتَغۡفِرُوۡنَ ١٨"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "وَفِيۡ٘ اَمۡوَالِهِمۡ حَقٌّ لِّلسَّآئِلِ وَالۡمَحۡرُوۡمِ ١٩"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "وَفِي الۡاَرۡضِ اٰيٰتٌ لِّلۡمُوۡقِنِيۡنَ ٢٠ﶫ"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "وَفِيۡ٘ اَنۡفُسِكُمۡﵧ اَفَلَا تُبۡصِرُوۡنَ ٢١"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "وَفِي السَّمَآءِ رِزۡقُكُمۡ وَمَا تُوۡعَدُوۡنَ ٢٢"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "فَوَرَبِّ السَّمَآءِ وَالۡاَرۡضِ اِنَّهٗ لَحَقٌّ مِّثۡلَ مَا٘ اَنَّكُمۡ تَنۡطِقُوۡنَ ٢٣ﶒ"
        }
    ]
}