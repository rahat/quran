{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَاَنۡتُمۡ اَشَدُّ خَلۡقًا اَمِ السَّمَآءُﵧ بَنٰىهَا ٢٧ﶨ"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوّٰىهَا ٢٨ﶫ"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَاَغۡطَشَ لَيۡلَهَا وَاَخۡرَجَ ضُحٰىهَا ٢٩ﶝ"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَالۡاَرۡضَ بَعۡدَ ذٰلِكَ دَحٰىهَا ٣٠ﶠ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "اَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعٰىهَا ٣١ﶝ"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَالۡجِبَالَ اَرۡسٰىهَا ٣٢ﶫ"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَاعًا لَّكُمۡ وَ لِاَنۡعَامِكُمۡ ٣٣ﶠ"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَاِذَا جَآءَتِ الطَّآمَّةُ الۡكُبۡرٰي ٣٤ﶛ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يَوۡمَ يَتَذَكَّرُ الۡاِنۡسَانُ مَا سَعٰي ٣٥ﶫ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ الۡجَحِيۡمُ لِمَنۡ يَّرٰي ٣٦"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَاَمَّا مَنۡ طَغٰي ٣٧ﶫ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَاٰثَرَ الۡحَيٰوةَ الدُّنۡيَا ٣٨ﶫ"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَاِنَّ الۡجَحِيۡمَ هِيَ الۡمَاۡوٰي ٣٩ﶠ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَاَمَّا مَنۡ خَافَ مَقَامَ رَبِّهٖ وَنَهَي النَّفۡسَ عَنِ الۡهَوٰي ٤٠ﶫ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَاِنَّ الۡجَنَّةَ هِيَ الۡمَاۡوٰي ٤١ﶠ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "يَسۡـَٔلُوۡنَكَ عَنِ السَّاعَةِ اَيَّانَ مُرۡسٰىهَا ٤٢ﶠ"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِيۡمَ اَنۡتَ مِنۡ ذِكۡرٰىهَا ٤٣ﶠ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "اِلٰي رَبِّكَ مُنۡتَهٰىهَا ٤٤ﶠ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "اِنَّمَا٘ اَنۡتَ مُنۡذِرُ مَنۡ يَّخۡشٰىهَا ٤٥ﶠ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَاَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوۡ٘ا اِلَّا عَشِيَّةً اَوۡ ضُحٰىهَا ٤٦ﶒ"
        }
    ]
}