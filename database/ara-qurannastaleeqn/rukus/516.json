{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "وَالۡمُرۡسَلٰتِ عُرۡفًا ١ﶫ"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فَالۡعٰصِفٰتِ عَصۡفًا ٢ﶫ"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "وَّالنّٰشِرٰتِ نَشۡرًا ٣ﶫ"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فَالۡفٰرِقٰتِ فَرۡقًا ٤ﶫ"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فَالۡمُلۡقِيٰتِ ذِكۡرًا ٥ﶫ"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عُذۡرًا اَوۡ نُذۡرًا ٦ﶫ"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "اِنَّمَا تُوۡعَدُوۡنَ لَوَاقِعٌ ٧ﶠ"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فَاِذَا النُّجُوۡمُ طُمِسَتۡ ٨ﶫ"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "وَاِذَا السَّمَآءُ فُرِجَتۡ ٩ﶫ"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "وَاِذَا الۡجِبَالُ نُسِفَتۡ ١٠ﶫ"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "وَاِذَا الرُّسُلُ اُقِّتَتۡ ١١ﶠ"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لِاَيِّ يَوۡمٍ اُجِّلَتۡ ١٢ﶠ"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "لِيَوۡمِ الۡفَصۡلِ ١٣ﶔ"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا يَوۡمُ الۡفَصۡلِ ١٤ﶠ"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ١٥"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "اَلَمۡ نُهۡلِكِ الۡاَوَّلِيۡنَ ١٦ﶠ"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثُمَّ نُتۡبِعُهُمُ الۡاٰخِرِيۡنَ ١٧"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ ١٨"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ١٩"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "اَلَمۡ نَخۡلُقۡكُّمۡ مِّنۡ مَّآءٍ مَّهِيۡنٍ ٢٠ﶫ"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فَجَعَلۡنٰهُ فِيۡ قَرَارٍ مَّكِيۡنٍ ٢١ﶫ"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "اِلٰي قَدَرٍ مَّعۡلُوۡمٍ ٢٢ﶫ"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فَقَدَرۡنَاﵲ فَنِعۡمَ الۡقٰدِرُوۡنَ ٢٣"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ٢٤"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ كِفَاتًا ٢٥ﶫ"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "اَحۡيَآءً وَّاَمۡوَاتًا ٢٦ﶫ"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَّجَعَلۡنَا فِيۡهَا رَوَاسِيَ شٰمِخٰتٍ وَّاَسۡقَيۡنٰكُمۡ مَّآءً فُرَاتًا ٢٧ﶠ"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ٢٨"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "اِنۡطَلِقُوۡ٘ا اِلٰي مَا كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ ٢٩ﶔ"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "اِنۡطَلِقُوۡ٘ا اِلٰي ظِلٍّ ذِيۡ ثَلٰثِ شُعَبٍ ٣٠ﶫ"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيۡلٍ وَّلَا يُغۡنِيۡ مِنَ اللَّهَبِ ٣١ﶠ"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "اِنَّهَا تَرۡمِيۡ بِشَرَرٍ كَالۡقَصۡرِ ٣٢ﶔ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَاَنَّهٗ جِمٰلَتٌ صُفۡرٌ ٣٣ﶠ"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ٣٤"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هٰذَا يَوۡمُ لَا يَنۡطِقُوۡنَ ٣٥ﶫ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُوۡنَ ٣٦"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ٣٧"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هٰذَا يَوۡمُ الۡفَصۡلِﵐ جَمَعۡنٰكُمۡ وَالۡاَوَّلِيۡنَ ٣٨"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَاِنۡ كَانَ لَكُمۡ كَيۡدٌ فَكِيۡدُوۡنِ ٣٩"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ٤٠ﶒ"
        }
    ]
}