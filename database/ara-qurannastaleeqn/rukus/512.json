{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "لَا٘ اُقۡسِمُ بِيَوۡمِ الۡقِيٰمَةِ ١ﶫ"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "وَلَا٘ اُقۡسِمُ بِالنَّفۡسِ اللَّوَّامَةِ ٢"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "اَيَحۡسَبُ الۡاِنۡسَانُ اَلَّنۡ نَّجۡمَعَ عِظَامَهٗ ٣ﶠ"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "بَلٰي قٰدِرِيۡنَ عَلٰ٘ي اَنۡ نُّسَوِّيَ بَنَانَهٗ ٤"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "بَلۡ يُرِيۡدُ الۡاِنۡسَانُ لِيَفۡجُرَ اَمَامَهٗ ٥ﶔ"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "يَسۡـَٔلُ اَيَّانَ يَوۡمُ الۡقِيٰمَةِ ٦ﶠ"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "فَاِذَا بَرِقَ الۡبَصَرُ ٧ﶫ"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "وَخَسَفَ الۡقَمَرُ ٨ﶫ"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "وَجُمِعَ الشَّمۡسُ وَالۡقَمَرُ ٩ﶫ"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "يَقُوۡلُ الۡاِنۡسَانُ يَوۡمَئِذٍ اَيۡنَ الۡمَفَرُّ ١٠ﶔ"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "كَلَّا لَا وَزَرَ ١١ﶠ"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "اِلٰي رَبِّكَ يَوۡمَئِذِ اِۨلۡمُسۡتَقَرُّ ١٢ﶠ"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "يُنَبَّؤُا الۡاِنۡسَانُ يَوۡمَئِذٍۣ بِمَا قَدَّمَ وَاَخَّرَ ١٣ﶠ"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "بَلِ الۡاِنۡسَانُ عَلٰي نَفۡسِهٖ بَصِيۡرَةٌ ١٤ﶫ"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "وَّلَوۡ اَلۡقٰي مَعَاذِيۡرَهٗ ١٥ﶠ"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "لَا تُحَرِّكۡ بِهٖ لِسَانَكَ لِتَعۡجَلَ بِهٖ ١٦ﶠ"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "اِنَّ عَلَيۡنَا جَمۡعَهٗ وَقُرۡاٰنَهٗ ١٧ﶗ"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "فَاِذَا قَرَاۡنٰهُ فَاتَّبِعۡ قُرۡاٰنَهٗ ١٨ﶔ"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ثُمَّ اِنَّ عَلَيۡنَا بَيَانَهٗ ١٩ﶠ"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "كَلَّا بَلۡ تُحِبُّوۡنَ الۡعَاجِلَةَ ٢٠ﶫ"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "وَتَذَرُوۡنَ الۡاٰخِرَةَ ٢١ﶠ"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ نَّاضِرَةٌ ٢٢ﶫ"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "اِلٰي رَبِّهَا نَاظِرَةٌ ٢٣ﶔ"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "وَوُجُوۡهٌ يَّوۡمَئِذٍۣ بَاسِرَةٌ ٢٤ﶫ"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "تَظُنُّ اَنۡ يُّفۡعَلَ بِهَا فَاقِرَةٌ ٢٥ﶠ"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "كَلَّا٘ اِذَا بَلَغَتِ التَّرَاقِيَ ٢٦ﶫ"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "وَقِيۡلَ مَنۡﶌ رَاقٍ ٢٧ﶫ"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "وَّظَنَّ اَنَّهُ الۡفِرَاقُ ٢٨ﶫ"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "وَالۡتَفَّتِ السَّاقُ بِالسَّاقِ ٢٩ﶫ"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "اِلٰي رَبِّكَ يَوۡمَئِذِ اِۨلۡمَسَاقُ ٣٠ﶢ"
        }
    ]
}