{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَاَرۡسَلَ فِرۡعَوۡنُ فِي الۡمَدَآئِنِ حٰشِرِيۡنَ ٥٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "اِنَّ هٰ٘ؤُلَآءِ لَشِرۡذِمَةٌ قَلِيۡلُوۡنَ ٥٤ﶫ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَاِنَّهُمۡ لَنَا لَغَآئِظُوۡنَ ٥٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَاِنَّا لَجَمِيۡعٌ حٰذِرُوۡنَ ٥٦ﶠ"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَاَخۡرَجۡنٰهُمۡ مِّنۡ جَنّٰتٍ وَّعُيُوۡنٍ ٥٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَّكُنُوۡزٍ وَّمَقَامٍ كَرِيۡمٍ ٥٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذٰلِكَﵧ وَاَوۡرَثۡنٰهَا بَنِيۡ٘ اِسۡرَآءِيۡلَ ٥٩ﶠ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَاَتۡبَعُوۡهُمۡ مُّشۡرِقِيۡنَ ٦٠"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَآءَ الۡجَمۡعٰنِ قَالَ اَصۡحٰبُ مُوۡسٰ٘ي اِنَّا لَمُدۡرَكُوۡنَ ٦١ﶔ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّاﵐ اِنَّ مَعِيَ رَبِّيۡ سَيَهۡدِيۡنِ ٦٢"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَاَوۡحَيۡنَا٘ اِلٰي مُوۡسٰ٘ي اَنِ اضۡرِبۡ بِّعَصَاكَ الۡبَحۡرَﵧ فَانۡفَلَقَ فَكَانَ كُلُّ فِرۡقٍ كَالطَّوۡدِ الۡعَظِيۡمِ ٦٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَاَزۡلَفۡنَا ثَمَّ الۡاٰخَرِيۡنَ ٦٤ﶔ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَاَنۡجَيۡنَا مُوۡسٰي وَمَنۡ مَّعَهٗ٘ اَجۡمَعِيۡنَ ٦٥ﶔ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ اَغۡرَقۡنَا الۡاٰخَرِيۡنَ ٦٦ﶠ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ٦٧"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ٦٨ﶒ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاتۡلُ عَلَيۡهِمۡ نَبَاَ اِبۡرٰهِيۡمَ ٦٩ﶭ"
        }
    ]
}