{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَالۡعٰدِيٰتِ ضَبۡحًا ١ﶫ"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَالۡمُوۡرِيٰتِ قَدۡحًا ٢ﶫ"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَالۡمُغِيۡرٰتِ صُبۡحًا ٣ﶫ"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَاَثَرۡنَ بِهٖ نَقۡعًا ٤ﶫ"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهٖ جَمۡعًا ٥ﶫ"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "اِنَّ الۡاِنۡسَانَ لِرَبِّهٖ لَكَنُوۡدٌ ٦ﶔ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَاِنَّهٗ عَلٰي ذٰلِكَ لَشَهِيۡدٌ ٧ﶔ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَاِنَّهٗ لِحُبِّ الۡخَيۡرِ لَشَدِيۡدٌ ٨ﶠ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "اَفَلَا يَعۡلَمُ اِذَا بُعۡثِرَ مَا فِي الۡقُبُوۡرِ ٩ﶫ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي الصُّدُوۡرِ ١٠ﶫ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "اِنَّ رَبَّهُمۡ بِهِمۡ يَوۡمَئِذٍ لَّخَبِيۡرٌ ١١ﶒ"
        }
    ]
}