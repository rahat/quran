{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَالنّٰزِعٰتِ غَرۡقًا ١ﶫ"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَّالنّٰشِطٰتِ نَشۡطًا ٢ﶫ"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَّالسّٰبِحٰتِ سَبۡحًا ٣ﶫ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَالسّٰبِقٰتِ سَبۡقًا ٤ﶫ"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَالۡمُدَبِّرٰتِ اَمۡرًا ٥ﶭ"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوۡمَ تَرۡجُفُ الرَّاجِفَةُ ٦ﶫ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا الرَّادِفَةُ ٧ﶠ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوۡبٌ يَّوۡمَئِذٍ وَّاجِفَةٌ ٨ﶫ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "اَبۡصَارُهَا خَاشِعَةٌ ٩ﶭ"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُوۡلُوۡنَ ءَاِنَّا لَمَرۡدُوۡدُوۡنَ فِي الۡحَافِرَةِ ١٠ﶠ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ١١ﶠ"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُوۡا تِلۡكَ اِذًا كَرَّةٌ خَاسِرَةٌ ١٢ﶭ"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَاِنَّمَا هِيَ زَجۡرَةٌ وَّاحِدَةٌ ١٣ﶫ"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَاِذَا هُمۡ بِالسَّاهِرَةِ ١٤ﶠ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ اَتٰىكَ حَدِيۡثُ مُوۡسٰي ١٥ﶭ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "اِذۡ نَادٰىهُ رَبُّهٗ بِالۡوَادِ الۡمُقَدَّسِ طُوًي ١٦ﶔ"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اِذۡهَبۡ اِلٰي فِرۡعَوۡنَ اِنَّهٗ طَغٰي ١٧ﶛ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَلۡ لَّكَ اِلٰ٘ي اَنۡ تَزَكّٰي ١٨ﶫ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَاَهۡدِيَكَ اِلٰي رَبِّكَ فَتَخۡشٰي ١٩ﶔ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَاَرٰىهُ الۡاٰيَةَ الۡكُبۡرٰي ٢٠ﶛ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصٰي ٢١ﶛ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ اَدۡبَرَ يَسۡعٰي ٢٢ﶛ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَﵴ فَنَادٰي ٢٣ﶛ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ اَنَا رَبُّكُمُ الۡاَعۡلٰي ٢٤ﶛ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَاَخَذَهُ اللّٰهُ نَكَالَ الۡاٰخِرَةِ وَالۡاُوۡلٰي ٢٥ﶠ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَعِبۡرَةً لِّمَنۡ يَّخۡشٰي ٢٦ﶢ"
        }
    ]
}