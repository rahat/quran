{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "اَلرَّحۡمٰنُ ١ﶫ"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ الۡقُرۡاٰنَ ٢ﶠ"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ الۡاِنۡسَانَ ٣ﶫ"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ الۡبَيَانَ ٤"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "اَلشَّمۡسُ وَالۡقَمَرُ بِحُسۡبَانٍ ٥ﶫ"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَّالنَّجۡمُ وَالشَّجَرُ يَسۡجُدٰنِ ٦"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ الۡمِيۡزَانَ ٧ﶫ"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "اَلَّا تَطۡغَوۡا فِي الۡمِيۡزَانِ ٨"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَاَقِيۡمُوا الۡوَزۡنَ بِالۡقِسۡطِ وَلَا تُخۡسِرُوا الۡمِيۡزَانَ ٩"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَالۡاَرۡضَ وَضَعَهَا لِلۡاَنَامِ ١٠ﶫ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيۡهَا فَاكِهَةٌ وَّالنَّخۡلُ ذَاتُ الۡاَكۡمَامِ ١١ﶯ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَالۡحَبُّ ذُو الۡعَصۡفِ وَالرَّيۡحَانُ ١٢ﶔ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ١٣"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ صَلۡصَالٍ كَالۡفَخَّارِ ١٤ﶫ"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ الۡجَآنَّ مِنۡ مَّارِجٍ مِّنۡ نَّارٍ ١٥ﶔ"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ١٦"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ الۡمَشۡرِقَيۡنِ وَرَبُّ الۡمَغۡرِبَيۡنِ ١٧ﶔ"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ١٨"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ الۡبَحۡرَيۡنِ يَلۡتَقِيٰنِ ١٩ﶫ"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيۡنَهُمَا بَرۡزَخٌ لَّا يَبۡغِيٰنِ ٢٠ﶔ"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ٢١"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يَخۡرُجُ مِنۡهُمَا اللُّؤۡلُؤُ وَالۡمَرۡجَانُ ٢٢ﶔ"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ٢٣"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ الۡجَوَارِ الۡمُنۡشَاٰتُ فِي الۡبَحۡرِ كَالۡاَعۡلَامِ ٢٤ﶔ"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ٢٥ﶒ"
        }
    ]
