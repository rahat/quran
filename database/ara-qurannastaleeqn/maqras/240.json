{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "اَفَلَا يَعۡلَمُ اِذَا بُعۡثِرَ مَا فِي الۡقُبُوۡرِ ٩ﶫ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي الصُّدُوۡرِ ١٠ﶫ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "اِنَّ رَبَّهُمۡ بِهِمۡ يَوۡمَئِذٍ لَّخَبِيۡرٌ ١١ﶒ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "اَلۡقَارِعَةُ ١ﶫ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "مَا الۡقَارِعَةُ ٢ﶔ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا الۡقَارِعَةُ ٣ﶠ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يَوۡمَ يَكُوۡنُ النَّاسُ كَالۡفَرَاشِ الۡمَبۡثُوۡثِ ٤ﶫ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وَتَكُوۡنُ الۡجِبَالُ كَالۡعِهۡنِ الۡمَنۡفُوۡشِ ٥ﶠ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فَاَمَّا مَنۡ ثَقُلَتۡ مَوَازِيۡنُهٗ ٦ﶫ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فَهُوَ فِيۡ عِيۡشَةٍ رَّاضِيَةٍ ٧ﶠ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "وَاَمَّا مَنۡ خَفَّتۡ مَوَازِيۡنُهٗ ٨ﶫ"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فَاُمُّهٗ هَاوِيَةٌ ٩ﶠ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا هِيَهۡ ١٠ﶠ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نَارٌ حَامِيَةٌ ١١ﶒ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "اَلۡهٰىكُمُ التَّكَاثُرُ ١ﶫ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حَتّٰي زُرۡتُمُ الۡمَقَابِرَ ٢ﶠ"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كَلَّا سَوۡفَ تَعۡلَمُوۡنَ ٣ﶫ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُوۡنَ ٤ﶠ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كَلَّا لَوۡ تَعۡلَمُوۡنَ عِلۡمَ الۡيَقِيۡنِ ٥ﶠ"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لَتَرَوُنَّ الۡجَحِيۡمَ ٦ﶫ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ الۡيَقِيۡنِ ٧ﶫ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ النَّعِيۡمِ ٨ﶒ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَالۡعَصۡرِ ١ﶫ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "اِنَّ الۡاِنۡسَانَ لَفِيۡ خُسۡرٍ ٢ﶫ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ وَتَوَاصَوۡا بِالۡحَقِّﵿ وَتَوَاصَوۡا بِالصَّبۡرِ ٣ﶒ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَيۡلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةِ ١ﶫ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "اِۨلَّذِيۡ جَمَعَ مَالًا وَّعَدَّدَهٗ ٢ﶫ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحۡسَبُ اَنَّ مَالَهٗ٘ اَخۡلَدَهٗ ٣ﶔ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّا لَيُنۣۡبَذَنَّ فِي الۡحُطَمَةِ ٤ﶛ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا الۡحُطَمَةُ ٥ﶠ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ اللّٰهِ الۡمُوۡقَدَةُ ٦ﶫ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "الَّتِيۡ تَطَّلِعُ عَلَي الۡاَفۡـِٕدَةِ ٧ﶠ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "اِنَّهَا عَلَيۡهِمۡ مُّؤۡصَدَةٌ ٨ﶫ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِيۡ عَمَدٍ مُّمَدَّدَةٍ ٩ﶒ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "اَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِاَصۡحٰبِ الۡفِيۡلِ ١ﶠ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "اَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِيۡ تَضۡلِيۡلٍ ٢ﶫ"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَّاَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا اَبَابِيۡلَ ٣ﶫ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرۡمِيۡهِمۡ بِحِجَارَةٍ مِّنۡ سِجِّيۡلٍ ٤ﶫ"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمۡ كَعَصۡفٍ مَّاۡكُوۡلٍ ٥ﶒ"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لِاِيۡلٰفِ قُرَيۡشٍ ١ﶫ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "اٖلٰفِهِمۡ رِحۡلَةَ الشِّتَآءِ وَالصَّيۡفِ ٢ﶔ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَلۡيَعۡبُدُوۡا رَبَّ هٰذَا الۡبَيۡتِ ٣ﶫ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "الَّذِيۡ٘ اَطۡعَمَهُمۡ مِّنۡ جُوۡعٍﵿ وَّاٰمَنَهُمۡ مِّنۡ خَوۡفٍ ٤ﶒ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "اَرَءَيۡتَ الَّذِيۡ يُكَذِّبُ بِالدِّيۡنِ ١ﶠ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذٰلِكَ الَّذِيۡ يَدُعُّ الۡيَتِيۡمَ ٢ﶫ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا يَحُضُّ عَلٰي طَعَامِ الۡمِسۡكِيۡنِ ٣ﶠ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَيۡلٌ لِّلۡمُصَلِّيۡنَ ٤ﶫ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "الَّذِيۡنَ هُمۡ عَنۡ صَلَاتِهِمۡ سَاهُوۡنَ ٥ﶫ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "الَّذِيۡنَ هُمۡ يُرَآءُوۡنَ ٦ﶫ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَيَمۡنَعُوۡنَ الۡمَاعُوۡنَ ٧ﶒ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "اِنَّا٘ اَعۡطَيۡنٰكَ الۡكَوۡثَرَ ١ﶠ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَانۡحَرۡ ٢ﶠ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "اِنَّ شَانِئَكَ هُوَ الۡاَبۡتَرُ ٣ﶒ"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قُلۡ يٰ٘اَيُّهَا الۡكٰفِرُوۡنَ ١ﶫ"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لَا٘ اَعۡبُدُ مَا تَعۡبُدُوۡنَ ٢ﶫ"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "وَلَا٘ اَنۡتُمۡ عٰبِدُوۡنَ مَا٘ اَعۡبُدُ ٣ﶔ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "وَلَا٘ اَنَا عَابِدٌ مَّا عَبَدۡتُّمۡ ٤ﶫ"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "وَلَا٘ اَنۡتُمۡ عٰبِدُوۡنَ مَا٘ اَعۡبُدُ ٥ﶠ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لَكُمۡ دِيۡنُكُمۡ وَلِيَ دِيۡنِ ٦ﶒ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "اِذَا جَآءَ نَصۡرُ اللّٰهِ وَالۡفَتۡحُ ١ﶫ"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "وَرَاَيۡتَ النَّاسَ يَدۡخُلُوۡنَ فِيۡ دِيۡنِ اللّٰهِ اَفۡوَاجًا ٢ﶫ"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فَسَبِّحۡ بِحَمۡدِ رَبِّكَ وَاسۡتَغۡفِرۡهُﵫ اِنَّهٗ كَانَ تَوَّابًا ٣ﶒ"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتۡ يَدَا٘ اَبِيۡ لَهَبٍ وَّتَبَّ ١ﶠ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَا٘ اَغۡنٰي عَنۡهُ مَالُهٗ وَمَا كَسَبَ ٢ﶠ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَيَصۡلٰي نَارًا ذَاتَ لَهَبٍ ٣ﶫ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَّامۡرَاَتُهٗﵧ حَمَّالَةَ الۡحَطَبِ ٤ﶔ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِيۡ جِيۡدِهَا حَبۡلٌ مِّنۡ مَّسَدٍ ٥ﶒ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قُلۡ هُوَ اللّٰهُ اَحَدٌ ١ﶔ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اَللّٰهُ الصَّمَدُ ٢ﶔ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لَمۡ يَلِدۡﵿ وَلَمۡ يُوۡلَدۡ ٣ﶫ"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "وَلَمۡ يَكُنۡ لَّهٗ كُفُوًا اَحَدٌ ٤ﶒ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلۡ اَعُوۡذُ بِرَبِّ الۡفَلَقِ ١ﶫ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِنۡ شَرِّ مَا خَلَقَ ٢ﶫ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِنۡ شَرِّ غَاسِقٍ اِذَا وَقَبَ ٣ﶫ"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِنۡ شَرِّ النَّفّٰثٰتِ فِي الۡعُقَدِ ٤ﶫ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِنۡ شَرِّ حَاسِدٍ اِذَا حَسَدَ ٥ﶒ"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قُلۡ اَعُوۡذُ بِرَبِّ النَّاسِ ١ﶫ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ النَّاسِ ٢ﶫ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "اِلٰهِ النَّاسِ ٣ﶫ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِنۡ شَرِّ الۡوَسۡوَاسِﵿ الۡخَنَّاسِ ٤ﶫ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "الَّذِيۡ يُوَسۡوِسُ فِيۡ صُدُوۡرِ النَّاسِ ٥ﶫ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ الۡجِنَّةِ وَالنَّاسِ ٦ﶒ"
        }
    ]
}