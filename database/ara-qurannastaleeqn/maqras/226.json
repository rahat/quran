{
    "maqras": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "نٓ وَالۡقَلَمِ وَمَا يَسۡطُرُوۡنَ ١ﶫ"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "مَا٘ اَنۡتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُوۡنٍ ٢ﶔ"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "وَاِنَّ لَكَ لَاَجۡرًا غَيۡرَ مَمۡنُوۡنٍ ٣ﶔ"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "وَاِنَّكَ لَعَلٰي خُلُقٍ عَظِيۡمٍ ٤"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "فَسَتُبۡصِرُ وَيُبۡصِرُوۡنَ ٥ﶫ"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "بِاَيِّىكُمُ الۡمَفۡتُوۡنُ ٦"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "اِنَّ رَبَّكَ هُوَ اَعۡلَمُ بِمَنۡ ضَلَّ عَنۡ سَبِيۡلِهٖﵣ وَهُوَ اَعۡلَمُ بِالۡمُهۡتَدِيۡنَ ٧"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "فَلَا تُطِعِ الۡمُكَذِّبِيۡنَ ٨"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "وَدُّوۡا لَوۡ تُدۡهِنُ فَيُدۡهِنُوۡنَ ٩"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "وَلَا تُطِعۡ كُلَّ حَلَّافٍ مَّهِيۡنٍ ١٠ﶫ"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "هَمَّازٍ مَّشَّآءٍۣ بِنَمِيۡمٍ ١١ﶫ"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "مَّنَّاعٍ لِّلۡخَيۡرِ مُعۡتَدٍ اَثِيۡمٍ ١٢ﶫ"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "عُتُلٍّۣ بَعۡدَ ذٰلِكَ زَنِيۡمٍ ١٣ﶫ"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "اَنۡ كَانَ ذَا مَالٍ وَّبَنِيۡنَ ١٤ﶠ"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "اِذَا تُتۡلٰي عَلَيۡهِ اٰيٰتُنَا قَالَ اَسَاطِيۡرُ الۡاَوَّلِيۡنَ ١٥"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "سَنَسِمُهٗ عَلَي الۡخُرۡطُوۡمِ ١٦"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "اِنَّا بَلَوۡنٰهُمۡ كَمَا بَلَوۡنَا٘ اَصۡحٰبَ الۡجَنَّةِﵐ اِذۡ اَقۡسَمُوۡا لَيَصۡرِمُنَّهَا مُصۡبِحِيۡنَ ١٧ﶫ"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "وَلَا يَسۡتَثۡنُوۡنَ ١٨"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "فَطَافَ عَلَيۡهَا طَآئِفٌ مِّنۡ رَّبِّكَ وَهُمۡ نَآئِمُوۡنَ ١٩"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "فَاَصۡبَحَتۡ كَالصَّرِيۡمِ ٢٠ﶫ"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "فَتَنَادَوۡا مُصۡبِحِيۡنَ ٢١ﶫ"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "اَنِ اغۡدُوۡا عَلٰي حَرۡثِكُمۡ اِنۡ كُنۡتُمۡ صٰرِمِيۡنَ ٢٢"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "فَانۡطَلَقُوۡا وَهُمۡ يَتَخَافَتُوۡنَ ٢٣ﶫ"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "اَنۡ لَّا يَدۡخُلَنَّهَا الۡيَوۡمَ عَلَيۡكُمۡ مِّسۡكِيۡنٌ ٢٤ﶫ"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "وَّغَدَوۡا عَلٰي حَرۡدٍ قٰدِرِيۡنَ ٢٥"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "فَلَمَّا رَاَوۡهَا قَالُوۡ٘ا اِنَّا لَضَآلُّوۡنَ ٢٦ﶫ"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "بَلۡ نَحۡنُ مَحۡرُوۡمُوۡنَ ٢٧"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "قَالَ اَوۡسَطُهُمۡ اَلَمۡ اَقُلۡ لَّكُمۡ لَوۡلَا تُسَبِّحُوۡنَ ٢٨"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "قَالُوۡا سُبۡحٰنَ رَبِّنَا٘ اِنَّا كُنَّا ظٰلِمِيۡنَ ٢٩"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "فَاَقۡبَلَ بَعۡضُهُمۡ عَلٰي بَعۡضٍ يَّتَلَاوَمُوۡنَ ٣٠"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "قَالُوۡا يٰوَيۡلَنَا٘ اِنَّا كُنَّا طٰغِيۡنَ ٣١"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "عَسٰي رَبُّنَا٘ اَنۡ يُّبۡدِلَنَا خَيۡرًا مِّنۡهَا٘ اِنَّا٘ اِلٰي رَبِّنَا رٰغِبُوۡنَ ٣٢"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "كَذٰلِكَ الۡعَذَابُﵧ وَلَعَذَابُ الۡاٰخِرَةِ اَكۡبَرُﶉ لَوۡ كَانُوۡا يَعۡلَمُوۡنَ ٣٣ﶒ"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "اِنَّ لِلۡمُتَّقِيۡنَ عِنۡدَ رَبِّهِمۡ جَنّٰتِ النَّعِيۡمِ ٣٤"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "اَفَنَجۡعَلُ الۡمُسۡلِمِيۡنَ كَالۡمُجۡرِمِيۡنَ ٣٥ﶠ"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "مَا لَكُمۡﵴ كَيۡفَ تَحۡكُمُوۡنَ ٣٦ﶔ"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "اَمۡ لَكُمۡ كِتٰبٌ فِيۡهِ تَدۡرُسُوۡنَ ٣٧ﶫ"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "اِنَّ لَكُمۡ فِيۡهِ لَمَا تَخَيَّرُوۡنَ ٣٨ﶔ"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "اَمۡ لَكُمۡ اَيۡمَانٌ عَلَيۡنَا بَالِغَةٌ اِلٰي يَوۡمِ الۡقِيٰمَةِﶈ اِنَّ لَكُمۡ لَمَا تَحۡكُمُوۡنَ ٣٩ﶔ"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "سَلۡهُمۡ اَيُّهُمۡ بِذٰلِكَ زَعِيۡمٌ ٤٠ﶕ"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "اَمۡ لَهُمۡ شُرَكَآءُﵑ فَلۡيَاۡتُوۡا بِشُرَكَآئِهِمۡ اِنۡ كَانُوۡا صٰدِقِيۡنَ ٤١"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "يَوۡمَ يُكۡشَفُ عَنۡ سَاقٍ وَّيُدۡعَوۡنَ اِلَي السُّجُوۡدِ فَلَا يَسۡتَطِيۡعُوۡنَ ٤٢ﶫ"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "خَاشِعَةً اَبۡصَارُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٌﵧ وَقَدۡ كَانُوۡا يُدۡعَوۡنَ اِلَي السُّجُوۡدِ وَهُمۡ سٰلِمُوۡنَ ٤٣"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "فَذَرۡنِيۡ وَمَنۡ يُّكَذِّبُ بِهٰذَا الۡحَدِيۡثِﵧ سَنَسۡتَدۡرِجُهُمۡ مِّنۡ حَيۡثُ لَا يَعۡلَمُوۡنَ ٤٤ﶫ"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "وَاُمۡلِيۡ لَهُمۡﵧ اِنَّ كَيۡدِيۡ مَتِيۡنٌ ٤٥"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "اَمۡ تَسۡـَٔلُهُمۡ اَجۡرًا فَهُمۡ مِّنۡ مَّغۡرَمٍ مُّثۡقَلُوۡنَ ٤٦ﶔ"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "اَمۡ عِنۡدَهُمُ الۡغَيۡبُ فَهُمۡ يَكۡتُبُوۡنَ ٤٧"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "فَاصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تَكُنۡ كَصَاحِبِ الۡحُوۡتِﶉ اِذۡ نَادٰي وَهُوَ مَكۡظُوۡمٌ ٤٨ﶠ"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "لَوۡلَا٘ اَنۡ تَدٰرَكَهٗ نِعۡمَةٌ مِّنۡ رَّبِّهٖ لَنُبِذَ بِالۡعَرَآءِ وَهُوَ مَذۡمُوۡمٌ ٤٩"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "فَاجۡتَبٰىهُ رَبُّهٗ فَجَعَلَهٗ مِنَ الصّٰلِحِيۡنَ ٥٠"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "وَاِنۡ يَّكَادُ الَّذِيۡنَ كَفَرُوۡا لَيُزۡلِقُوۡنَكَ بِاَبۡصَارِهِمۡ لَمَّا سَمِعُوا الذِّكۡرَ وَيَقُوۡلُوۡنَ اِنَّهٗ لَمَجۡنُوۡنٌ ٥١ﶭ"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "وَمَا هُوَ اِلَّا ذِكۡرٌ لِّلۡعٰلَمِيۡنَ ٥٢ﶒ"
        }
    ]
}