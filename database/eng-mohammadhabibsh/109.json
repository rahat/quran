        {
            "chapter": 109,
            "verse": 1,
            "text": "Say: O unbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not serve that which you serve"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nor do you serve Him Whom I serve"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Nor am I going to serve that which you serve"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nor are you going to serve Him Whom I serve"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "You shall have your religion and I shall have my religion"
        }
    ]
