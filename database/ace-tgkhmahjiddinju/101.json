{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Uroe Qariʻah uroe kiamat"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Tapeugah siat peue roh Qariʻah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Na roe tateupue peue Qariʻah nyan"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Insan watee nyan hanaban peugah Meusiseu bandum meuhambo yoh nyan Ka lagee bambang dalam blang luah"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Buket ngon gunong sang bulee gapeuh Lagee ka geupruh meuhambo sudah"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Teuma barangsoe nyang brat timbangan"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Seunang that gopnyan udep lam mewah"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Teuma barangsoe nyang phui timbangan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Teumpat awak nyan dalam hawiyah"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Na roe tateupue peue hawiyah nyan"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Tutong hanaban nuraka juah"
        }
    ]
}