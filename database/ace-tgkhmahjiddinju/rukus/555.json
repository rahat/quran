{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Takheun lon lakee lindong bak Tuhan Po seukalian makheuluk Allah"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Nibak buet jeuheut makheuluk Tuhan"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Lom jeuheut malam ‘oh seupot sudah"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Lom jeuheut inong nyang pruh-pruh neukue ‘Oh lheuh jikue-kue teuma jirajah"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Lom jeuheut ureueng nyang pubuet deungki Watee jideungki keu hamba Allah"
        }
    ]
}