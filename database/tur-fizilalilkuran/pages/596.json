{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak bedbaht kimse girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "O ki yalanladı ve döndü"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En çok korkan ondan uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki malını Allah rızası için vererek arınır, yücelir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "O yaptığı iyiliği birinden karşılık görmek için yapmaz"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ancak yüce Rabbinin hoşnutluğunu gözeterek yapar"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette kendisi de hoşnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine andolusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Durgunlaşan geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Andolsun senin için ahiret dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Şaşırmış bulup da yol göstermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Fakir iken seni zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yetime gelince sakın onu üzme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksula gelince sakın onu azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ey Muhammed! Senin göğsünü açmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yükünü üzerinden almadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ki o belini bükmüştü"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin şanını yüceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Muhakkak ki her güçlükle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gerçekten güçlükle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Öyleyse bir işi bitirince diğerine giriş"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ümit edeceğini Rabbinden iste"
        }
    ]
}