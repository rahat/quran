        {
            "chapter": 86,
            "verse": 1,
            "text": "Göğe ve târıka and olsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Târıkın ne olduğunu bilir misin sen"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O karanlığı delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hiçbir can yoktur ki başında bir koruyucu olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Öyleyse insan neden yaratıldığına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Fışkıran su damlacığından yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel ile kaburga kemikleri arasından çıkan"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah onu tekrar yaratmaya kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizli işlerin ortaya çıkarıldığı günde"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun hiçbir gücü ve hiçbir yardımcısı olmaz"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yağmurun sahibi göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Bitkinin yeşerdiği yere andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Şüphesiz Kur´an kesin bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O saçma bir söz değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar bir tuzak kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen kâfirlere mühlet ver. Onlara biraz zaman tanı"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Rabbinin yüce adını takdis et"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "O yaratan ve düzeltendir"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ölçüleri belirleyip yolunu gösterendir"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Yemyeşil meraları bitirendir"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sonra da onları kupkuru çöpe çevirendir"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ey Muhammed! Sana Kur´an´ı biz okutacağız ve asla unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Yalnız Allah´ın dilediği başka. O açığı da bilir gizliyi de"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Seni en kolay yolu tutmağa muvaffak edeceğiz"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "O halde hatırlatmak fayda verirse hatırlat"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Allah´tan korkan, öğüt alır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Bedbaht olan ondan kaçacaktır"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "O en büyük ateşe yaslanacaktır"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sonra onun içinde ne ölür ne de yaşar"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Doğrusu mutluluğa ermiştir arınan"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin adını anıp namaz kılan"
        }
    ]
