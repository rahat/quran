        {
            "chapter": 80,
            "verse": 1,
            "text": "Surat astı ve döndü"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Yanına âma geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bileceksin sen belki o arınacak"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut öğüt alacak da bu öğüt, kendisine fayda verecek"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kendisini yeterli görüp tenezzül etmeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen onunla ilgileniyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun arınmamasından sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Fakat koşarak sana gelene"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Allah´tan sakınarak gelmişken"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen onunla ilgilenmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Asla olmaz böyle şey! Kur´an ayetleri birer hatırlatmadır öğüttür"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen onu düşünüp öğüt alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Sahifeler içindedirler, değerli, şanslı"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yükseltilen ve tertemiz tutulan (sahifeler)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Taşıyıcıların ellerindedirler"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(Allah´a göre) değerli ve çok iyi (yazıcı ve taşıyıcıların)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası insan ne kadar da nankördür"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Allah onu hangi şeyden yarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Nutfe (sperm)den. Onu yarattı ve ona biçim verdi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolu kolaylaştırmıştır"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu öldürdü, kabre koydurdu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra dilediği zaman onu yeniden diriltti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır, insan hala Allah´ın kendisine emrettiğini yapmadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "İnsan yiyeceğine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "O suyu döktükçe döktük"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra toprağı güzelce yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Orada bitirdik, taneleri"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Üzümler, yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinler, hurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "İri ve sık ağaçlı bahçeler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler ve çayırlar"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın yararına"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kulakları sağır edercesine yüksek o gürültü geldiği zaman"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "İşte o gün kişi kaçar, kardeşinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından, babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Eşinden ve oğullarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gün herkesin başından aşkın işi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Bazı yüzler o gün parıl parıldır"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Güleç ve sevinçli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Bazı yüzler o gün tozlanmış"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Karanlıklar bürümüştür onları"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "İşte onlar hayasız pis kafirlerdir"
        }
