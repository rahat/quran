            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kâfirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin taptıklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim taptığıma tapmazsınız"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben sizin taptıklarınıza tapacak değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Sizler de benim taptığıma tapacak değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
