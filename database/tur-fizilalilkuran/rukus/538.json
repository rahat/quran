{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Yaratan Rabbinin adıyla oku"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "O, insanı bir kan pıhtısından yarattı"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku, Rabbin en büyük kerem sahibidir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "O, insana kalemle yazmayı öğretti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "İnsana bilmediğini öğretti"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır insan azar"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendini zengin gördüğü için"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Dönüş Rabbinedir"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Gördün mü şu men edeni"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Namaz kılarken bir kulu"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gördün mü, ya o kul doğru yolda ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Yahut kötülüklerden sakınmayı emrederse"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gördün mü, ya bu adam yalanlar, yüz çevirirse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "O, Allah´ın gördüğünü bilmiyor mu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Hayır eğer bundan vazgeçmezse onu perçeminden yakalarız"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "O yalancı günahkar perçeminden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "O zaman gitsin de taraftarlarını çağırsın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz de zebanileri çağıracağız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hayır ona boyun eğme. Rabbine secde et ve yaklaş"
        }
    ]
}