{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki; «Sığınırım ben, karanlığı yarıp sabahı ortaya çıkaran Rabbime"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlığı çöktüğü zaman gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Düğümlere üfleyip tüküren büyücü kadınların şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ve hased ettiği zaman hasedcinin şerrinden.»"
        }
    ]
