{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "İnsanları dilleri ile arkalarından çekiştiren ve karşılarında kaş, göz hareketleri ile onları aşağılayan herkese yazıklar olsun"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Malı toplayıp onu teker teker sayana"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının kendisini ölümsüzleştireceğini zannedene yazıklar olsun"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır. O kırıp geçen yere atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "O kırıp geçenin ne olduğunu bilir misin sen"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah´ın tutuşturulmuş ateşidir o"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Tırmanıp yüreklerin üstüne çıkan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Cehennem onların üzerine kapatılacaktır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzun sütunlar içinde"
        }
    ]
}