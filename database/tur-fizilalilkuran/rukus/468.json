{
    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin huzuruna çıkacağı andan korkanlara cennette bir konut verilecektir"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Bu cennet konutlarının bahçeleri sık dallı ağaçlarla kaplıdır"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Bu iki konutta birer pınar akmaktadır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Bu konutların bahçelerindeki ağaçlarda her meyvanın iki çeşidi vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Bu konutlarda ağırlananlar astarları yaldızlı atlastan minderlere yaslanırlar. Her iki konutun bahçelerindeki ağaçların meyvaları yere yakındır, kolayca devşirilebilirler"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Bu konutlarda gözleri erkeklerinden başkasını görmeyen, daha önce ne insan ve ne de cin kökenli bir erkeğin, el değdirmediği eşler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "O eşler sanki birer yakut ve mercandırlar"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "İyiliğin, iyilikten başka bir karşılığı olabilir mi"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu iki cennet konutunda ali düzeyde iki cennet konutu daha vardır"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Bu konutların renkleri koyu yeşildir"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "İki konutta sürekli kaynayan iki pınar vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "İki konutun bahçelerinde de çeşitli meyva, hurma ve nar ağaçları vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "O konutlarda iyi huylu, güzel kadınlar vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "O kadınlar ceylan gözlüdürler ve çadırlarının dışına hiç çıkmazlar"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha önce onlara ne cin ne insan kökenli hiçbir erkeğin eli değmemiştir"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Bu konutlarda ağırlananlar yeşil yastıklara ve güzel işlemeli minderlere yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Peki, Rabbinizin hangi nimetini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Kerem sahibi, ulu Rabbinin adı ne yücedir"
        }
    ]
}