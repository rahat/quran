{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun Allah yolunda koştukça koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Andolsun kıvılcımlar saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah akşam akına çıkanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ve tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Düşman topluluğu içine dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan Rabbine karşı çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna şahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Doğrusu o, malı çok sever"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi o, kabirlerde olanlar dışarı atıldığı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerde olanlar ortaya konulduğu zaman"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Doğrusu Rabbleri o gün herşeyinden haberdardır"
        }
    ]
}