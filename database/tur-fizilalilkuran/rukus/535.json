{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine andolusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Durgunlaşan geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Andolsun senin için ahiret dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Şaşırmış bulup da yol göstermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Fakir iken seni zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yetime gelince sakın onu üzme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksula gelince sakın onu azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        }
    ]
}