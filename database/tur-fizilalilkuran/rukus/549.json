{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalanlayanı gördün mü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "İşte o, öksüzü iter, kakar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulu doyurmaya önayak olmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay, o namaz kılanların haline ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onlar kıldıkları namazdan gafildirler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar gösteriş yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "En ufak bir yardımı esirgerler"
        }
    ]
}