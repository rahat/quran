{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun incir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Andolsun sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Andolsun bu güvenli Mekke şehrine"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel bir şekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu aşağıların en aşağısı kıldık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız inanan iyi işler yapanlar hariç. Onlar için kesintisiz bir mükafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ey insan! Öyleyken sana dini yalan saydırtan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hükmedenlerin en güzel hükmedeni değil midir"
        }
    ]
}