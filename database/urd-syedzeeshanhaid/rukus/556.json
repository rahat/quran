{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "اے رسول کہہ دیجئے کہ میں انسانوں کے پروردگار کی پناہ چاہتا ہوں"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "جو تمام لوگوں کا مالک اور بادشاہ ہے"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "سارے انسانوں کا معبود ہے"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "شیطانی وسواس کے شر سے جو نام خدا سن کر پیچھے ہٹ جاتا ہے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "اور جو لوگوں کے دلوں میں وسوسے پیدا کراتا ہے"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "وہ جنات میں سے ہو یا انسانوں میں سے"
        }
    ]
}