{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "رات کی قسم جب وہ دن کو ڈھانپ لے"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "اور دن کی قسم جب وہ چمک جائے"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "اور اس کی قسم جس نے مرد اور عورت کو پیدا کیا ہے"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "بے شک تمہاری کوششیں مختلف قسم کی ہیں"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "پھر جس نے مال عطا کیا اور تقویٰ اختیار کیا"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "اور نیکی کی تصدیق کی"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "تو اس کے لئے ہم آسانی کا انتظام کردیں گے"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "اور جس نے بخل کیا اور لاپروائی برتی"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "اور نیکی کو جھٹلایا ہے"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "اس کے لئے سختی کی راہ ہموار کردیں گے"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "اور اس کا مال کچھ کام نہ آئے گا جب وہ ہلاک ہوجائے گا"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "بے شک ہدایت کی ذمہ داری ہمارے اوپر ہے"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "اور دنیا و آخرت کا اختیار ہمارے ہاتھوں میں ہے"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "تو ہم نے تمہیں بھڑکتی ہوئی آگ سے ڈرایا"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "جس میں کوئی نہ جائے گا سوائے بدبخت شخص کے"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "جس نے جھٹلایا اور منھ پھیرلیا"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "اور اس سے عنقریب صاحبِ تقویٰ کو محفوظ رکھا جائے گا"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "جو اپنے مال کو دے کر پاکیزگی کا اہتمام کرتا ہے"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "جب کہ اس کے پاس کسی کا کوئی احسان نہیں ہے جس کی جزا دی جائے"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "سوائے یہ کہ وہ خدائے بزرگ کی مرضی کا طلبگار ہے"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "اور عنقریب وہ راضی ہوجائے گا"
        }
    ]
}