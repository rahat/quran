{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "ہوشیار ہمیں چاند کی قسم"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "اور جاتی ہوئی رات کی قسم"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "اور روشن صبح کی قسم"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "یہ جہنمّ بڑی چیزوں میں سے ایک چیز ہے"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "لوگوں کے ڈرانے کا ذریعہ"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "ان کے لئے جو آگے پیچھے ہٹنا چاہیں"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "ہر نفس اپنے اعمال میں گرفتار ہے"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "علاوہ اصحاب یمین کے"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "وہ جنتوں میں رہ کر آپس میں سوال کررہے ہوں گے"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "مجرمین کے بارے میں"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "آخر تمہیں کس چیز نے جہنمّ میں پہنچادیا ہے"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "وہ کہیں گے کہ ہم نماز گزار نہیں تھے"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "اور مسکین کو کھانا نہیں کھلایا کرتے تھے"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "لوگوں کے بفِے کاموں میں شامل ہوجایا کرتے تھے"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "اور روزِ قیامت کی تکذیب کیا کرتے تھے"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "یہاں تک کہ ہمیں موت آگئی"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "تو انہیں سفارش کرنے والوں کی سفارش بھی کوئی فائدہ نہ پہنچائے گی"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "آخر انہیں کیا ہوگیا ہے کہ یہ نصیحت سے منہ موڑے ہوئے ہیں"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "گویا بھڑکے ہوئے گدھے ہیں"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "جو شیر سے بھاگ رہے ہیں"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "حقیقتا ان میں ہر آدمی اس بات کا خواہش مند ہے کہ اسے کِھلی ہوئی کتابیں عطا کردی جائیں"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "ہرگز نہیں ہوسکتا اصل یہ ہے کہ انہیں آخرت کا خوف ہی نہیں ہے"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "ہاں ہاں بیشک یہ سراسر نصیحت ہے"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "اب جس کا جی چاہے اسے یاد رکھے"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "اور یہ اسے یاد نہ کریں گے مگر یہ کہ اللہ ہی چاہے کہ وہی ڈرانے کا اہل اور مغفرت کا مالک ہے"
        }
    ]
}