{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "کھڑکھڑانے والی"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "اور کیسی کھڑکھڑانے والی"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "اور تمہیں کیا معلوم کہ وہ کیسی کھڑکھڑانے والی ہے"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "جس دن لوگ بکھرے ہوئے پتنگوں کی مانند ہوجائیں گے"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "اور پہاڑ دھنکی ہوئی روئی کی طرح اُڑنے لگیں گے"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "تو اس دن جس کی نیکیوں کا پلّہ بھاری ہوگا"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "وہ پسندیدہ عیش میں ہوگا"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "اور جس کا پلہّ ہلکا ہوگا"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "اس کا مرکز ہاویہ ہے"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "اور تم کیا جانو کہ ہاویہ کیا مصیبت ہے"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "یہ ایک دہکی ہوئی آگ ہے"
        }
    ]
}