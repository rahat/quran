{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "آسمان اور رات کو آنے والے کی قسم"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "اور تم کیا جانو کہ طارق کیا ہے"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "یہ ایک چمکتا ہوا ستارہ ہے"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "کوئی نفس ایسا نہیں ہے جس کے اوپر نگراں نہ معین کیا گیا ہو"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "پھر انسان دیکھے کہ اسے کس چیز سے پیدا کیا گیا ہے"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "وہ ایک اُچھلتے ہوئے پانی سے پیدا کیا گیا ہے"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "جو پیٹھ اور سینہ کی ہڈیوں کے درمیان سے نکلتا ہے"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "یقینا وہ خدا انسان کے دوبارہ پیدا کرنے پر بھی قادر ہے"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "z جس دن رازوں کو آزمایا جائے گا"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "تو پھر نہ کسی کے پاس قوت ہوگی اور نہ مددگار"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "قسم ہے چکر کھانے والے آسمان کی"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "اور شگافتہ ہونے والی زمین کی"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "بے شک یہ قول فیصل ہے"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "اور مذاق نہیں ہے"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "یہ لوگ اپنا مکر کررہے ہیں"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "اور ہم اپنی تدبیر کررہے ہیں"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "تو کافروں کو چھوڑ دو اور انہیں تھوڑی سی مہلت دے دو"
        }
    ]
}