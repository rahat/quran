{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "تمہیں باہمی مقابلہ کثرت مال و اولاد نے غافل بنادیا"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "یہاں تک کہ تم نے قبروں سے ملاقات کرلی"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "دیکھو تمہیں عنقریب معلوم ہوجائے گا"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "اور پھر خوب معلوم ہوجائے گا"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "دیکھو اگر تمہیں یقینی علم ہوجاتا"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "کہ تم جہنمّ کو ضرور دیکھو گے"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "پھر اسے اپنی آنکھوں دیکھے یقین کی طرح دیکھو گے"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "اور پھر تم سے اس دن نعمت کے بارے میں سوال کیا جائے گا"
        }
    ]
}