{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "اے رسول کہہ دیجئے کہ وہ اللہ ایک ہے"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اللہ برحق اور بے نیاز ہے"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "اس کی نہ کوئی اولاد ہے اور نہ والد"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "اور نہ اس کا کوئی کفو اور ہمسر ہے"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "کہہ دیجئے کہ میں صبح کے مالک کی پناہ چاہتا ہوں"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "تمام مخلوقات کے شر سے"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "اور اندھیری رات کے شر سے جب اس کا اندھیرا چھا جائے"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "اور گنڈوں پر پھونکنے والیوں کے شر سے"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "اور ہرحسد کرنے والے کے شر سے جب بھی وہ حسد کرے"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "اے رسول کہہ دیجئے کہ میں انسانوں کے پروردگار کی پناہ چاہتا ہوں"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "جو تمام لوگوں کا مالک اور بادشاہ ہے"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "سارے انسانوں کا معبود ہے"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "شیطانی وسواس کے شر سے جو نام خدا سن کر پیچھے ہٹ جاتا ہے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "اور جو لوگوں کے دلوں میں وسوسے پیدا کراتا ہے"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "وہ جنات میں سے ہو یا انسانوں میں سے"
        }
    ]
}