{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Při pádících, odfrkujících"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "jiskry vykřesávajících"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "za jitra se vynořujících"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "oblaka prachu v něm zviřujících"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "a do středu tlupy vtrhujících"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Vůči svému Pánu člověk vskutku nevděčný je"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "a dokonce sám to dosvědčuje"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Horlivý však je věru v lásce k majetku"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Cožpak neví, že až bude to, co je v hrobech, převráceno"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a to, co v hrudích je, odhaleno"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Pán jejich v ten den dokonale o nich bude zpraven"
        }
    ]
}