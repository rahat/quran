{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "On almužnu nedával ani modlitbu nekonal"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "však odvracel se a za lež prohlašoval"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "a pak ke své rodině pyšně kráčeje se odebral"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Ach běda ti, běda"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "A znovu běda ti, běda"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Myslí si člověk snad, že sám sobě bude zůstaven"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Což nebyl kapkou semene vystříknutého jen"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Potom se kapkou přilnavou stal a Bůh jej stvořil a vyrovnal"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "a dvě pohlaví, mužské a ženské, z něho udělal"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Což není v moci jeho, aby mrtvým opět život dal"
        }
    ]
}