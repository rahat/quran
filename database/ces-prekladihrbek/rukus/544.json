{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Vy rádi v rozmnožování jmění soupeříte"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "pokud hroby nenavštívíte"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Však pozor! Brzy poznáte"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu pozor! Brzy poznáte"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Však pozor! Kéž zřetelným poznáním poznáte"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "a věru oheň planoucí uzříte"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "a potom vskutku okem jistým jej spatříte"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "a posléze v ten den na blahobyt tázáni budete"
        }
    ]
}