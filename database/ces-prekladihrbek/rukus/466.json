{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milosrdný"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Koránu naučil"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "člověka stvořil"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "vysvětlení jej naučil"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Slunce i měsíc své počty obsahují"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "a traviny a stromy se před ním sklánějí"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Nebe On vyzvedl a váhy stanovil"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "na váze nepodvádějte"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "spravedlivě ji stanovte a na váhách neubírejte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "A zemi, tu pro lidstvo rozprostřel"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na ní ovoce a palmy s obaly plodů zrají"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i obilí na stéblech a rostliny, jež vůni libou mají"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "A člověka z hlíny podobné hrnčířské stvořil"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a džiny z plamene bezdýmného vytvořil"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "On Pánem je obou východů, i Pánem obou západů"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "A volně dal téci oběma mořím, jež se setkají"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "však mezi nimi překážka stojí, již nezdolají"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "A z obou moří se perly velké i malé loví"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Jeho jsou i lodě plující, jež na moři ční jak znamení"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Které z dobrodiní Pána svého můžete popírat"
        }
    ]
}