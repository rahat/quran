{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Při denním jasu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a při noci, když je tichá"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pán tvůj tě neopustil ani tebou nepohrdá"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A bude pro tebe poslední věru lepší než první"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Pán tvůj tě zajisté obdaří k tvému uspokojení"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Což nenašel tě jako sirotka - a v útulek tě nepřijal"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Bloudícím tě nalezl - a správnou cestu ti ukázal"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "nuzákem tě nalezl - a bohatství ti věru dal"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Co sirotka se týče, toho neutiskuj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "co žebráka se týče, toho neodstrkuj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "však o dobrodiní Pána svého jiným vypravuj"
        }
    ]
}