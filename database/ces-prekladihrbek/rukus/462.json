{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Co soudíš o tom, jenž zády se obrací"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "a málo jen rozdává a lakotí"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Má snad vědomost o nepoznatelném, takže je vidí"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Či nebylo mu sděleno, co obsahují svitky Mojžíše"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "a Abrahama, jenž byl věrný"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Že totiž žádná duše obtížená břímě jiné duše neponese"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "že člověku se jen toho, oč usiloval, dostane"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "že výsledek jeho úsilí bude viděn"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "a potom plnou odměnou bude odměněn"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "že u Pána tvého je konečný cíl jen"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "že On rozesmává i rozplakává"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "že On smrt i život dává"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "že On stvořil pohlaví mužské a ženské"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "z kapky semene vystříknuté"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "že Jemu přísluší stvoření druhé"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "že On obohacuje a uspokojuje"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "že On je ten, kdo Siria pánem je"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "že On dávným ´Ádovcům zahynout dal"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "i Thamúdovcům, a žádného nenechal"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "a před nimi lidu Noemovu, neb to lid velmi nespravedlivý a vzpurný byl"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "a také Vyvrácené město zahubil"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "a pokrylo je to, co je pokrylo"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "A které z dobrodiní Pána svého chceš popírat"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "A toto je varování podobné varováním dřívějších"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Přiblížila se blížící"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "a není kromě Boha nikoho, kdo by ji vyjevil"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Tento příběh vás snad údivem naplnil"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "že místo abyste plakali, se smějete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "a jen v zábavách dlíte"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Před Bohem na zem padejte a Jeho uctívejte"
        }
    ]
}