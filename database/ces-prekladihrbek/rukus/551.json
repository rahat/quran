{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Rci: \"Nevěřící"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Já nebudu uctívat to, co vy uctíváte"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a vy nejste ctitelé toho, co já uctívám"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "a já nejsem ctitelem toho, co vy jste uctívali"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "a vy neuctíváte to, co já uctívám"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vy máte své náboženství a já své náboženství mám"
        }
    ]
}