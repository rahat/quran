{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Však pozor! Při měsíci"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "při noci, když ustupuje"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "při úsvitu, když se rozjasňuje"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Saqar je věru jeden z trestů největších"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "jako varování smrtelníkům daný"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "těm z vás, kdož chtějí k víře vpředu jít anebo se chtějí opozdit"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Každá duše je ručitelkou toho, co si sama vysloužila"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "kromě lidi po pravici"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "kteří v zahradách vzájemně se budou ptát"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "na hříšníky"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Co přivedlo vás do Saqaru"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "odvětí: \"Nebyli jsme mezi modlitbu konajícími"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ani mezi nuzné krmícími"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "ale tlachali jsme mezi tlachajícími"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "a den soudný jsme za lež prohlásili"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "až přišla na nás tato jistota"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "A nebude jim nic platná přímluva přímluvců"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Co s nimi je, že se odvracejí od připomenutí"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "jako by byli osly splašenými"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "před lvem prchajícími"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Věru, chce každý z nich, aby svitky rozevřené mu byly přineseny"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Však pozor! Oni se naopak neobávají života budoucího"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Však pozor! Toto jest připomenutí"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "a kdo chce, ať na ně si vzpomene"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Však vzpomenou si jen tehdy, když Bůh si to bude přát a Jemu náleží, aby se Ho báli, a Jemu patří odpuštění dát"
        }
    ]
}