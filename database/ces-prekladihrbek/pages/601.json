{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Při osudu"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Věru že člověk spěje v záhubu"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Kromě těch, kdož uvěřili a zbožné skutky konali a vzájemně se k pravdě a neochvějnosti nabádali"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Běda každému pomlouvači, jenž na cti utrhá"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "který bohatství hromadí a stále je počítá"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "a že jmění jeho jej učiní nesmrtelným, se domnívá"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Pozor však, vždyť věru bude uvržen do Drtící"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Víš ty vůbec, co je to Drtící"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Boží oheň zažehnutý"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "až k srdcím šlehající"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "a nad nimi se v klenbu pojící"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "a jak sloupy protažené stojící"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Neviděls, jak naložil Pán tvůj s lidmi slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Což neobrátil úklad jejich ve zmatky"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "a neposlal na ně v hejnech ptáky"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "kteří po nich vrhali oblázky z hlíny pálené"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "a učinil, že podobnými byli slámě ohryzané"
        }
    ]
}