{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "v němž hořet bude jen ten nejbídnější"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "jenž za lež to pokládal a zády se obracel"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Však vzdálen bude od něho bohabojný"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "jenž ze jmění svého dával, aby čistým se stal"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "a dobrodiní neprokazoval jen proto, aby byl odměněn"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "leč jen z touhy po tváři Pána svého nejvyššího to konal"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A ten věru bude spokojen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Při denním jasu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a při noci, když je tichá"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pán tvůj tě neopustil ani tebou nepohrdá"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A bude pro tebe poslední věru lepší než první"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Pán tvůj tě zajisté obdaří k tvému uspokojení"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Což nenašel tě jako sirotka - a v útulek tě nepřijal"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Bloudícím tě nalezl - a správnou cestu ti ukázal"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "nuzákem tě nalezl - a bohatství ti věru dal"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Co sirotka se týče, toho neutiskuj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "co žebráka se týče, toho neodstrkuj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "však o dobrodiní Pána svého jiným vypravuj"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Což jsme ti hruď tvou neotevřeli"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "a nesňali s tebe tvé břemeno"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jež záda tvá tak tížilo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "a pověst tvou jsme do výše nezvedli"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Vždyť se strastmi i úleva přichází"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "věru že se strastmi i úleva přichází"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "A až se uvolníš, pak dál se namáhej"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "a k Pánu svému v touze spěj"
        }
    ]
}