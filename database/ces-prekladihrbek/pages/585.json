{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Zakabonil se a odvrátil"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "že slepec se na něho obrátil"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Co můžeš vědět? Snad on chtěl se očistit"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "či připomenutí vyslechnout, a prospěšné mu to mohlo být"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Však pokud o boháče jde"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "o něho ty se zajímáš"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a že neočišťuje se, nic nedbáš"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "O toho však, kdo spěchá k tobě pln snahy"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a jat z Boha obavami"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "o toho ty zájem nemáš"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Tak ne! Vždyť toto je připomenutí"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "a kdokoliv chce, nechť na ně si vzpomene"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "psané na svitcích úctyhodných"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "povznesených a očištěných"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "rukama písařů"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "vznešených, neposkvrněných"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Nechť zhyne ten člověk! Ach, jak je nevděčný"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z čeho jej Pán jeho stvořil"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z kapky semene jej stvořil a osud jeho určil"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "a pak cestu mu ulehčil"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potom zemřít jej nechá a do hrobu ho vložit dá"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "a posléze, až zachce se mu, z mrtvých vstát mu dá"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Však pozor! Člověk ještě nesplnil, co Pán jeho mu poručil"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Nechť člověk pohlédne na svou potravu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "a na to, jak spustili jsme vody záplavu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "a jak v zemi jsme dali puknout trhlinám"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "a z nich vyrůst obilovinám"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vinné révě a travinám"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "olivovníku a datlovým palmám"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "hustým zahradám"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ovoci i pastvinám"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "v užitek vašim stádům i vám"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A až nastane třesk ohlušující"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v ten den prchne člověk od bratra svého"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "matky své i otce svého"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "družky své i synů svých"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "neboť každý člověk v ten den bude mít práci sám se sebou jen"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V ten den budou tváře zjasněné"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "rozesmáté a veselé"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "v ten den budou i tváře jak prach šedivé"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "a chmurou pokryté"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to věru budou nevěřící, ti hříšníci"
        }
    ]
}