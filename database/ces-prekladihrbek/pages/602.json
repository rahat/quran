{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudržnost Kurajšovců"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "pro soudržnost jejich, když připravuji zimní a letní karavanu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "nechť slouží chrámu tohoto Pánu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenž nasytil je a ochránil před hladem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co soudíš o tom, jenž nazývá náboženství lží"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To je ten, jenž sirotka odhání"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a k nakrmení chudáka nepobízí"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Běda těm, kdož při modlení"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "modlitbu svou se nestarají"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kteří chtějí být jen viděni"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a dávat podporu odmítají"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Hojnosti, dali jsme ti věru plnou míru"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "modli se proto k svému Pánu a přinášej mu obětinu"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vždyť ten, jenž nenávidí tě, bez mužských potomků je věru"
        }
    ]
}