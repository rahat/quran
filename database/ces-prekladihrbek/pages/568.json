{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "takže zde nemá dnes druha vřelého"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "ani krom splašek pokrmu jiného"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "jež jísti budou jen těžcí hříšníci"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Hle, já přísahám při tom, co vidíte"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i při tom, co nezříte"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "že toto zajisté je řeč posla vznešeného"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Nejsou to slova básníkova - jak málo vy věříte"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "ani řeč věštcova - jak krátkou paměť máte"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "leč je to od Pána lidstva seslání"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A kdyby si on proti vůli Naší vymýšlel nějaká rčení"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "věru bychom jej vzali za jeho pravici"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a pak mu prořízli jeho srdečnici"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "a žádný z vás by tomu nemohl zabránit"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Korán vskutku je připomenutí bohabojným"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "a dobře víme, že mezi vámi jsou ti, kdo nazývají jej vylhaným"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "A věru on zármutku nevěřících je příčinou"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "a vskutku je pravdou jistotnou"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Chval tedy jméno Pána svého mocného"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Ptal se tázající na trest, jenž dopadne"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "na nevěřící: není nikoho, kdo jej odrazí"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "vždyť od Boha, Pána stupňů, přichází"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Stoupají k němu po nich andělé i duch v den, jehož délka let padesát tisíc měří"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Neochvějný buď a pěkně jen vytrvej"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Nevěřící daleko vidí ten den"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "zatímco my nablízku jej vidíme"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "V ten den nebe bude roztavené mosazi podobné"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "a hory budou jak vločky vlny barevné"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "a nebude se ptát přítel vřelý na přítele"
        }
    ]
}