{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Při nebi a hvězdě nocí putující"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Víš ty vůbec, co je to hvězda nocí putující"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hvězda pronikavě zářící"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "A věru duše každá nad sebou strážce má"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Z čeho byl stvořen, nechť člověk posoudí"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvořen byl z vystříknuté tekutiny"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jež z místa mezi bedry a žebry proudí"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Bůh zajisté je schopen k životu jej navrátit"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v den, kdy taje budou zkoumány"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "a kdy člověk nebude ani sílu, ani pomocníka mít"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Při nebi návratů plném"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "při zemi rozpuklin plné"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Věru že toto je slovo rozhodné"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "a není řečeno jen žertem"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tito lidé věru strojí úklad"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "však já rovněž chystám lest"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Jen poshov nevěřícím a krátký jim popřej odklad"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Oslavuj jméno Pána svého nejvyššího"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jenž vše stvořil a vyrovnal"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "každý osud předurčil a usměrnil"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "jenž pastviny k životu vyvolal"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom je v pěnu zčernalou přeměnil"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Naučíme tě přednášet a nezapomeneš nic z věci té"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "leda co Bůh bude chtít - a On zná zjevné i skryté"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "a ulehčíme ti přístup k věci snadné"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Připomínej, vždyť připomenutí je užitečné"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "A kdo Boha se bojí, ten na ně si vzpomene"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "však odvrátí se od něho jen bídník nejhorší"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "jehož spalovat bude oheň největší"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "v něm potom nezemře ani nebude žít"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Prospívat naopak bude ten, kdo je očištěn"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "a jméno Pána svého vzpomíná a modlitbu koná"
        }
    ]
}