{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "بگو او خداوند یگانه است‌"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "خداوند صمد"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نه فرزند آرد و نه از کسی زاده است‌"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "و او را هیچ کس همتا نیست‌"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "بگو به پروردگار فلق پناه می‌برم‌"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "از شر هر آنچه آفریده است‌"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "و از شر شب چون در آید"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "و از شر زنان افسونگر دمنده در گره‌ها"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "و از شر رشک‌بری که رشک برد"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "بگو به پروردگار مردم پناه می‌برم‌"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "فرمانروای مردم‌"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "خدای مردم‌"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "از شر وسوسه‌گر پنهانکار"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "کسی که در دلهای مردم وسوسه می‌کند"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "اعم از جن و انسان‌"
        }
    ]
}