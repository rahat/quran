{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "آنگاه فرعون گردآورندگان را به شهرها فرستاد"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "[آنان درباره یاران موسی‌] گفتند اینان گروهکی اندکشمارند"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "و ایشان ما را به خشم آورده‌اند"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "و ما انبوهی مسلح هستیم‌"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "آنگاه ایشان را از باغها و چشمه‌ها آواره کردیم‌"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "و از گنجها و جایگاه نیکو"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "و بدین‌سان آنها را به بنی‌اسرائیل به میراث دادیم‌"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "سپس ایشان را به هنگام طلوع آفتاب دنبال کردند"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "و چون دو گروه همدیگر را دیدند، یاران موسی گفتند، ما گیر افتاده‌ایم‌"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "[موسی‌] گفت چنین نیست، پروردگارم با من است و به زودی مرا راهنمایی خواهد کرد"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "سپس به موسی وحی کردیم که با عصایت به دریا بزن، آنگاه [دریا] بشکافت و هر پاره‌ای از آن همچون کوهی بزرگ بود"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "و دیگران را به آنجا نزدیک کردیم‌"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "و موسی و همراهانش، همگیشان را رهانیدیم‌"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "سپس دیگران را غرقه ساختیم‌"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "بی‌گمان در این مایه عبرتی هست، و بیشترینه آنان مؤمن نبودند"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "همانا پروردگارت پیروزمند مهربان است‌"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "و بر آنان داستان ابراهیم را بخوان‌"
        }
    ]
}