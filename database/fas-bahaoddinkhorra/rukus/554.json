{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "بگو او خداوند یگانه است‌"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "خداوند صمد"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نه فرزند آرد و نه از کسی زاده است‌"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "و او را هیچ کس همتا نیست‌"
        }
    ]
}