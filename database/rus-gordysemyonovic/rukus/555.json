{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Скажи: \"Ищу убежища у Господа рассвета"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "От злотворности того, что сотворил Он"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "От злотворности ночной темноты, когда она все покрывает"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "От злотворности женщин, дующих на узлы"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "От злотворности завистника, когда он завидует"
        }
    ]
}