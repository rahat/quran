{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Высоко цените вы здешнюю жизнь"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Но будущая лучше, потому что вечна"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Так и в прежних свитках"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "В свитках Авраама и Моисея"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Доходила ли до тебя весть о накрывающем"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "В этот день некоторые будут с лицами поникшими"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Томными, унылыми"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Опаляемые жгучим огнем"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Питье им будет из сильно кипящего источника"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Пищею им будет только терн дзариг"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Он не утучнит, да и голода не утолит"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "В этот день некоторые будут с лицами радостными"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Удовлетворяясь своими подвигами"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "В горнем рае"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "В нем не услышишь празднословия"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "В нем источник проточный"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "В нем седалища высокие"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Кубки расставленные"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Подушки, рядами наложенные"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ковры разостланные"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Уже ли они не обращают взора на верблюдов, как они созданы"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "На небо, как оно возвышено"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "На горы, как они поставлены"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "На землю, как она разостлана"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Учи их; ты их учитель"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Над ними ты не надзиратель"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Тех же, которые отворотятся и будут неверующими"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Бог накажет самым великим наказанием"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Истинно, к Нам будет возвращение их"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Тогда нашим делом будет рассчитать их"
        }
    ]
}