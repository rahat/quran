{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "Kalu enü´minü leke vettebeakel erzelun"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Kale vema ılmı bima kanu ya´melun"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "İn hısabühüm illa ala rabbı lev teş´urun"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ve ma ene bi taridil mü´minın"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "İn ene illa nezırum mübın"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Kalu le il lem tentehi ya nuhu le tekunenne minel mercumın"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Kale rabbi inne kavmı kezzebun"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Fettah beynı ve beynehüm fethav ve neccinı ve mem meıye minel mü´minın"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fe enceynahü ve mem meahu fil fülkil meşhun"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Sümme ağrakna ba´dül bakıyn"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "İnne fı zalik le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kezzebet adünil murselın"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "İz kale lehüm ehuhüm hudün ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Ve ma es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "E tebnune bi külli riy´ın ayeten ta´besun"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Ve tettehızune mesanıa lealleküm tahlüdun"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Ve iza betaştüm betaştüm cebbarın"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Vettekullezı emeddeküm bima ta´lemun"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Emeddeküm bi en´amiv ve benın"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Ve cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "İnnı ehafü aleyküm azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Kalu sevaün aleyna e veazte em lem teküm minel vaızıyn"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "İn haza illa hulükul evvelın"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Ve ma nahnü bi müazzebın"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fe kezzebuhü fe ehleknahüm inne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kezzebet semudül murselın"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "İz kale lehüm ehuhüm salihun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Ve ma es´elüküm aleyhi men ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "E tütrakune fı ma hahüna aminın"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fı cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Ve züruıv ve nahlin tal´uha hedıym"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Ve tenhıtune minel cibali büyuten farihın"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Ve la tütıy´u emral müsrifın"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Ellezıne yüfsidune fil erdı ve la yuslihun"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Kalu innema ente minel müsahharın"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ma ente illa beşerum mislüna fe´ti bi ayetin in künte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Kale hazihı nakatül leha şirbüv ve leküm şirbü yevmim ma´lum"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Ve la temessuha bi suin fe ye´huzeküm azabü yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fe akaruha fe asbehu nadimın"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fe ehazehümül azab inne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmü lutınil murselun"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "İz kale lehüm ehuhüm lutun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te´tunez zükrane minel alemın"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerune ma haleka leküm rabbüküm min ezvaciküm bel entüm kavmün adun"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kalu leil lem tentehi ya lutu le tekunenne minel muhracın"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kale innı li ameliküm minel kalın"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccinı ve ehlı mimma ya´melun"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynahü ve ehlehu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "İlla acuzen fil ğabirın"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Sümme demmernel aharın"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarna aleyhim metara fe sae metarul münzerın"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabül eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "İz kale lehüm şüaybün ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        }
    ]
}