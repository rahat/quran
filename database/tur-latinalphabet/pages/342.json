{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Kad eflehal mü´minun"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Ellezıne hüm fı salatihim haşiun"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Vellezıne hüm anil lağvi mu´ridun"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Vellezıne hüm liz zekati faılun"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Vellezıne hüm li fürucihim hafizun"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "İlla ala ezvacihim ev ma meleket eymanühüm fe innehüm ğayru melumın"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Fe menibteğa verae zalike fe ülaike hümül adun"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Vellezıne hüm li emanatihim ve ahdihim raun"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Vellezıne hüm ala salevatihim yühafizun"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Ülaike hümül varisun"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Ellezıne yerisunel firdevs hüm fıha halidun"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Ve le kad halaknel insane min sülaletim min tıyn"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Sümme cealnahü nutfeten fı kararim mekın"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Sümme halaknen nutfete alekaten fe halaknel alekate mudğaten fe halaknel mudğate ızamen fe kesevnel ızame lahmen sümme enşe´nahü halkan ahar fe tebarakellahü ahsenül halikıyn"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Sümme inneküm ba´de zalike le meyyitun"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Sümme inneküm yevmel kıyameti tüb´asun"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Ve le kad halakna fevkaküm seb´a taraika ve ma künna anil halkı ğafilın"
        }
    ]
}