        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmü lutınil murselun"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "İz kale lehüm ehuhüm lutun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te´tunez zükrane minel alemın"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerune ma haleka leküm rabbüküm min ezvaciküm bel entüm kavmün adun"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kalu leil lem tentehi ya lutu le tekunenne minel muhracın"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kale innı li ameliküm minel kalın"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccinı ve ehlı mimma ya´melun"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynahü ve ehlehu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "İlla acuzen fil ğabirın"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Sümme demmernel aharın"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarna aleyhim metara fe sae metarul münzerın"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabül eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "İz kale lehüm şüaybün ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 181,
