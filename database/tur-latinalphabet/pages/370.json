{
    "pages": [
        {
            "chapter": 26,
            "verse": 61,
            "text": "Felemma terael cem´ani kale ashabü musa inna le müdrakun"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Kale kella inne meıye rabbı seyehdın"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fe evhayna ila masa enıdrib bi asakel bahr fenfeleka fe kane küllü firkın ket tavdil azıym"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Ve ezlefna semmel aharın"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Ve enceyna musa ve mem meahu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sümme ağraknel aharın"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vetlü aleyhim nebee ibrahım"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "İz kale li ebıhi ve kavmihı ma ta´büdun"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Kalu na´büdü asnamen fe nezallü leha akifın"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Kale hel yesmeuneküm iz ted´un"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Ev yenfeuneküm ev yedurrun"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Kalu bel vecedna abaena kezalike yef´alun"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Kale e feraeytüm ma küntüm ta´büdun"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Entüm ve abaükümül akdemun"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fe innehüm adüvvül lı illa rabbel alemın"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Ellezı halekanı fe hüve yehdın"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Vellezı hüve yut´ımünı ve yeskıyn"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Ve iza merıdtü fe hüve yeşfın"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Vellezı yümıtünı sümme yuhyın"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Vellezı at´meu ey yağfira lı hatıy´etı yevmeddın"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi heb lı hukmev ve elhıknı bis salihıyn"
        }
    ]
}