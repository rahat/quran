{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekullezı halekaküm vel cibilletel evvelın"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kalu innema ente minel müsahharın"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve ma ente illa beşerum mislüna ve in nezunnüke le minel kazibın"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleyna kisefem mines semai in künte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kale rabbı a´lemü bi ma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebuhü fe ehazehüm azabü yevmiz zulleh innehu kane azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Ve innehu le tenzılü rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nezele bihir ruhul emın"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala kalbike li tekune minel münzirın"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bi lisanin arabiyyim mübın"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Ve innehu lefı zübüril evvelın"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "E ve lem yekül lehüm ayeten ey ya´lemehu ulemaü benı israıl"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Ve lev nezzelnahü ala ba´dıl a´cemın"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Fe karaehu aleyhim ma kanu bihı mü´minın"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kezalike seleknahü fı kulubil mücrimın"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yü´minune bihı hatta yeravül azabel elım"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Fe ye´tiyehüm bağtetev ve hüm la yeş´urun"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fe yekul hel nahnü münzarun"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "E fe bi azabina yesta´cilun"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "E feraeyte im metta´nahüm sinın"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Sümme caehüm ma kun yuadun"
        }
    ]
}