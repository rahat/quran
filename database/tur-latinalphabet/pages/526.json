{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ven necmi iza heva"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalle sahıbukum ve ma ğava"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ve ma yentıku anil heva"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "İn huve illa vahyuy yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allemehu şedidul kuva"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Zu mirrah festeva"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Ve huve bil ufukıl a´la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Summe dena fe tedella"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fe kane kabe kavseyni ev edna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fe evha ila abdihi ma evha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kezebel fuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Efe tumarunehu ala ma yera"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Ve le kad raahu nezleten uhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inde sidratil munteha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indeha cennetul me´va"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "İz yağşes sidrate ma yağşa"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zağal besaru ve ma tağa"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Le kad raa min ayati rabbihil kubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "E fe raeytumul late vel uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Ve menates salisetel uhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "E lekumuz zekeru ve lehul unsa"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilke izen kısmetun dıyza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "İn hiye illa esmaun semmeytumuh entum ve abaukum ma enzelellahu biha min sultan iy yettebiune illaz zane ve ma tehvel enfus ve le kad caehum mir rabbihimul huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Em lil insani ma temenna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Fe lillahil ahıratu ve ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ve kem mim melekin fis semavati la tuğni şefaatuhum şey´en illa mim ba´di ey ye´zenellahu li mey yeşau ve yerda"
        }
    ]
}