        {
            "chapter": 20,
            "verse": 13,
            "text": "Ve enahtertüke festemı´ lima yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "İnnenı enallahü la ilahe illa ene fa´büdnı ve ekımıs salate li zikrı"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "İnnes saate atiyetün ekadü uhfıha li tücza küllü nefsim bi ma tes´a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fe la yesuddenneke anha mel la yü´minü biha vettebea hevahü fe terda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Ve ma tilke bi yemınike ya musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Kale hiye asay etevekkeü aleyha ve ehüşşü biha ala ğanemı ve liye fıha mearibü uhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Kale elkıha ya musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fe elkaha fe iza hiye hayyetün tes´a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Kale huzha ve la tehaf se nüıydüha sıratehel ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Vadmün yedeke ila cenahıke tahrüc beydae min ğayri suin ayeten uhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Li nüriyeke min ayatinel kübra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "İzheb ila fir´avne innehu tağa"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Kale rabbişrah lı sadrı"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Ve yessir lı emrı"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Vahlül ukdetem mil lisanı"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yefkahu kavlı"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Vec´al li vezıram min ehlı"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Harune ehıy"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Üşdüd bihı ezrı"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Ve eşrikhü fı emrı"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Key nüsebbihake kesıra"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Ve nezkürake kesıra"
