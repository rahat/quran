{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ven necmi iza heva"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalle sahıbukum ve ma ğava"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ve ma yentıku anil heva"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "İn huve illa vahyuy yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allemehu şedidul kuva"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Zu mirrah festeva"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Ve huve bil ufukıl a´la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Summe dena fe tedella"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fe kane kabe kavseyni ev edna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fe evha ila abdihi ma evha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kezebel fuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Efe tumarunehu ala ma yera"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Ve le kad raahu nezleten uhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inde sidratil munteha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indeha cennetul me´va"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "İz yağşes sidrate ma yağşa"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zağal besaru ve ma tağa"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Le kad raa min ayati rabbihil kubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "E fe raeytumul late vel uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Ve menates salisetel uhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "E lekumuz zekeru ve lehul unsa"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilke izen kısmetun dıyza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "İn hiye illa esmaun semmeytumuh entum ve abaukum ma enzelellahu biha min sultan iy yettebiune illaz zane ve ma tehvel enfus ve le kad caehum mir rabbihimul huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Em lil insani ma temenna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Fe lillahil ahıratu ve ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ve kem mim melekin fis semavati la tuğni şefaatuhum şey´en illa mim ba´di ey ye´zenellahu li mey yeşau ve yerda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "İnnellezine la yu´minune bil ahırati le yusemmunel melaiket tesmiyetel unsa"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Ve ma lehum bihi mim ılm iy yettebiune illez zann ve innez zanne la yuğni minel hakkı şey´a"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Fe a´rıd am men tevella an zikrina ve lem yurid illel hayated dunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Zalike mebleğuhum minel ılm inne rabbeke huve a´lemu bi men alle an sebilihi ve huve a´lemu bi menihteda"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Ve lillahi ma fis semavati ve ma fil erdı li yecziyellezine esau bima amilu ve yecziyellezine ahsenu bil husna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Ellezine yectenibune kebairal ismi vel fevahışe illel lemem inne rabbeke vasiul mağfirah huve a´lemu bi kum iz enşeekum minel erdı ve iz entum ecinnetun fi butuni ummehatikum fe la tuzekku enfusekum huve a´lemu bi menitteka"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "E fe raeytellezi tevella"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Ve a´ta kalilev ve ekda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Eındehu ılmul ğaybi fe huve yera"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Em lem yunebbe´ bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve ibrahimellezi veffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ella teziru vaziratuv vizra uhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Ve el leyse lil insani illa ma sea"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ve enne sa´yehu sevfe yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Summe yuczahul cezael evfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Ve enne ila rabbikel munteha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ve ennehu huve adhake ve ebka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Ve ennehu huve emate ve ahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ve ennehu halekaz zevceyniz zekara vel unsa"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfetin iza tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Ve enne aleyhin neş´etel uhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Ve ennehu huve ağna ve akna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Ve ennehu huve rabbuş şı´ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ve ennehu ehleke adenil ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Ve semude fema ebka"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve kavme nuhım min kabl innehum kanu hum azleme ve atğa"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Vel mu´tefikete ehva"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Fe ğaşşaha ma ğaşşa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fe bi eyyi alai rabbike tetemara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Haza nezirum minen nuzuril ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Ezifetil azifeh"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Leyse leha min dunillahi kaşifeh"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "E fe min hazel hadisi ta´cebun"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ve tadhakune ve la tebkun"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve entum samidun"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fescudu lillahi va´budu"
        }
    ]
}