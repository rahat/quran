{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "E raeytellezi yükezzibü bid din"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Fe zalikellezi yedu´ul yetim"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ve la yehuddu ala taamil miskin"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Fe veylün lil müsallin"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ellezine hüm an salatihim sahun"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ellezine hüm yüraun"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yemneunel maun"
        }
    ]
}