    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "İnne şeceratez zekkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Taamül esiym"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kel mühl yağlı fil bütun"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Ke ğalyil hamiym"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Huzuhü fa´tiluhü ila sevail cehıym"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Sümme subbu fevka ra´sihı min azabil hamiym"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Zuk inneke entel aziyzül keriym"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "İnne haza ma küntüm bihı temterun"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "İnnel müttekıyne fı mekamin emiyn"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fi cennativ ve uyun"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yelbesune min sündüsiv ve istebrakım mütekabiliyn"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kezali ve zevvecnahüm bi hurin ıyn"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Yed´une fiha bi külli fakihetin aminiyn"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "La yezukune fiyhel mevte illel mevtetel ula ve vekahüm azabel cehıym"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fadlem mir rabbik zalike hüvel fevzül azıym"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fe innema yessernahü bi lisanike leallehüm yetezekkerun"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Fertekıb innehüm mirtek"
        }
    ]
