    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "İz kale li ebıhi ve kavmihı ma ta´büdun"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Kalu na´büdü asnamen fe nezallü leha akifın"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Kale hel yesmeuneküm iz ted´un"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Ev yenfeuneküm ev yedurrun"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Kalu bel vecedna abaena kezalike yef´alun"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Kale e feraeytüm ma küntüm ta´büdun"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Entüm ve abaükümül akdemun"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fe innehüm adüvvül lı illa rabbel alemın"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Ellezı halekanı fe hüve yehdın"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Vellezı hüve yut´ımünı ve yeskıyn"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Ve iza merıdtü fe hüve yeşfın"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Vellezı yümıtünı sümme yuhyın"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Vellezı at´meu ey yağfira lı hatıy´etı yevmeddın"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi heb lı hukmev ve elhıknı bis salihıyn"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Vec´al lı lisane sıdkın fil ahırın"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Vec´alnı miv veraseti cennetin neıym"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Vağfir li ebı innehu kane mined dallın"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Ve la tuhzinı yevme yüb´asun"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yevme la yenfeu malüv ve la benun"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "İlla men etellahe bi kalbin selim"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Ve üzlifetil cennetü lil müttekıyn"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Ve bürrizetil cehıymü li ğavın"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Ve kıyle lehüm eyne ma küntüm ta´büdun"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min dunillah hel yensuruneküm ev yentesırun"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fe kübkibu fıhahüm vel ğavun"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Ve cünudü iblıse ecmeun"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Kalu ve hüm fıha yahtesımun"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Tellahi in künna le fı dalalim mübın"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "İz nüsevvıküm bi rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Ve ma edalleha illel mücrimun"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fe ma lena min şafiıyn"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Ve la sadıkın hamım"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Fe lev enne lena kerraten fe nekune minel mü´minın"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        }
    ]
