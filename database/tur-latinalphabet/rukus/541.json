{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "İza zülziletil erdu zilzaleha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ve ahracetilerdu eskaleha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ve kalel insanü ma leha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yevmeizin tühaddisü ahbaraha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bienne rabbeke evha leha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yevmeiziy yasdürun nasü eştatel li yürav a´malehüm"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Fe mey ya´mel miskale zerratin hayray yerah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ve mey ya´mel miskale zerratin şerray yerah"
        }
    ]
}