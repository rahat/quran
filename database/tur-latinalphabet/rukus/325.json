{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashabül eyketil murselın"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "İz kale lehüm şüaybün ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "İnnı leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve ma es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Evfül keyle ve la tekunu minel muhsirın"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Vezinu bil kıstasil müstekıym"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Ve la tebhasün nase eşyaehüm ve la ta´sev fil erdı müsidın"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekullezı halekaküm vel cibilletel evvelın"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kalu innema ente minel müsahharın"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve ma ente illa beşerum mislüna ve in nezunnüke le minel kazibın"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleyna kisefem mines semai in künte mines sadikıyn"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kale rabbı a´lemü bi ma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebuhü fe ehazehüm azabü yevmiz zulleh innehu kane azabe yevmin azıym"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        }
    ]
}