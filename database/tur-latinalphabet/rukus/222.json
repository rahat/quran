{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "Felemma cae ale lutnil murselun"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Kale inneküm kavmümü münkerun"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Kalu bel ci´nake bima kanu fıhi yemterun"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Ve eteynake bil hakkı ve inna le sadikun"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Fe esri bi ehlike bi kıt´ım minel leyli vettebı´ edbarahüm ve la yeltefit minküm ehadüv vemdu haysü tü´merun"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Ve kadayna ileyhi zalikel emra enne dabira haülai maktuum musbihıyn"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Ve cae ehlül medıneti yestebşirun"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Kale inne haülai dayfı fe la tefdahun"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Vettekullahe ve la tuhzun"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Kalu e ve lem nenheke anil alemın"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Kale haülai benatı in küntüm faılın"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "Le amruke innehüm le fı sekratihim ya´mehun"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fe ehazethümüs sayhatü müşrikıyn"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Fe cealna aliyeha safileha ve emtarna aleyhim hıcaratem min siccıl"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "İnne fı zalike le ayatil lil mütevessimın"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Ve inneha le bisebılim mükıyn"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "İnne fı zalike le ayatel lil mü´minın"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Ve in kane ashabül eyketi le zalimın"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fentekamna minhüm ve innehüma le bi imamim mübın"
        }
    ]
}