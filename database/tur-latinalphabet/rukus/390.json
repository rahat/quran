{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunüse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "İz ebeka ilel fülkil meşhun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel müdhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehül hutü ve hüve mülım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel müsebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yüb´asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahü bil arai ve hüve sekıym"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve embenta aleyhi şeceratem miy yaktıyn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahü ila mieti elfin ev yezıdün"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta´nahüm ila hıyn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatü ve lehümül benun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaiket inasev ve hüm şahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehüm min ifkihim le yekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledellahü ve innehüm le kazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benın"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma leküm keyfe tahkümun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em leküm sültanüm mübın"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe´tu bi kitabiküm in küntüm sadikıyn"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba ve le kad alimetil cinnetü innehüm le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Sübhanellahi amma yesıun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "İlla ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe inneküm ve ma ta´büdun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entüm aleyhi bi fatinın"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "İlla men hüve salil cehıym"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehü mekamüm ma´lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnüs saffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnül müsebbihün"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne ındena zikram minel evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lekünna ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferu bih fe sevfe ya´lemun"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve le kad sebekat kelimetüna li ıbadinel murselın"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "İnnehüm le hümül mensurun"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cündena lehümül ğalibun"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhüm hatta hıyn"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsırhüm fe sevfe yübsırun"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe biazabina yesta´cilun"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe iza nezele bi sahatihim fe sae sabahul münzerın"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhüm hatta hıyn"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsır fe sevfe yübsırun"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Sübhane rabbike rabbil ızzeti amma yesfun"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selamün alel murselın"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdü lillahi rabbil alemın"
        }
    ]
}