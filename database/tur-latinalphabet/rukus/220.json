{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Ve le kad halaknel insane min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Vel canne halaknahü min kablü min naris semum"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Ve iz kale rabbüke lil melaiketi innı haliküm beşeram min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fe iza sevveytühu ve nefahtü fıhi mir ruhıy fekau lehu sacidın"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fe secedel melaiketü küllühüm ecmeun"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "İlla iblıs eba ey yekune meas sacidın"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Kale ya iblısü ma leke ella tekune meas sacidın"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Kale lem ekül li escüde li beşerin halaktehu min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Kale fahruc minha fe inneke racım"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Ve inne aleykel la´nete ila yevmid dın"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Kale rabbi fe enzırnı ila yevmi yüb´asun"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Kale fe inneke minel münzarın"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "İla yevmil vaktil ma´lum"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Kale rabbi bima ağveytenı le üzeyyinenne lehüm fil erdı ve le uğviyennehüm ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "İlla ıbadeke minhümül muhlesıyn"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Kale haza sıratun aleyye müstekıym"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "İnne ıbadı leyse leke aleyhim sültanün illa menittebeake minel ğavın"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Ve inne cehenneme le mev´ıdühüm ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Leha seb´atü ebvab likülli babim minhüm cüz´üm maksum"
        }
    ]
}