{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "Ve innehu le tenzılü rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nezele bihir ruhul emın"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala kalbike li tekune minel münzirın"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bi lisanin arabiyyim mübın"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Ve innehu lefı zübüril evvelın"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "E ve lem yekül lehüm ayeten ey ya´lemehu ulemaü benı israıl"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Ve lev nezzelnahü ala ba´dıl a´cemın"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Fe karaehu aleyhim ma kanu bihı mü´minın"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kezalike seleknahü fı kulubil mücrimın"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yü´minune bihı hatta yeravül azabel elım"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Fe ye´tiyehüm bağtetev ve hüm la yeş´urun"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fe yekul hel nahnü münzarun"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "E fe bi azabina yesta´cilun"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "E feraeyte im metta´nahüm sinın"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Sümme caehüm ma kun yuadun"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma ağna anhüm ma kanu yümetteun"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Ve ma ehlekna min karyetin illa leha münzirun"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Zikra ve ma künna zalimın"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Ve ma tenezzelet bihiş şeyatıyn"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Ve ma yembeğıy lehüm ve ma yestetıy´un"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "İnnehüm anis sem´ı le ma´zulun"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fe la ted´u meallahi ilahen ahara fe tekune minel müazzebın"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Ve enzir aşiratekel akrabın"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Vahfıd cenahake li menit tebeake minel mü´minın"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fe in asavke fe kul innı berıüm mimma ta´melun"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Ve tevekkel alel azızir rahıym"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Ellezı yerake hıyne tekum"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Ve tekallübeke fis sacidın"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "İnnehu hüves semıul alım"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hel ünebbiüküm ala men tenezzelüş şeyatıyn"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tenezzelü ala külli effakin esım"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yülkunes sem´a ve ekseruhüm kazibun"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Veş şüaraü yettebiuhümül ğavun"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "E lem tera ennehüm fı külli vadiy yehımun"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Ve ennehüm yekulune ma la yef´alun"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "İllellezıne amenu ve amilus salihati ve zekerullahe kesırav ventesaru mim ba´di ma zulimu ve seya´lemüllezıne zalemu eyye münkalebiy yenkalibun"
        }
    ]
}