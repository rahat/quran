{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "İnnel müttekıyne fı cennativ ve uyun"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Üdhuluha bi selamin aminın"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "Ve neza´na ma fı sudurihim min ğıllin ıhvanen ala sürurim mütekabilın"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La yemessühüm fıha nesabüv ve ma hüm minha bi muhracın"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nebbi´ ıbadı ennı enel ğafurur rahıym"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Ve enne azabı hüvel azabül elım"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Ve nebbi´hüm an dayfi ibrahım"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "İz dehalu aleyhi fe kalu selama kale inna minküm vecilun"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Kalu la tevcel inna nübeşşiruke bi ğulamin alım"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Kale e beşşertümunı ala em messeniyel kiberu fe bime tübeşşirun"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Kalu beşşernake bil hakkı fe la teküm minel kanitıyn"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Kale ve mey yaknetu mir rahmeti rabbihı illed dallun"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Kale fe ma hatbuküm eyyühel murselun"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Kalu inna ürsilna ila kavmim mücrimın"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "İlla ale lut inna le müneccuhüm ecmeıyn"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "İllemraetehu kadderna inneha le minel ğabirın"
        }
    ]
}