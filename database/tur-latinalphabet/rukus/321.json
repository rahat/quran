        {
            "chapter": 26,
            "verse": 105,
            "text": "Kezzebet kavmü nuhınil murselın"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "İz kale lehüm ehuhüm nuhun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "İnni leküm rasulün emın"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Ve ma es´elüküm aleyhi min ecr in ecriye illa ala rabbil alemın"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fettekullahe ve etıy´un"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Kalu enü´minü leke vettebeakel erzelun"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Kale vema ılmı bima kanu ya´melun"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "İn hısabühüm illa ala rabbı lev teş´urun"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ve ma ene bi taridil mü´minın"
