{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fe ersele fir´avnü fil medaini haşirın"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "İnne haülai le şirzimetün kalılun"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve innehüm lena le ğaizun"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Ve inna le cemıun hazirun"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fe ahracnahüm min cennativ ve uyun"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Ve künuziv ve mekamin kerım"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kezalik ve evrasnaha benı israıl"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fe etbeuhüm müşrikıyn"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Felemma terael cem´ani kale ashabü musa inna le müdrakun"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Kale kella inne meıye rabbı seyehdın"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fe evhayna ila masa enıdrib bi asakel bahr fenfeleka fe kane küllü firkın ket tavdil azıym"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Ve ezlefna semmel aharın"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Ve enceyna musa ve mem meahu ecmeıyn"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sümme ağraknel aharın"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "İnne fı zalike le ayeh ve ma kane ekseruhüm mü´minın"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve inne rabbeke le hüvel azızür rahıym"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vetlü aleyhim nebee ibrahım"
        }
    ]
}