{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Kunem se gradom ovim –"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "a tebi ce biti dopusteno sve u gradu ovome –"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i roditeljem i onim koga je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Mi covjeka stvaramo da se trudi"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li on da mu niko nista ne moze"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Utrosio sam blago nebrojeno!\" – reci ce"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Zar misli da ga niko vidio nije"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo dali oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i dobro i zlo mu objasnili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa, zasto on na blagodatima zahvalan bio nije? –"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A sta ti mislis: kako se moze na blagodatima zahvalan biti? –"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "roba ropstva osloboditi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili, kad glad hara, nahraniti"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "siroce bliska roda"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili ubogoga nevoljnika"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "a uz to da je od onih koji vjeruju, koji jedni drugima izdrzljivost preporucuju i koji jedni drugima milosrđe preporucuju"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "oni ce biti – sretnici"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u dokaze Nase, oni ce biti – nesretnici"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "iznad njih ce vatra zatvorena biti"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako Mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i Mjeseca kada ga prati"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dana kada ga vidljivim ucini"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noci kada ga zakloni"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i neba i Onoga koji ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i Zemlje i Onoga koji je ravnom ucini"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i duse i Onoga koji je stvori"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "pa joj put dobra i put zla shvatljivim ucini"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "uspjece samo onaj ko je ocisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a bice izgubljen onaj ko je na stranputicu odvodi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud je zbog obijesti svoje poricao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "kad se jedan nesretnik između njih podigao"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahov poslanik im je doviknuo: \"Brinite se o Allahovoj kamili i vremenu kad treba da pije"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ali mu oni nisu povjerovali, vec su je zaklali – i Gospodar njihov ih je zbog grijeha njihovih unistio i do posljednjeg istrijebio"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "i ne strahujuci zbog toga od odgovornosti"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noci kada tmine razastre"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dana kad svane"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i Onoga koji musko i zensko stvara –"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vas trud je, zaista, razlicit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "onome koji udjeljuje i ne grijesi"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i ono najljepse smatra istinitim –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "njemu cemo Dzennet pripremiti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "a onome koji tvrdici i osjeca se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i ono najljepse smatra laznim –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "njemu cemo Dzehennem pripremiti"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i bogatstvo njegovo mu, kad se strovali, nece koristiti"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mi smo duzni ukazati na Pravi put"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "i jedino Nama pripada i onaj i ovaj svijet"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Zato vas opominjem razbuktalom vatrom"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "u koju ce uci samo nesretnik"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "onaj koji bude poricao i glavu okretao"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "a od nje ce daleko biti onaj koji se bude Allaha bojao"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "onaj koji bude dio imetka svoga udjeljivao, da bi se ocistio"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ne ocekujuci da mu se zahvalnoscu uzvrati"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "vec jedino da bi naklonost Gospodara svoga Svevisnjeg stekao"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "i on ce, zbilja, zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noci kada se utisa –"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Onaj svijet je, zaista, bolji za tebe od ovoga svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj ce tebi, sigurno, dati, pa ces zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar nisi siroce bio, pa ti je On utociste pruzio"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i za pravu vjeru nisi znao, pa te je na Pravi put uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromah si bio, pa te je imucnim ucinio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato siroce ne ucvili"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a na prosjaka ne podvikni"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}