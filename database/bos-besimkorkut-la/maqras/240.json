{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "A zar on ne zna da ce, kada budu ozivljeni oni koji su u grobovima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i kada iziđe na vidjelo ono sto je u srcima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Gospodar njihov toga Dana, sigurno, sve znati o njima"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Smak svijeta"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Sta je Smak svijeta"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "i sta ti znas o Smaku svijeta"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Toga dana ce ljudi biti kao leptiri rastrkani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a planine kao sarena vuna iscupana"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onaj u koga njegova djela budu teska –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "u ugodnu zivotu ce zivjeti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onaj u koga njegova djela budu lahka –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "boraviste ce mu bezdan biti"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A znas li ti sta ce to biti? –"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Vatra uzarena"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas nastojanje da sto imucniji budete"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok grobove ne naselite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne valja tako, saznacete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I jos jednom, ne valja tako! Saznacete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, neka znate pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dzehennem cete vidjeti jasno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I jos jednom, doista cete ga vidjeti ocigledno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim cete toga dana za sladak zivot biti pitani sigurno"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Tako mi vremena –"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "covjek, doista, gubi"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "samo ne oni koji vjeruju i dobra djela cine, i koji jedni drugima istinu preporucuju i koji jedni drugima preporucuju strpljenje"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Tesko svakom klevetniku-podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koji blago gomila i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i misli da ce ga blago njegovo besmrtnim uciniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A ne valja tako! On ce, sigurno, biti bacen u Dzehennem"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znas li ti sta je Dzehennem"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova razbuktana"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koja ce do srca dopirati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ona ce iznad njih biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "plamenim stupovima zasvođena"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Zar nisi cuo sta je sa vlasnicima slona Gospodar tvoj uradio"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Zar lukavstvo njihovo nije omeo"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "i protiv njih jata ptica poslao"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Koje su na njih grumenje od gline pecene bacale"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "pa ih On kao lisce koje su crvi istocili ucinio"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog navike Kurejsija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "navike njihove da zimi i ljeti putuju"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "neka se oni Gospodaru hrama ovoga klanjaju"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "koji ih gladne hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Znas li ti onoga koji onaj svijet porice"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroce"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah – ne podstice"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "A tesko onima koji, kada molitvu obavljaju"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "molitvu svoju kako treba ne izvrsavaju"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo pretvaraju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i nikome nista ni u naruc ne daju"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mi smo ti, uistinu, mnogo dobro dali"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "zato se Gospodaru svome moli i kurban kolji"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "onaj koji tebe mrzi sigurno ce on bez spomena ostati"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: \"O vi nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ja se necu klanjati onima kojima se vi klanjate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a ni vi se necete klanjati Onome kome se ja klanjam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ja se nisam klanjao onima kojima ste se vi klanjali"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "a i vi se niste klanjali Onome kome se ja klanjam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "vama – vasa vjera, a meni – moja"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kada Allahova pomoc i pobjeda dođu"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "i vidis ljude kako u skupinama u Allahovu vjeru ulaze –"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ti velicaj Gospodara svoga hvaleci Ga i moli Ga da ti oprosti, On je uvijek pokajanje primao"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Neka propadne Ebu Leheb, i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nece mu biti od koristi blago njegovo, a ni ono sto je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "uci ce on, sigurno, u vatru rasplamsalu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zena njegova, koja spletkari"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "o vratu njenu bice uze od licine usukane"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Reci: \"On je Allah – Jedan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah je Utociste svakom"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nije rodio i rođen nije"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i niko Mu ravan nije"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: \"Utjecem se Gospodaru svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "od zla onoga sto On stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i od zla mrkle noci kada razastre tmine"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i od zla smutljivca kad smutnje sije"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i od zla zavidljivca kad zavist ne krije"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Trazim zastitu Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zla sejtana-napasnika"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koji zle misli unosi u srca ljudi –"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "od dzina i od ljudi"
        }
    ]
}