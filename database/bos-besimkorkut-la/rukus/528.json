{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Danice"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znas li ti sta je Danica"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda blistava"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema covjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek covjek pogleda od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine koja se izbaci"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kicme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On je, zaista, kadar da ga ponovo stvori"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onoga Dana kada budu ispitivane savjesti"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada covjek ni snage ni branioca nece imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba puna kise"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i Zemlje koja se otvara da rastinje nikne"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur'an je, doista, govor koji rastavlja istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva on nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni se sluze spletkama"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a Ja ih unistavam –"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima jos vremena dadni, jos koji trenutak ih ostavi"
        }
    ]
}