{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Reci ti Meni o onome koji istinu izbjegava"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "i malo udjeljuje, i posve prestane udjeljivati"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "zna li on ono sto je skriveno, pa vidi"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Zar on nije obavijesten o onome sto se nalazi u listovima Musaovim"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "i Ibrahimovim – koji je obaveze potpuno ispunjavao –"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "da nijedan grjesnik tuđe grijehe nece nositi"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "i da je covjekovo samo ono sto sam uradi"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "i da ce se trud njegov, sigurno, iskazati"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "i da ce prema njemu u potpunosti nagrađen ili kaznjen biti"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "i da ce se Gospodaru tvome ponovo vratiti"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "i da On na smijeh i na plac navodi"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "i da On usmrcuje i ozivljava"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "i da On par, musko i zensko, stvara"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "od kapi sjemena kad se izbaci"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "i da ce ih On ponovo oziviti"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "i da On daje bogatstvo i moc da stjecu"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "i da je On Sirijusa Gospodar"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "i da je On drevni narod Ad unistio"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "i Semud, i da nikog nije postedio"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "i jos prije Nuhov narod, koji je najokrutniji i najobjesniji bio"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "i prevrnuta naselja On je prevrnuo"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "i snaslo ih je ono sto ih je snaslo"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "pa, u koju blagodat Gospodara svoga ti jos sumnjas"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ovaj Kur'an je opomena kao i prijasnje opomene"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Smak svijeta se priblizava"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Allah ce ga jedini otkriti"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Pa zar se ovom govoru iscuđavate –"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "i smijete se, a ne placete –"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "gordo dignutih glava"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Bolje padajte licem na tle pred Allahom i klanjajte se"
        }
    ]
}