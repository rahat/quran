{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tako mi smokve i masline"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "i Sinajske gore"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "i grada ovog, sigurnog –"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Mi covjeka stvaramo u skladu najljepsem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "zatim cemo ga u najnakazniji lik vratiti"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "samo ne one koji budu vjerovali i dobra djela cinili, njih ceka nagrada neprekidna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pa sta te onda navodi da porices onaj svijet –"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "zar Allah nije sudija najpravedniji"
        }
    ]
}