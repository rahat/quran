{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Neka propadne Ebu Leheb, i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nece mu biti od koristi blago njegovo, a ni ono sto je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "uci ce on, sigurno, u vatru rasplamsalu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zena njegova, koja spletkari"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "o vratu njenu bice uze od licine usukane"
        }
    ]
}