{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Entao, juro pelo que enxergais"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "E pelo que nao enxergais"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Por certo, ele e o dito de um nobre Mensageiro"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "E nao um dito de poeta; Quao pouco credes"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Nem um dito de adivinho; Quao pouco meditais"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "E revelacao do Senhor dos Mundos"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "E, se ele Nos atribuisse certos ditos"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Apanha-lo-iamos pela destra"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Em seguida, cortar-lhe-iamos a aorta"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Entao, nenhum de vos seria barreira contra sua punicao"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "E, por certo, ele e lembranca para os piedosos"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "E, por certo, sabemos que, entre vos, ha desmentidores"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "E, por certo, ele e motivo de aflicao para os renegadores da Fe"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "E, por certo, ele e a Verdade certa"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Entao, glorifica o nome de teu Magnifico Senhor"
        }
    ]
}