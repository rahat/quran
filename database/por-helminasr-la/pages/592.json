{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Mas vos dais preferencia a vida terrena"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Enquanto a Derradeira Vida e melhor e mais permanente"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Por certo, isto esta nas primeiras Paginas"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Nas Paginas de Abraao e de Moises"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Suratu Al-Ghashiyah. Chegou-te o relato da Envolvente"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Nesse dia, havera faces humilhadas"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Preocupadas, fatigadas"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Queimar-se-ao em um Fogo incandescente"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Dar-se-lhes-a de beber de escaldante fonte"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Para eles, nao havera alimento, senao o de uma arvore seca esinhosa"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Que nao engorda e de nada vale contra a fome"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Nesse dia, havera faces cheias de graca"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Agradadas de seu esforco"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Estarao em um alto Jardim"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Nele, tu nao ouviras frivolidade alguma"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Nele, havera uma fonte corrente"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Nele, havera leitos elevados"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "E copos dispostos"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "E almofadas enfileiradas"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "E tapetes espalhados"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "E nao olham eles aos camelos, como foram criados"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "E ao ceu, como foi elevado"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "E as montanhas, como foram armadas"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "E a terra, como foi distendida"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Entao, lembra-lhes o Alcorao. Es, apenas lembrador"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nao es, sobre eles, dono absoluto"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Mas a quem volta as costas e renega a Fe"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah castiga-lo-a com o castigo maior"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Por certo, a Nos sera sua volta"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Em seguida, por certo, impender-Nos-a sua conta"
        }
    ]
}