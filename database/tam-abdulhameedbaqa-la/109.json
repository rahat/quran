{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(napiye! Nirakarikkum makkalai nokki) kuruviraka: Nirakarippavarkale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ninkal vanankupavarrai nan vanankamatten"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nan vanankupavanai ninkal vanankavillai"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "(avvare) iniyum ninkal vanankupavarrai nan vanankupavan allan"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nan vanankupavanai ini ninkalum vanankupavarkal allar"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "unkal (ceyalukkuriya) kuli unkalukkum; en (ceyalukkuriya) kuli enakkum (kitaikkum)"
        }
    ]
}