{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(nam napi) katukatuttar; purakkanittar. (Etarkaka)"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "tannitam or parvaiyarravar vantatarkaka"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "(napiye! Um'mitam vanta) avar paricuttavanaka irukkalam enpatai nir arivira"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "allatu avar nallunarvu peruvar. (Umatu) nallupatecam avarukkup payanalikkalam (enpatai nir arivira? Avvarirukka, avarai nir en katukatuttup purakkanittir)"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "(napiye! Markkattai) evan alatciyam ceykirano"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "avanai varaverpatil nir atika ciramattai etuttuk kolkirir"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "avan paricuttavanaka akavittal ataip parri um mitu oru kurramum illai(ye)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "evar (tanakave) um'mitam oti varukiraro"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "avartan (allahvukkup) payappatukiravar"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "eninum, nir avarai alatciyam ceytu vitukirir"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Avvaru ceyyatir. (Tiru kur'anakiya) itu oru nallupatecamtan"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "evar (nerana valiyil cella) virumpukiraro avar itai(c ceviyurru) napakattil vaittuk kolvar"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "itu (lavhul mahhpul ennum) mikka kanniyamana puttakattil (varaiyappattullatu)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uyarvumikka tuymaiyana itattil vaikkappattullatu; (atu) mikap paricuttamanatu"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "elutupavarkalin kaikalinal (varaiyappattatu)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(avarkal) mika kanniyamanavarkal, mika nallavarkal"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(pavam ceyyum) manitanukkuk ketutan. Avan evvalavu nanrikettavanaka irukkiran"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "etaikkontu (iraivan) avanaip pataittirukkiran (enpatai avan kavanittana)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "oru tuli intiriyattaik kontutan avan avanaip pataikkiran. (Avan irukkinra ivvare, avanai manitanaka amaittu) avanukkuc caktiyaik kotuttan"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "pinnar, avan ceyyakkutiya (nanmai timaikkuriya) valiyai avanukku elitakki vaittan"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pinnar, avanai maranikkac ceytu camatiyil pukuttukiran"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pinnar (avan virumpiyapolutu uyir kotuttu) avane avanai uyirppippan"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "eninum, niccayamaka manitan iraivanutaiya kattalaiyai niraiverruvatillai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "manitan tan unavai (atu enkiruntu evvaru varukiratu enpataic) ciritu kavanittup parkkavum"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "niccayamaka name eralamana malaiyai poliyac ceytom"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pinnar, pumiyaiyum pilan(tu vetikkac cey)tom"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "pinnar, atiliruntu vittukkalai mulaittu valarumpati ceykirom"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "(ivvaru) tiratcaik kanikalaiyum marra kaykarikalaiyum"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "jaittunaiyum, periccai marattaiyum"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "kilaikal atarnta toppukalaiyum"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "kanivarkkankalaiyum, purpuntukalaiyum"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "unkalukkum unkal kalnataikalukkum, payanalikkumaru (mulaikka vaikkirom)"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "(Ulaka mutivinpolutu cevikalai) cevitakkumpatiyana (payankarac) captam erpatumayin"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "annalil manitan tan cakotaranai vittum veruntotuvan"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tan tayai vittum, tan tantaiyai vittum"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "tan manaiviyai vittum, tan pillaikalai vittum (otuvan)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "annalil, avarkalil ovvoru manitanukkum, marravarkalaik kavanikka mutiyatavaru contak kavalai erpattuvitum"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "eninum, annalil cila mukankal pirakacamullavaiyakavum"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "cantosattal cirittavaiyakavum irukkum"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "annalil veru cila mukankal mitu, puluti patintu kitakkum"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "karumai irul avarrai mutikkollum, (tukkattal avarkalatu mukankal irulataintu kitakkum)"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ivarkaltan (marumaiyai) nirakarittup pavam ceypavarkal"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "(ulaka mutivukkakac) curiyanin pirakacam manka vaikkappatumpotu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "natcattirankal utirntuvitumpotu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "malaikal atanitankalil iruntu akarrappatum potu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "(inta amalikalal pattumata) karppamatainta ottakankal (kavanippinri) vitappatumpotu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kattu mirukankal (payantu urkalil vantu) onrukutumpotu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "katal nir neruppaka marrappatumpotu. (Ivvaru ulakam mutivuperru, vicaranaik kalam erpatum)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "appotu uyirkal utalutan (mintum) cerkkappatum"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "appotu (uyirutan) putaikkappatta pen kulantaikalai nokki ketkappatum"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "â€˜â€˜enta kurrattirkaka ninkal (uyirutan putaikkappattuk) kolai ceyyappattirkal?'' Enru"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "appotu (vicaranaikkaka manitarkalutaiya) etukal virikkappatum"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "appotu vanam (pilantu) akarrappattuvitum"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "appotu narakam erikkappatum"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "appotu corkkam camipamakak kontu varappatum"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "(annalil) ovvor atmavum (nanmaiyo timaiyo) tan ceytu kontu vantiruppatai nankarintu kollum"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "(Manitarkale!) Cenravaliye melum cellum natcattirankal mitu cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "tonri maraiyum (val) natcattirankal mitu cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "celkinra iravin mitum cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "utayamakum kalaiyin mitum cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niccayamaka (tiru kur'an ennum) itu mikka kanniyamulla (jiprayil ennum) oru (vanavat) tutar mulam kurappattatakum"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "avar mikka palavan. Avarukku arsutaiyavanitattil perum pataviyuntu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "(avar avvitattilulla vanavarkalin) talaivar; mikka nampikkaiyutaiyavar"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "(makkavacikale! Nam tutarakiya) unkal tolar paittiyakkararalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "niccayamaka avar (jiprayilai) telivana vanattin kotiyil meyyakave kantar"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "(accamayam avar kanta) maraivanavarrai (arivippatil) kancattanam ceypavaralla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "innum, itu veruttappatta saittanin collumalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "akave, (itaivittu) ninkal enku cellukirirkal"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "itu ulakattar anaivarukkum oru nallupatecame tavira verillai"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "unkalil evar nerana pataiyil cella virumpukiraro avarukku (itu payanalikkum)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "eninum, ulakattarin iraivanakiya allah natinal tavira, ninkal (nallunarcci pera) virumpamattirkal"
        }
    ]
}