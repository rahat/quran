{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "(napiye!) Kuli kotukkum nalaip poyyakkupavanai nir (kavanittup) parttira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "(Ataravarra) anataikalai veruttupavan avantan"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "avan elaikalukku (unavalikkatatutan) unavalikkumpati (pirarait) tuntuvatumillai"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "(kavanamarra) tolukaiyalikalukkum ketutan"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarkal tankal tolukaikalai vittum marantavarkalaka irukkirarkal"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "(melum,) avarkal (tolutapotilum makkalukkuk) kanpikkave tolukirarkal"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "(melum, uci ponra) arpap porulaiyum (iraval kotukkatu) tatuttuk kolkirarkal"
        }
    ]
}