{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرࣱ مِّنۡ أَلۡفِ شَهۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ"
        }
    ]
}