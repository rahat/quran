{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "عَبَسَ وَتَوَلَّىٰٓ"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "وَهُوَ يَخۡشَىٰ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "كَلَّآ إِنَّهَا تَذۡكِرَةࣱ‏"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "فَمَن شَآءَ ذَكَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "فِي صُحُفࣲ مُّكَرَّمَةࣲ‏"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "مَّرۡفُوعَةࣲ مُّطَهَّرَةِۭ"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "بِأَيۡدِي سَفَرَةࣲ‏"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "كِرَامِۭ بَرَرَةࣲ‏"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبࣰّ ا"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقࣰّ ا"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "فَأَنۢبَتۡنَا فِيهَا حَبࣰّ ا"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "وَعِنَبࣰ ا وَقَضۡبࣰ ا"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "وَزَيۡتُونࣰ ا وَنَخۡلࣰ ا"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "وَحَدَآئِقَ غُلۡبࣰ ا"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "وَفَٰكِهَةࣰ وَأَبࣰّ ا"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "مَّتَٰعࣰ ا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "وَأُمِّهِۦ وَأَبِيهِ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "لِكُلِّ ٱمۡرِيࣲٕ مِّنۡهُمۡ يَوۡمَئِذࣲ شَأۡنࣱ يُغۡنِيهِ"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "وُجُوهࣱ يَوۡمَئِذࣲ مُّسۡفِرَةࣱ‏"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ضَاحِكَةࣱ مُّسۡتَبۡشِرَةࣱ‏"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "وَوُجُوهࣱ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةࣱ‏"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "تَرۡهَقُهَا قَتَرَةٌ"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ"
        }
    ]
}