{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "ثُلَّةࣱ مِّنَ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "وَثُلَّةࣱ مِّنَ ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "فِي سَمُومࣲ وَحَمِيمࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "وَظِلࣲّ مِّن يَحۡمُومࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "لَّا بَارِدࣲ وَلَا كَرِيمٍ"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مُتۡنَا وَكُنَّا تُرَابࣰ ا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمࣲ مَّعۡلُومࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "لَأٓكِلُونَ مِن شَجَرࣲ مِّن زَقُّومࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَّكَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمࣰ ا فَظَلۡتُمۡ تَفَكَّهُونَ"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "أَءِنَّا لَمُغۡرَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجࣰ ا فَلَوۡلَا تَشۡكُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةࣰ وَمَتَٰعࣰ ا لِّلۡمُقۡوِينَ"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ"
        }
    ]
}