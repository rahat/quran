{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّيۡنَٰهُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَرۡبِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرۡنَٰهُمۡ فَكَانُواْ هُمُ ٱلۡغَٰلِبِينَ"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَيۡنَٰهُمَا ٱلۡكِتَٰبَ ٱلۡمُسۡتَبِينَ"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَيۡنَٰهُمَا ٱلصِّرَٰطَ ٱلۡمُسۡتَقِيمَ"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكۡنَا عَلَيۡهِمَا فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَٰمٌ عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلۡيَاسَ لَمِنَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذۡ قَالَ لِقَوۡمِهِۦٓ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدۡعُونَ بَعۡلࣰ ا وَتَذَرُونَ أَحۡسَنَ ٱلۡخَٰلِقِينَ"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ٱللَّهُ رَبُّكُمۡ وَرَبُّ ءَابَآئِكُمُ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِينَ"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَٰمٌ عَلَىٰٓ إِلۡ يَاسِينَ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوطࣰ ا لَّمِنَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذۡ نَجَّيۡنَٰهُ وَأَهۡلَهُۥٓ أَجۡمَعِينَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوزࣰ ا فِي ٱلۡغَٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرۡنَا ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَيۡهِم مُّصۡبِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِٱلَّيۡلِۚ أَفَلَا تَعۡقِلُونَ"
        }
    ]
