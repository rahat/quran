{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَٱلۡمُورِيَٰتِ قَدۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَٱلۡمُغِيرَٰتِ صُبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡعࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذࣲ لَّخَبِيرُۢ"
        }
    ]
}