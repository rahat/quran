{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ٱلَّذِيٓ أَنقَضَ ظَهۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرࣰ ا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِيٓ أَحۡسَنِ تَقۡوِيمࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "أَن رّٜءٜاهُ ٱسۡتَغۡنَىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبۡدًا إِذَا صَلَّىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَةࣲ كَٰذِبَةٍ خَاطِئَةࣲ‏"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلۡيَدۡعُ نَادِيَهُۥ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرࣱ مِّنۡ أَلۡفِ شَهۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "لَمۡ يَكُنِ ٱلَّذِينَ كَفَرُواْ مِنۡ أَهۡلِ ٱلۡكِتَٰبِ وَٱلۡمُشۡرِكِينَ مُنفَكِّينَ حَتَّىٰ تَأۡتِيَهُمُ ٱلۡبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "رَسُولࣱ مِّنَ ٱللَّهِ يَتۡلُواْ صُحُفࣰ ا مُّطَهَّرَةࣰ‏"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "فِيهَا كُتُبࣱ قَيِّمَةࣱ‏"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "وَمَا تَفَرَّقَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ إِلَّا مِنۢ بَعۡدِ مَا جَآءَتۡهُمُ ٱلۡبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "وَمَآ أُمِرُوٓاْ إِلَّا لِيَعۡبُدُواْ ٱللَّهَ مُخۡلِصِينَ لَهُ ٱلدِّينَ حُنَفَآءَ وَيُقِيمُواْ ٱلصَّلَوٰةَ وَيُؤۡتُواْ ٱلزَّكَوٰةَۚ وَذَٰلِكَ دِينُ ٱلۡقَيِّمَةِ"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "إِنَّ ٱلَّذِينَ كَفَرُواْ مِنۡ أَهۡلِ ٱلۡكِتَٰبِ وَٱلۡمُشۡرِكِينَ فِي نَارِ جَهَنَّمَ خَٰلِدِينَ فِيهَآۚ أُوْلَٰٓئِكَ هُمۡ شَرُّ ٱلۡبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ أُوْلَٰٓئِكَ هُمۡ خَيۡرُ ٱلۡبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "جَزَآؤُهُمۡ عِندَ رَبِّهِمۡ جَنَّٰتُ عَدۡنࣲ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُ خَٰلِدِينَ فِيهَآ أَبَدࣰ اۖ رَّضِيَ ٱللَّهُ عَنۡهُمۡ وَرَضُواْ عَنۡهُۚ ذَٰلِكَ لِمَنۡ خَشِيَ رَبَّهُۥ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "إِذَا زُلۡزِلَتِ ٱلۡأَرۡضُ زِلۡزَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "وَأَخۡرَجَتِ ٱلۡأَرۡضُ أَثۡقَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "وَقَالَ ٱلۡإِنسَٰنُ مَا لَهَا"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "يَوۡمَئِذࣲ تُحَدِّثُ أَخۡبَارَهَا"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "بِأَنَّ رَبَّكَ أَوۡحَىٰ لَهَا"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "يَوۡمَئِذࣲ يَصۡدُرُ ٱلنَّاسُ أَشۡتَاتࣰ ا لِّيُرَوۡاْ أَعۡمَٰلَهُمۡ"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "فَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَيۡرࣰ ا يَرَهُۥ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "وَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةࣲ شَرࣰّ ا يَرَهُۥ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَٱلۡمُورِيَٰتِ قَدۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَٱلۡمُغِيرَٰتِ صُبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡعࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ"
        }
    ]
}