{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذࣲ لَّخَبِيرُۢ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "مَا ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فَهُوَ فِي عِيشَةࣲ رَّاضِيَةࣲ‏"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فَأُمُّهُۥ هَاوِيَةࣱ‏"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا هِيَهۡ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نَارٌ حَامِيَةُۢ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لَتَرَوُنَّ ٱلۡجَحِيمَ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَٱلۡعَصۡرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ ٱلۡإِنسَٰنَ لَفِي خُسۡرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ وَتَوَاصَوۡاْ بِٱلۡحَقِّ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَيۡلࣱ لِّكُلِّ هُمَزَةࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ٱلَّذِي جَمَعَ مَالࣰ ا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا ٱلۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيۡهِم مُّوصَدَةࣱ‏"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِي عُمُدࣲ مُّمَدَّدَةِۭ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِأَصۡحَٰبِ ٱلۡفِيلِ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرۡمِيهِم بِحِجَارَةࣲ مِّن سِجِّيلࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمۡ كَعَصۡفࣲ مَّأۡكُولِۭ"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لِإِيلَٰفِ قُرَيۡشٍ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "إِۦلَٰفِهِمۡ رِحۡلَةَ ٱلشِّتَآءِ وَٱلصَّيۡفِ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَلۡيَعۡبُدُواْ رَبَّ هَٰذَا ٱلۡبَيۡتِ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "ٱلَّذِيٓ أَطۡعَمَهُم مِّن جُوعࣲ وَءَامَنَهُم مِّنۡ خَوۡفِۭ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَيۡلࣱ لِّلۡمُصَلِّينَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "إِنَّآ أَعۡطَيۡنَٰكَ ٱلۡكَوۡثَرَ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَٱنۡحَرۡ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "إِنَّ شَانِئَكَ هُوَ ٱلۡأَبۡتَرُ"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "وَلَآ أَنَا۠ عَابِدࣱ مَّا عَبَدتُّمۡ"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لَكُمۡ دِينُكُمۡ وَلِي دِينِ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "إِذَا جَآءَ نَصۡرُ ٱللَّهِ وَٱلۡفَتۡحُ"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "وَرَأَيۡتَ ٱلنَّاسَ يَدۡخُلُونَ فِي دِينِ ٱللَّهِ أَفۡوَاجࣰ ا"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فَسَبِّحۡ بِحَمۡدِ رَبِّكَ وَٱسۡتَغۡفِرۡهُۚ إِنَّهُۥ كَانَ تَوَّابَۢا"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتۡ يَدَآ أَبِي لَهَبࣲ وَتَبَّ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَآ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَيَصۡلَىٰ نَارࣰ ا ذَاتَ لَهَبࣲ‏"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِي جِيدِهَا حَبۡلࣱ مِّن مَّسَدِۭ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ٱللَّهُ ٱلصَّمَدُ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "وَلَمۡ يَكُن لَّهُۥ كُفُؤًا أَحَدُۢ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِن شَرِّ مَا خَلَقَ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "إِلَٰهِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ"
        }
    ]
}