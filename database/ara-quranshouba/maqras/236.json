{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتۡ"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "وَإِذَا ٱلۡأَرۡضُ مُدَّتۡ"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "وَأَلۡقَتۡ مَا فِيهَا وَتَخَلَّتۡ"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدۡحࣰ ا فَمُلَٰقِيهِ"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "فَسَوۡفَ يُحَاسَبُ حِسَابࣰ ا يَسِيرࣰ ا"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "وَيَنقَلِبُ إِلَىٰٓ أَهۡلِهِۦ مَسۡرُورࣰ ا"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهۡرِهِۦ"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "فَسَوۡفَ يَدۡعُواْ ثُبُورࣰ ا"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "وَيَصۡلَىٰ سَعِيرًا"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "إِنَّهُۥ كَانَ فِيٓ أَهۡلِهِۦ مَسۡرُورًا"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "بَلَىٰٓۚ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرࣰ ا"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "فَلَآ أُقۡسِمُ بِٱلشَّفَقِ"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "وَٱلَّيۡلِ وَمَا وَسَقَ"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "وَٱلۡقَمَرِ إِذَا ٱتَّسَقَ"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "لَتَرۡكَبُنَّ طَبَقًا عَن طَبَقࣲ‏"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُونَ"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "وَإِذَا قُرِئَ عَلَيۡهِمُ ٱلۡقُرۡءَانُ لَا يَسۡجُدُونَۤ۩"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "بَلِ ٱلَّذِينَ كَفَرُواْ يُكَذِّبُونَ"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "وَٱللَّهُ أَعۡلَمُ بِمَا يُوعُونَ"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "فَبَشِّرۡهُم بِعَذَابٍ أَلِيمٍ"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونِۭ"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "وَشَاهِدࣲ وَمَشۡهُودࣲ‏"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودࣱ‏"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودࣱ‏"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءࣲ شَهِيدٌ"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتࣱ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "فَعَّالࣱ لِّمَا يُرِيدُ"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "فِرۡعَوۡنَ وَثَمُودَ"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبࣲ‏"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "بَلۡ هُوَ قُرۡءَانࣱ مَّجِيدࣱ‏"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "فِي لَوۡحࣲ مَّحۡفُوظِۭ"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا ٱلطَّارِقُ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "إِن كُلُّ نَفۡسࣲ لَّمَّا عَلَيۡهَا حَافِظࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خُلِقَ مِن مَّآءࣲ دَافِقࣲ‏"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فَمَا لَهُۥ مِن قُوَّةࣲ وَلَا نَاصِرࣲ‏"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "إِنَّهُۥ لَقَوۡلࣱ فَصۡلࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وَمَا هُوَ بِٱلۡهَزۡلِ"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدࣰ ا"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "وَأَكِيدُ كَيۡدࣰ ا"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"
        }
    ]
}