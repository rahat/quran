{
    "pages": [
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنۡ هَٰذَآ إِلَّا خُلُقُ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهۡلَكۡنَٰهُمۡۚ إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كَذَّبَتۡ ثَمُودُ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ صَٰلِحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيٓ إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "أَتُتۡرَكُونَ فِي مَا هَٰهُنَآ ءَامِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "فِي جَنَّٰتࣲ وَعِيُونࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وَزُرُوعࣲ وَنَخۡلࣲ طَلۡعُهَا هَضِيمࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وَتَنۡحِتُونَ مِنَ ٱلۡجِبَالِ بِيُوتࣰ ا فَٰرِهِينَ"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "وَلَا تُطِيعُوٓاْ أَمۡرَ ٱلۡمُسۡرِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "ٱلَّذِينَ يُفۡسِدُونَ فِي ٱلۡأَرۡضِ وَلَا يُصۡلِحُونَ"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ ٱلۡمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "مَآ أَنتَ إِلَّا بَشَرࣱ مِّثۡلُنَا فَأۡتِ بِـَٔايَةٍ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قَالَ هَٰذِهِۦ نَاقَةࣱ لَّهَا شِرۡبࣱ وَلَكُمۡ شِرۡبُ يَوۡمࣲ مَّعۡلُومࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "وَلَا تَمَسُّوهَا بِسُوٓءࣲ فَيَأۡخُذَكُمۡ عَذَابُ يَوۡمٍ عَظِيمࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فَعَقَرُوهَا فَأَصۡبَحُواْ نَٰدِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فَأَخَذَهُمُ ٱلۡعَذَابُۚ إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        }
    ]
