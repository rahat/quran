    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Kŭlna se v sledobeda"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "chovekŭt e v zaguba"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "osven onezi, koito vyarvat i vŭrshat pravedni dela, i vzaimno se nastavlyavat za istinata, i vzaimno se nastavlyavat za tŭrpenieto"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gorko na vseki klevetnik-khulitel"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koĭto trupa bogat·stvo i vse go broi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Smyata, che negovoto bogat·stvo shte go uvekovechi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Ala ne! Shte bŭde khvŭrlen v Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A otkŭde da znaesh ti kakvo e Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Razpaleniyat Ogŭn na Allakh"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koĭto pronikva do sŭrtsata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Toĭ shte bŭde zatvoren nad tyakh"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "vŭv visoki stŭlbove"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ne vidya li [o, Mukhammad] kak postŭpi tvoyat Gospod s vladetelite na slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ne prevŭrna li Toĭ kovarstvoto im v zabluda"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I izprati sreshtu tyakh yata ot ptitsi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "koito gi zameryakha s kamŭni ot pechena glina"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "I gi stori da prilichat na proyadeni lista"
        }
