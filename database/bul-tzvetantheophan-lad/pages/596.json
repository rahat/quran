{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Shte gori v nego samo zliyat"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "koĭto otricha i se otvrŭshta"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "I shte bŭde predpazen ot nego bogoboyazliviyat"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "koĭto razdava svoeto bogat·stvo, za da se prechisti"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "na nikogo ne pravi blagodeyanie, za da bŭde vŭznagraden"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "a samo v stremezh kŭm Lika na svoya Gospod, Vŭrkhovniya"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "I nepremenno shte e dovolen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kŭlna se v utroto"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i v noshtta, kogato nastŭpva mrak"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ne te izostavi tvoyat Gospod [o, Mukhammad] i ne te vŭznenavidi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Otvŭdniyat zhivot e po-dobŭr za teb ot nastoyashtiya"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I shte ti vŭzdade tvoyat Gospod, i shte si dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ne te li nameri Toĭ sirak i ne te li priyuti"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I te nameri zabluden, i te napŭti"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I te nameri beden, i te zamogna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Tŭĭ che ne ogorchavaĭ siraka"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I ne otpŭzhdaĭ umolyavashtiya"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "I za blagodatta na tvoya Gospod vŭzvestyavaĭ"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ne raztvorikhme li Nie tvoyata grŭd [o, Mukhammad]"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "I snekhme ot teb bremeto"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "koeto tegneshe na tvoya grŭb"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "I vŭzvisikhme tvoeto spomenavane"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "S trudnostta ima i lesnina"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Da, s trudnostta ima i lesnina"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "I kogato priklyuchish, [pak] se trudi usŭrdno"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "i na svoya Gospod se posveshtavaĭ"
        }
    ]
}