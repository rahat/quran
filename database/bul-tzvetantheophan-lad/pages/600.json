        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I otkŭde da znaesh ti kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Denya, v koĭto khorata shte bŭdat kato prŭsnati peperudi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "i planinite shte bŭdat kato razpokŭsana vŭlna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onzi, chiito vezni natezhat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "shte ima zhivot v dovolstvo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onzi, chiito vezni oleknat"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "zavrŭshtaneto mu e bezdna"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A otkŭde da znaesh ti kakvo e tya"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Plamtyasht ogŭn"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Uvlicha vi preumnozhavaneto"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "dodeto legnete v grobovete"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ala ne! Shte uznaete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I pak ne! Shte uznaete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ala ne, ako znaete s dostovernoto znanie"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "shte vidite Ada"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Posle shte go vidite s dostoveren pogled"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Posle shte bŭdete pitani v tozi Den za nasladite [na zemyata]"
