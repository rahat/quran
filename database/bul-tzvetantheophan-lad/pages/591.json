{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kŭlna se v nebeto i vŭv Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A otkŭde da znaesh ti kakvo e Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvezdata s pronizvashta svetlina"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nad vsyaka dusha ima nadzornik"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "I da pogledne chovekŭt ot kakvo be sŭtvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Be sŭtvoren ot iztlaskvashta se voda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koyato izliza izmezhdu grŭbnaka i rebrata"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Sposoben e Toĭ da go vŭrne"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v Denya, kogato shte bŭdat izyaveni taĭnite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "i ne shte ima toĭ nito sila, nito izbavitel"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Kŭlna se v nebeto, ot koeto vali"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i v zemyata, koyato se razpukva"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "che tova e slovo-razgranichenie"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "A ne e shega"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Te zamislyat s umisŭl"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I Az zamislyam s umisŭl"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "I daĭ ot·srochka na nevernitsite, ot·srochi gi za kratko"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Proslavyaĭ prechistoto ime na tvoya Gospod, Vŭrkhovniya"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koĭto sŭtvori i osŭrazmeri"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Koĭto pretseni i napŭti"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Koĭto izvazhda [trevata na] pasbishteto"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "i go prevrŭshta v cherno stŭrnishte"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Shte ti dadem da chetesh [Korana, o, Mukhammad] i ne shte zabravish"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osven kakvoto Allakh pozhelae. Toĭ znae yavnoto i skritoto"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I shte te ulesnim za lesnoto"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Napomnyaĭ, ako ima polza ot napomnyaneto"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Shte si spomni onzi, koĭto se strakhuva"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "I shte se otdrŭpne ot tova naĭ-loshiyat"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koĭto shte gori v naĭ-golemiya Ogŭn"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Posle tam nito shte umre, nito shte e zhiv"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Spoluchil e onzi, koĭto se prechistva"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomenava imeto na svoya Gospod, i ot·sluzhva molitvata"
        }
    ]
}