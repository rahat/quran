    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Toĭ e Allakh - Edinstveniya"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allakh, Tselta [na vsichki vŭzhdeleniya]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nito e razhdal, nito e roden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i nyama raven Nemu.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Opazil me Gospodŭt na razsŭmvaneto"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ot zloto na onova, koeto e sŭtvoril"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i ot zloto na tŭmninata, kogato nastŭpi"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i ot zloto na dukhashti po vŭzlite [mag’osnitsi]"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i ot zloto na zavistnik, kogato zavidi!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Opazil me Gospodŭt na khorata"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladetelyat na khorata"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogŭt na khorata"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zloto na shepneshtiya sŭblazni [satana], pobyagvashtiya"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koĭto nashepva v gŭrdite na khorata"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "[satana] ot dzhinovete i ot khorata!”"
        }
    ]
}