{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogato Sŭbitieto nastŭpi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nikoĭ ne shte vŭzpre negovoto nastŭpvane"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "prinizyavashto i vŭzdigashto"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kogato zemyata se raztŭrsi v trus"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i planinite se razlomyat na otlomki"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanat razpilyana prakh"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vie shte stanete na tri grupi"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "edni - otdyasno, - kakvo shte e za tezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i drugi - otlyavo, - kakvo shte e za tezi otlyavo"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i pŭrventsite [v bogoboyazlivostta], - te sa pŭrventsite [v Raya]"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "tezi sa priblizhenite [na Allakh]"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v Gradinite na blazhenstvoto"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "povecheto sa ot predtsite"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i maltsina ot setnite"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na ukraseni prestoli"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblegnati tam edin sreshtu drug"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Vechnomladi yunoshi im podnasyat"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bokali i kani, i chashi [vino] ot izvor"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ne shte gi boli glava ot tova i ne shte gubyat um"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I plod, kakŭvto izberat"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i ptiche meso, kakvoto obichat"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I shte imat khubavitsi s golemi ochi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "podobni na skriti biseri"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "vŭzdayanie za delata im"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ne shte slushat tam nito praznoslovie, nito podbuda kŭm gryakh"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "a samo slovoto: “Mir! Mir!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A onezi otdyasno... Kakvo shte e za onezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Sred lotosi bez bodli"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i natezhali bananovi dŭrveta"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i prostorna syanka"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i leeshta se voda"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i obilen plod"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nesekvasht i nevŭzbranen"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i vŭzdignati posteli"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Sŭtvorikhme gi Nie s [prekrasno] sŭtvoryavane"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i otredikhme da sa devitsi"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lyubeshti, vrŭstnichki"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za onezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "grupa ot predtsite"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "i grupa ot setnite"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A onezi otlyavo... Kakvo shte e za onezi otlyavo"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Sred izgaryasht vyatŭr i vryashta voda"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i syanka ot cheren dim"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "nito prokhladna, nito priyatna"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Te zhiveekha sred razkosh predi tova"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I uporstvakha v ogromniya gryakh"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I kazvakha: “Nima kato umrem i stanem prŭst i kosti, nima naistina shte bŭdem vŭzkreseni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "A nima i drevnite ni predtsi?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kazhi [o, Mukhammad]: “I drevnite, i setnite"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "naistina shte bŭdat sŭbrani po vreme na znaĭniya Den.”"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Posle, o, vie, zabludenite, otrichashtite"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "naistina shte yadete ot dŭrvoto Zokum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "i shte si pŭlnite ot nego stomasite"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "i shte piete sled tova ot vryashtata voda"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Shte piete, kakto piyat prezhadnelite kamili"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Tova e ugoshtenieto im v Sŭdniya den"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nie vi sŭtvorikhme. Zashto togava ne povyarvate"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ne vizhdate li semeto, koeto izlivate"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Vie li go sŭtvoryavate, ili Nie sme Tvoretsa"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nie otredikhme smŭrtta za vas i ne shte bŭdem vŭzpreni"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "da promenim takiva kato vas i da vi presŭzdadem v neshto, koeto ne vi e poznato"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "I uznakhte veche pŭrvoto sŭtvoryavane. Ne shte li se pouchite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "I ne vizhdate li kakvo seete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Vie li go karate da raste, ili Nie go karame"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ako pozhelaekhme, bikhme go storili na sukhi sŭchki i shtyakhte da ostanete slisani"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Naistina sme zadlŭzhneli"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Da, i sme lisheni [ot prekhrana].”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "I ne vizhdate li vodata, koyato piete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Vie li ya izsipvate ot oblaka ili Nie ya izsipvame"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ako pozhelaekhme, bikhme ya storili solena, zashto togava ne ste priznatelni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ne vizhdate li ogŭnya, koĭto razpalvate"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Vie li sŭzdavate dŭrvoto za nego, ili Nie sme Sŭzdatelya"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nie go storikhme za pouchenie i za polzvane ot onezi, koito se nuzhdayat"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Zatova proslavyaĭ imeto na tvoya Gospod, Prevelikiya"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "No ne, kŭlna se v zalezite na zvezdite"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "a tova e kletva - ako znaete - velika"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "tova naistina e sveshten Koran"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "v sŭkhranenata Kniga [-maĭka]"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Dokosvat go samo prechistenite"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Nizposlanie ot Gospoda na svetovete"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Nima tova Slovo prenebregvate"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "I [vmesto blagodarnost] za prepitanieto vie go vzimate za lŭzha"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "I kogato [dushata na umirashtiya] stigne gŭrloto"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "togava vie samo gledate"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "a Nie sme po-blizo do nego ot vas, ala ne vizhdate"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "i zashto, ako ne shte bŭdete sŭdeni"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "ne ya vŭrnete obratno, ako govorite istinata"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "I ako [umirashtiyat] e ot priblizhenite [na Allakh]"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "togava - pokoĭ i udovletvorenie, i Gradinata na blazhenstvoto"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A ako e ot onezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "togava [shte mu se kazhe]: “Mir na teb, zashtoto si ot onezi otdyasno!”"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A ako e ot otrichashtite, zabludenite"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "togava - ugoshtenie ot vryashta voda"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "i izgaryane v Ogŭnya"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Tova e dostovernata istina"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Zatova proslavyaĭ imeto na tvoya Gospod, Prevelikiya"
        }
    ]
}