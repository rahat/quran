{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "O, ti, koĭto se obvivash [v svoite odezhdi]"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "stani i preduprezhdavaĭ"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "i tvoya Gospod vŭzvelichavaĭ"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "i drekhite si pochistvaĭ"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "i skvernostta otbyagvaĭ"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "i davaĭki, ne se stremi da poluchish vpoveche"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "i v imeto na tvoya Gospod bŭdi tŭrpeliv"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "I kogato se protrŭbi s Roga"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "togava tozi Den shte e tezhŭk Den"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "za nevernitsite - nelek"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ostavi na Men onzi, kogoto sŭtvorikh samoten"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "i komuto otredikh golyamo bogat·stvo"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "i sinove do nego"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "i vsichko mu ulesnikh"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "A posle toĭ lamti da mu nadbavya"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Ala ne! Pred Nashite znameniya toĭ uporstva"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Shte go podlozha na neponosimo mŭchenie"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Razmisli toĭ i pretseni"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "i - proklet da e! - kak pretseni"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "I otnovo - proklet da e! - kak pretseni"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Posle pogledna"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Posle se namrŭshti i navŭsi"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Posle se otvŭrna i se vŭzgordya"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "I reche: “Tova e samo razprostranena magiya"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Tova e samo khorska rech.”"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Shte go peka Az v Preizpodnyata"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "I otkŭde da znaesh ti kakvo e Preizpodnyata"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Tya nishto ne ostavya i ne poshtadyava"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "obgarya kozhata"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Pazyat ya devetnadeset [angeli]"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "I storikhme paziteli na Ogŭnya samo angeli, i storikhme broya im edinstveno za izpitanie na nevernitsite, za da se ubedyat darenite s Pisanieto i da se usili vyarata na povyarvalite, i da ne se sŭmnyavat darenite s Pisanieto i vyarvashtite, i za da kazhat onezi, v chii"
        }
    ]
}