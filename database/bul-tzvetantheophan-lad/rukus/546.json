{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gorko na vseki klevetnik-khulitel"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koĭto trupa bogat·stvo i vse go broi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Smyata, che negovoto bogat·stvo shte go uvekovechi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Ala ne! Shte bŭde khvŭrlen v Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A otkŭde da znaesh ti kakvo e Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Razpaleniyat Ogŭn na Allakh"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koĭto pronikva do sŭrtsata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Toĭ shte bŭde zatvoren nad tyakh"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "vŭv visoki stŭlbove"
        }
    ]
}