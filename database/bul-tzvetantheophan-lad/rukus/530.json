{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Stigna li do teb razkazŭt za Vsepokrivashtiya den"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Edni litsa v tozi Den shte sa svedeni"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "obremeneni, iznureni"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "shte goryat v plamtyasht ogŭn"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "shte im se dade ot kipyasht izvor da piyat"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ne shte imat druga khrana osven ot trŭni"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "koito nito ugoyavat, nito izbavyat ot glad"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "A drugi litsa v tozi Den shte sa blazheni"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "poradi svoeto staranie - dovolni"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "vŭv vŭzvishena Gradina"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ne shte slushat v neya praznoslovie"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ima tam techasht izvor"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Ima tam vŭzdignati prestoli"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i gotovi chashi"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i podredeni vŭzglavnitsi"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i razstlani kilimi"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "I nima ne vizhdat kak byakha sŭtvoreni kamilite"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i kak be vŭzdignato nebeto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i kak byakha vŭzpraveni planinite"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i kak be razprostryana zemyata"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "I napomnyaĭ! Ti si samo za da napomnyash"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ne si nad tyakh vlastvasht"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "no koĭto se otvŭrne i ne povyarva"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "nego Allakh shte mŭchi s naĭ-golyamoto mŭchenie [v Ada]"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Pri Nas e tyakhnoto zavrŭshtane"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Posle Nasha grizha e tyakhnata ravnosmetka"
        }
    ]
}