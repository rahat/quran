{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Kŭlna se v [konete -] prepuskashtite s prŭkhtene"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "i myatashtite iskri s chatkane"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "i vrŭkhlitashtite sutrin"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "i vdigashti s tova prakholyak"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "i vryazvashti se v mnozhestvo [ot vragove]"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "chovekŭt e golyam neblagodarnik kŭm svoya Gospod"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "I sam na tova e svidetel"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "I e silna lyubovta mu kŭm blagata"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nima ne znae, che kogato bŭde izkarano tova, koeto e v grobovete"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i bŭde razkrito tova, koeto e v gŭrdite"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "tekhniyat Gospod e svedusht za tyakh v tozi Den"
        }
    ]
}