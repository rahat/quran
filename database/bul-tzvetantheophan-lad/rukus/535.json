{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kŭlna se v utroto"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i v noshtta, kogato nastŭpva mrak"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ne te izostavi tvoyat Gospod [o, Mukhammad] i ne te vŭznenavidi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Otvŭdniyat zhivot e po-dobŭr za teb ot nastoyashtiya"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I shte ti vŭzdade tvoyat Gospod, i shte si dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ne te li nameri Toĭ sirak i ne te li priyuti"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I te nameri zabluden, i te napŭti"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I te nameri beden, i te zamogna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Tŭĭ che ne ogorchavaĭ siraka"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I ne otpŭzhdaĭ umolyavashtiya"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "I za blagodatta na tvoya Gospod vŭzvestyavaĭ"
        }
    ]
}