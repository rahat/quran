{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “O, nevernitsi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "az ne sluzha na tova, na koeto vie sluzhite"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "i vie ne sluzhite na tova, na koeto az sluzha"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I az ne sluzha na tova, na koeto vie sluzhite"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "i vie ne sluzhite na tova, na koeto az sluzha"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vie si imate vashata religiya i az imam svoyata religiya.”"
        }
    ]
}