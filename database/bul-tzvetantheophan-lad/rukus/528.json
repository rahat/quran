{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kŭlna se v nebeto i vŭv Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A otkŭde da znaesh ti kakvo e Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvezdata s pronizvashta svetlina"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nad vsyaka dusha ima nadzornik"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "I da pogledne chovekŭt ot kakvo be sŭtvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Be sŭtvoren ot iztlaskvashta se voda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koyato izliza izmezhdu grŭbnaka i rebrata"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Sposoben e Toĭ da go vŭrne"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v Denya, kogato shte bŭdat izyaveni taĭnite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "i ne shte ima toĭ nito sila, nito izbavitel"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Kŭlna se v nebeto, ot koeto vali"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i v zemyata, koyato se razpukva"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "che tova e slovo-razgranichenie"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "A ne e shega"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Te zamislyat s umisŭl"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I Az zamislyam s umisŭl"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "I daĭ ot·srochka na nevernitsite, ot·srochi gi za kratko"
        }
    ]
}