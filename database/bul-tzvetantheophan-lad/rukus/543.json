{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I otkŭde da znaesh ti kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Denya, v koĭto khorata shte bŭdat kato prŭsnati peperudi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "i planinite shte bŭdat kato razpokŭsana vŭlna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onzi, chiito vezni natezhat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "shte ima zhivot v dovolstvo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onzi, chiito vezni oleknat"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "zavrŭshtaneto mu e bezdna"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A otkŭde da znaesh ti kakvo e tya"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Plamtyasht ogŭn"
        }
    ]
}