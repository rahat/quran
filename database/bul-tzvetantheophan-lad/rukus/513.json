{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "togava kŭm tvoya Gospod shte e otpravyaneto"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Toĭ nito vyarvashe, nito ot·sluzhvashe molitvata"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "a otrichashe i se otvrŭshtashe"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Posle otivashe pri svoite khora s gordeliva pokhodka"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Gorko ti, gorko"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "I otnovo - gorko ti, gorko"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Nima chovek smyata, che shte bŭde ostaven bez nadzor"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Posle be sŭsirek, a Toĭ go sŭtvori i osŭrazmeri"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "I sŭzdade ot neya dvata pola - mŭzha i zhenata"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Nima Toĭ ne e sposoben da sŭzhivi mŭrtvite"
        }
    ]
}