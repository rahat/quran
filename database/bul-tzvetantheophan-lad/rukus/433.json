{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Dŭrvoto Zokum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "e khranata na vseki greshnik"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Podobno raztopen metal tya shte kipi v stomasite"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "syakash kipi vryashta voda"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "“Vzemete go i go zamŭknete v sredata na Ada"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Posle izleĭte vŭrkhu glavata mu ot mŭchenieto na vryashtata voda"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "“Vkusi! Nali ti be mogŭshtiyat, znatniyat.”"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Tova e, v koeto se sŭmnyavakhte"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Bogoboyazlivite shte sa na sigurno myasto"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "sred gradini i izvori"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "shte sa oblecheni v koprina i brokat, edin sreshtu drug"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Taka! I shte im dadem za sŭprugi krasavitsi s golemi ochi"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Shte pozhelavat tam vsyakŭkŭv plod - v sigurnost"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Ne shte sa vkusili tam druga osven pŭrvata smŭrt [v zemniya im zhivot]. Shte gi opazi Toĭ ot mŭchenieto na Ada"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "blagodat ot tvoya Gospod. Tova e velikoto spasenie"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Storikhme go [Korana] lesen - na tvoya ezik, za da se pouchat"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "I chakaĭ! Te sŭshto ochakvat"
        }
    ]
}