{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Toĭ se namrŭshti i obŭrna grŭb"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "che pri nego e doshŭl slepetsŭt"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "No ti otkŭde znaesh, mozhe da se prechisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ili da se pouchi, ta da mu e ot polza poukata"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A onzi, koĭto e bezrazlichen"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "s nego si se zael"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "I ne e tvoya grizha, che ne se prechistva"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A ot onzi, koĭto e doshŭl pri teb s uporitost"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i e bogoboyazliv"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ot nego ti se otklonyavash"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ala ne! Tova e pouchenie"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i koĭto pozhelae, toĭ se pouchava"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "v Pisaniya pochitani"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vŭzdignati, prechisteni"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "[prepisani] ot rŭtse na [angeli-] pisari"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "dostoĭni, blagochestivi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet da e chovekŭt! Kolko e neblagodaren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ot kakvo neshto go e sŭtvoril Toĭ"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ot chastitsa sperma Toĭ go sŭtvori i osŭrazmeri"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Posle pŭtya mu oblekchi"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Posle go umŭrtvi i poveli da bŭde pogreban"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Posle, kogato Toĭ pozhelae, shte go vŭzkresi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ala ne! Ne izpŭlni toĭ Negovata povelya"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Da pogledne chovekŭt svoeto prepitanie"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Nie prolivame obilno vodata"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Posle razpukvame zemyata"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i pravim ot neya da poniknat zŭrna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grozde, i zelenina"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i maslini, i palmi"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i gŭsti gradini"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i plod, i treva"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "za polzvane ot vas i ot vashiya dobitŭk"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "I kogato Grokhotŭt nastŭpi"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v Denya, kogato mŭzhŭt izbyaga ot brat si"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i ot maĭka si, i ot bashta si"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i ot zhena si, i ot detsata si"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "vseki ot tyakh v tozi Den shte e zaet s delo, koeto shte go poglŭshta"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Edni litsa v tozi Den shte bŭdat siyaeshti"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "zasmeni, vŭzradvani"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a po drugi v tozi Den shte ima prakh"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "shte gi pokriva chernilka"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tezi sa nevernitsite, nechestivtsite"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogato slŭntseto bŭde obvito [v mrak]"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kogato zvezdite izpopadat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kogato planinite bŭdat razdvizheni"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kogato bremennite kamili bŭdat izostaveni"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kogato zverovete bŭdat nasŭbrani"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kogato moretata zakipyat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kogato dushite se sŭedinyat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kogato zhivopogrebanoto momiche bŭde popitano"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakŭv gryakh e bilo ubito"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kogato knigite [za delata] bŭdat razgŭrnati"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kogato nebeto bŭde premakhnato"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kogato Adŭt bŭde nazhezhen"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kogato Rayat bŭde priblizhen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "togava vseki shte uznae kakvo e izvŭrshil"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "O, kŭlna se v zalyazvashtite planeti"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "dvizheshti se, skrivashti se"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i v noshtta, kogato se zdrachava"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i v utrinta, kogato se razvidelyava"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "toĭ [Koranŭt] e slovo na dostoen pratenik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "imasht sila, utvŭrden pri Vladetelya na Trona"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Pokoryavat mu se [angelite] tam, doveren e"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I ne e lud vashiyat drugar [Mukhammad]"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Toĭ go vidya na yasniya khorizont"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I [vesti] ot nevedomoto ne ukriva"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "I ne e [Koranŭt] slovo na prokuden satana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kŭde otivate togava"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tova e samo pouchenie za svetovete"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "za vseki ot vas, koĭto zhelae da e na Praviya pŭt"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Togava shte pozhelavate samo onova, koeto zhelae Allakh, Gospodŭt na svetovete"
        }
    ]
}