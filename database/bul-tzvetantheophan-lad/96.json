{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Cheti [o, Mukhammad] v imeto na tvoya Gospod, Koĭto sŭtvori"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sŭtvori choveka ot sŭsirek"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Cheti! Tvoyat Gospod e Naĭ-shtedriya"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Onzi, Koĭto nauchi chrez kalema"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "nauchi choveka na onova, koeto ne e znael"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ala ne! Chovek prestŭpva"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "zashtoto se e vidyal bogat"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Kŭm tvoya Gospod e zavrŭshtaneto"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidya li ti onzi, koĭto prechi"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "na rab, kogato ot·sluzhva molitvata"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Vidya li dali toĭ be na praviya pŭt"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili povelyava bogoboyazŭnta"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Vidya li dali toĭ otricha i se otmyata"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ne znae li toĭ, che Allakh vizhda"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ala ne! Ako ne prestane, shte go povlechem za perchema"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "perchema lŭzhliv, greshen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "I da zove toĭ svoeto sborishte"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nie shte prizovem strazhata [ot angelite, koito pazyat Ada]"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ala ne! Ne mu se podchinyavaĭ! I svezhdaĭ chelo do zemyata v sudzhud, i [kŭm Allakh] se priblizhavaĭ"
        }
    ]
}