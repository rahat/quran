            "verse": 1,
            "text": "Di: \"El es Al-lah, Uno"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Al-lah es el Absoluto"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "No engendro ni fue engendrado"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Y no hay nada ni nadie que sea semejante a El"
        }
    ]
