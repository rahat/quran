{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ana jinyyiche abali ja chakuunichila (ŵandu ni yogoyo yakwe, jajili Kiyama)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ngope sine lyele lisikulyo sichiŵa syakunyosyeka"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Syasyapanganyisye masengo (gasambi ku duniya), syakupela (nikwaluka ku Akhera)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Sichijinjila ku Moto Wakupoteka nnope"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Sichimwesyedwa mu tutumule jwansemule kusyene"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Ngasakola ŵanganyao yakulya ikaŵeje miŵa janchongwe"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Jangajimbasya soni jangakamuchisya ku sala"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ngope sine lyele lisikulyo sichiŵa m’chindimba"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Ligongo lya masengo gakwe (gambone) sichiŵa syakusengwa"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Ku Mbepo Japenani"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ngasisija kupikana kwalakweko yangali mate"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Kweleko kwana tutumule jwakujilimaga"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Kweleko kwana itengu yauchimbichimbi yenyakulidwe"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ni migawo jeŵichidwe (chilepe)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ni misago jejalidwe mu ntungo wambone"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ni yakutandika yaofuofu yetandichidwe posepose"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ana ngakujiona ngamiya yajatite kagumbidwe"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ni liwunde yalyatite kanyakulidwe"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ni matumbi yagatite kasimichidwe"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ni litaka yalyatite katandichidwe"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Basi kumbusyani, chisimu mmwejo ni nkukumbusya"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nganim'batu mmwe ŵakwakanganichisya ŵanganyao (chikulupi)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ikaŵeje jwatagalauche (kuleka masengo gambone) ni kufulu (kuleka Usilamu)"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Basi Allah tachinnagasya ni ilagasyo yekulungwa nnope"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Chisimu Kukwetu pe ni kwakuujila kwao"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Soni chisimu kuŵalanjidwa kwao kuli Nowe"
        }
    ]
}