{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ngulilumbila liwunde ni chakwisa chilo"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ana ni chichi champaka chimmanyisye ya chakwisa chilo"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Jalakwejo ni) ndondwa jakupowola (chipi ligongo lya kuŵala nnope)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pangali mundu jwalijose ikaŵeje kwa jwalakwejo kwana nkunnolela (jwaakusunga itendo yakwe)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Basi achetelele mundu, ana agumbidwe kuumila ku chichi"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Agumbidwe kuumila ku mesi gakusumbika"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Gakopokaga kutyochela chilikati cha maupa ga n'gongo (wa ŵalume) ni maupa ga chidali (cha ŵakongwe)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Chisimu Jwalakwe (Allah) pakum'buchisya (kuwumi mundujo) aŵele Jwakombola kusyene"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Lisiku lyasichionechela papaswela asili (syosope)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ni ngasakola (mundujo) machili galigose namuno nkulupusyo"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ngukulumbila kwinani kwagwisyagwisya wula"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ni ngulilumbila soni litaka lyaliganjikaga (pakoposya mmela)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Chisimu jalakwe (Qur’aniji) gaŵele maloŵe gakulekanganya (chilikati cha yakuona ni yaunami)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Soni jalakweji nganijiŵa chindu chambenje"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Chisimu ŵanganyao akuŵa ali nkutenda malindi (kwa Asilamu)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "None ni ngwauchisya malindi (gaogo pangaapa ilagasyo chitema kuti alilechelele)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Basi, mwalechelesyani achimakafili, mwalechelelani panandipe (chaiwone)"
        }
    ]
}