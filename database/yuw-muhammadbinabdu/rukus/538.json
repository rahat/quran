{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Soomani mu lina lya Ambuje ŵenu (Allah) ŵaagumbile"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Agumbile mundu kuumila mu chipande chamyasi jekamatane"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Soomani! Soni Ambuje ŵenu ni Ŵantuka kusyene"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Aŵala ŵaajiganyisye (kulemba) ni chakulembela"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "An'jiganyisye mundu yaŵalekaga ngaimanya"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Yakuona! Chisimu mundu akuŵaga jwakusumba mpika (pa kulikwesya)"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ligongo lyakuliona kusichila"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Chisimu kwa Ambuje ŵenu pe (Allah) ni kwakuujila"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ana mum'bweni ajula jwaakuŵa nkunkaanya"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Kapolo (jwa Allah) ndema jajukusaka kuswali"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ana nkuyiona uli najuŵaga (Muhammadi ﷺ) juli pachongoko"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Kapena jukulamula ya woga (wakun'jogopa Allah)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Nambi nkuyiona uli naŵaga (Abujahalio) akukanila ni kusyulula"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ana ngaakumanyilila yanti Allah akwaona (yaakutenda)"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ngwamba! Chisimu naga ngaakuleka, chiitwawuta umbo syapewuwo (pakwaponya ku Moto)"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Liwuwo lyakwe lyaunami soni lyakulemwa"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Basi ŵaaŵilanje ŵandu ŵakwe (kuti ankamuchisye)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nowe chitwaŵilanje Azabaniya (asilikali ŵa ku Moto)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ngwamba! Mmwe ngasimunkunda jwalakwejo, nambo nsujuduji (kuswali) ni n’diŵandichisyeje (kwa Allah)"
        }
    ]
}