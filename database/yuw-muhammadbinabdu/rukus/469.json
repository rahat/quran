{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Patichitendekwa chakutendekwa (Kiyama)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Pangali jwakanila yakutendekwa kwakwe"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Chalakwe ni) chakwaatulusya (achimakafili pakwajinjisya ku Moto) ni kwakwesya (Asilamu pakwajinjisya ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Ndema jalichitenganyika litaka kutenganyika kwamachili"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Matumbi ni kusyajidwa-syajidwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Ni kuŵa luwundu lyakuwululusyidwa"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ni tinchiŵa jenumanja (pa Kiyama) mikutula jitatu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Basi (pachiŵa pana) ŵandu ŵa kunkono wan’dyo, ana ŵandu ŵa kunkono wan’dyo ni ŵapi (uchimbichimbi wao uchiŵa uli)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ni (pachiŵa pana soni) ŵandu ŵa kunkono wanchiji, ana ŵandu ŵa kunkono wanchiji ni ŵapi (kusakala kwao kuchiŵa uli)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ni (pachiŵa pana soni) ŵaŵalongolele (pakutenda yambone ku duniya) ŵatachiŵa ŵakulongolela (ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ŵanganyao ni ŵatachiŵa ŵakuŵandichisyidwa (kwa Allah)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Tachiŵa m’Matimbe Gachindimba (ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Likuga lyejinji (lya akulongolela ku yambone) lichiŵa lyakutyochela mu ŵandu ŵandanda"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Sano (likuga) lyannono (lya akulongolela ku yambone) lichiŵa lyakutyochela mu ŵandu ŵakumalichisya"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ali pa itengu yauchimbichimbi yeluche (ni golodi ni mitapo jine)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Tachiyijegamaga mwakulolegana"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tachasyungulaga (pa kwatumichila) achachanda ŵakutamilichika (pa uchanda)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Ni migao kwisa soni mabilika (ga yakumwa), ni ipanda ya ukana wakuumila mu tutumule jwakujilimaga"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ngasalwala nao ntwe namuno kutyoka lunda"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ni isogosi iliyose yatakaisaguleje"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ni nyama ja ijuni jatakajilaluchileje"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ni achimmasyeto ŵameso gakulanguka soni gekulungwa mwakusalalila"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Ŵakuswejela) nti chisawu ngalekale syesunjidwe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Kuŵa malipilo pa yaŵaliji nkupanganya"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ngaaja kupikana mwalakwemo maloŵe gangali mate, namuno gakutendekasya kupata sambi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ikaŵeje maloŵe ganti: Ntendele, ntendele"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Sano (pachiŵa pana) ŵandu ŵa kunkono wan’dyo, ana ŵandu ŵa kunkono wan’dyo ni ŵapi (uchimbichimbi wao uchiŵa uli)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Tachiŵa) mu itela ya masaawu yetyosye miŵa"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ni itela ya magombo yesogole magombo gakwe mwakusajikana chenene"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ni miwulili jakwendelechela"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ni mesi gakujilima ndaŵi ni katema"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ni isogosi yejinji"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Yangakatichisya kasogole nambo soni yangasiŵiilidwa"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ni yakutandika yenyakulidwe (pa itengu yauchimbichimbi)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Chisimu Uwwe chitukaaguumbe ŵanganyao (achakongwe ŵakuumila ku duniya) kagumbidwe kane (kakusalala nnope kulekangana ni muŵaŵelele ku duniya)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Basi chitukatende kuŵa ŵaali"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Ŵakulinonyelesya kwa achalume ŵao, ŵakulandana misingu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "(Tachiŵa) ŵa (achalume) ŵa kunkono wan’dyo"
        }
    ]
}