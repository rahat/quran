{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ngusilumbila hachi syasikuwutuka sili nkupumasika"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ni syasikoposya moto mu ikotwa mwakwe (pakuwutuka m’maganga)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Ni syasikwagungumuchila amagongo kumasikusiku"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Nikututumya palakwepo luwundu lwejinji"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Nikulijinjilila palakwepo likuga (lya amagongo)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Chisimu mundu kwa Ambujegwe (Allah) juŵele jwakanila (ukoto waakumpa)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Soni chisimu jwalakwe jukuŵichila umboni pa yalakweyo (kupitila mu itendo yakwe)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Soni chisimu jwalakwe pakuchinonyela chipanje juŵele jwakupelenganya nnope"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ana ngajukumanyilila pandema jaichikoposyedwa yaili m’malembe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Nikwisa kwilochesya papaswela yaili m’mitima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Chisimu lyele lisikulyo M’mbuje gwao (Allah) tachamanya abali syosope (syakwamba yaŵapanganyaga ŵanganyao)"
        }
    ]
}