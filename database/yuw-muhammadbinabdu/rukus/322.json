{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Achina Adi ŵakaaniile achimitenga"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Ndema jaŵasalile nnongo n’jao Hudu kuti: “Ana ngankogopa?”"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "“Chisimu une kukwenu ni Ntenga jwakulupichika.”"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "“Basi mun’jogopani Allah ni kumbikanila une.”"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "“Soni ngangun’juga pa yalakweyi malipilo, nganigaŵatu malipilo gangu ikaŵeje kwa M’mbuje jwa iwumbe yosope.”"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "“Ana nkutaŵa pachikwesya palipose pakusalala nyumba syeleuleu mwamasanje (pagamba kusaka kulosya machili ngaŵa syagonamo)?”"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "“Ni nkulitaŵila makome gamakulungwakulungwa nti chisawu tinnonjele?”"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "“Soni ntite mpute ngondo nkasaputa mwakulikwesya.”"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "“Basi mun’jogopani Allah ni kumbikanila une.”"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "“Soni mun’jogopani Ajula jwampele ichindu (yambonembone) yankuimanyilila.”"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "“Ampele ilango ni ŵanache.”"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "“Kwisa soni migunda ni sulo syatutumule.”"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "“Chisimu une ngun’jogopela ŵanganyammwe ilagasyo ya lisiku lyekulungwa.”"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Ŵanganyao ŵatite: “Yalumope kukwetu, n’jamuche kapena nkaŵa mwa akwamuka.”"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "“Nganiyiŵatu ayi (yatukutendayi) ikaŵeje nyendele ja ŵandu ŵandanda.”"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "“Soni nganituŵatu uwwe ŵanti ni kupedwa ilagasyo.”"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Basi ŵankaaniile, ni twajonasile. Chisimu mu yalakweyi mwana chilosyo, nambo ŵajinji mwa ŵanganyao nganaŵa ŵakulupilila"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Soni chisimu Ambuje ŵenu ni ŵali Ŵamachili gakupunda, Ŵachanasa channope"
        }
    ]
}