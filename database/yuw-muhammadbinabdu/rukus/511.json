{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Ngwamba! Nguulumbila mwesi"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Ni chilo ndema jachikutyoka (pakucha)"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ni kumasikusiku ndema jakukulanguka"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Chisimu Walakwe (Motowo) uŵele limpepe (m’makungu) gekulungwa nnope"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Chitetelo kwa ŵandu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Kwa jwalijose mwa jenumanja jwaasachile kuti alongolele (pakutenda yambone) kapena kusigalila (pakupanganya yakusakala)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Mundu jwalijose tachiŵa uwombosi wakuliombolela ku (yakusakala) yaŵapanganyisye (pakutaŵidwa ku Moto)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Ikaŵeje aŵala ŵakunkono wan'dyo"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "(Chachiŵa) m’Matimbe ga ku Mbepo achiwuusyanaga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Ya (ŵandu) ŵakuleŵa"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Ana ni chichi chachin'jinjisye ŵanganyammwe ku Saqara?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "(Pakwanga) chachitiji: “Nganituŵa mwa akuswali.”"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "“Soni nganituŵa nkwaaliisya masikini.”"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "“Ni twaliji nkutiŵila (mu sambi) pamo ni akutiŵila.”"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "“Ni twaliji soni nkukanila ya Lisiku Lyamalipilo.”"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "“Mpaka chatuichilile chiwa.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Basi ngasichija kwakamuchisya ŵanganyao chondechonde cha akuchondelela"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Ana chipali chichi kukwao chikwatendekasya kuchitundumalila chikumbusyo (Qur’an)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "(Kakwipuche kakwe kuti akajipikana Qur’an) gamba kuŵa ŵanganyao mpela mbunda (syamwitinji) syasikwipusyidwe"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Syasikutila lisimba"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Yeti mundu jwalijose mwa ŵanganyao akusakaga ali apedwile ikalakala yeunukule-unukule (kuumila kwa Allah kutendela umboni yakuŵecheta Muhammadi ﷺ)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Ngwamba! Nambo kuti ŵanganyao ngaakujijogopa Akhera"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Ngwamba! Chisimu jalakwe (Qur’aniji) jiŵele chikumbusyo"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Basi jwaakusaka taalikumbusye najo"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ni nganaŵa alikumbwisye ŵanganyao ikaŵeje naga Allah ali asachile. Jwalakwe ni jwaali Jwakuŵajilwa kogopedwa soni ni Nysene chikululuko"
        }
    ]
}