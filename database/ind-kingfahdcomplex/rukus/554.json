{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Katakanlah, \"Dia-lah Allah, Yang Maha Esa"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah adalah Tuhan yang bergantung kepada-Nya segala sesuatu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Dia tiada beranak dan tiada pula diperanakkan"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "dan tidak ada seorang pun yang setara dengan Dia"
        }
    ]
}