{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Katakanlah, \"Hai orang-orang kafir"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Aku tidak akan menyembah apa yang kamu sembah"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Dan kamu bukan penyembah Tuhan yang aku sembah"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Dan aku tidak pernah menjadi penyembah apa yang kamu sembah"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "dan kamu tidak pernah (pula) menjadi penyembah Tuhan yang aku sembah"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Untukmulah agamamu dan untukkulah agamaku"
        }
    ]
}