{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Katakanlah, \"Aku berlindung kepada Tuhan (yang memelihara dan menguasai) manusia"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Raja manusia"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Sembahan manusia"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Dari kejahatan (bisikan) setan yang biasa bersembunyi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "yang membisikkan (kejahatan) ke dalam dada manusia"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "dari (golongan) jin dan manusia"
        }
    ]
}