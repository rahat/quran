{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki: O Allah, birdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Her sey ve herkes, ona muhtactır, onun zevali yoktur, birseye muhtac degildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Dogurmaz ve dogmamıstır"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ve ona, bir tek esit ve benzer olamaz, yoktur"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sıgınırım karanlıgı yarıp ısıtan sabahın Rabbine"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattıgı mahlukların serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Cokup etrafı kapladıgı zaman karanlıgın serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve dugumlere ufleyen kadınların serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hasetcinin haset ettigi zaman, serrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sıgınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların sahibine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların mabuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Gizlice, sinsisinsi vesveseler verenin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Oylesine ki insanların gonullerine vesveseler sokar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinden olsun, insandan olsun, bu cesit kisilerin serrinden"
        }
    ]
}