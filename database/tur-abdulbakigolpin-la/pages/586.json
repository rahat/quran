{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Gunes durulunce"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Ve yıldızlar kararınca"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Ve daglar yurutulunce"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Ve disi develer bile bası bos bırakılınca"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Ve vahsi hayvanlar bile bir araya toplanınca"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ve denizler, cosup kabarınca"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Ve insanlar, haldesleriyle birlesince"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Diridiri gomulen kıza sorulunca"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Hangi suc yuzunden olduruldun diye"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ve sahifeler dagılınca"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Ve gogun perdesi kaldırılınca"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Ve cehennem alevlendirilince"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ve cennet yaklastırılınca"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Herkes bilir ne hazırladıgını"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Artık andolsun donup kaybolan"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Dogup yuruyen ve burclarına giren yıldızlara"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Ve gecmeye basladıgı cagda, geceye"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ve ısıdıgı cagda, sabaha"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Suphe yok ki Kur'an, buyuk bir elcinin sozudur"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Kuvvetlidir, ars sahibinin katında kadri yuce"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Itaat edilir, emniyetlidir de"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Sizinle konusan, deli degildir"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ve andolsun, onu, apaydın tanyerinde gordu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Arkadasınız, gizli seyler hakkında da nekes degildir"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ve Kur'an, taslanmıs Seytan'ın sozu de degildir"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Artık nereye gidiyorsunuz oyleyse"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "O, butun alemlere bir oguttur ancak"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ve hele icinizden dogru hareket etmek isteyene"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ve isteyemezsiniz, alemlerin Rabbi Allah istemedikce"
        }
    ]
}