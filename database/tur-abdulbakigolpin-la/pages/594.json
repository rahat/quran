{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keske der, onceden, daha sagken iyilik etseydim"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Derken o gun oylesine bir azaplandırır onu ki kimsecikler, o cesit azab edemez"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ve oylesine baglar onu ki kimsecikler, o cesit baglayamaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey iyideniyiye inanmıs, supheden kurtulmus can"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Don Rabbine, ondan razı olarak ve rızasını kazanmıs bulunarak"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Artık katıl kullarımın arasına"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Ve gir cennetime"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Andolsun bu sehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen oturmadasın bu sehirde"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ve babaya ve ogula"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Gercekten de biz insanı sıkıntı icinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Hicbir kimsenin, ona gucu yetmez mi sanır"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Ben, bircok mal helak ettim der"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Hicbir kimse, onu gormez mi sanır"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Onun icin halketmedik mi iki goz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ve bir dille iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ve ona iki sarp yol gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Derken dayanmadı o yokusa"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ve bilir misin, yokus nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Bir kul azat etmek"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Yahut aclık, kıtlık gununde doyurmak"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlıgı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut yerlere dosenmis bir yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra da inananlardan ve birbirlerine sabrı tavsiye edenlerden ve acımayı tavsiye edenlerden olmak"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Onlardır iste sag taraf ehli"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Delillerimize kafir olanlara gelince: Onlardır sol taraf ehli"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlaradır kapıları, ustlerine ortulmus ates"
        }
    ]
}