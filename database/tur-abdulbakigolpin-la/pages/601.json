{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Andolsun zamana"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Suphe yok ki insan, elbette zararda, ziyanda"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak inananlar ve iyi islerde bulunanlar ve birbirlerine gercegi gozetmeyi ve sabretmeyi tavsiye edenler baska"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Butun incedeninceye alay eden kovucuların vay hallerine"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oylesine ki mal yıgar ve onu sayardurur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanır ki gercekten de malı, onu ebedilestirir"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Is oyle degil, andolsun ki o, kırıp doken, silip supuren cehenneme atılır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ve ne bildirdi sana, o kırıp doken, silip supuren cehennem nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah'ın tutusturulmus bir atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oylesine ates ki yurekleri sarar, kaplar"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Suphe yok ki ustlerine kapıları kapanmıstır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Upuzun uzatılmıs direklerle"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Fil ashabına Rabbin ne yaptı, neler etti, gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Duzenlerini bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ve onlara, cesitli yerlerden bolukboluk, birbiri ardınca kuslar gondermedi mi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onları, balcıktan taslarla tasladılar"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Onlar da ici bos ekin saplarına, kırılıp ezilmis samanlara donduler"
        }
    ]
}