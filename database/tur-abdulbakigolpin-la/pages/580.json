{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Ve geceleyin de secde et artık ona ve tenzih et uzun gecelerde onu"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Suphe yok ki bunlar cabucak gelipgeceni severler de o agır gunu artlarına atar, bırakırgiderler"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Biz yarattık onları ve kuvvetlendirdik yaratılıslarını ve dilersek onları degistiririz de yerlerine, onlara benzer baskalarını getiririz"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Suphe yok ki bu, bir oguttur, artık kim dilerse Rabbine dogru, bir yol tutar"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Ve Allah dilemedikce onlar, dileyemezler; suphe yok ki Allah, her seyi bilir, hukum ve hikmet sahibidir"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Diledigini rahmetine alır; ve zalimlere gelince: Elemli bir azap hazırlamıstır onlara"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Andolsun, ardı ardınca, iyilikle gonderilenlere"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Siddetle esip yelenlere"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Bulutları yayıp surenlere"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Gercekle aslı olmayanı ayırt edenlere"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Ogutleri telkin edenlere"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Ozurle sucu yok etmek hususunda olsun, yahut korkutma hususuna ait bulunsun"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Suphe yok ki size vaat edilen, mutlaka olacak"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Yıldızların ısıkları sonunce"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ve gok yarılınca"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ve daglar, yerlerinden kopup dumduz olunca"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ve peygamberler toplanınca"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Hangi gun icin geciktirildi bunlar"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Ayırma gunu icin"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ve nedir, bilir misin ayırma gunu"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Vay hallerine o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Once gelenleri helak etmedik mi"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Sonra da son gelenleri tutar, katarız onlara"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Boyle yaparız gunahkarlara"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Vay hallerine o gun yalanlayanların"
        }
    ]
}