{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluga"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve geceye, karanlıgı basınca"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin, seni ne terketti, ne de darıldı sana"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve elbette ahiret, onceki dunyadan da hayırlıdır sana"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve elbette yakında Rabbin, oyle seyler verecek ki sana, sonucu razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir yetim olarak bulup da yeryurt vermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni, yol yitirmis bulup da yol gostermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ve seni yoksul bulup da zenginlik vermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Artık sen de yetimi horlama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve bir sey dileyeni bos cevirme, azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini an, soyle"
        }
    ]
}