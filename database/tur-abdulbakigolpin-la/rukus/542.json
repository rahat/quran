{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun soluya soluya kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Tırnaklarıyle bastıkca tastan kıvılcım sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah cagı, dusmanı basanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Derken her yanı toza, dumana boganlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken dusman toplulugunun ta ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Suphe yok ki insan, Rabbine karsı pek inatcıdır, pek nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve suphe yok ki o, buna tanıktır"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve suphe yok ki insan, hayrına yarıyan malamulke karsı da pek duskundur, pek nekestir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Fakat bilmez mi ki kabirlerdekiler, dısarı cıkınca"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve gonullerdekiler, meydana vurulup bilinince"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Suphe yok ki Rabbin, o gun, onların her seyini bilir elbette"
        }
    ]
}