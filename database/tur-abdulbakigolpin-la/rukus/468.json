        {
            "chapter": 55,
            "verse": 69,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "O cennetlerde guzel huylu guzeller var"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Beyaz tenli, kara gozlu, otaglarda huriler"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Eslerinden once ne bir insan dokunmus onlara, ne bir cin"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil ipeklilerle dusenmis sedirlere ve guzelim dosemelere yaslanırlar"
