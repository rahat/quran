        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin tapısına varmaktan korkana iki cennet var"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisinde de turluturlu nimetler var, cesitcesit agaclar"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Ikisinde de iki ırmak var, akar"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her cesit meyve, ciftercifter var"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Oylesine dosemelere yaslanacaklar ki astarları kalın ipekten, her iki cennetin, devsirilip toplanacak meyveleri de ellerinin altında, pek yakın"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "O cennetlerde, gozlerini, eslerinden ayırmayan ve eslerinden once ne bir insan tarafından dokunulmus, ne bir cin tarafından dokunulmus esler var"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "O esler, sanki yakut ve mercan"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı, iyilikten baska bir sey olabilir mi"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu iki cennetten baska iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Ikisi de koyu yesil"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de iki pınar var, fıskırıp cıkar da akar"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Her ikisinde de meyveler ve hurma ve nar var"
