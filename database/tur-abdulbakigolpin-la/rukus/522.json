{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yuzunu eksitti ve dondurdu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Yanına kor geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Belki o, arınacaktır, ne bilirsin"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut da ogut alacaktır da ondan faydalanacaktır"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Fakat ihtiyacı olmayana gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Artık sen onun ustune dustukce dusuyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "O arınmazsa sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ve fakat sana kosup gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ve korkan kisi"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen ondan gaflet ediyor, ona aldırıs bile etmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Oyle degil, suphe yok ki Kur'an, ancak bir oguttur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen dinler, ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Buyuk, serefli sayfalardadır"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yuceltilmistir, arıtılmıstır"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Yazıcıların ellerinde"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Buyuklerdir, hayırlı ve itaatlilerdir"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Geberesice insan, ne de kafirdir"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Onu, neden yaratmıstır"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir katre sudan; yaratmıstır onu da halden hale dondurmustur"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolu kolaylatmıstır da dunyaya getirmistir"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra oldurmustur onu da kabre sokmustur"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra da dilerse diriltir onu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Gercekten de insan, onun emrini tam yerine getirmedi gitti"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Artık insan, yedigine de bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Suphe yok ki biz, bir yagmurdur, yagdırdık"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra yeryuzunu bir iyice yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Derken orada tohumlar bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ve uzum ve yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ve zeytin ve hurma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ve cesitli buyuk agacları bulunan bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ve meyveler ve otlaklar"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın faydası icin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Derken adeta kulakları sagır eden o bagırıs gelip cattı mı"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "O gun, bir gundur ki kisi kacar kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ve anasından ve babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ve esinden ve cocugundan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ve onların herbirinin bir derdi var ki baskalarına bakmaya vakti bile yok"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Nice yuzler o gun parılparıl parlar"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Guler, sevinir"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ve nice yuzler o gun tozlarla bulanır"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Ustlerine bir karalıktır coker"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlardır kafirler, suclular"
        }
    ]
}