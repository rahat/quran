{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun goge ve geceleyin gelene"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve bilir misin nedir geceleyin gelen"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parıl parıl parlıyan yıldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir kimse yoktur ki onun bir gozetip koruyan memuru bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Artık insan, bir baksın neden yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yaratıldı sıcrayarak akan bir sudan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Belden cıkar ve kaburga kemiklerinin arasından"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Suphe yok ki olumden sonra tekrar onu hayata dondurmiye de gucu yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun, butun gizli seyler, meydana vurulur"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun ne bir gucu kalır, ne de ona yardım eden olur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun yagmur yagdıran goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve nebat bitirmek icin catlayıp yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Suphe yok ki o, her seyi ayırt eden kesin bir soz elbet"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve o, saka degil elbet"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Suphe yok ki onlar, bir duzendir, kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ben de onlara karsı koyup duruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Artık muhlet ver kafirlere muhlet ver onlara az bir muddet"
        }
    ]
}