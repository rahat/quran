{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Gordum mu yalanlayanı dini"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Iste budur o kimse ki horlar yetimi"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ve doyurmaz da, onayak olmaz da doyurmaya yoksulu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay hallerine o namaz kılanların"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Oylesine namaz kılanların ki namazlarını unuturlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ve onlar, butun islerini gosteris icin yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve zekat vermeyi menederler"
        }
    ]
}