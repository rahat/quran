{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Suphe yok ki cekinenler, golgeliklerdedir ve pınar baslarında"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "arzuladıkları meyveleri bulurlar"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Yiyin ve icin, afiyetler olsun yaptıklarınıza karsılık"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Suphe yok ki boyle mukafatlandırırız iyilik edenleri"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Vay hallerine o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Yiyin ve gecinin az bir muddet, suphe yok ki suclularsınız siz"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Vay hallerine o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Ruku edin denince onlara, ruku etmezler"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Vay hallerine o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Bundan sonra artık hangi soze inanırlar ki"
        }
    ]
}