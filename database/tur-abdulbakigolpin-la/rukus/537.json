{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun Tin'e ve Zeytun'a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ve Turı Sina'ya"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu emin sehre"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Gercekten de biz, insanı, en guzel bir surete sahip olarak yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da onu dondurduk, asagıların en asagısına attık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak inananlar ve iyi islerde bulunanlar baska, gercekten de onlara bitmez tukenmez, basa kakılmaz bir mukafat var"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Artık dini yalanlamana sebep nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hukmedenlerin en ustunu degil midir"
        }
    ]
}