{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Gok yarılınca"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Ve yıldızlar dokulup sacılınca"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ve denizler, kaynayıp karısınca"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ve kabirlerin altı ustune gelince"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Bilir herkes, neyi one surmustur, neyi sona bırakmıs"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "A insan, kerem sahibi Rabbine karsı seni gururlandıran ne"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Oylesine Rab ki seni yarattı, azanı duzup kostu da seni duzgun bir hale getirdi"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Diledigi surete de benzetti seni"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Is, sandıgınız gibi degil, hayır siz ceza gununu de yalanlıyorsunuz"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ve suphe yok ki size koruyucular memur edilmistir elbette"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Buyuktur onlar, yazarlar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Bilirler ne yaparsanız"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Ve suphe yok ki itaat eden iyi kisiler, elbette cennettedir"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ve suphe yok ki kotuluk edenler, elbette cehennemde"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Ceza gununde oraya girerler"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ve oradan hic ayrılmazlar"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ve bilir misin, nedir ceza gunu"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Sonra gene de bilir misin nedir ceza gunu"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Bir gundur ki hicbir kimse, hicbir kimseye yardım edemez o gun ve hukum, o gun Allah'ın"
        }
    ]
}