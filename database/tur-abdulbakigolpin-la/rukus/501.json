{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Artık is, sizin sandıgınız gibi degil, andolsun gordugunuze"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve gormediginize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Suphe yok ki bu, kerem sahibi bir elcinin sozu elbet"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve bu, sair sozu degil, ne de az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ve kahin sozu de degil, ne de az dusunursunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbinden indirilmistir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ve eger bize isnad ederek bazı laflar etseydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu kudretimizle alırdık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da elbette sah damarını ceker koparırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Artık buna mani olamazdı sizden hicbir kimsecik"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ve suphe yok ki Kur'an, cekinenlere oguttur"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ve suphe yok ki biz, elbette biliriz, sizden, yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve suphe yok ki Kur'an, kafirlere adeta bir hasrettir"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve suphe yok ki o, elbette gercegin ta kendisidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Artık pek ulu Rabbinin adını anarak tenzih et onu"
        }
    ]
}