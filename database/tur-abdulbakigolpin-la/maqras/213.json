{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kur'an'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "insanı halketti"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "ona dilleri, konusmayı belletti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes ve ay, hesapla hareket eyler"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Ve govdesiz bitki ve govdeli agac secde eder"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ve gogu yuceltti ve olcuyu koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Olcude, tartıda insafsızlık etmeyin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Teraziyi dogru tutun, adaletle tartın ve eksik tartmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryuzunu alcalttı halka"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyveler ve lifli, kabuklu hurmalar var"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yapraklı taneler var ve guzel kokulu otlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Iyice pismis gibi kupkuru balcıktan, insanı halketti"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Ve cinleri, cosup kaynayan atesten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlıyabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Rabbidir iki dogunun ve Rabbidir iki batının"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Iki denizi salmıstır, nerdeyse karısacaklar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Fakat aralarında bir berzah var, birbirlerine karısmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlıyabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Her ikisinden de inci ve mercan cıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Ve onundur denizde akıp giden daglar gibi gemiler"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Yerin ustunde ne varsa fanidir"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Ve ancak ululuk ve kerem ıssı Rabbinin zatıdır kalan"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Ondan ister kim varsa goklerde ve yeryuzunde; o, her gun bir istedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey iki agır topluluk, insanlar ve cinler, yakında hesabınıza bakacagız"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey cin ve insan toplulugu, olumden kurtulmak icin, goklerin ve yer yuzunun bucaklarından gecip kacmaya gucunuz yeterse gecin, kacın; fakat gecemezsiniz bir kudret olmadıkca"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Ustunuze bir ates yalımı ve erimis bakır gonderilir de kacamazsınız"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Derken gok yarılıp kırmızı bir gul rengine gelerek yag gibi eriyince"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "Ne insan, ne cin, artık o gun suclu mudur, sorulmaz"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular, yuzlerindeki alametten tanınırlar da percemlerinden ve ayaklarından tutulurlar"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste bu, sucluların yalanladıkları cehennem"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Cehennemle icecekleri kaynar suyun arasında dolanıp dururlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin tapısına varmaktan korkana iki cennet var"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisinde de turluturlu nimetler var, cesitcesit agaclar"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Ikisinde de iki ırmak var, akar"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her cesit meyve, ciftercifter var"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Oylesine dosemelere yaslanacaklar ki astarları kalın ipekten, her iki cennetin, devsirilip toplanacak meyveleri de ellerinin altında, pek yakın"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "O cennetlerde, gozlerini, eslerinden ayırmayan ve eslerinden once ne bir insan tarafından dokunulmus, ne bir cin tarafından dokunulmus esler var"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "O esler, sanki yakut ve mercan"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı, iyilikten baska bir sey olabilir mi"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu iki cennetten baska iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Ikisi de koyu yesil"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de iki pınar var, fıskırıp cıkar da akar"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Her ikisinde de meyveler ve hurma ve nar var"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "O cennetlerde guzel huylu guzeller var"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Beyaz tenli, kara gozlu, otaglarda huriler"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Eslerinden once ne bir insan dokunmus onlara, ne bir cin"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil ipeklilerle dusenmis sedirlere ve guzelim dosemelere yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Ululuk ve kerem ıssı Rabbinin adının sanı, pek yucedir"
        }
    ]
}