{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "potomu chto k nemu podoshel slepoy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Otkuda tebe znat'? Vozmozhno, on by ochistilsya"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili pomyanul by nastavleniye, i pominaniye prineslo by yemu pol'zu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Tomu, kto reshil, chto on ni v chem ne nuzhdayetsya"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ty udelyayesh' vnimaniye"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Chto zhe budet tebe, yesli on ne ochistitsya"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A togo, kto prikhodit k tebe so rveniyem"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i strashitsya Allakha"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty ostavlyayesh' bez vnimaniya"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No net! Eto yest' Nazidaniye"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i pust' pomyanet yego vsyakiy zhelayushchiy"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ono zapisano v svitkakh pochitayemykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "voznesennykh i ochishchennykh"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "v rukakh poslantsev"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "blagorodnykh i pokornykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Da sginet chelovek! Kak zhe on neblagodaren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Iz chego On sotvoril yego"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "On sotvoril yego iz kapli i sorazmeril (ili predopredelil stadii yego razvitiya)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "potom oblegchil yemu put'"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potom umertvil yego i pomestil v mogilu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potom, kogda pozhelayet, On voskresit yego"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "No net! On ne vypolnyayet togo, chto On prikazal yemu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' posmotrit chelovek na svoye propitaniye"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "My prolivayem obil'nyye livni"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "zatem rassekayem zemlyu treshchinami"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i vzrashchivayem na ney zerna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vinograd i lyutsernu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "masliny i finikovyye pal'my"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "sady gustyye (ili s moguchimi derev'yami)"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "frukty i travy"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na pol'zu vam i vashey skotine"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kogda zhe razdastsya Oglushitel'nyy glas"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v tot den' chelovek brosit svoyego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "svoyu mat' i svoyego ottsa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "svoyu zhenu i svoikh synovey"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ibo u kazhdogo cheloveka svoikh zabot budet spolna"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V tot den' odni litsa budut siyat'"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smeyat'sya i likovat'"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Na drugikh zhe litsakh v tot den' budet prakh"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "pokryvayushchiy ikh mrakom"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Eto budut neveruyushchiye greshniki"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogda solntse budet svernuto (ili poteryayet svoye siyaniye, ili budet vybrosheno)"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kogda padut zvezdy"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "kogda gory budut sdvinuty s mest (ili privedeny v dvizheniye)"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "kogda verblyuditsy na desyatom mesyatse beremennosti ostanutsya bez prismotra"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kogda dikiye zveri budut sobrany"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kogda morya zapylayut (ili perepolnyatsya; ili opusteyut; ili peremeshayutsya; ili stanut krasnymi)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "kogda dushi ob\"yedinyatsya (pravedniki ob\"yedinyatsya s pravednikami, a greshniki - s greshnikami; ili pravedniki soyedinyatsya s rayskimi guriyami, a neveruyushchiye - s d'yavolami; ili dushi soyedinyatsya s telami)"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kogda zarytuyu zhiv'yem sprosyat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakoy grekh yeye ubili"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kogda svitki s deyaniyami rabov budut razvernuty"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "kogda nebo budet sdernuto"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "kogda Ad budet razozhzhen s novoy siloy"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "kogda Ray budet priblizhen k pravednikam"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "togda poznayet dusha, chto ona prinesla"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "No net! Klyanus' telami nebesnymi - otstupayushchimi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "peredvigayushchimisya i ischezayushchimi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Klyanus' noch'yu, kogda ona nastupila (ili otstupila)"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Klyanus' rassvetom, kogda on zabrezzhil"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Eto - slova blagorodnogo poslantsa"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "obladatelya sily pri Vladyke Trona, pochitayemogo"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "kotoromu tam (na nebesakh) povinuyutsya, doverennogo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Vash tovarishch (Mukhammad) ne yavlyayetsya oderzhimym"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "On videl yego (Dzhibrila) na yasnom gorizonte"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i ne skupitsya peredat' sokrovennoye"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Eto - ne rechi d'yavola izgnannogo, pobivayemogo"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kuda zhe vy napravlyayetes'"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Eto - lish' Napominaniye miram"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tem iz vas, kto zhelayet sledovat' pryamym putem"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "No vy ne pozhelayete etogo, yesli etogo ne pozhelayet Allakh, Gospod' mirov"
        }
    ]
}