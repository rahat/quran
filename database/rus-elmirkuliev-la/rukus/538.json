{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Chitay vo imya tvoyego Gospoda, Kotoryy sotvoril vse sushcheye"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sotvoril cheloveka iz sgustka krovi"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Chitay, ved' tvoy Gospod' - Samyy velikodushnyy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "On nauchil posredstvom pis'mennoy trosti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "nauchil cheloveka tomu, chego tot ne znal"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "No net! Chelovek prestupayet granitsy dozvolennogo"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "kogda yemu kazhetsya, chto on ni v chem ne nuzhdayetsya"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "No k tvoyemu Gospodu predstoit vozvrashcheniye"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Chto ty dumayesh' o tom, kto meshayet"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "rabu, kogda tot molitsya"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Chto ty dumayesh', a yesli on byl na pravom puti"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili prizyval k bogoboyaznennosti"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Chto ty dumayesh', a yesli on schel lozh'yu istinu i otvernulsya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Neuzheli on ne znal, chto Allakh vidit yego"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "No net, yesli on ne perestanet, to My skhvatim yego za khokhol"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "khokhol lzhivyy, greshnyy"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pust' on zovet svoye sborishche"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My zhe pozovem adskikh strazhey"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Tak net! Ne povinuysya yemu, a padi nits i priblizhaysya k Allakhu"
        }
    ]
}