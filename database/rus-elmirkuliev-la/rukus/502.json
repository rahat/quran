{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "Prosyashchiy prosil mucheniya, kotoryye postigayut"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "neveruyushchikh. Nikto ne smozhet predotvratit' yego"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "vopreki Allakhu, Vladyke stupeney"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Angely i Dukh (Dzhibril) voskhodyat k Nemu v techeniye dnya, ravnogo pyatidesyati tysyacham let"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Proyavi zhe krasivoye terpeniye"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Oni schitayut yego dalekim"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "My zhe vidim, chto ono blizko"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "V tot den', kogda nebo stanet podobno rasplavlennomu metallu (ili osadku masla; ili krovavomu gnoyu)"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "a gory stanut podobny shersti"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "rodstvennik ne stanet rassprashivat' rodstvennika"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "khotya oni budut videt' drug druga. Greshnik pozhelayet otkupit'sya ot mucheniy togo dnya svoimi synov'yami"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "svoyey suprugoy i svoim bratom"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "svoim rodom, kotoryy ukryval yego"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "i vsemi obitatelyami zemli, chtoby zatem spastis'"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "No net! Eto - Adskoye plamya"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "sdirayushcheye kozhu s golovy"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "zovushcheye tekh, kto povernulsya spinoy i otvernulsya"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "kto kopil i pryatal"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Voistinu, chelovek sozdan neterpelivym"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "bespokoynym, kogda yego kasayetsya beda"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "i skupym, kogda yego kasayetsya dobro"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Eto ne otnositsya k molyashchimsya"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "kotoryye regulyarno sovershayut svoy namaz"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "kotoryye vydelyayut izvestnuyu dolyu svoyego imushchestva"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "dlya prosyashchikh i obezdolennykh"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "kotoryye veruyut v Den' vozdayaniya"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "kotoryye trepeshchut pered mucheniyami ot svoyego Gospoda"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "ved' mucheniya ot ikh Gospoda nebezopasny"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "kotoryye oberegayut svoi polovyye organy ot vsekh"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "krome svoikh zhen i nevol'nits, kotorymi ovladeli ikh desnitsy, za chto oni ne zasluzhivayut poritsaniya"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "togda kak zhelayushchiye sverkh etogo yavlyayutsya prestupnikami"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "kotoryye sokhranyayut doverennoye im i soblyudayut dogovory"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "kotoryye stoyki v svoikh svidetel'stvakh"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "i kotoryye oberegayut svoy namaz"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Im budet okazano pochteniye v Rayskikh sadakh"
        }
    ]
}