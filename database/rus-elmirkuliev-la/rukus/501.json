{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "O da! Klyanus' tem, chto vy vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i tem, chego vy ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Eto - slova blagorodnogo poslantsa (Mukhammada ili Dzhibrila)"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Eto - ne slova poeta. Malo zhe vy veruyete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Eto - ne slova proritsatelya. Malo zhe vy pominayete nazidaniya"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Eto - Nisposlaniye ot Gospoda mirov"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Yesli by on pripisal Nam nekotoryye slova"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "to My skhvatili by yego za pravuyu ruku (ili skhvatili by yego krepko)"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a potom pererezali by yemu aortu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "i nikto iz vas ne izbavil by yego"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Voistinu, eto - Nazidaniye dlya bogoboyaznennykh"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Nam izvestno, chto sredi vas yest' schitayushchiye yego lozh'yu"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Voistinu, eto - gore dlya neveruyushchikh"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Voistinu, eto - ubeditel'naya istina"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Proslavlyay zhe imya Gospoda tvoyego Velikogo"
        }
    ]
}