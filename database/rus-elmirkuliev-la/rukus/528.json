{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanus' nebom i nochnym putnikom"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Otkuda ty mog znat', chto takoye nochnoy putnik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Eto - zvezda pronizyvayushchaya nebesa svoim svetom"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Net dushi, pri kotoroy ne bylo by khranitelya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pust' posmotrit chelovek, iz chego on sozdan"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On sozdan iz izlivayushcheysya zhidkosti"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kotoraya vykhodit mezhdu chreslami i grudnymi kostyami"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voistinu, On sposoben vernut' yego"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "V tot den' budut ispytany (ili raskroyutsya) tayny"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "i togda ne budet u nego ni sily, ni pomoshchnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanus' nebom vozvrashchayushchim (vozvrashchayushchim vodu zemle ili vozvrashchayushchim nebesnyye tela, kotoryye voskhodyat s odnoy storony i zakhodyat v drugoy)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Klyanus' zemley raskalyvayemoy (pri prorastanii rasteniy, ili pri vykhode iz zemli vody, ili pri voskreshenii mertvykh)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Voistinu, eto - Slovo razlichayushcheye"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "a ne shutka"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni zamyshlyayut kozni"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "i YA zamyshlyayu kozni"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Predostav' zhe neveruyushchim otsrochku, pomedli s nimi nedolgo"
        }
    ]
}