{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "O da! Klyanus' lunoy"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Klyanus' noch'yu, kogda ona otstupayet"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Klyanus' zarey, kogda ona zanimayetsya"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Eto yest' odno iz velichayshikh yavleniy"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "predosteregayushcheye chelovechestvo"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "tekh iz vas, kto zhelayet prodvigat'sya vpered blagodarya pravednym deyaniyam ili otstupat' nazad, sovershaya grekhi"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kazhdyy chelovek yavlyayetsya zalozhnikom togo, chto on priobrel"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "krome lyudey pravoy storony"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "V Rayskikh sadakh oni budut rassprashivat' drug druga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o greshnikakh"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Chto privelo vas v Preispodnyuyu"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Oni skazhut: «My ne byli v chisle tekh, kotoryye sovershali namaz"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "My ne kormili bednyakov"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "My pogruzhalis' v slovobludiye vmeste s pogruzhavshimisya"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "My schitali lozh'yu Posledniy den'"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "poka k nam ne yavilas' ubezhdennost' (smert')»"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Zastupnichestvo zastupnikov ne pomozhet im"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Chto zhe s nimi? Pochemu oni otvorachivayutsya ot Napominaniya"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "slovno napugannyye osly"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "begushchiye ot strelka (ili l'va)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "No ved' kazhdyy iz nikh zhelayet poluchit' razvernutyye svitki"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "No net! Oni ne boyatsya Posledney zhizni"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "No net! Eto yest' Nazidaniye"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Pomyanet yego tot, kto zakhochet"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "No oni ne pomyanut yego, yesli etogo ne pozhelayet Allakh. On - Tot, Kto dostoin strakha i sposoben na proshcheniye"
        }
    ]
}