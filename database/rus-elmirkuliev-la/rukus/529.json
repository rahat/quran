{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slav' imya Gospoda tvoyego Vsevyshnego"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Kotoryy sotvoril vse sushcheye i vsemu pridal sorazmernost'"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Kotoryy predopredelil sud'bu tvoreniy i ukazal put'"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Kotoryy vzrastil pastbishcha"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom prevratil ikh v temnyy sor"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My pozvolim tebe prochest' Koran, i ty ne zabudesh' nichego"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "krome togo, chto pozhelayet Allakh. On znayet yavnoye i to, chto sokryto"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My oblegchim tebe put' k legchayshemu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Nastavlyay zhe lyudey, yesli napominaniye prineset pol'zu"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Vosprimet yego tot, kto strashitsya"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "i uklonitsya ot nego samyy neschastnyy"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "kotoryy voydet v Ogon' velichayshiy"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Ne umret on tam i ne budet zhit'"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Preuspel tot, kto ochistilsya"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "pominal imya svoyego Gospoda i sovershal namaz"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "No net! Vy otdayete predpochteniye mirskoy zhizni"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "khotya Poslednyaya zhizn' - luchshe i dol'she"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Voistinu, eto zapisano v pervykh svitkakh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "svitkakh Ibrakhima (Avraama) i Musy (Moiseya)"
        }
    ]
}