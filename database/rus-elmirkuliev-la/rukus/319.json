{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faraon razoslal po gorodam sborshchikov"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "On skazal: «Eto - vsego lish' malochislennaya kuchka"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Oni razgnevali nas"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "i my vse dolzhny byt' nastorozhe»"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "My vynudili ikh pokinut' sady i istochniki"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "sokrovishcha i blagorodnyye mesta"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Vot tak! My pozvolili unasledovat' eto synam Israila (Izrailya)"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Oni posledovali za nimi na voskhode"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Kogda dva sborishcha uvideli drug druga, spodvizhniki Musy (Moiseya) skazali: «Nas nepremenno nastignut»"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "On skazal: «O net! So mnoy - moy Gospod', i On ukazhet mne pryamoy put'»"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Togda My vnushili Muse (Moiseyu): «Udar' svoim posokhom po moryu». Ono razverzlos', i kazhdaya chast' yego stala podobna ogromnoy gore"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "My priblizili k nemu drugikh (voysko Faraona)"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "My spasli Musu (Moiseya) i tekh, kto byl s nim"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "a zatem potopili vsekh ostal'nykh"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Voistinu, v etom - znameniye, no bol'shinstvo ikh ne stali veruyushchimi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Voistinu, tvoy Gospod' - Mogushchestvennyy, Miloserdnyy"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Prochti im istoriyu Ibrakhima (Avraama)"
        }
    ]
}