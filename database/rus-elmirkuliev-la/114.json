    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: «Pribegayu k zashchite Gospoda lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Tsarya lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga lyudey"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zla iskusitelya otstupayushchego (ili szhimayushchegosya) pri pominanii Allakha"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "kotoryy naushchayet v grudi lyudey"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "i byvayet iz dzhinnov i lyudey»"
        }
    ]
}