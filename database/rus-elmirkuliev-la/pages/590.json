{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanus' nebom s sozvezdiyami Zodiaka"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Klyanus' dnem obeshchannym"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Klyanus' svidetel'stvuyushchim i zasvidetel'stvovannym"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Da sginut sobravshiyesya u rva"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ognennogo, podderzhivayemogo rastopkoy"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Vot oni uselis' vozle nego"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "buduchi svidetelyami togo, chto tvoryat s veruyushchimi"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Oni vymeshchali im tol'ko za to, chto te uverovali v Allakha Mogushchestvennogo, Dostokhval'nogo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Kotoromu prinadlezhit vlast' nad nebesami i zemley. Allakh zhe - Svidetel' vsyakoy veshchi"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Tem, kotoryye podvergli iskusheniyu (ili sozhgli v ogne) veruyushchikh muzhchin i zhenshchin i ne raskayalis', ugotovany mucheniya v Geyenne, mucheniya ot obzhigayushchego Ognya"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Tem zhe, kotoryye uverovali i sovershali pravednyye deyaniya, ugotovany Rayskiye sady, v kotorykh tekut reki. Eto - velikoye preuspeyaniye"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Voistinu, Khvatka tvoyego Gospoda surova"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Voistinu, On nachinayet i povtoryayet (sozdayet tvoreniye v pervyy raz i vossozdayet yego ili nachinayet nakazyvat' v etom mire i vozobnovlyayet nakazaniye v Posledney zhizni)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On - Proshchayushchiy, Lyubyashchiy (ili Lyubimyy)"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vladyka Trona, Slavnyy (ili Vladyka Trona slavnogo)"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "On vershit to, chto pozhelayet"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Doshel li do tebya rasskaz o voinstvakh"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o Faraone i samudyanakh"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "No neveruyushchiye schitayut eto lozh'yu"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Allakh zhe okruzhayet ikh szadi"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Da, eto - slavnyy Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "v Khranimoy skrizhali"
        }
    ]
}