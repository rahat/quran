{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "No net! Vy otdayete predpochteniye mirskoy zhizni"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "khotya Poslednyaya zhizn' - luchshe i dol'she"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Voistinu, eto zapisano v pervykh svitkakh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "svitkakh Ibrakhima (Avraama) i Musy (Moiseya)"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Doshel li do tebya rasskaz o Pokryvayushchem (Dne voskreseniya)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Odni litsa v tot den' budut unizheny"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "iznureny i utomleny"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Oni voydut v Ogon' zharkiy"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Ikh budut poit' iz istochnika kipyashchego"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "i kormit' tol'ko yadovitymi kolyuchkami"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ot kotorykh ne popravlyayutsya i kotoryye ne utolyayut goloda"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Drugiye zhe litsa v tot den' budut radostny"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Oni budut dovol'ny svoimi staraniyami"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v Vyshnikh sadakh"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Oni ne uslyshat tam slovobludiya"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tam yest' istochnik tekushchiy"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Tam vozdvignuty lozha"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "rasstavleny chashi"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "razlozheny podushki"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i razostlany kovry"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Neuzheli oni ne vidyat, kak sozdany verblyudy"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "kak vozneseno nebo"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "kak vodruzheny gory"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "kak rasprosterta zemlya"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Nastavlyay zhe, ved' ty - nastavnik"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "i ty ne vlasten nad nimi"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A tekh, kto otvernetsya i ne uveruyet"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allakh podvergnet velichayshim mucheniyam"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "K Nam oni vernutsya"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "i zatem My potrebuyem u nikh otcheta"
        }
    ]
}