{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Radi yedineniya kureyshitov"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yedineniya ikh vo vremya zimnikh i letnikh poyezdok"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pust' zhe oni poklonyayutsya Gospodu etogo Doma (Kaaby)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Kotoryy nakormil ikh posle goloda i izbavil ikh ot strakha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Videl li ty togo, kto schitayet lozh'yu vozdayaniye"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Eto - tot, kto gonit sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne pobuzhdayet nakormit' bednyaka"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kotoryye nebrezhny k svoim namazam"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i otkazyvayut dazhe v melochi"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "My darovali tebe al'-Kausar (beschislennyye blaga, v tom chisle odnoimennuyu reku v Rayu)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Posemu sovershay namaz radi svoyego Gospoda i zakalyvay zhertvu"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Voistinu, tvoy nenavistnik sam okazhetsya bezvestnym"
        }
    ]
}