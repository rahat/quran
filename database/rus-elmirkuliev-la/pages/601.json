{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Klyanus' predvechernim vremenem (ili vremenem)"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Voistinu, kazhdyy chelovek v ubytke"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "krome tekh, kotoryye uverovali, sovershali pravednyye deyaniya, zapovedali drug drugu istinu i zapovedali drug drugu terpeniye"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu khulitelyu i obidchiku"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy kopit sostoyaniye i pereschityvayet yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "dumaya, chto bogatstvo uvekovechit yego"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No net! Yego nepremenno vvergnut v Ogon' sokrushayushchiy"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Otkuda ty mog znat', chto takoye Ogon' sokrushayushchiy"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Eto - razozhzhennyy Ogon' Allakha"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya nad serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Voistinu, on somknetsya nad nimi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "vysokimi stolbami (mucheniki budut prikovany k etim stolbam ili vrata Ada budut zaperty i zakrepleny etimi stolbami)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Razve ty ne videl, chto sdelal tvoy Gospod' s vladel'tsami slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Razve On ne zaputal ikh kozni"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "i ne naslal na nikh ptits stayami"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Oni brosali v nikh kamen'ya iz obozhzhennoy gliny"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "i prevratili ikh v podobiye iz\"yedennykh issokhshikh zlakovykh list'yev"
        }
    ]
}