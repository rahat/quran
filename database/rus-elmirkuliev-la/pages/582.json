{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O chem oni rassprashivayut drug druga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O velikoy vesti"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "otnositel'no kotoroy oni raskhodyatsya vo mneniyakh"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "No net, oni uznayut"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Yeshche raz net, oni uznayut"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Razve My ne sdelali zemlyu lozhem"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a gory - kolyshkami"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "My sotvorili vas parami"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i sdelali vash son otdykhom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "i sdelali noch' pokryvalom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "i sdelali den' zhalovaniyem"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "i vozdvigli nad vami sem' tverdyn'"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "i ustanovili pylayushchiy svetil'nik"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "i nizveli iz oblakov obil'no l'yushchuyusya vodu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "chtoby vzrastit' yeyu zerna i rasteniya"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i gustyye sady"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Voistinu, Den' razlicheniya naznachen na opredelennoye vremya"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "V tot den' zatrubyat v Rog, i vy pridete tolpami"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "i nebo raskroyetsya i stanet vratami"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "i gory pridut v dvizheniye i stanut marevom"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Voistinu, Geyenna yavlyayetsya zasadoy"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "i mestom vozvrashcheniya dlya tekh, kto prestupayet granitsy dozvolennogo"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Oni probudut tam dolgiye gody"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ne vkushaya ni prokhlady, ni pit'ya"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "a tol'ko kipyatok i gnoy"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Eto budet podobayushchim vozmezdiyem"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Voistinu, oni ne nadeyalis' na raschet"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i polnost'yu otritsali Nashi znameniya"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "My zhe vsyakuyu veshch' podschitali i zapisali"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Vkushayte zhe! My ne pribavim vam nichego, krome mucheniy"
        }
    ]
}