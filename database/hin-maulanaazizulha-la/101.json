{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "vah khadakhada dene vaalee"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kya hai vah khadakha dene vaalee"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "aur tum kya jaano ki vah khadakhada dene vaalee kya hai"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "jis din log, bikhare patingon ke samaan (vyaakool) honge"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "aur parvat, dhunee huee oon ke samaan udenge"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "to jisake palade bhaaree hue"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "to vah manachaahe sukh mein hoga"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "tatha jisake palad halke hue"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "to usaka sthaan haaviya hai"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "aur tum kya jaano ki vah (haaviya) kya hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "vah dahaktee aag hai"
        }
    ]
}