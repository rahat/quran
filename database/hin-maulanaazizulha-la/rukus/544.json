{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "tumhen adhik (dhan) ke lobh ne magan kar diya"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "yahaan tak ki tum qabristaan ja pahunche"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "nishchay tumhen gyaan ho jaayega"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "phir nishchay hee tumhen gyaan ho jaayega"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "vaastav mein, yadi tumhen vishvaas hota (to aisa na karate)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "tum narak ko avashy dekhoge"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "phir use vishvaas kee aankh se dekhoge"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "phir us din tumase sukh sampada ke vishay mein avashy poochh gachh hogee"
        }
    ]
}