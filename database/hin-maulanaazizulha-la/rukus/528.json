{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "shapath hai aakaash tatha raat mein \"prakaash pradaan karane vaale\" kee"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "aur tum kya jaano ki vah \"raat mein prakaash pradaan karane vaala\" kya hai"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "vah jyotimay sitaara hai"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "pratyek praanee par ek rakshak hai"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "insaan, ye to vichaar kare ki vah kis cheez se paida kiya gaya hai"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "uchhalate paanee (veery) se paida kiya gaya hai"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jo peeth tatha seene ke panjaron ke madhs se nikalata hai"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "nishchay vah, use lautaane kee shakti rakhata hai"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "jis din man ke bhed parakhe jaayenge"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "to use na koee bal hoga aur na usaka koee sahaayak"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "shapath hai aakaash kee, jo barasata hai"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "tatha phatane vaalee dharatee kee"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "vaastav mein, ye (quraan) do-took nirnay (faisala) karane vaala hai"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "hansee kee baat nahin"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "vah chaal baazee karate hain"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "main bhee chaal baazee kar raha hoon"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "atah, kaafiron ko kuchh thoda avasar de do"
        }
    ]
}