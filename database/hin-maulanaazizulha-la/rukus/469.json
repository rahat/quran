{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "jab hone vaalee, ho jaayegee"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "usaka hona koee jhooth nahin hai"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "neecha-ooncha karane[1] vaalee"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "jab dharatee tezee se dolane lagegee"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "aur choor-choor kar diye jaayenge parvat"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "phir ho jaayenge bikharee huee dhool"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "tatha tum ho jaoge teen samooh"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "to daayen vaale, to kya hain daayen vaale"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "aur baayen vaale, to kya hain baayen vaale"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "aur agragaamee to agragaamee hee hain"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "vahee sameep kiye[1] hue hain"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "ve sukhon ke svargon mein honge"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "bahut-se agale logon mein se"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "tatha kuchh pichhale logon mein se honge"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "svarn se bune hue takhton par"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "takiye lagaaye unapar, ek-doosare ke sammukh (aaseen) honge"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "phirate honge unakee seva ke lie baalak, jo sada (baalak) rahenge"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pyaale tatha suraahî€¼iyaan lekar tatha madira ke chhalakate pyaale"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "na to sir chakaraayega unase, na ve nirbodh honge"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "tatha jo phal ve chaahenge"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tatha pakshee ka jo maans ve chaahenge"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "aur goriyaan bade nainon vaalee"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "chhupaakar rakhee hueen motiyon ke samaan"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "usake badale, jo ve (sansaar mein) karate rahe"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "nahin sunenge unamen vyarth baat aur na paap kee baat"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "keval salaam hee salaam kee dhvani hogee"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "aur daayen vaale, kya (hee bhaagyashaalee) hain daayen vaale"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "bin kaante kee bairee mein honge"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "tatha tah par tah kelon mein"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "phailee huee chhaaya[1] mein"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "aur pravaahit jal mein"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "tatha bahut-se phalon mein"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "jo na samaapt honge, na roke jaayenge"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "aur oonche bistar par"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "hamane banaaya hai (unakee) patniyon ko ek vishesh roop se"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "hamane banaay hai unhen kumaariyaan"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "premikaayen samaayu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "daahine vaalon ke lie"
        }
    ]
}