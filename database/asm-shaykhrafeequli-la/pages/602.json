{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Yihetu kburaa'ichasakala abhyasta"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "sita arau graismakalara bhramanata sihamtara asakti ache"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Eteke sihamte yena e'i gharara pratipalakara ibadata karae"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Yijane sihamtaka ksudha arasthata khadya pradana karaiche arau santraasara paraa niraapatta diche"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi taka dekhichane yiye hicapa pratidanaka asbikara karae"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "si se'ijane'i, yiye anathaka kathorabharae khedi pathiyaya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "arau si dukhiyaka khana khuraabalai utsaha nidiye"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Eteke dhbansa se'ibidha chalata adayakaraisakalara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yisakale nijara chalatata amanoyogi"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yisakale anaka dekhuraabalaihe karae"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "arau sarau-suraa grhasamagrai pradanata badha diye"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niscaya ami tomaka ala-ka'uchara pradana karaichom"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Eteke tumi tomara pratipalakara uddesye chalata adaya karaa arau korabani karaa"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Niscaya tomara prati bidbesa posanakaraiye'i nirbansa"
        }
    ]
}