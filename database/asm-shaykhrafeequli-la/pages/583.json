{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Niscaya muttakbisakalara babe ache saphalata"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Udyanasamuha arau angurasamuha"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Arau samabayasiya uda`bhinna nara-yurati tarauni"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Arau paraipurna panapatra"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tata te'omloke kono anarthaka katha arau micha bakya'o suna napaba"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Aponara pratipalakara phalara paraa puraskara, yathocita danasbaraupa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Yijana akasasamuha arau prthirai arau e'i dutara majata yi ache sakalorae pratipalaka, parama karaunamaya, te'omra ocarata abedana-nibedanara sakti sihamtara karao nathakiba"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Se'idina rauha arau phiraistasakala saraibad'dhabharae thiya ha’ba; rahamane yaka anumati diba tara bahirae kone'o katha naka’ba, arau te'om sathika katha ka’ba"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "E'i dinato satya, eteke yara iccha siye tara pratipalakara ocarata asraya grahana karaka"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niscaya ami tomalokaka ati nikatararti sasti samparke satarka karailo. Yidina manuhe nija krtakarma dekhibalai paba arau kaphirae ka’ba, haya! Ma'i yadi mati ha'lohemtena"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Sapata nirmamabharae utpatanakaraisakalara"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Arau mrdubharae bandhanamuktakaraisakalara"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Arau tibragatita samturai phuraasakalara"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Arau drautagatita agabarhi yoraasakalara"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Tara pichata yaratiya kama nirbahakaraisakalara"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Se'idina kampanakaraiye prakampita karaiba"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Taka anusarana karaiba parabarti kampanakarai"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Se'idina kichumana hrdaya, bhayata santrasta ha’ba"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Sihamtara drstisamuha atankita hai nata ha’ba"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Sihamte kaya, ‘ami akau agara arasthata ubhati yama ne"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "yetiya ami curnabicurna asthita parainata hama’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "sihamte kaya, ‘yadi e'itorae haya tente e'ito eta ksatikara pratyarartana’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Prakrtate e'ito eta biraata sabda"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tatksanata bhu-prsthata sihamtara abirbhara ha’ba"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Muchara ghatanato tumi pa'ichane"
        }
    ]
}