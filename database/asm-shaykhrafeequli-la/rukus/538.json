{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "parha tomara pratipalakara namata, yiye srsti karaiche"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "te'om manuhaka alakbara paraa srsti karaiche"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Parha, karana tomara pratipalaka mahamahima"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Yijane kalamara sahayata siksa diche"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Te'om manuhaka ene siksa diche, yito manuhe jana nachila"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Bastarate, manuhe simalanghana karai thake"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Karana si nijake sbanirbharasila buli bharae"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Niscaya tomara pratipalakara ocarate'i pratyarartana"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Tumi taka dekha na'ine? Yiye nisedha karae"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Ejana bandaka, yetiya te'om chalata adaya karae"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Koraacona, yadi'o te'om hidayatara oparata pratisthita"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athaba takbaraara nirdesa diye; (tathapi'o te'omka kenekai badha diye)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "koraacona yadi si (nisedhakarai) asbikara karae arau mukha ghuraa'i laya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "si najane neki, niscaya allahe dekhi ache"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ketiya'o nahaya, yadi si birata nathake, tente niscaya ami taka agaculita dharai tani anima"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Michaliya, papisthara agaculi (kapala)"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Eteke si nija paraisadaka mati anaka"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Anatipalame ami mati anima jahannamara kathora praharaisakalaka"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ketiya'o nahaya, tumi tara anusarana nakaraiba. Arau tumi (tomara pratipalakaka) chajada karaa arau naikatya labha karaa"
        }
    ]
}