    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Koraa- ma'i manuhara pratipalakara asraya praarthana karaichom"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manuhara adhipatira"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manuhara ilahara ocarata"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "atmagopanakarai kumantranadatara anistara paraa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "yiye manuhara manata kumantrana diye"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jinara majara paraa arau manuhara majara paraa"
        }
    ]
