{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bhitiprada maha bipada"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Bhitiprada maha bipada ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "bhitiprada maha bipada samparke tumi ki jana"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "se'idina manuha ha’ba biksipta kita-patangara darae"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Arau parbatasamuha ha’ba dhuna rangina unara darae"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Eteke yara tarjusamuha gadhura ha’ba"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "te'om thakiba santosajanaka jiranata"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "arau yara tarjusamuha patala ha’ba"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tara thikana ha’ba ‘ha-raiyaha’"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Arau tumi ki jana se'itono ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "i eta atyanta uttapta agnikunda"
        }
    ]
}