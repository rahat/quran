{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Koraa, ‘te'om allaha, eka-adbitiya"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allaha haiche chamada; (te'om karao mukhapeksi nahaya, sakalo te'omrae'i mukhapeksi)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "te'om kako janma diya na'i arau teraom janma loraa na'i"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "arau te'omra samatulya kono na'i’"
        }
    ]
}