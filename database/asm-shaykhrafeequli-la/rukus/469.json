{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yetiya kiyamata sanghatita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "(tetiya) iyara sanghatanara asbikarakarai kono nathakiba"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "I karaobaka adhahpatana karaiba, karaobaka samunnata karaiba"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "yetiya prthirai prabala prakampanerae prakampita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "arau parbatasamuha curna-bicurna hai paraiba"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "phalata se'ibora biksipta dhulikanata parainata ha’ba"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "arau tomaloke bibhakta hai paraiba tinita dalata"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "eteke somphalara dala; arau somphalara dalato kimana ye saubhagyabana"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "arau ba'omphalara dala; arau ba'omphalara dalato kimana ye durbhagiya"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "arau agragamisakale'i agragami"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "te'omloke'i sannidhyapraapta"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "te'omloka thakiba niyamatapurna jannatasamuhata"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "bahusankhyaka ha’ba purbarartisakalara majara paraa"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "arau kama sankhyaka ha’ba parabartisakalara majara paraa"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Sonerae nirmita asanamuhata"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "sihamte parasparae mukhamukhikai a'uji bahiba"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Sihamtara ase-pase ghuraiphuraiba cira kisorasakale"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "prabahita nijaraara suraapurna panapatra arau piyalabora lai"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "se'i suraa pana karaile te'omlokara mura nughuraaya arau te'omloka jnanaharaa'o naha’ba"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "arau (ghuraiphuraiba) te'omlokara pachandaniya phala-mula lai"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "arau te'omlokara rauci anuyayi caraa'ira mansa lai"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Arau te'omlokara babe thakiba mrganayani (danara cakubisista) hura"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "yeniba sihamta suraksita mukutahe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "sihamte yi amala karaichila tara puraskarasbaraupa"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tata sihamte kono alagatiyala katha arau kono papabakya sunibalai napaba"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "suniba kerala chalama arau chalama"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Arau somphalara dala; kimana ye bhagyabana somphalara dalato"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Te'omloke thakiba ene udyanata, ya’ta ache kamitabihina bagarai gacha"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "arau thokerae bharaa kala gacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "arau samprasaraita cham"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "arau sadaya prabahamana pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "arau pracura phala-mula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yibora sesa naha'ba arau nisid'dha'o naha’ba"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Arau (sihamta thakiba) su'ucca sayyasamuhata"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niscaya ami sihamtaka (hurasakalaka) srsti karaicho bisesaraupata"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "tara pichata sihamtaka kumarai bana'icho"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "nija sbamira prati asakta arau samabayasiya"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "somphalara lokasakalara babe"
        }
    ]
}