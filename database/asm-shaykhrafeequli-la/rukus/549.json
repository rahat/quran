{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi taka dekhichane yiye hicapa pratidanaka asbikara karae"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "si se'ijane'i, yiye anathaka kathorabharae khedi pathiyaya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "arau si dukhiyaka khana khuraabalai utsaha nidiye"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Eteke dhbansa se'ibidha chalata adayakaraisakalara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yisakale nijara chalatata amanoyogi"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yisakale anaka dekhuraabalaihe karae"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "arau sarau-suraa grhasamagrai pradanata badha diye"
        }
    ]
}