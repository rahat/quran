{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Қориъаҳ, рӯзест, ки дилҳо аз ҳавли он кӯбида мешаванд"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Чист қориъаҳ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ва ту чӣ донӣ, ки қориъаҳ чист"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Рӯзест, ки мардумон ҳамчун парвонаҳои пароканда хоҳанд буд"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ва кӯҳҳо монанди пашми задашуда бо ҳаллоҷӣ парешон карда шуда хоҳанд шуд"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Аммо ҳар касеро паллаи некиҳои тарозуяш вазнин бошад"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "пас ӯ дар ҷаннат дар зиндагии писандида бошад"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ва аммо ҳар касеро сабук шуд паллаи некиҳои тарозуяш"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ҷойгоҳаш дар ҳовияҳ аст"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ва ту чӣ донӣ, ки ҳовияҳ чист"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Оташи бузург ва бисёр гарму сӯзонест"
        }
    ]
}