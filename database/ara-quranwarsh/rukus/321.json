{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتْ قَوْمُ نُوحٍ اِ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ نُوحٌ اَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينࣱۖ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَيٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قَالُوٓاْ أَنُومِنُ لَكَ وَاتَّبَعَكَ اَ۬لَارْذَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلْمِے بِمَا كَانُواْ يَعْمَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنْ حِسَابُهُمُۥٓ إِلَّا عَلَيٰ رَبِّے لَوْ تَشْعُرُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَآ أَنَا بِطَارِدِ اِ۬لْمُومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنَ اَنَا إِلَّا نَذِيرࣱ مُّبِينࣱۖ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُواْ لَئِن لَّمْ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ اَ۬لْمَرْجُومِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوْمِے كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَافْتَحْ بَيْنِے وَبَيْنَهُمْ فَتْحاࣰ وَنَجِّنِے وَمَن مَّعِيَ مِنَ اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَيْنَٰهُ وَمَن مَّعَهُۥ فِے اِ۬لْفُلْكِ اِ۬لْمَشْحُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغْرَقْنَا بَعْدُ اُ۬لْبَاقِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        }
    ]
}