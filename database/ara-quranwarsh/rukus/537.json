{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَالتِّينِ وَالزَّيْتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا اَ۬لْبَلَدِ اِ۬لَامِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِےٓ أَحْسَنِ تَقْوِيمࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ فَلَهُمُۥٓ أَجْرٌ غَيْرُ مَمْنُونࣲۖ"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِۖ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيْسَ اَ۬للَّهُ بِأَحْكَمِ اِ۬لْحَٰكِمِينَۖ"
        }
    ]
}