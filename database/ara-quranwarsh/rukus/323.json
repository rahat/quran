{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "كَذَّبَتْ ثَمُودُ اُ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ صَٰلِحٌ اَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينࣱۖ"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَيٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "۞أَتُتْرَكُونَ فِے مَا هَٰهُنَآ ءَامِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "فِے جَنَّٰتࣲ وَعُيُونࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وَزُرُوعࣲ وَنَخْلࣲ طَلْعُهَا هَضِيمࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وَتَنْحِتُونَ مِنَ اَ۬لْجِبَالِ بُيُوتاࣰ فَرِهِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "وَلَا تُطِيعُوٓاْ أَمْرَ اَ۬لْمُسْرِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "اَ۬لذِينَ يُفْسِدُونَ فِے اِ۬لَارْضِ وَلَا يُصْلِحُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ اَ۬لْمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "مَآ أَنتَ إِلَّا بَشَرࣱ مِّثْلُنَا فَاتِ بِـَٔايَةٍ اِن كُنتَ مِنَ اَ۬لصَّٰدِقِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قَالَ هَٰذِهِۦ نَاقَةࣱ لَّهَا شِرْبࣱۖ وَلَكُمْ شِرْبُ يَوْمࣲ مَّعْلُومࣲۖ"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "وَلَا تَمَسُّوهَا بِسُوٓءࣲ فَيَاخُذَكُمْ عَذَابُ يَوْمٍ عَظِيمࣲۖ"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فَعَقَرُوهَا فَأَصْبَحُواْ نَٰدِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فَأَخَذَهُمُ اُ۬لْعَذَابُۖ إِنَّ فِے ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        }
    ]
}