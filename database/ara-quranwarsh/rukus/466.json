{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "اِ۬لرَّحْمَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ اَ۬لْقُرْءَانَۖ"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ اَ۬لِانسَٰنَ"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ اُ۬لْبَيَانَۖ"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "اَ۬لشَّمْسُ وَالْقَمَرُ بِحُسْبَانࣲۖ"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَٰنِۖ"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ اَ۬لْمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "أَلَّا تَطْغَوْاْ فِے اِ۬لْمِيزَانِ"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَأَقِيمُواْ اُ۬لْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُواْ اُ۬لْمِيزَانَۖ"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَالَارْضَ وَضَعَهَا لِلَانَامِ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيهَا فَٰكِهَةࣱ وَالنَّخْلُ ذَاتُ اُ۬لَاكْمَامِ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَالْحَبُّ ذُو اُ۬لْعَصْفِ وَالرَّيْحَانُۖ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ اَ۬لِانسَٰنَ مِن صَلْصَٰلࣲ كَالْفَخّ۪ارِ"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ اَ۬لْجَآنَّ مِن مَّارِجࣲ مِّن نّ۪ارࣲۖ"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ اُ۬لْمَشْرِقَيْنِ وَرَبُّ اُ۬لْمَغْرِبَيْنِۖ"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ اَ۬لْبَحْرَيْنِ يَلْتَقِيَٰنِ"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيْنَهُمَا بَرْزَخࣱ لَّا يَبْغِيَٰنِۖ"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يُخْرَجُ مِنْهُمَا اَ۬للُّؤْلُؤُاْ وَالْمَرْجَانُۖ"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ اُ۬لْجَوَارِ اِ۬لْمُنشَأَٰتُ فِے اِ۬لْبَحْرِ كَالَاعْلَٰمِۖ"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
        }
    ]
}