{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَاليْلِ إِذَا يَغْش۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَالنَّه۪ارِ إِذَا تَجَلّ۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ اَ۬لذَّكَرَ وَالُانث۪يٰٓ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعْيَكُمْ لَشَتّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنَ اَعْط۪يٰ وَاتَّق۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِالْحُسْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلْيُسْر۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَاسْتَغْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِالْحُسْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلْعُسْر۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغْنِے عَنْهُ مَالُهُۥٓ إِذَا تَرَدّ۪يٰٓۖ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَيْنَا لَلْهُد۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلَاخِرَةَ وَالُاول۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرْتُكُمْ نَاراࣰ تَلَظّ۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصْلَيٰهَآ إِلَّا اَ۬لَاشْقَي"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "اَ۬لذِے كَذَّبَ وَتَوَلّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا اَ۬لَاتْقَي"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "اَ۬لذِے يُوتِے مَالَهُۥ يَتَزَكّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةࣲ تُجْز۪يٰٓ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا اَ۪بْتِغَآءَ وَجْهِ رَبِّهِ اِ۬لَاعْل۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوْفَ يَرْض۪يٰۖ"
        }
    ]
}