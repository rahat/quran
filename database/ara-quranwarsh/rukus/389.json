{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "۞وَلَقَدْ مَنَنَّا عَلَيٰ مُوس۪يٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّيْنَٰهُمَا وَقَوْمَهُمَا مِنَ اَ۬لْكَرْبِ اِ۬لْعَظِيمِۖ"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرْنَٰهُمْ فَكَانُواْ هُمُ اُ۬لْغَٰلِبِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَيْنَٰهُمَا اَ۬لْكِتَٰبَ اَ۬لْمُسْتَبِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَيْنَٰهُمَا اَ۬لصِّرَٰطَ اَ۬لْمُسْتَقِيمَۖ"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكْنَا عَلَيْهِمَا فِے اِ۬لَاخِرِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَٰمٌ عَلَيٰ مُوس۪يٰ وَهَٰرُونَۖ"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلْيَاسَ لَمِنَ اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدْعُونَ بَعْلاࣰ وَتَذَرُونَ أَحْسَنَ اَ۬لْخَٰلِقِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "اَ۬للَّهُ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ اُ۬لَاوَّلِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لَاخِرِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَٰمٌ عَلَيٰٓ ءَالِ يَاسِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوطاࣰ لَّمِنَ اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذْ نَجَّيْنَٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوزاࣰ فِے اِ۬لْغَٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرْنَا اَ۬لَاخَرِينَۖ"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِاليْلِۖ أَفَلَا تَعْقِلُونَۖ"
        }
    ]
}