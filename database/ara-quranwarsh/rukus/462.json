{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَٰٓيْتَ اَ۬لذِے تَوَلّ۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعْط۪يٰ قَلِيلاࣰ وَأَكْد۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلْمُ اُ۬لْغَيْبِ فَهُوَ يَر۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمْ لَمْ يُنَبَّأْ بِمَا فِے صُحُفِ مُوس۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبْرَٰهِيمَ اَ۬لذِے وَفّ۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَةࣱ وِزْرَ أُخْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيْسَ لِلِانسَٰنِ إِلَّا مَا سَع۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعْيَهُۥ سَوْفَ يُر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجْز۪يٰهُ اُ۬لْجَزَآءَ اَ۬لَاوْف۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَيٰ رَبِّكَ اَ۬لْمُنتَه۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "وَأَنَّهُۥ هُوَ أَضْحَكَ وَأَبْك۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحْي۪اۖ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ اَ۬لزَّوْجَيْنِ اِ۬لذَّكَرَ وَالُانث۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطْفَةٍ اِذَا تُمْن۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "۞وَأَنَّ عَلَيْهِ اِ۬لنَّشْأَةَ اَ۬لُاخْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّهُۥ هُوَ أَغْن۪يٰ وَأَقْن۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّهُۥ هُوَ رَبُّ اُ۬لشِّعْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥٓ أَهْلَكَ عَاداࣰ اَ۬لُّاول۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُوداࣰ فَمَآ أَبْق۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوْمَ نُوحࣲ مِّن قَبْلُۖ إِنَّهُمْ كَانُواْ هُمُۥٓ أَظْلَمَ وَأَطْغ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَالْمُوتَفِكَةَ أَهْو۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشّ۪يٰهَا مَا غَشّ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِيرࣱ مِّنَ اَ۬لنُّذُرِ اِ۬لُاول۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ اِ۬لَازِفَةُۖ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيْسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌۖ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "اَفَمِنْ هَٰذَا اَ۬لْحَدِيثِ تَعْجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضْحَكُونَ وَلَا تَبْكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمْ سَٰمِدُونَۖ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاسْجُدُواْ لِلهِ وَاعْبُدُواْ۩ۖ"
        }
    ]
}