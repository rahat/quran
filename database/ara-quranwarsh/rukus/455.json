{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "وَالذَّٰرِيَٰتِ ذَرْواࣰ‏"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "فَالْحَٰمِلَٰتِ وِقْراࣰ‏"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "فَالْجَٰرِيَٰتِ يُسْراࣰ‏"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "فَالْمُقَسِّمَٰتِ أَمْراً"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "اِنَّمَا تُوعَدُونَ لَصَادِقࣱ‏"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "وَإِنَّ اَ۬لدِّينَ لَوَٰقِعࣱۖ"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "وَالسَّمَآءِ ذَاتِ اِ۬لْحُبُكِ"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "إِنَّكُمْ لَفِے قَوْلࣲ مُّخْتَلِفࣲ‏"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "يُوفَكُ عَنْهُ مَنُ ا۟فِكَۖ"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "قُتِلَ اَ۬لْخَرَّٰصُونَ"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "اَ۬لذِينَ هُمْ فِے غَمْرَةࣲ سَاهُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "يَسْـَٔلُونَ أَيَّانَ يَوْمُ اُ۬لدِّينِ"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "يَوْمَ هُمْ عَلَي اَ۬لنّ۪ارِ يُفْتَنُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ذُوقُواْ فِتْنَتَكُمْۖ هَٰذَا اَ۬لذِے كُنتُم بِهِۦ تَسْتَعْجِلُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "إِنَّ اَ۬لْمُتَّقِينَ فِے جَنَّٰتࣲ وَعُيُونٍ"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "اٰخِذِينَ مَآ ءَات۪يٰهُمْ رَبُّهُمُۥٓۖ إِنَّهُمْ كَانُواْ قَبْلَ ذَٰلِكَ مُحْسِنِينَۖ"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "كَانُواْ قَلِيلاࣰۖ مِّنَ اَ۬ليْلِ مَا يَهْجَعُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "وَبِالَاسْح۪ارِ هُمْ يَسْتَغْفِرُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "وَفِےٓ أَمْوَٰلِهِمْ حَقࣱّ لِّلسَّآئِلِ وَالْمَحْرُومِۖ"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "وَفِے اِ۬لَارْضِ ءَايَٰتࣱ لِّلْمُوقِنِينَۖ"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "وَفِےٓ أَنفُسِكُمُۥٓۖ أَفَلَا تُبْصِرُونَۖ"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "وَفِے اِ۬لسَّمَآءِ رِزْقُكُمْۖ وَمَا تُوعَدُونَ"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "فَوَرَبِّ اِ۬لسَّمَآءِ وَالَارْضِ إِنَّهُۥ لَحَقࣱّ مِّثْلَ مَآ أَنَّكُمْ تَنطِقُونَۖ"
        }
    ]
}