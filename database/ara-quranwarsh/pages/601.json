{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَالْعَصْرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ اَ۬لِانسَٰنَ لَفِے خُسْرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "اِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَتَوَاصَوْاْ بِالْحَقِّ وَتَوَاصَوْاْ بِالصَّبْرِۖ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "ۖ وَيْلࣱ لِّكُلِّ هُمَزَةࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "اِ۬لذِے جَمَعَ مَالاࣰ وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحْسِبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِے اِ۬لْحُطَمَةِۖ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْحُطَمَةُۖ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ اُ۬للَّهِ اِ۬لْمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "اُ۬لتِے تَطَّلِعُ عَلَي اَ۬لَافْـِٕدَةِۖ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيْهِم مُّوصَدَةࣱ‏"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِے عَمَدࣲ مُّمَدَّدَةٍۖ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ اِ۬لْفِيلِۖ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِے تَضْلِيلࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرْسَلَ عَلَيْهِمْ طَيْراً اَبَابِيلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرْمِيهِم بِحِجَارَةࣲ مِّن سِجِّيلࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمْ كَعَصْفࣲ مَّاكُولࣲۖ"
        }
    ]
}