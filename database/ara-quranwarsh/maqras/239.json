{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "اَلَمْ نَشْرَحْ لَكَ صَدْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "وَوَضَعْنَا عَنكَ وِزْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "اَ۬لذِےٓ أَنقَضَ ظَهْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "وَرَفَعْنَا لَكَ ذِكْرَكَۖ"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فَإِنَّ مَعَ اَ۬لْعُسْرِ يُسْراً"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "اِنَّ مَعَ اَ۬لْعُسْرِ يُسْراࣰۖ"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فَإِذَا فَرَغْتَ فَانصَبْ"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "وَإِلَيٰ رَبِّكَ فَارْغَبْۖ"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَالتِّينِ وَالزَّيْتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا اَ۬لْبَلَدِ اِ۬لَامِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِےٓ أَحْسَنِ تَقْوِيمࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ فَلَهُمُۥٓ أَجْرٌ غَيْرُ مَمْنُونࣲۖ"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِۖ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيْسَ اَ۬للَّهُ بِأَحْكَمِ اِ۬لْحَٰكِمِينَۖ"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "اَ۪قْرَأْ بِاسْمِ رَبِّكَ اَ۬لذِے خَلَقَۖ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ اَ۬لِانسَٰنَ مِنْ عَلَقٍۖ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اِ۪قْرَأْ وَرَبُّكَ اَ۬لَاكْرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "اُ۬لذِے عَلَّمَ بِالْقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ اَ۬لِانسَٰنَ مَا لَمْ يَعْلَمْۖ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّآ إِنَّ اَ۬لِانسَٰنَ لَيَطْغ۪يٰٓ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "أَن رّ۪ء۪اهُ اُ۪سْتَغْن۪يٰٓۖ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "إِنَّ إِلَيٰ رَبِّكَ اَ۬لرُّجْع۪يٰٓۖ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "أَرَٰٓيْتَ اَ۬لذِے يَنْه۪يٰ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبْداً اِذَا صَلّ۪يٰٓۖ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَٰٓيْتَ إِن كَانَ عَلَي اَ۬لْهُد۪يٰٓ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوَ اَمَرَ بِالتَّقْو۪يٰٓۖ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "أَرَٰٓيْتَ إِن كَذَّبَ وَتَوَلّ۪يٰٓ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "أَلَمْ يَعْلَم بِأَنَّ اَ۬للَّهَ يَر۪يٰۖ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعاَۢ بِالنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَةࣲ كَٰذِبَةٍ خَاطِئَةࣲۖ"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلْيَدْعُ نَادِيَهُۥ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدْعُ اُ۬لزَّبَانِيَةَۖ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبِۖ۩"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "اِنَّآ أَنزَلْنَٰهُ فِے لَيْلَةِ اِ۬لْقَدْرِۖ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَآ أَدْر۪يٰكَ مَا لَيْلَةُ اُ۬لْقَدْرِۖ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَيْلَةُ اُ۬لْقَدْرِ خَيْرࣱ مِّنَ اَلْفِ شَهْرࣲۖ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ اُ۬لْمَلَٰٓئِكَةُ وَالرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرࣲۖ"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّيٰ مَطْلَعِ اِ۬لْفَجْرِۖ"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "لَمْ يَكُنِ اِ۬لذِينَ كَفَرُواْ مِنَ اَهْلِ اِ۬لْكِتَٰبِ وَالْمُشْرِكِينَ مُنفَكِّينَ حَتَّيٰ تَاتِيَهُمُ اُ۬لْبَيِّنَةُۖ"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "رَسُولࣱ مِّنَ اَ۬للَّهِ يَتْلُواْ صُحُفاࣰ مُّطَهَّرَةࣰ‏"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "فِيهَا كُتُبࣱ قَيِّمَةࣱۖ"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "وَمَا تَفَرَّقَ اَ۬لذِينَ أُوتُواْ اُ۬لْكِتَٰبَ إِلَّا مِنۢ بَعْدِ مَا جَآءَتْهُمُ اُ۬لْبَيِّنَةُۖ"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "وَمَآ أُمِرُوٓاْ إِلَّا لِيَعْبُدُواْ اُ۬للَّهَ مُخْلِصِينَ لَهُ اُ۬لدِّينَ حُنَفَآءَ وَيُقِيمُواْ اُ۬لصَّلَوٰةَ وَيُوتُواْ اُ۬لزَّكَوٰةَۖ وَذَٰلِكَ دِينُ اُ۬لْقَيِّمَةِۖ"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "إِنَّ اَ۬لذِينَ كَفَرُواْ مِنَ اَهْلِ اِ۬لْكِتَٰبِ وَالْمُشْرِكِينَ فِے ن۪ارِ جَهَنَّمَ خَٰلِدِينَ فِيهَآۖ أُوْلَٰٓئِكَ هُمْ شَرُّ اُ۬لْبَرِيَٓٔةِۖ"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "إِنَّ اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ أُوْلَٰٓئِكَ هُمْ خَيْرُ اُ۬لْبَرِيَٓٔةِۖ"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "جَزَآؤُهُمْ عِندَ رَبِّهِمْ جَنَّٰتُ عَدْنࣲ تَجْرِے مِن تَحْتِهَا اَ۬لَانْهَٰرُ خَٰلِدِينَ فِيهَآ أَبَداࣰۖ رَّضِيَ اَ۬للَّهُ عَنْهُمْ وَرَضُواْ عَنْهُۖ ذَٰلِكَ لِمَنْ خَشِيَ رَبَّهُۥٓۖ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "إِذَا زُلْزِلَتِ اِ۬لَارْضُ زِلْزَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "وَأَخْرَجَتِ اِ۬لَارْضُ أَثْقَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "وَقَالَ اَ۬لِانسَٰنُ مَا لَهَا"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "يَوْمَئِذࣲ تُحَدِّثُ أَخْبَارَهَا"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "بِأَنَّ رَبَّكَ أَوْح۪يٰ لَهَاۖ"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "يَوْمَئِذࣲ يَصْدُرُ اُ۬لنَّاسُ أَشْتَاتاࣰ لِّيُرَوَاْ اَعْمَٰلَهُمْۖ"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "فَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْراࣰ يَرَهُۥۖ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "وَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةࣲ شَرّاࣰ يَرَهُۥۖ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَالْعَٰدِيَٰتِ ضَبْحاࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَالْمُورِيَٰتِ قَدْحاࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَالْمُغِيرَٰتِ صُبْحاࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرْنَ بِهِۦ نَقْعاࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطْنَ بِهِۦ جَمْعاً"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "اِنَّ اَ۬لِانسَٰنَ لِرَبِّهِۦ لَكَنُودࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَيٰ ذَٰلِكَ لَشَهِيدࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ اِ۬لْخَيْرِ لَشَدِيدٌۖ"
        }
    ]
}