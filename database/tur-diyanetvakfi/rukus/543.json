{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Kapı çalan"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir o kapı çalan"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "O kapı çalanın ne olduğunu bilir misin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "İnsanların, ateşin etrafını sarmış pervaneler gibi olur"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Dağların da atılmış renkli yüne dönüştüğü gündür (o Karia)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "O gün kimin tartılan ameli ağır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "İşte o, hoşnut edici bir yaşayış içinde olur"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ameli yeğni olana gelince"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "İşte onun anası (yeri, yurdu) Haviye'dir"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Nedir o (Haviye) bilir misin"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Kızgın ateş"
        }
    ]
}