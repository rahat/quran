{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureyş'e kolaylaştırıldığı"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Evet, kış ve yaz seyahatleri onlara kolaylaştırıldığı için"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Onlar, şu evin Rabbine kulluk etsinler, ki"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Kendilerini açlıktan doyuran ve her çeşit korkudan emin kıldı"
        }
    ]
}