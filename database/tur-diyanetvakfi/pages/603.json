{
    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Ey Muhammed! De ki: \"Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin taptıklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptıgıma da sizler tapmazsınız"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de sizin taptığınıza tapacak dağilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Benim taptığıma da sizler tapmıyorsunuz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim banadır"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Allah'ın yardımı ve zaferi geldiği"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ve insanların bölük bölük Allah'ın dinine girmekte olduklarını gördüğün vakit"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Rabbine hamdederek O'nu tesbih et ve O'ndan mağfiret dile. Çünkü O, tevbeleri çok kabul edendir"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ebu Leheb'in iki eli kurusun! Kurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Malı ve kazandıkları ona fayda vermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, alevli bir ateşte yanacak"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Odun taşıyıcı olarak karısı da (ateşe girecek)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Ve boynunda hurma lifinden bükülmüş bir ip olduğu halde"
        }
    ]
}