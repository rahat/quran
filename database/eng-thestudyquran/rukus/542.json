    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "By the panting chargers"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "by the strikers of sparks"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "by the raiders at dawn"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "raising thereby a trail of dust"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "penetrating to the center altogether"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "truly man is ungrateful to his Lord"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "and truly he is a witness to that"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "and truly he is fierce in his love for good things"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Does he not know that when what lies within graves is turned inside out"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "and what lies within breasts is made known"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "truly on that Day their Lord shall be aware of them"
        }
    ]
