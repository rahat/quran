{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Кад се небо расцепи"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "и послуша свога Господара, а оно ће да буде то дужно"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "и када Земља буде растегнута"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "и избаци оно што је у њој, и потпуно се испразни"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "и послуша свога Господара, а она је обавезна да то учини"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "О човече, ти се трудиш идући своме Господару, па ћеш свој труд да нађеш"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Онај коме буде његова књига у његову десну руку дата"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Лак ће обрачун полагати"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "и својима ће радостан да се врати"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "а онај коме буде његова књига иза његових леђа дата"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "пропаст ће да призива"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "и у Огњу ће да гори"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Заиста је он са својом породицом био радостан"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "и мислио је да никада неће да се врати"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "а хоће! Његов Господар о њему, заиста, све зна"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Али не! Кунем се вечерњим руменилом"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "и ноћи, и оним што она сакупља"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "и Месецом пуним"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "ви ћете сигурно из стања у стање да прелазите"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Па шта им је па не верују"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "и кад им се Кур'ан учи на тло лицем не падају"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Напротив, они који не верују поричу"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "а Аллах најбоље зна шта они у себи крију"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "зато им наговести болну патњу"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "А они који буду веровали и радили добра дела, имаће непрекидну награду"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Тако ми неба са великим звездама"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "и Дана већ обећаног"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "и сведока и посведоченог"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "проклети били они који су ровове ископали"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "и ватру са горивом"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "када су око ње седели"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "и били сведоци онога што су верницима радили"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "А светили су им се само зато што су у Аллаха, Силнога и Хваљенога, веровали"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Чија је власт и на небесима и на Земљи, а Аллах је свему Сведок"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Заиста они који су кажњавали вернике и вернице, а затим се нису покајали, имаће казну у Паклу, имаће огњену казну"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Заиста онима који су веровали и радили добра дела припадају рајске баште кроз које теку реке, а то је велики успех"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Кажњавање твога Господара заиста је жестоко"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Он ни из чега ствара, и поново ће то да учини"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Он је Опроститељ греха, Онај који пуно воли"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Господар Престола, Величанствени"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Он ради шта хоће"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Да ли је до тебе допрла вест о војскама"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "о фараону и Семуду"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Али, ови који не верују стално поричу"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "а Аллах је иза њих, окружује их"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Напротив! Ово је величанствени Кур'ан"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "на помно чуваној Плочи"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Тако ми неба и звезде која се појављује ноћу"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "А знаш ли ти шта је то ноћни посетилац"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Продирућа звезда"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Нема човека над којим неко не бди"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Нека човек погледа од чега је створен"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Створен је од течности која избија у млазу"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "која између кичме и груди излази"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "и Он је, заиста, у стању поновно да га створи"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "На дан када тајне буду откривене"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "када човек ни снаге ни помагача неће да има"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "И тако ми неба пуног кише која се враћа"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "и Земље пуне распукнућа"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Кур'ан је, заиста, говор који раставља Истину од неистине"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "није никаква лакрдија"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Они, заиста, праве замке"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "па и Ја њима замке правим"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "зато неверницима дај још времена, остави их још који тренутак"
        }
    ]
}