{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "У њима ће да буде воћа и палми и шипака"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "У њима ће да буду лепотице нарави дивних"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Хурија у шаторима скривених"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Које никада ни човек ни дух није додирнуо"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Они ће да буду наслоњени, на прекривачима зеленим и ћилимима чудесно украшеним и лепим"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Нека је Узвишено име твога Господара, Величанственог и Племенитог"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Када се Неизбежни догађај догоди"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "догађање његово нико неће порицати"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Неке ће да понизи, а неке да узвиси"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Кад се Земља јако потресе"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "и брда се у комадиће здробе"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "и постану разасута прашина"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "вас ће да буде три врсте"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "они са десне стране - шта ће да буде са оним са десне стране"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "и они са леве стране - шта ће да буде са онима са леве стране"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "И они који су предњачили – они ће предњачити"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "То су они блиски"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "у рајским баштама биће наслада"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Биће их много из првих"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "а мало из задњих"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "на диванима извезеним"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "једни према другима на њима ће да буду наслоњени"
        }
    ]
}