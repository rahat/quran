{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Тако ми неба и звезде која се појављује ноћу"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "А знаш ли ти шта је то ноћни посетилац"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Продирућа звезда"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Нема човека над којим неко не бди"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Нека човек погледа од чега је створен"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Створен је од течности која избија у млазу"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "која између кичме и груди излази"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "и Он је, заиста, у стању поновно да га створи"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "На дан када тајне буду откривене"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "када човек ни снаге ни помагача неће да има"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "И тако ми неба пуног кише која се враћа"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "и Земље пуне распукнућа"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Кур'ан је, заиста, говор који раставља Истину од неистине"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "није никаква лакрдија"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Они, заиста, праве замке"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "па и Ја њима замке правим"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "зато неверницима дај још времена, остави их још који тренутак"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Слави и узвисуј име свога Господара, Свевишњег"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Који све ствара и чини складним"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "и Који одређује, па упућује"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "и Који чини да изникне растиње"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "а потом чини да постане суво, потамнело"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ми ћемо да те научимо да учиш па ништа нећеш да заборавиш"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "осим онога што Аллах хоће. Он, заиста, зна и оно што на јаву износите и оно што кријете"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "И доступним да ти учинимо оно што је лако"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "па ти опомињи док опомена буде користила"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "опоменуће се онај који се боји Аллаха"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "а избегаваће је онај најгори"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "који ће у великој ватри да гори"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "па у њој неће ни да умире ни да живи"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Успео је онај који се очисти"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "и спомене име свога Господара па молитву обави"
        }
    ]
}