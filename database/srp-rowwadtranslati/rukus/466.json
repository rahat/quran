{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Свемилосни"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "подучава Кур'ану"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "ствара човека"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "учи га изговору"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Сунце и Месец плове по тачно одређеном прорачуну"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "и звезде и дрвеће се клањају Богу"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "а небо је уздигао. И мерила поставио"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "да не прелазите границе правде"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "и право мерите и на ваги не закидајте"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "А Земљу је учинио погодном за створења"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "И на њој има воћа и палми са плодом у чашицама"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "и жито са лишћем и мирисног цвећа"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Он је човека створио од сасушене иловаче, као што је грнчарија"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "а духове од пламена ватре –"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Господар два истока и два запада –"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Пустио је два мора да се сусрећу"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "између њих је преграда и она се не мешају –"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Из њих се ваде бисери и украси корала –"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "па, коју благодат свога Господара поричете"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Његове су и лађе које се као брда уздижу високо по мору –"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "па, коју благодат свога Господара поричете"
        }
    ]
}