    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Зар твоје груди нисмо учинили пространим"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "и твоје бреме скинули"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "које је твоја плећа притискало"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "и спомен на тебе високо уздигли"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Па, заиста, са муком је олакшање"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "заиста, са муком је олакшање"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "А кад завршиш, предај се молитви"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "и тежи своме Господару"
        }
