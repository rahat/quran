    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "И фараон посла сакупљаче по градовима"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "„Заиста су ови мала скупина"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "и расрдили су нас"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "а сви смо спремни!“"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "И Ми их изведосмо из вртова и река"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "из ризница и дивних двораца"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Ето, тако је било, и Ми смо дали да то наследе Израиљеви синови"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "и они их, у зори, сустигоше"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "па кад једни друге угледаше, Мојсијеви другови повикаше: “Сустиће нас сигурно!”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "“Неће!” Рече он, “Господар мој је са мном, Он ће ми пут показати.”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "И Ми објависмо Мојсију: „Удари штапом својим по мору!“ И оно се растави и свака његова страна је била као велико брдо"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "и Ми онда тамо оне друге приближисмо"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Мојсија и све оне који су били са њим спасисмо"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "а оне друге потописмо"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "У томе је, заиста, знак, али већина њих нису били верници"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "А, заиста, твој Господар је Силни и Милостиви"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "И казуј им вест о Авраму"
        }
    ]
