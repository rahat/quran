{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Āmarā ki tōmāra bakṣa praśasta karē di'i ni"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "āra āmarā tōmāra thēkē lāghaba karēchi tōmāra bhāra"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yā cēpē basēchila tōmāra piṭhē"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "āra āmarā tōmāra jan'ya unnata karēchi tōmāra nāmōllēkha."
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ata'ēba kaṣṭēra saṅgē'i tō ārāma raẏēchē"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niścaẏa kaṣṭēra saṅgē ārāma raẏēchē."
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Sutarāṁ yakhana tumi mukta haẏēchē takhana kaṭhōra pariśrama karō"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "āra tōmāra prabhura prati tabē ēkāntabhābē manōnibēśa karō."
        }
    ]
}