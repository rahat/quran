{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Tumi balō -- ''tini āllāh‌, ēkaka-adbitīẏa"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "āllāh -- parama nirbharasthala."
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Tini janma dēna nā, ēbaṁ janma nēna'ō ni"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ēbaṁ kē'u'i tām̐ra samatulya hatē pārē nā.’’"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Tumi balō -- ''āmi āśraẏa cā'ichi niśibhōrēra prabhura kāchē"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "tini yā sr̥ṣṭi karēchēna tāra aniṣṭa thēkē"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "āra andhakārēra aniṣṭa thēkē yakhana tā ācchanna karē phēlē"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "āra gām̐thanitē phuṯkāriṇīdēra aniṣṭa thēkē"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "āra hinsākārīra aniṣṭa thēkē yakhana sē hinsā karē.’’"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tumi balē yā'ō -- ''āmi āśraẏa cā'ichi mānuṣēra prabhura kāchē"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "mānuṣēra mālikēra"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānuṣēra upāsyēra"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "gōpanē ānāgōnākārīra kumantraṇāra aniṣṭa thēkē"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "yē mānuṣēra bukēra bhētarē kumantraṇā dēẏa"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jinēra athabā mānuṣēra madhyē thēkē.’’"
        }
    ]
}