{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tādēra cāridikē ghurē bēṛābē ciranabīna taruṇērā"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pānapātra ō sōrā'i niẏē ō nirmala pānīẏēra pēẏālā."
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Tādēra māthā dharabē nā tātē, āra tādēra nēśā'ō dharabē nā."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Āra phala-mūla yā tārā pachanda karē"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "āra pākhira mānsa yā tārā kāmanā karē"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "āra āẏatalōcana hūragaṇa"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ābr̥ta muktāra udāharaṇēra n'yāẏa"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "yā tārā karatō tāra puraskāra."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tārā sēkhānē śunabē nā kōnō khēlōkathā, nā kōnō pāpabākya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "śudhu ē'i kathā chāṛā -- ''sālāma! Sālāma!’’"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Āra ḍānadikēra dala, -- kēmanatara ē'i ḍānadikēra dala"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "kām̐ṭā bihīna sidarāha-gāchēra nīcē"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "āra sāri sāri sājānō kalāgācha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "āra sudūrabistr̥ta chāẏā"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "āra uchalē ōṭhā pāni"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "āra pracura parimāṇē phalamūla"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "byāhata habāra naẏa ēbaṁ niṣid'dha habāra'ō naẏa."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Āra um̐cudarēra gālicā."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Niḥsandēha āmarā ōdēra sr̥ṣṭi karēchi biśēṣa sr̥ṣṭitē"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "āra tādēra bāniẏēchi cirakumārī"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "sōhāginī, samabaẏaskā"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "dakṣiṇapanthī lōkadēra jan'ya."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Prathamakālīnadēra thēkē adhika saṅkhyāẏa"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "āra parabartīkālīnadēra madhyē thēkē'ō adhika saṅkhyāẏa."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Kintu bāmapanthīdala -- kēmanatara ē'i bāmapanthī dala."
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Uttapta bātāsē ō phuṭanta pānitē"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "āra kālō dhōm̐ẏāra chāẏāẏa"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "śītala naẏa ēbaṁ sam'mānajanaka'ō naẏa."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Athaca tārā tō ēra āgē chila bhōgabilāsē magna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "āra tārā ghōratara pāpācārē jēda dharē thākata"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "āra tārā balata -- ''kī! Āmarā yakhana marē yāba ō māṭi ō hāḍḍi haẏē yāba takhana ki āmarā ādau punarut'thita haba"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ēbaṁ āmādēra pūrbabartī pitr̥puruṣarā'ō"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "tumi balō -- ''niḥsandēha pūrbabartīrā ēbaṁ parabartīrā"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "abaśya'i sabā'ikē ēkatrita karā habē ēka subidita dinēra nirdhārita sthānē-kṣaṇē"
        }
    ]
}