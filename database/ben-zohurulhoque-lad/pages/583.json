{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Dharmabhīrudēra jan'ya niścaẏa'i raẏēchē mahāsāphalya"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "phalēra bāgāna ō āṅura"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "āra samabaẏaska phuṭaphuṭē kiśōra"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "āra paripūrṇa pānapātra."
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tārā sēkhānē khēlō kathā śunabē nā, āra mithyākathā'ō naẏa."
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Tōmāra prabhura kācha thēkē pratiphala, -- hisābamatō puraskāra"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "mahākāśamanḍala ō pr̥thibīra ēbaṁ tādēra madhyē yā-kichu āchē tāra prabhu, parama karuṇāmaẏa, tām̐ra kāchē baktabya rākhāra kōnō kṣamatā tārā rākhē nā."
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Sē'idina āra-rūha ō phiriś‌tāgaṇa sārabēm̐dhē dām̐ṛābē, parama karuṇāmaẏa yākē anumati dēbēna sē byatīta tādēra kē'u kathā balatē pārabē nā, āra sē saṭhika kathā balabē."
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Ē'iṭā'i mahāsatyēra dina. Ata'ēba yē kē'u cāẏa sē tāra prabhura kāchē āśraẏasthala khum̐juka."
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niścaẏa āmarā tōmādēra satarka karachi ēka nikaṭabartī śāsti sanbandhē, -- yēdina mānuṣa dēkhatē pābē tāra hāta dukhānā kī āgabāṛiẏēchē, āra abiśbāsī byakti balabē -- ''hāẏa āmāra āphasōsa! Āmi yadi dhulō haẏē yētāma!’’"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Bhābō pracēṣṭākārīdēra pracanḍa-pracēṣṭāra kathā"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "āra kṣipragāmīdēra tbarita ēgunōẏa"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "āra santaraṇakārīdēra drata santaraṇē"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "āra agragāmīrā ēgiẏē'i calēchē"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "tārapara ghaṭanāniẏantraṇakārīdēra kathā"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "sēdina spandita habē birāṭa spandanē"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "parabartī ghaṭanā tākē anusaraṇa karabē'i."
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Hr̥daẏa sēdina santrasta habē"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "tādēra cōkha habē abanata."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Tārā balachē -- ''āmarā ki satyi'i prathamābasthāẏa pratyābartita haba"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "yakhana āmarā galā-pacā hāḍḍi haẏē yāba takhana'ō?’’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Tārā balē -- ''tā'i yadi haẏa tabē ē habē sarbanāśā pratyābartana.’’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kinta ēṭi niścaẏa'i habē ēkaṭi mahāgarjana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "takhana dēkhō! Tārā habē jāgrata."
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Tōmāra kāchē mūsāra kāhinī paum̐chēchē ki"
        }
    ]
}