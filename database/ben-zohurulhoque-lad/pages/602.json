{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kurā'iśadēra nirāpattāra jan'ya"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "śītakālīna ō grīkālīna bidēśayātrāẏa tādēra nirāpattāra jan'ya."
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ata'ēba tārā ē'i gr̥hēra prabhura upāsanā karuka"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yini kṣudhāẏa tādēra āhāra diẏēchēna, āra bhaẏa-bhīti thēkē tādēra nirāpada rēkhēchēna."
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi ki tākē dēkhēcha yē dharmakarmakē pratyākhāna karē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "sē tō ai jana yē ētīmadēra hām̐kiẏē dēẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "āra garība-duḥkhīkē khā'ōẏānōra kṣētrē uṯsāha dēkhāẏa nā."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'ēba dhik sē'isaba nāmāya-paṛuẏādēra prati"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yārā sbaẏaṁ tādēra nāmāya sanbandhē udāsīna"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yārā nijērā'i hacchē lōka-dēkhiẏē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "āra yārā niṣēdha karē sāhāyya-sahāẏatākaraṇa."
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niḥsandēha āmarā tōmākē prācurya diẏēchi."
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sutarāṁ tōmāra prabhura uddēśyē nāmāya ādāẏa karō ēbaṁ kurabāni karō."
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Tōmāra bidbēṣakārī'i tō sbaẏaṁ bañcita."
        }
    ]
}