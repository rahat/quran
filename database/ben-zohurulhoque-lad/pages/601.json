{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Bhābō bikālabēlāra kathā."
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Niḥsandēha mānuṣa ālabaṯ lōkasānē paṛēchē"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "tārā byatīta yārā īmāna ēnēchē ō saṯkāja karachē, āra parasparakē satya abalanbanēra jan'ya mantraṇā dicchē, ēbaṁ parasparakē adhyabasāẏa abalanbanēra parāmarśa dicchē."
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Dhik pratyēka nindākārī kuṯsāraṭanākārīra prati"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "yē dhanasampada jamā karachē ēbaṁ tā gunachē"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "sē bhābachē yē tāra dhanasampatti tākē amara karabē."
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kakhanō nā! Tākē abaśya'i nikṣēpa karā habē sarbanāśā durghaṭanāẏa."
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Āra kisē tōmākē bōjhānō yābē sē'i hutāmāh ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "tā āllāh‌ra hutāśana, prajjbalita raẏēchē"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yā udita haẏēchē hr̥daẏēra uparē."
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Niḥsandēha ēṭi hacchē tādēra cārapāśē ēka bēṛā"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sārisāri khuṭiram̐ bhētarē."
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Tumi ki dēkhō ni tōmāra prabhu kēmana karēchilēna hasti-bāhinīra prati"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tādēra cakrānta tini ki byarthatāẏa paryabasita karēna ni"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "āra tādēra uparē tini pāṭhālēna jhām̐kē jhām̐kē pākhīra dala"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "yārā tādēra āchaṛē chila śakta-kaṭhina pātharēra gāẏē"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "phalē tini tādēra bāniẏē dilēna khēẏē phēlā khaṛēra matō."
        }
    ]
}