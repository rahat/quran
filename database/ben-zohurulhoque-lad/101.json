{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Mahāsaṅkaṭa"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kī sē mahāsaṅkaṭa"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "hāẏa, kibhābē tōmākē bōjhānō yābē sē'i mahāsaṅkaṭa ki"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sē'idina mānuṣarā habē bikṣipta paṅgapālēra matō"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "āra pāhāṛagulō haẏē yābē dhōnā paśamēra matō."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sutarāṁ tāra kṣētrē yāra pāllā bhārī habē"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "sē tō takhana habē santōṣajanaka jībanayāpanē."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kinta tāra kṣētrē yāra pāllā habē hālkā"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tāra mātā habē hābiẏāh‌."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Hāẏa, ki karē tōmākē bōjhānō yābē kī sē'i"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "jbalanta āguna."
        }
    ]
}