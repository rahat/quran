{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Bhābō ḍumurēba, āra jalapā'iẏēra kathā"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "āra sinā'i parbatēra kathā"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "āra ē'i nirāpada nagarēra kathā"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "suniścaẏa āmarā mānuṣakē sr̥ṣṭi karēchi śrēṣṭha-sundara ākr̥titē."
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Tārapara āmarā tākē pariṇata kari hīnadēra madhyē hīnatamē"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "tārā byatīta yārā īmāna ēnēchē ō saṯkāja karachē, tādēra jan'ya tabē raẏēchē bādhā-biratibihīna pratidāna."
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Tabē kī yā ēraparē tōmākē bicārasanbandhē mithyārōpa karatē dēẏa"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "āllāh ki bicārakadēra madhyē sarbaśrēṣṭha bicāraka nana"
        }
    ]
}