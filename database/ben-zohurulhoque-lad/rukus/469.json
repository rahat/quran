{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yakhana birāṭa ghaṭanāṭi ghaṭabē"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ēra saṅghaṭanakē mithyā balāra kē'u thākabē nā."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ēṭi lāñchita karabē, ēṭi karabē samunnata."
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yakhana pr̥thibī ālōṛita habē ālōṛanē"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "āra pāhāṛagulō bhēṅgē paṛabē cūrṇa-bicūrṇa haẏē"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "phalē tā haẏē yābē bikṣipta dhūlikaṇā"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "āra tōmarā haẏē paṛabē tinaṭi śrēṇītē"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "yathā ḍāna dikēra dala, -- kēmanatara ē'i ḍānadikēra dala"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "āra bām̐dikēra dala, -- kēmanatara ē'i bām̐dikēra dala"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "āra agragāmīgaṇa tō agragāmī"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ērā'i habē naikaṭyaprāpta"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "ānandamaẏa udyānē."
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Prathamakālīnadēra thēkē adhika saṅkhyāẏa"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "āra parabartīkālīnadēra thēkē alpa saṅkhyāẏa."
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Kārukāryamaẏa sinhāsanē"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "tātē tārā hēlāna diẏē āsana grahaṇa karabē paraspara mukhōmukhi haẏē."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tādēra cāridikē ghurē bēṛābē ciranabīna taruṇērā"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pānapātra ō sōrā'i niẏē ō nirmala pānīẏēra pēẏālā."
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Tādēra māthā dharabē nā tātē, āra tādēra nēśā'ō dharabē nā."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Āra phala-mūla yā tārā pachanda karē"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "āra pākhira mānsa yā tārā kāmanā karē"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "āra āẏatalōcana hūragaṇa"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ābr̥ta muktāra udāharaṇēra n'yāẏa"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "yā tārā karatō tāra puraskāra."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tārā sēkhānē śunabē nā kōnō khēlōkathā, nā kōnō pāpabākya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "śudhu ē'i kathā chāṛā -- ''sālāma! Sālāma!’’"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Āra ḍānadikēra dala, -- kēmanatara ē'i ḍānadikēra dala"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "kām̐ṭā bihīna sidarāha-gāchēra nīcē"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "āra sāri sāri sājānō kalāgācha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "āra sudūrabistr̥ta chāẏā"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "āra uchalē ōṭhā pāni"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "āra pracura parimāṇē phalamūla"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "byāhata habāra naẏa ēbaṁ niṣid'dha habāra'ō naẏa."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Āra um̐cudarēra gālicā."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Niḥsandēha āmarā ōdēra sr̥ṣṭi karēchi biśēṣa sr̥ṣṭitē"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "āra tādēra bāniẏēchi cirakumārī"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "sōhāginī, samabaẏaskā"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "dakṣiṇapanthī lōkadēra jan'ya."
        }
    ]
}