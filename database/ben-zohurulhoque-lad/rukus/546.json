{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Dhik pratyēka nindākārī kuṯsāraṭanākārīra prati"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "yē dhanasampada jamā karachē ēbaṁ tā gunachē"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "sē bhābachē yē tāra dhanasampatti tākē amara karabē."
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kakhanō nā! Tākē abaśya'i nikṣēpa karā habē sarbanāśā durghaṭanāẏa."
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Āra kisē tōmākē bōjhānō yābē sē'i hutāmāh ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "tā āllāh‌ra hutāśana, prajjbalita raẏēchē"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yā udita haẏēchē hr̥daẏēra uparē."
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Niḥsandēha ēṭi hacchē tādēra cārapāśē ēka bēṛā"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sārisāri khuṭiram̐ bhētarē."
        }
    ]
}