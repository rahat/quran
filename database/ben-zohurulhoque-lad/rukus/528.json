{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bhābō ākāśēra ō rātēra āgantukēra kathā"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "āra kī tōmākē bujhatē dēbē kē sē'i rātēra āgantuka"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ēkaṭi atyujjbala nakṣatra."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyēka sāra pakṣē'i -- tāra uparē ēkajana tattbābadhāẏaka raẏēchē."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Sutarāṁ mānuṣa bhēbē dēkhuka kisē thēkē tākē sr̥ṣṭi karā haẏēchē."
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Tākē sr̥ṣṭi karā haẏēchē sabēgē-skhalita pāni thēkē"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yā nirgata haẏa mērudanḍa ō bukēra pām̐jarēra madhya hatē."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niḥsandēha tini tāra pratyābartanē abaśya'i kṣamatābāna."
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sē'idina lukōnō saba-kichukē prakāśa karā habē"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "takhana tāra thākabē nā kōnō kṣamatā ō nā kōnō sāhāyyakārī."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Bhābō barṣaṇōnnukha ākāśēra kathā"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "āra pr̥thibīra kathā yā bidīrṇa haẏa."
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Niḥsandēha ēṭi sumīmānsākārī baktabya"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "āra ēṭi kōnō tāmāśāra jinisa naẏa."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Niḥsandēha tārā cāla cālachē"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "āra āmi'ō parikalpanā udbhābana karachi."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ata'ēba tumi abiśbāsīdēra abakāśa dā'ō, tādēra abakāśa dā'ō kichuṭā samaẏa."
        }
    ]
}