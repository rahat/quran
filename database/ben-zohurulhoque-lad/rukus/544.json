{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Prācuryēra pratiyōgitā tōmādēra matibhrama ghaṭāẏa"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "yatakṣaṇa nā tōmarā kabarē āsō."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nā! Śīghra'i tōmarā jānatē pārabē"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ābāra bali, -- nā, śīghra'i tōmarā jānatē pārabē"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "nā, yadi tōmarā jānatē niścita jñānē"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "tōmarā tō bhaẏaṅkara āguna dēkhabē'i."
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ābāra bali, tōmarā abaśya'i ēṭi dēkhabē niścita dr̥ṣṭitē."
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Ērapara sē'idina tōmādēra abaśya'i jijñāsā karā habē abadāna samparkē."
        }
    ]
}