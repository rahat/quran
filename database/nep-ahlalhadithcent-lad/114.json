{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tapā'ī bhanidinus ki ma mānisaharūkō pālanakartākō śaraṇa mām̐gdachu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mānisaharūkō svāmīkō"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ra mānisaharūkō upāsyakō (śaraṇa)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kharāba vicāra hālnē ra (uksā'ēra) pachi haṭī jānēbāṭa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jasalē mānisaharūkō hr̥dayamā narāmrō karmakō san̄ckāra hāldacha"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Tyō cāhē jinnaharūmadhyēbāṭa hōs vā mānisaharūmadhyēbāṭa"
        }
    ]
}