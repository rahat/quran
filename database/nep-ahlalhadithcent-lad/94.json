{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Kē hāmīlē timrō chātī khōlidi'ēnauṁ"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ra timīmāthibāṭa timrō bōjha pani hāmīlē utāridiyauṁ"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Jasalē timrō kam'mara bhām̐cirākhēkō thiyō"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ra hāmīlē timrō maryādā ucca pāryauṁ"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Niḥsandēha kaṭhinā'ikō sāthamā saralatā pani cha"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Niḥsandēha kaṣṭakō sātha sajilō (saralatā) cha"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Jaba timīlā'ī phursada huncha, taba ibādata (pūjā)mā pariśrama gara"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ra āphnō pālanakartātirai pravr̥tta bha'ihālnē gara"
        }
    ]
}