{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Jaba ākāśa kṣatavikṣata bha'ihālnēcha"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "ra jaba tārāharū jharihālnēchan"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ra jaba samundra bagēra jānēchan"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ra jaba cihānaharū utkhanita hunēchan"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Taba hara vyaktilē thāhā pā'unēcha ki usalē agāḍi kē paṭhā'ēkō thiyō ra pachāḍi kē chōḍēkō thiyō"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "hē mānava! Kuna kurālē timīlā'ī āphnō udāra pālanakartākō viṣayamā bhramamā pārēkō cha"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "(tyahī nahō) jasalē timīlā'ī sr̥ṣṭi garyō ra phēri (timrā aṅga–pratyaṅgaharūlā'ī) ṭhīka–ṭhīka pārī timīlā'ī santulita banāyō"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Ra juna rūpamā cāhyō, timīlā'ī jōḍēra tayāra garyō"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kēhī hō'ina, baru timīlē pratiphala di'inē dinalā'ī jhuṭhlā'um̐chau"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Jabaki timīmāthi nigarānī garnēharū niyukta chan"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Pratiṣṭhita lēkhnēharū niyukta chan"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Timīlē jē gardachau, (tyasalā'ī) unīharū jāndachan"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Niḥsandēha satkarmī mānisaharū svargakā aiśō ārāma ra anugrahamā hunēchan"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ra niḥsandēha dūrācārīharū narkamā hunēchan"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "(Arthāt) pratiphalakō dina tyasamā pravēśa garnēchan"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ra tyasabāṭa umkēra jāna saknē chainan"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ra timīharūlā'ī kē thāhā ki pratiphalakō dina kastō cha"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "ma phēri bhanchu timīlā'ī kē thāhā ki pratiphalakō dina kastō cha"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Juna dina kunai vyakti kasaikō nimti kunai kurākō adhikārī hunēchaina, ra sabai ādēśa tyasa dina allāhakai hunēchan"
        }
    ]
}