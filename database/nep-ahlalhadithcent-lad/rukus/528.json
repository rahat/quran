{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kasama cha ākāśa ra rātimā prakāśita hunēvālākō"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ra timīlā'ī kē thāhā ki rātimā prakaṭa hunēvālā kē hō"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tyō tārā hō, camkilō"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Kunai vyakti yastō chaina jasamāthi ē'uṭā phariśtā nirīkṣaka niyukta nahōs"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Manuṣyalā'ī āvaśyaka cha ki dēkhōs ki ū kuna kurōbāṭa sr̥ṣṭi gari'ēkō cha"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Tyō ē'uṭā ucāli'ēkō pānībāṭa janmēkō cha"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Jō piṭhyum̐ ra chātīkō bīcabāṭa niskancha"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niścaya nai ū usalā'ī pharkā'idinē (arthāt punaḥ jīvita pāridinē) kurōmā sāmathryavāna cha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Juna dina (manamā) lukā'i'ēkā kurāharū jām̐cinēchan"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ta tyasabēlā mānisakō sāthamā na ta tyasakō śakti hunēcha ra na kōhī sahāyaka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ākāśakō kasama jasalē vr̥ṣṭi gardacha"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ra dharatīkō kasama juna cyātinēvālā cha"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ki yō vāṇī ‘‘kura'āna’’ saṭīka phaisalā garnēvālā vāṇī hō"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Tyō kunai ṭhaṭṭā hō'ina"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Unīharū ē'uṭā ṣaḍyantra garirākhēkā chan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ra mailē pani ē'uṭā cālacalī rākhēkō chu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ataḥ timīlē kāphiraharūlā'ī mōhalata di'ihāla, kēhī dina unalā'ī chāḍidinu"
        }
    ]
}