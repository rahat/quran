{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(Tyō) khaḍakhaḍā'unē carkō svara garnēvālā"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Khaḍakhaḍā'unē kē hō"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ra timīlā'ī kē thāhā ki tyō khaḍakhaḍā'ī dinē kē hō"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "juna dina mānisaharū yastā hunchan jastō ki chari'ēkā bhusunāharū"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ra pahāḍaharū dhuni'ēkā ruṅga viraṅgakā ūna, jastā hunēchan"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Phēri jasakā (karmaharūkō) vajana garuṅgō hunēcha"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ū ta icchita ārāmakō jindagīmā hunēchan"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ra jasakō vajana halakā hunēchan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "unīharūkō pharkēra jānē ṭhā'um̐ ‘‘hāviyā’’ (gahirō khālḍō) hō"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ra timīlā'ī kē thāhā ki ‘‘hāviyā’’ kē hō"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(tyō) prajvalita āgō hō"
        }
    ]
}