{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Barbādī cha, tyastō pratyēka mānisakō jō arkākō avaguṇa khōjnē gardacha ra cukulī garirahancha"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Jasalē mālasampatti thupārdai jāncha ra tyasalā'ī gandai rākhcha"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Usalē ṭhāncha ki usakō sampatti usasam̐ga sadhaiṁ rahanēcha"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kadāpi hō'ina, yō ta avaśya ṭukrā–ṭukrā pāridinē āgōmā phyām̐kinēcha"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ra timīlā'ī kē thāha ki yastō (āgō) hutmā kē hō"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "tyō allāhadvārā prajvalita āgō hō"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Jō chātīmāthi caḍhdai jānēcha"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ra (tyō āgō) unīmāthi,sabai diśābāṭa banda huncha"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ṭhūlāṭhūlā stambhaharūmā"
        }
    ]
}