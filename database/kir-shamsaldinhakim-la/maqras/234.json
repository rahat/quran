{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Paygambar) kabagın burkop, juzun burup aldı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "aldına (masele surap ) sokur kisi kelgende"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "(O, Muhammad!) Sen kaydan bilesiŋ, al (sokur kisi jaman kulk-munozdordon) tazadır"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Je bolboso, (Allaһ Taalanı) eskereer, anan eskermesi aga payda bereer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al emi, muktaj emes adam(dar kelgende)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "sen oso(lor)go burulup aldıŋ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "(Birok), alardın tazarbay (ıymanga kelbey) kaluusuna sen joopker emessiŋ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al emi, saga curkap kelgen (sokur ) adam"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "(Allaһtan jana Kıyamat kundon) korkot ele"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen aga koŋul burbay, (baskalar menen alagdı bolup) kaldıŋ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Jok! (O, Muhammad! Emi anday kılbagın!) Bul (ayattar saga Allaһ tarabınan) eskerme-sabak"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kim kaalasa — anı eskeret"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Ayattar Lavhul Mahfuz dep atalgan) Uluk Baraktar da"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "(Lavhul Mahfuz) Taza—-Pakiza (jana kadır barkı) Jogoru"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Jana) peristelerdin kolunda"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(Alar) Oto uluk, ayabay taza (isenimduu)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Naalatı insan! Anı emne (mıncalık) asa capkan kaapırga aylanttı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Al (Allaһ) anı emneden jarattı ele"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamcı suudan jaratkan! Anan anı(n jaraluu baskıctarın olcop,) tagdır kılgan"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Andan kiyin (bul duynogo kelgende) aga joldordu? oŋoy kılıp berdi"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Andan soŋ anın janın alıp, murzogo koydu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Andan soŋ Ozu kaalagan kezde (Kıyamatta) kayradan tiriltip alat"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jok! Al ozuno (Allaһ) buyurgan parızdardı ec atkarbadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Emi, insan (boy kotoro berbey) ozunun tamak-asına karabaybı"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Suunu (asmandan) mol-kol kılıp mına Biz jaadırdık"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Andan soŋ jerdi jarıp"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "(Ar turduu) dandı (jana osumduktordu) ondurduk"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Juzumdordu, kok coptordu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zaytundardı, kurmalardı"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Koyuu baktardı"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Momo-jemisterdi, jasılcalardı"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Siler jana carba aybandarıŋar paydalangıla dep"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Emi, kacan (kulaktardı) tunduruucu (Kıyamattagı) aykırık (Sur kerneyinin ekinci tartıluusu) kelgende"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Al kunu kisi ozunun aga-inisinen kacıp kalat"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Apası menen atasınan dagı"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ayalı menen baldarınan dagı"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "(Antkeni) al kunu ar bir adam ucun anı (baskalarga) koŋul burdurbay koyo turgan (oto oor) abal bolot"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Al kunu (ıymanduu) juzdor albırıp"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "kulup-jaynap, kubanıctuu"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "(ıymansız) juzdordu al kundo caŋ-tozon kaptap"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "karaŋgılık orop alat"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Dal osolor kaapır, sukursuz adamdar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kun orolup (nuru jogolup)"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "jıldızdar (kıyrap) togulup"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Toolor (tıtılgan jun sıyaktuu) ucurulup"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kursagında on aylık balası bar (kımbat baaluu) toolor dagı koŋul sırtında kalıp,”"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Japayı aybandar (urkup, bir jerge) cogulup"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Deŋizder janıp-ot bolup"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Jandar biri - birine kostolup...°"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Tiruuloy komup jiberilgen bobok kızdar suralganda —"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kaysı kunoo(ŋor) ucun olturuldu(ŋor dep)"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Jana (bardık adamdarga) amal barakcaları taratılıp"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Asman sıyrılıp alınganda"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Tozok (katuu) kızıtılıp"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Beyis (ıymanduu adamdarga) jakın alıp kelingende"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "(Osol kunu bardık) jan (Akıret ucun) emne alıp kelgenin bilet"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Jok. Artta kala turgan jıldızdarga ant icip"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(alarga) joldos, korunbogon jıldızdarga ant icip"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "(karaŋgısı menen) kelgen tungo ant icip"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "agargan taŋga ant icip (aytamın)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Cınında, al (Kuraan) Uluu Elcinin Sozu"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ars Eesinin? aldında (Jebreyildin) turuktuu urmatı bar (jana al) oto kuc — kubattuu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Al jerde (jogorku jamaat arasında) sozu otumduu (jana) isenimduu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "(Ey, musrikter!) Silerdin dayım birge jurgon serigiŋer (Muhammad) jindi emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Al cınında anı (Jebireyildi) aykın meykindikte korgon"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Jana al kayıptı (silerden) kızganuucu emes"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Jana al” kargıska jolukkan Saytandın sozu da emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Emi, (cındıktan juz burup) kayda bara jatasıŋar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Al (Kuraan) albette, aalamdar ucun eskertuu"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Silerdin araŋardan oŋoluunu kaalgandar ucun (eskertuu)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Jana siler (o, pendeler) aalamdardın Rabbisi kaalagandan baskanı kaalay albaysıŋar"
        }
    ]
}