{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asırgaant"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Albette, (ar bir) insan zıyanda"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Al emi, ıyman keltirgen, salih (soop) amaldardı jasagan jana biri-birine akıykattık menen sabırduuluktu osuyat (nasaat) kılgandar gana (zıyanda emes)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Adamdardı kılıgı jana tili menen sıldıŋdagan bardık sıldıŋcıga azap-tozok bolsun"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al mal-mulktu (adal-aramına karabay) cogulttu jana eseptedi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Duyno-mulku anı tuboluk jasatat dep oyloyt"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jok! Alar (Kıyamatta) sozsuz «Hutamaga» tastalat"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "(O, Muhammad!) Sen «Hutama» emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Al) Allaһtın aloolongon tozogu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(Deneden tesip otup) juroktorgo ceyin jete turgan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Cınında, (tozok) alardın ustunon jaap koyulgan"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(Esikteri) uzun cınjırlar menen (bekemdelgen)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "(O, Muhammad!) Rabbiŋ pil eelerin emne kılganın bilbediŋbi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "(Rabbiŋ) alardın (Kaabanı buzup saluu ucun oylogon) ayla-amaldarın tekke ketirbedibi"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Jana alarga karsı top-top kustardı jiberdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "(Kustar ceŋgelindegi) copo-tastar menen alardı tasbaraŋga alıstı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "(Allaһ) alardı moysolgon jalbıraktay kılıp koydu"
        }
    ]
}