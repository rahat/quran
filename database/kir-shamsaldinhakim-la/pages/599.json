    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Rabbisinin aldında alardın sıylıktarı — anda \"Adn\" Beyister! Alardın tubunon darıyalar agıp turat jana alar anda tuboluk kalısat. (Munun sebebi) alardan Allaһ ıraazı boldu, alar Allaһtan ıraazı bolustu. Bul (sıylıktar) Rabbisinen korkkon adamdar ucun"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer katuu titiregende"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "jana ozunun “juktorun” cıgarıp tastaganda"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Insan (korkup jana taŋ kalıp) “buga emne boldu?!” deyt"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Osol Kundo jer oz kabarların suyloyt"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Antkeni, aga Rabbiŋ (“suylo!” dep) vahiy kılgan.”"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Al Kunu adamdar amaldarı korgozulusu ucun top-top bolup keliset"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Jana kim kıpın salmagınday* jaksılık kılgan bolso — anı korot"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "jana kim kıpın salmagınday jamandık jasagan bolso — osonu korot"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Alkınıp curkagan tulparlarga ant"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot cacırata curkagan attarga ant"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Taŋ azanda (dusmanga) cabuul koygon attarga ant"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Osondo alar (cabuulu menen) topuraktı ızgıttı"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Andan soŋ (dusman) toptorunun ortosuna jarıp kirdi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan degen anıgında, Rabbisine nasukur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Buga anın ozu dele kubodur"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Al baylıktı katuu suyot"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilbeybi al murzodogu oluktor cıgarılganda"
        }
    ]
}