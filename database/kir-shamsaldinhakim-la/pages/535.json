{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "(Alardın kızmatında) tuboluk jas jigitter tegerenip jurusot"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Tattuu sarap tolturulgan, kozoloru, kumuraları, coycoktoru menen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Al saraptan alardın bastarı oorubayt jana mas boluspayt"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "(Kızmatcı jigitter, alar) tandagan momolor menen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tabiti kaalagan (ar turduu, bıskan) kus etteri menen (tegerenip jurusot)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Jana (alar ucun) bakıraŋ koz ur kızdar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Sedepte) jasıngan bermet sıyaktuu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jasagan (soop) isterinin sıylıgına"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(Beyisiler) al jerde maanisiz jana uyat (kunoo) sozdordu uguspayt"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Bir gana «salam-salam» (degen) sozdordu ugusat"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri ne degen (bakıt)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Alar) tikensiz «Sidr»! daragının arasında"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Momoluu banan baktarında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Jayılgan (kenen) kolokolordo"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(Sar) akkan suular(dın boyun)da"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kop momo-jemisterdin icinde —"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tugonbogon, uzulbogon"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "(Jana) balpaygan (jumsak, jibek) tosoktordo"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz alardı (Beyistegi ur kızdardı) ozgoco jaratuu menen jarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Biz alardı bikir* kılıp jaratıp"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bir tegiz kurakta jana oto suymoncuk (nazik) kılıp koyduk"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oŋ tarap eeleri ucun"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Alar (oŋ tarap eeleri) abalkılardan kop jamaattar"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Akırkılardan azıraak jamaattar"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Jana sol tarap eeleri. Sol tarap eeleri kanday (tomon daraja)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Alar «samum» menen «hamimdin!»!icinde"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "jana kap-kara tutundun «kolokosundo»"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Anın salkını da jok, korku da jok"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Alar murda(gı jasoosunda) aramkor adamdar ele"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Alar uzgultuksuz, coŋ kunoolordu jasaskan"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Jana «Biz olup topurak jana (cirigen) sook bolup kalsak dele kayra tirilet bekenbiz"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ata-babalarıbız dagı (kayra tirilet beken?)»-desken"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Alarga ayt): «Cınında, abalkılar dagı, akırkılar dagı"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Belgilengen kundun ubadasına cogultulat"
        }
    ]
}