{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Asman jarılıp"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Jıldızdar togulup"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "deŋizder (tolup-tasıp), biri-birine kosulganda"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Jana murzolor oodarılganda"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "(Ar bir) jan (eesi) emne isti alıp kelgenin jana (emne isti keciktirip) kaltırganın bilip alat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O, (kaapır) insan! Seni Uluk Rabbiŋ jonundo (tuura oylonuudan) emne aldap koydu"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Alseni jaratıp, kelbetiŋdi kelistirgen ele"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Jana Al seni Ozu kaalagan surotto kalıptandırgan"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Jok! siler (oylonup, moyun sunuunun orduna) surak kunun «jalgan» dediŋer"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Cınında, silerdin ustuŋordo (ar bir isiŋerdi) saktoocu peristeler bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "(Ar bir nerseni) jazıp turgan uluk peristeler bar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Silerdin bardık isiŋerdi bilip turusat"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Cınında jaksı kisiler neemat beyisterinde"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Al emi, buzuku kisiler tozokto bolusat"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Alar aga (Tozokko) surak kunu tastalat"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Anan andan cıga alıspayt"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "(O, Muhammad! Biz aytpasak) surak kunu emne ekenin sen kaydan bilmeksiŋ"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Anan, surak kunu emne ekenin sen kaydan bilmeksiŋ?”"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Al kunu (Allaһtan uruksat bolmoyunca) ec kim ec kimge (payda) jetkire albayt! Al kunu bardık is Allaһtın ıktıyarında"
        }
    ]
}