{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "(Kıyamattık) okuya bolgondo"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Anın bolgonun ec kim «jalgan» dey albayt"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Al tomondotuucu, biyiktetuucu"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer katuu silkinip, titiregende"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Toolor dagı talkalanıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "caŋ-tozongo aylanganda"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Jana siler (tomondogu) uc taypaga bolungondo"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri kanday (biyik daraja)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Janasol tarap eeleri. Sol tarap eeleri kanday (tomon daraja)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Aldıŋkılar — aldıŋkılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Osolor (Allaһka) jakın bolgondor"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Alar «Naim» (neematı eŋ kop) beyisterde bolusat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(Naim beyisine) abalkı momundardan koburook jamaattar"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Akırkılardan azıraak jamaattar (tusot)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alar (asıl tastar aralas) tokulgan sorulordo"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jambastap, biri-birine betmaŋday (suylosup) oturusat"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "(Alardın kızmatında) tuboluk jas jigitter tegerenip jurusot"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Tattuu sarap tolturulgan, kozoloru, kumuraları, coycoktoru menen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Al saraptan alardın bastarı oorubayt jana mas boluspayt"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "(Kızmatcı jigitter, alar) tandagan momolor menen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tabiti kaalagan (ar turduu, bıskan) kus etteri menen (tegerenip jurusot)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Jana (alar ucun) bakıraŋ koz ur kızdar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Sedepte) jasıngan bermet sıyaktuu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jasagan (soop) isterinin sıylıgına"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(Beyisiler) al jerde maanisiz jana uyat (kunoo) sozdordu uguspayt"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Bir gana «salam-salam» (degen) sozdordu ugusat"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri ne degen (bakıt)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Alar) tikensiz «Sidr»! daragının arasında"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Momoluu banan baktarında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Jayılgan (kenen) kolokolordo"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(Sar) akkan suular(dın boyun)da"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kop momo-jemisterdin icinde —"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tugonbogon, uzulbogon"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "(Jana) balpaygan (jumsak, jibek) tosoktordo"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz alardı (Beyistegi ur kızdardı) ozgoco jaratuu menen jarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Biz alardı bikir* kılıp jaratıp"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bir tegiz kurakta jana oto suymoncuk (nazik) kılıp koyduk"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oŋ tarap eeleri ucun"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Alar (oŋ tarap eeleri) abalkılardan kop jamaattar"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Akırkılardan azıraak jamaattar"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Jana sol tarap eeleri. Sol tarap eeleri kanday (tomon daraja)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Alar «samum» menen «hamimdin!»!icinde"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "jana kap-kara tutundun «kolokosundo»"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Anın salkını da jok, korku da jok"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Alar murda(gı jasoosunda) aramkor adamdar ele"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Alar uzgultuksuz, coŋ kunoolordu jasaskan"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Jana «Biz olup topurak jana (cirigen) sook bolup kalsak dele kayra tirilet bekenbiz"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ata-babalarıbız dagı (kayra tirilet beken?)»-desken"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Alarga ayt): «Cınında, abalkılar dagı, akırkılar dagı"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Belgilengen kundun ubadasına cogultulat"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Kiyin, ey, akıretti «jalgan» dep adaskandar"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "«Zakkum» daragıŋn(ın acuu, tikenektuu «momosun») jeysiŋer"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Anı menen kursagıŋardı tolturup"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Ustunon kaynak suu icesiŋer"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Suusagan too sıyaktuu (jutkunup) icesiŋer!»"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Alardın Kıyamattagı «sıytamagı» — usul"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "(O, insandar!) Silerdi biz jaratkanbız, isengile"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ozuŋor agızgan bel suu jonundo oylop korduŋorbu"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Anı (adam kılıp) ozuŋor jaratıp alasıŋarbı je biz jaratabızbı"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Silerdin araŋardagı olumdu da Biz tagdır kılganbız jana Biz alsız emespiz"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Silerge oksogondordu (kıyratıp, baska elge) almastıruuga jana (Kıyamatta) silerge belgisiz abalda kayra jaratuuga"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Bilesiŋergo, (Allaһ jasagan) abalkı jaratuunu. Esiŋerge kelbeysiŋerbi"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ozuŋordun eginderiŋerge karabaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Anı ozuŋor ondurduŋorbu je Biz ondurdukpu"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Eger kaalasak Biz anı kuuratıp koymokbuz. Anan siler taŋırkagan boydon kaygırıp"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«Cınında biz zıyanga battık"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "(Irıskıdan) kol juup kaldık!»-demeksiŋer"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Icip jatkan suuŋarga karabaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Anı bulut arasınan siler tusurduŋorbu je Biz tusurdukpu"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Eger kaalasak Biz anı tuzduu kılıp koymokbuz. Sugur kılbaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ozuŋor jagıp jatkan otko baykoo saldıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Anın (otun bolgon) daragın ozuŋor jaratıp aldıŋarbı je Biz jaratıp berdikbi"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Biz jaratkanbız anı, (Kıyamattagı tozok otunan) eskerme iretinde jana musapırlar paydalanısı ucun"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Emi (o, Muhammad) Uluk Rabbiŋdin ısımın (kemcildiktuu sıpattardan) aruula"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Men jıldızdardın (batkandagı jana cıkkandagı) ordu menen ant icemin"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Bul oto uluk ant, eger bilseŋer"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Al- Uluk Kuraan"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Saktalgan-jasıruun Kitepte"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Anı (Kuraandı) taza adamdar gana karmayt"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Al—- aalamdardın Rabbisi tarabınan tusurulgon"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Siler bul (Uluk) Sozdu baalabaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Anı «jalgan» dep, cetke kaguunu onor-ırıskı kılıp alasıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "(Eger bir jakınıŋardın) jan(ı) alkımga kelse"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Osondo (koluŋardan ec nerse kelbey) karap kalasıŋar"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "(Antkeni), alarga (ilim-kuduretibiz jana peristeleribiz menen) silerge karaganda Biz jakınıraakpız. Birok, siler (alardı) korboysuŋor"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Eger siler (olbos jana) esep berbes bolsoŋor"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "(Alkımına kelgen) jandı kaytargılacı kana, eger cıncıl bolsoŋor"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Emi eger (janı alkımga kelgen) adam aldıŋkılardan bolso"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "(Aga narkı duynodo) ırahat, jırgal jana «Naim» beyisi dayar"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Al emi al oŋ tarap eelerinen biri bolso"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Anda saga (ey janı alkımına kelgen pende), oŋ tarap eelerinin salamı bolsun"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Egerde al adaskan jana (Kuraandı) «jalgan» degenderden bolso"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Aga kaynak suudan «sıytamak» berilip"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Tozokko tastalat"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Cınında bul (aytılgandar) ec kumonsuz Akıykat"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Demek, Uluk Rabbiŋdin ısımın aruulagın (ar ubak)"
        }
    ]
}