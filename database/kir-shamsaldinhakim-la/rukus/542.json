{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Alkınıp curkagan tulparlarga ant"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot cacırata curkagan attarga ant"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Taŋ azanda (dusmanga) cabuul koygon attarga ant"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Osondo alar (cabuulu menen) topuraktı ızgıttı"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Andan soŋ (dusman) toptorunun ortosuna jarıp kirdi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan degen anıgında, Rabbisine nasukur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Buga anın ozu dele kubodur"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Al baylıktı katuu suyot"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilbeybi al murzodogu oluktor cıgarılganda"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kokuroktogu sırlar askere kılınganda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Osol Kunu Rabbisi kabardar go, alardan"
        }
    ]
}