{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer katuu titiregende"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "jana ozunun “juktorun” cıgarıp tastaganda"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Insan (korkup jana taŋ kalıp) “buga emne boldu?!” deyt"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Osol Kundo jer oz kabarların suyloyt"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Antkeni, aga Rabbiŋ (“suylo!” dep) vahiy kılgan.”"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Al Kunu adamdar amaldarı korgozulusu ucun top-top bolup keliset"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Jana kim kıpın salmagınday* jaksılık kılgan bolso — anı korot"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "jana kim kıpın salmagınday jamandık jasagan bolso — osonu korot"
        }
    ]
}