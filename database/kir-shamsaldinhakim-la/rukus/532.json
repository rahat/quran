{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Usul saarga” ant"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen (o, Muhammad) usul saarda jasaysıŋ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "(Adam)-Ataga jana urpaktarga ant"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insandı masakatka jarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "(Kaapır) insan ozuno ec kimdin kucu jetpeyt dep oyloybu"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Al “esepsiz mal-duyno korottum” deyt"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Alozun ec kim korboy turat dep oyloybu"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Biz aga eki koz berbedikpi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Til jana eki erin berbedikpi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Jana Biz anı eki jolgo bagıttadık"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Al (kaapır insan) oor jolgo jurbodu"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "(O, Muhammad) sen oor jol emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(Oor jol — bul) kul azat kıluu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "je bolboso kaatcılık kundo tamak-as beruu —"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "tuugancılıgı bar jetimge"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Je caŋga bulangan kedeyge"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Kiyin (bul kayrımduuluktu jasagan) adam ıymanduulardan jana biri-birin sabırga, ırayımduulukka cakıruuculardan bolso"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Dal osolor oŋ tarap eeleri"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Al emi, Bizdin ayattarıbızga kaapır bolgondor sol tarap eeleri"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Alardı ar jaktan ot kaptap algan"
        }
    ]
}