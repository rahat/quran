{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "(O, Muhammad!) saga Orop aluucunun kabarı keldibi?”"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Al Kundo (kaapır) juzdor korkuuda(n kubarıp, jer karayt)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "(Tozoktogu kisenderden) carcagan, kıynalgan abalda"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "kızıgan otko kiret"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "(Alarga) kaynagan bulaktan suu icirilet"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Alar ucun (tozokto) tikenden baska tamak jok"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "(Al) semirtpeyt jana toydurbayt"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Al kundo (ıymanduu) juzdor sat — kuunak"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "(Bul duynodogu adal, soop) is — araketterine ıraazı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Jogorku Beyiste"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Anda maanisiz sozdordu ukpayt"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Anda agıp turgan bulaktar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Biyik sorulor"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "(Iciske dayardap) koyulgan coycoktor"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Sap-sap tizilgen jazdıktar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Jana tosolgon kilemder bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Alar (Kaapırlar) karaspaybı, toonun kanday jaratılganına"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Asmandın kanday kotorup koyulganına"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Toolordun kanday sayıp koyulganına"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Jana jerdin kanday jayıp koyulganına"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(O, Muhammad) eskertkin! Antkeni, sen eskertuucusuŋ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen alarga ustomduk kıluucu emessiŋ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Birok, kim juz bursa jana kaapır bolso"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Anı Allaһ coŋ azapka salat"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Alar (olgondon soŋ) Bizge kaytısat"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kiyin alar menen Ozubuz eseptesip alabız"
        }
    ]
}