{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku, Jaratkan Rabbiŋdin ısımı menen"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Alinsandı uyugan kandan jarattı"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku, senin Rabbiŋ eŋ uluk Zat"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Al insanga kalem menen (jazuunu) uyrottu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Insanga bilbegenin (Kuraandı jana baska ilimderdi) bilgizdi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Jok! Cınında, insan cekten asat"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ozun bay-beymuktaj korgondo"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Anıgında, kaytıp baruu Rabbiŋe gana"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Korduŋbu sen tıyuu salgan kisini"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "namaz okugan pendege"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Emne dep oyloysuŋ, eger al (namaz okuucu) tuz joldo bolso"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Je (adamdardı) takıbalıkka buyursa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "(O, Muhammad), emne deysiŋ, eger al (kaapır cındıktı) “jalgan” dese jana juz uyrup ketse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Bilbeybi al Allaһ korup turganın"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Jok! Eger (karsılıgın) toktotposo, Biz anın maŋdayınan katuu capcıp alabız"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Dal osol jalgancı, kunookor maŋdayınan"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Emi, al ozunun (isengen) adamdarın cakıra bersin"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz dagı (“Zabaaniya peristeleri” dep atalgan) tozok sakcıların cakırabız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Jok, sen aga! moyun sunba! (Rabbiŋe) sajda kıl jana (usul sıyaktuu sajdalar jana baska ibadattar menen Allaһ Taalaga) jakın bol"
        }
    ]
}