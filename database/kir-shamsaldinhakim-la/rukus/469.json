{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "(Kıyamattık) okuya bolgondo"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Anın bolgonun ec kim «jalgan» dey albayt"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Al tomondotuucu, biyiktetuucu"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer katuu silkinip, titiregende"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Toolor dagı talkalanıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "caŋ-tozongo aylanganda"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Jana siler (tomondogu) uc taypaga bolungondo"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri kanday (biyik daraja)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Janasol tarap eeleri. Sol tarap eeleri kanday (tomon daraja)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Aldıŋkılar — aldıŋkılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Osolor (Allaһka) jakın bolgondor"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Alar «Naim» (neematı eŋ kop) beyisterde bolusat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(Naim beyisine) abalkı momundardan koburook jamaattar"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Akırkılardan azıraak jamaattar (tusot)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alar (asıl tastar aralas) tokulgan sorulordo"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jambastap, biri-birine betmaŋday (suylosup) oturusat"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "(Alardın kızmatında) tuboluk jas jigitter tegerenip jurusot"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Tattuu sarap tolturulgan, kozoloru, kumuraları, coycoktoru menen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Al saraptan alardın bastarı oorubayt jana mas boluspayt"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "(Kızmatcı jigitter, alar) tandagan momolor menen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tabiti kaalagan (ar turduu, bıskan) kus etteri menen (tegerenip jurusot)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Jana (alar ucun) bakıraŋ koz ur kızdar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Sedepte) jasıngan bermet sıyaktuu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jasagan (soop) isterinin sıylıgına"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(Beyisiler) al jerde maanisiz jana uyat (kunoo) sozdordu uguspayt"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Bir gana «salam-salam» (degen) sozdordu ugusat"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri ne degen (bakıt)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Alar) tikensiz «Sidr»! daragının arasında"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Momoluu banan baktarında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Jayılgan (kenen) kolokolordo"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(Sar) akkan suular(dın boyun)da"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kop momo-jemisterdin icinde —"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tugonbogon, uzulbogon"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "(Jana) balpaygan (jumsak, jibek) tosoktordo"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz alardı (Beyistegi ur kızdardı) ozgoco jaratuu menen jarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Biz alardı bikir* kılıp jaratıp"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bir tegiz kurakta jana oto suymoncuk (nazik) kılıp koyduk"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oŋ tarap eeleri ucun"
        }
    ]
}