{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "(Ey, kaapırlar!) silerdin jaratıluuŋar tataalıraakpı je asmandınbı?! Anı (Allaһ) jarattı"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Sıbın kotorup, bekem-kıntıksiz kıldı"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Tunun karaŋgı kıldı jana (taŋ attırıp) jarıgın cıgardı"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Andan kiyin jerdi jayıp (koozdop) koydu"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Andan suuların, jayloolorun cıgardı"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Toolordu (bekem) ornottu"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Siler ucun jana silerdin aybandarıŋar ucun paydalanuu (ırıskı) bolsun dep"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Kacan coŋ Kıyamat kelse"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Al kundo insan (duynodogu) is -araketterin esteyt"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Karagan adamdarga tozok (anık) korgozulot"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Kimde-kim cekten cıkkan bolso"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "jana duyno jasoosun (Akıretten) artık korgon bolso"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "anın ordu albette, tozokto"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Al emi, kim Rabbisinin aldında (surak berip) turuudan korkup, ozun (aram) kaaloo-kumarlardan tıysa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "anın ordu albette, Beyiste"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Kayra tiriluudon sektengen adamdar) senden Kıyamattın kacan bolusun surasat"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Anı(n ubaktısın) biluudon sen alıssıŋ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Anın (kacan bolooru jonundogu ilimdin bası jana) ayagı Rabbiŋe gana tieseluu"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Sen bolgonu andan (Kıyamattan) korko turgan kisilerdi eskertuucusuŋ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Alar (Kıyamattı «jalgan» degender) anı (oz kozdoru menen) korgon kundo, (bul duynodo) bir kun kecke ceyin je tusko ceyin gana turgan sıyaktuu bolup kalısat"
        }
    ]
}