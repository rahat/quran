{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Jok, Men ayga ant (icip)"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "kaytıp ketken tungo ant (icip)"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "(agarıp) kele jatkan taŋga ant (icip)"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "(Aytamın): Cınında al (“Sakar” tozogu) coŋ baleelerdin biri"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "(“Sakar” tozogu) adamdarga eskertuu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Silerdin araŋardagı, (Allaһtın ıraazıcılıgına) umtuluunu je bolboso ketenciktep ketuunu kaalagandar ucun"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Ar bir jan (eesi) ozu jasagan (kunoo) isterdin tutkunu"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Oŋ tarap eeleri gana anday (tutkun) emes"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Alar Beyisterde turup surasat"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "(tozoktogu) kılmıskerlerden"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "«Silerdi «Sakar» tozoguna emne tusurdu?»"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Alar aytısat: «Biz namaz okuuculardan bolboduk"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Jana bey-becaralardı tamaktandırbadık"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "(Kunoo deŋizine) comulgondor menen birge comulduk"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Jana Akıret kundu jalgan dedik"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Basıbızga olum kelgenge ceyin»"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Emi alarga (tozokularga) ec bir sapaatcının sapaatı payda berbeyt"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Alarga (kaapırlarga) emne boldu, nasaattan (Kuraandan) juz burup kacısat"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Japayı esekterge oksop"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "arstandan urkup kackan"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Jok, alardın ar biri (jeke ozuno gana, atayın arnalgan) acık kitepter berilisin kaalasat!”"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Jok, anday emes! (cınında) alar Akıretten korkuspayt"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Jok, anday emes! Cınında, bul (Kuraan eŋ coŋ) eskerme-nasaat"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Kim kaalasa andan sabak alat"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Birok, (andan) Allaһ kaalagan pendeler gana nasaat ala biliset. (Antkeni), Al (Allaһ Taala) gana korkuuga jana kecirim suroogo tatıktuu"
        }
    ]
}