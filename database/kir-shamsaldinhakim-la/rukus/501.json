{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Men siler korgon nerseler menen ant icip"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Jana koro albagan (bardık) nerseler menen ant icip (aytamın)"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "«Al (Kuraan) Uluk paygambardın (Allaһ uyrotkon) Sozu"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Al akındın sozu emes! Oto az ıyman keltirip jatasıŋar"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Al kozu acıktın da sozu emes!! (Allaһtı) oto az eskerip jatasıŋar"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Al aalamdardın Rabbisi tarabınan tusurulgon"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger al (Muhammad) (Kuraandagı) kee bir sozdordu (oylop cıgargan jana «usul — Allaһtın sozu» dep, anı) Bizge oodargan bolso"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz anı kuc menen karmap, oc almakpız"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Kiyin jurok tamırın uzup jibermekpiz"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Anan silerdin araŋardan ec kim anı (Bizden) kutkarıp kala almak emes"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Al (Kuraan) takıbaa kisiler ucun eskerme-sabak"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Silerdin araŋarda (Kuraandı) «jalgan» degender bar ekenin Biz eŋ jaksı bilebiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Al kaapırlarga kaygı!”"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Cındıgında, bul anık cındık"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Osentip, Eŋ Uluu Rabbiŋdin ısımın daŋazalagıla"
        }
    ]
}