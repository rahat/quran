{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Koria"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Koria emne ozu"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "(O, Muhammad!) Sen Koria emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Al kundo adamdar tozup ketken (tuŋku) kopoloktor sıyaktuu bolup (durboloŋgo tusup) kalat"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Toolor tıtılgan jun sıyaktuu bolot"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Emi, (al kundo) kimdin (soop) tarazası oor kelse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Koŋulundogudoy jasoodo (Beyiste) bolot"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al emi, kimdin (soop) tarazası jeŋil kelse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Anın enesi— «Haaviya!»"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "(O, Muhammad) sen «Haaviya» emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Al-— katuu kızıtılgan tozok"
        }
    ]
}