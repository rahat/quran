{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "soganda che akasana ane ratamam pragata thanarana"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "tamane khabara pana che ke te ratamam pragata thanara sum che"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "te camakato taro che"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "ko'i (jiva) evo nathi jena upara dekharekha rakhanara (pharista) na hoya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "manavi'e jovum jo'i'e ke te ka'i vastuthi peda karavamam avyo che"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "te eka uchalata panithi peda karavamam avyo che"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "je pitha ane chati vaccethi nikale che"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "cokkasapane te (allaha) tene pharivara lavava para saktimana che"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "je divase gupta rahasyoni tapasa thase"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "to nahim hoya teni pase kami sakti, na sahayaka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "varasadavala akasana soganda"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ane phatavavali jaminana soganda"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "vastavamam a (kura'ana) sampurna phesalo karavavalo kalama che"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "a thaththa-majaka (phayada vagarani) vata nathi"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "kharekhara inkari'o yukti kari rahya che"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ane hum pana eka yukti kari rahyo chum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "tu inkari'one samaya apa, temane thodaka divasa chodi de"
        }
    ]
}