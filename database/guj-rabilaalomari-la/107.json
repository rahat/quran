{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "sum tame (tene pana) joyo je badalana divasane juthalave che"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "a ja te che je anathane dhakka mare che"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ane garibane khavadavavanum protsahana nathi apato"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "te namajhi'o mate kheda che (ane vaila namani jahannamani jagya) che"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "je potani namajhathi beparavaha che"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "je dekhado kare che"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ane saman'ya jaruriyatathi roke che"
        }
    ]
}