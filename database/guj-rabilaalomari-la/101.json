{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "khatakhatavi nakhanara"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "sum che te khatakhatavi nakhanara"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "tane sum khabara te khatakhatavi nakhanara sum che"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "je divase manavi vikharayela patangiyani maphaka tha'i jase"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ane parvato pinjayela rangina una jeva tha'i jase"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "pachi jena palla bhare hase"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "to te'o manapasanda esa'arama mam hase"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "ane jena palla'o halka hase"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "temanum thekanum haviyaha che"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "tane sum khabara ke te sum che"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "bhadake balati aga (che)"
        }
    ]
}