{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "sum ame tamari chati nathi kholi nakhi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ane tamara para thi tamaro bhara ame utari didho"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jene tamari pitha todi didhi hati"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ane ame tamara smaranane unnati api"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "basa! Ni:Sanka tangini sathe saralata che"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "cokkasapane tangini sathe saralata che"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "basa jyare tame paravari java to bandagi mam mahenata karo"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ane potana palanahara tarapha ja magna tha'i java"
        }
    ]
}