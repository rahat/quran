{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "potana sarvocca palanaharana namani pavitrata bayana kara"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jene sarjana karyu ane thika-thaka karyo"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "ane jene (thika-thaka) bhagya banavyum ane pachi marga batavyo"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ane jene taji vanaspati'o upajavi"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "pachi tene tene (sukavine) kalo kacaro banavi didho"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "ame tamane padhavisu pachi tame nahim bhulo"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "sivaya je kami allaha icche, te jahera ane chupi (vatone pana) jane che"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ame tamara mate saralata peda kari da'isum"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "to tame sikhamana apata raho, jo sikhamana labhadayaka hoya"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "daravavalo to sikhamana grahana karase"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "(ha) durbhagi tenathi dura rahese"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "je moti agamam jase"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "tyam na to te mrtyu pamase na to jivase. (Jana-kanini avasthamam hase)"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "kharekhara tene saphalata prapta kari je pavitra tha'i gayo"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "ane jene potana palanaharana namanum smarana karyu, ane namajha padhato rahyo"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "parantu tame to duniyavi jivanane prathamikata apo cho"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "ane akhirata sarvottama ane avinasi che"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "a vato prathama granthomam pana che"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "(etale ke) ibrahima ane musana granthomam"
        }
    ]
}