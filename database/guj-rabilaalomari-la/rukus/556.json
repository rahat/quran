{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "tame kahi do! Ke hum lokona palanaharani saranamam avum chum"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "lokona malikana (ane)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "lokona ma'abudana (saranamam)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "sanka'o nakhanara, pacha hati janarani bura'ithi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "je lokona hrdayomam sanka'o nakhe che"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(pachi) te jinnatomam thi hoya athava to manusyomam thi"
        }
    ]
}