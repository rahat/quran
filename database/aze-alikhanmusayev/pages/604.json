        {
            "chapter": 112,
            "verse": 1,
            "text": "De: “O Allah Təkdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah Möhtac deyildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O, nə doğub, nə doğulub"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Onun bənzəri də yoxdur”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De: “Sığınıram sübhün Rəbbinə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Məxluqatının şərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "çökən zülmətin şərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "düyünlərə üfürən cadugər qadınların şərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "və həsəd apardığı zaman paxılın şərindən!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De: “Sığınıram insanların Rəbbinə"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hökmdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların məbuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vəsvəsə verənin, Allahın adı çəkiləndə isə hövlündən geri çəkilən şeytanın şərindən"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O şeytan ki, insanların ürəklərinə vəsvəsə salır"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cinlərdən də olur, insanlardan da!”"
        }
    ]
