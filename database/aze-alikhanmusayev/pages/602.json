{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qureyşin birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "onların qış və yay səfərlərindəki birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "qoy onlar bu Evin Rəbbinə ibadət etsinlər"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Allah onlara aclıqdan sonra yemək verdi və qorxudan sonra əmin-amanlıq bəxş etdi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalan sayan o kimsəni gördünmü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "O elə adamdır ki, yetimi qovar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "və heç kəsi kasıba əl tutmağa sövq etməz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay halına namaz qılanların –"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "o şəxslərin ki, onlar namazlarında səhlənkardırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar riyakardırlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "və az bir şey verməkdən belə imtina edirlər"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Həqiqətən, Biz Kovsəri sənə bağışladıq"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sən də Rəbbin üçün namaz qıl və qurban kəs"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Doğrusu, sənə ədavət bəsləyənin özü sonsuzdur"
        }
    ]
}