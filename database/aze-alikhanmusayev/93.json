{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "And olsun səhərə"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "And olsun sakitləşən gecəyə"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rəbbin nə səni tərk etmişdi, nə də sənə acığı tutmuşdu"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Həqiqətən, Axirət sənə dünya həyatından daha xeyirlidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Şübhəsiz ki, Rəbbin sənə nemət bəxş edəcək, sən də razı qalacaqsan"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Məgər O, səni yetim çağında qoruyub sığınacaq vermədimi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Səni çaşqın olduğun halda doğru yola yönəltmədimi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Kasıb olduğun zaman səni varlandırmadımı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Elə isə yetimə xor baxma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Səndən kömək diləyəni qovma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Və Rəbbinin nemətindən danış"
        }
    ]
}