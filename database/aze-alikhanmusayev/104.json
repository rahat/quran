{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hər bir qeybət edənin, tənə vuranın vay halına"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O şəxs ki, mal-dövlət toplayıb onu təkrar-təkrar sayır"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "və elə güman edir ki, var-dövləti onu əbədi saxlayacaqdır"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Xeyr! O, mütləq Hutəməyə atılacaqdır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Sən haradan biləsən ki, Hutəmə nədir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bu, Allahın qaladığı Oddur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "O Od ki, qalxıb ürəklərə çatacaq"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Həqiqətən, o Od onların üzərində qapanacaqdır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Özləri də orada yüksək dirəklərə bağlanmış olacaqlar"
        }
    ]
}