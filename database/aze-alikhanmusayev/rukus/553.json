{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Əbu Ləhəbin əlləri qurusun, qurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nə var-dövləti, nə də qazandıqları onu qurtara bilməyəcək"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, alovlanan oda atılacaqdır"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Onun odun şələsi daşıyan arvadı da"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Arvadının boynunda xurma liflərindən kəndir olacaqdır"
        }
    ]
}