{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "O qaşqabağını sallayıb üzünü yana çevirdi –"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "yanına bir korun gəlməsinə görə"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Nə bilirdin ki, bəlkə o günahlardan təmizlənəcəkdi"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "və yaxud nəsihət dinləyib faydalanacaqdı"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Nəsihətə ehtiyacı olmayana gəlincə isə –"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "sən ona diqqət yetirirsən"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun təmizlənməməyinin sənə bir dəxli yoxdur"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Səy göstərib sənin yanına gələn"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Allahdan qorxan kimsəyə gəlincə"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ona məhəl qoymursan"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Xeyr! Şübhəsiz ki, bu, bir nəsihətdir"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kim istəsə, qoy onu xatırlayıb ibrət alsın"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Bu Quran kəramətli səhifələrdədir"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "yüksək və pak səhifələrdədir"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bu səhifələr elçilərin əllərində –"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "möhtərəm və müti olanların əllərindədir"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qoy ölsün kafir insan! O necə də nankordur"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Qoy baxıb görsün Allah onu nədən yaradıb"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Allah onu nütfədən yaratdı və müəyyən şəkildə formalaşdırdı"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra da onun yolunu asanlaşdırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu öldürüb qəbirə yerləşdirdi"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "İstədiyi bir zaman onu dirildəcəkdir"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Xeyr! O, hələ də Allahın ona buyurduğunu yerinə yetirməmişdir"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Qoy insan yediyinə diqqət yetirsin bir az"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Həqiqətən, yağışı bol-bol Biz yağdırırıq"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra da yeri çat-çat yarıb"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "orada taxıl yetişdiririk"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Habelə üzüm və yonca"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "zeytun və xurma ağacları"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "sıx-sıx bağlar"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "meyvələr və otlar da bitiririk"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Bu nemətlər sizin istifadəniz və mal-qaranızın faydalanması üçündür"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Qulaqbatırıcı bir səs gələndə –"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "o gün insan qaçacaq öz qardaşından"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "anasından, atasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "arvadından və övladlarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gün onlardan hər birinin yetərincə işi olacaqdır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "O gün neçə-neçə üzlər parlayacaq"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "güləcək və fərəhlənəcəkdir"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "O gün neçə-neçə üzlərə toz-torpaq hopacaq"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "onları zülmət bürüyəcəkdir"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Məhz onlar kafirlər, günahkarlardır"
        }
    ]
}