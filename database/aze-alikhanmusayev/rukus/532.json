{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "And içirəm bu şəhərə (Məkkəyə)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sən də o şəhərin sakinisən"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "And olsun valideynə və onun törəməsinə"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insanı çətinlik içində xəlq etdik"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "O, heç kəsin ona gücü çatmayacağınımı güman edir"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "O deyir: “Mən çoxlu var-dövlət sərf etmişəm!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "O, heç kəsin onu görməyəcəyinimi zənn edir"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Məgər ona iki gözü Biz verməmişikmi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dillə iki dodaq verməmişikmi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Onu iki yoxuşlu yola yönəltmədikmi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O isə sərt yoxuşu aşmağa can atmadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Sən nə bilirsən ki, sərt yoxuş nədir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "O, bir kölə azad etməkdir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "və ya aclıq günündə möhtaca yemək verməkdir –"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "qohumluq əlaqəsi çatan bir yetimə"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "yaxud aclıqdan yerə uzanmış kasıba"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra isə o, iman gətirən, bir-birinə səbir və mərhəmət tövsiyə edənlərdən olmaqdır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Onlar sağ tərəf sahibləridir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayələrimizi inkar edənlər isə sol tərəf sahibləridir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Od onların üzərində qapanacaqdır"
        }
    ]
}