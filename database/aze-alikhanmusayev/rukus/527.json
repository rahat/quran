{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "And olsun bürclü göyə"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "And olsun vədə verilmiş günə"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "And olsun şahidlik edənə və edilənə"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Məhv olsun xəndək kənarında yığışanlar"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Odunla alışdırılmış xəndək"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Onlar o xəndəyin qırağında oturub"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "möminlərə öz verdikləri işgəncələrə şahidlik edirdilər"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Kafirlər onlardan yalnız Qüdrətli, Tərifəlayiq Allaha iman gətirdiklərinə görə intiqam alırdılar"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Halbuki, göylərdə və yerdə mütləq hakimiyyət Ona məxsusdur. Allah hər şeyə Şahiddir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "O kəslər ki, mömin kişi və qadınları yandırmaqla sınağa çəkmiş, sonra da tövbə etməmişlər, onlara Cəhənnəm əzabı və onları yandırıb-yaxacaq od əzabı hazırlanmışdır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Şübhəsiz ki, iman gətirib yaxşı işlər görənlər üçün ağacları altından çaylar axan Cənnət bağları vardır. Bu, böyük müvəffəqiyyətdir"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Doğrudan da, Rəbbinin günahkarları yaxalaması çox şiddətlidir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Həqiqətən, O, hər şeyi ilkin yaradır və onu yenidən bərpa edir"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Bağışlayan da, Sevən də Odur"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ərşin sahibidir, Şan-şöhrətlidir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "İstədiyini edəndir"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Qoşunlar haqqında əhvalat sənə çatıbmı"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firon və Səmud qoşunlarının"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Xeyr, küfr edənlər haqqı yalan saymaqdadırlar"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Allah onları arxadan əhatə etmişdir"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bəli, bu şanlı Qurandır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "O, mühafizə edilən lövhədədir"
        }
    ]
}