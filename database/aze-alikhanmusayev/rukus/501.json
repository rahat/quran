{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "And içirəm həm gördüklərinizə"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "və həm də görmədiklərinizə"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Bu, möhtərəm bir Elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, şair sözü deyildir! Necə də az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kahin sözü də deyildir! Necə də az düşünürsünüz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, aləmlərin Rəbbi tərəfindən vəhy edilmişdir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Əgər Muhəmməd bəzi sözləri özündən uydurub Bizə aid etsəydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu sağ əlindən yaxalayar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sonra da şah damarını kəsərdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "İçərinizdən heç kəs buna mane ola bilməzdi"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Həqiqətən, bu Quran müttəqilər üçün bir nəsihətdir"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Aranızdan Quranı yalan sayanlar olduğunu da bilirik"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Həqiqətən, bu, kafirlər üçün bir peşmançılıqdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Sözsüz ki, bu Quran əsl həqiqətdir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Elə isə Böyük Rəbbinin adına təriflər de"
        }
    ]
}