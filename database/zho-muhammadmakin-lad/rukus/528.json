{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Yǐ cāngqióng hé qǐ míngxīng méngshì"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "nǐ zěn néng zhīdào qǐ míngxīng shì shénmó"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Shì nà cànlàn de míngxīng"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Měi gèrén, dōu yǒuyīgè bǎohù zhě"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Rén yīngdāng xiǎng yī xiǎng, tā zìjǐ shì yòng shénme zàochéng de"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Tā shì bèi shèchū de jīngyè zàochéng de"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Nà jīngyè shì cóng jǐzhù hé lèigǔ zhī jiān fāchū de"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Ān lā què shì néng shǐ tā fùyuán de"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "nà shì yīqiè mìmì bèi jiēchuān zhī rì"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Tā jué méiyǒu nénglì, yě méiyǒu yuánzhù zhě"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yǐ hán yǔ de yún"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "hé yǒu fèng dì dì fāshì"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "zhè què shì fēnbié zhēn wěi de yáncí"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "zhè bùshì xiàohuà"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tāmen bìdìng yào yòng jìmóu"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "wǒ yě yào yòng jìmóu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Suǒyǐ nǐ dāng kuānxiàn bù xìndào de rénmen, nǐ dāng kuānxiàn tāmen yīxià"
        }
    ]
}