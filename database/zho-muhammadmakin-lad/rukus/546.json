{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Shāng zāi! Měi gè fěibàng zhě, dǐhuǐ zhě"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "tā jùjī cáichǎn, ér dàng zuò wǔqì"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tā yǐwéi tā de cáichǎn, néng shǐ tā bù miè"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jué bùrán, tā bìdìng yào bèi tóu zài huǐmiè kēng zhōng"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Nǐ zěn néng zhīdào huǐmiè kēng shì shénme"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Shì ān lā de ránzhe de lièhuǒ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Néng shēng dào rén de xīn shàng"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Tāmen bìdìng yào bèi guān zài lièhuǒ zhōng"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "diào zài xǔduō gāo zhùzi shàng"
        }
    ]
}