{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Chángshēng bùlǎo de tóng pū, lúnliú zhefú dài tāmen"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pěngzhe zhǎn hé hú, yǔ mǎn bēi de lǐquán"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "tāmen bù yīn nà lǐquán ér tóutòng, yě bù mǐngdīng"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tāmen yǒu zìjǐ suǒ xuǎnzé de shuǐguǒ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "hé zìjǐ suǒ àihào de niǎo ròu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Hái yǒu báixī de, měi mùdì qīzi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hǎoxiàng cáng zài bàng ké lǐ de zhēnzhū yīyàng"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Nà shì wèile bàochóu tāmen de shànxíng"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tāmen zài lèyuán lǐ, tīng bù dào è yán hé huǎnghuà"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "dàn tīng dào shuō: Zhù nǐmen píng'ān! Zhù nǐmen píng'ān"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Xìngfú zhě, xìngfú zhě shì héděng de rén"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tāmen xiǎngshòu wú cì de suānzǎo shù"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jiēshi lěilěi de (xiāngjiāo) shù"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "mànmàn de shù yīn"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "fànfàn de liúshuǐ"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "fēngfù de shuǐguǒ"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "sì shí bù jué, kěyǐ suíyì zhāi shí"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "yǔ bèi shēng qǐ de chuángtà"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Wǒ shǐ tāmen chóngxīn shēngzhǎng"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "wǒ shǐ tāmen cháng wèi chǔnǚ"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "yīliàn zhàngfū, bǐcǐ tóng suì"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "zhèxiē dōu shì xìngfú zhě suǒ xiǎngshòu de"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Xǔduō qián rén"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "hé xǔduō hòu rén"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Bómìng zhě, bómìng zhě shì héděng de rén"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Tāmen zài dú fēng hé fèishuǐ zhōng"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "zài hēi yān de yīnyǐng xià"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "jì bù liángshuǎng, yòu bù měiguān"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Yǐqián tāmen què shì shēchǐ de"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "què shì gùzhí dàzuì de"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "tāmen cháng shuō:“Nándào wǒmen sǐ hòu, yǐ biàn chéng chéntǔ hé kūgǔ de shíhòu, wǒmen bìdìng yào fùhuó ma? ”"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Lián wǒmen de zǔxiān, yě yào fùhuó ma?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Nǐ shuō: Qián rén hé hòu rén"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "zài yīgè zhùmíng de rìqí hé tèdìng shíjiān bìdìng yào bèi jíhé"
        }
    ]
