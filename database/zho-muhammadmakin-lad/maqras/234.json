{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Tā céng zhòuméi, érqiě zhuǎnshēn lí qù"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "yīnwèi nàgè mángrén lái dào tā de miànqián"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Nǐ zěn néng zhīdào ne? Tā yěxǔ néng shòu xūntáo"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "huò tīng zhōnggào, ér méng jiàoyì"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Zhìyú zìmǎn zhě"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "nǐ dōu féngyíng tā"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "tā bù shòu xūntáo, yú nǐ hé gàn"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Zhìyú yīnqín de lái qǐngjiào nǐ"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "érqiě xīnhuái jìngwèi zhě"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "nǐ què dàimàn tā"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Jué bùrán! Zhè què shì yī zhǒng jiàoxùn"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Shéi yù sòngdú tā, jiù jiào shéi sòngdú tā ba"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "tā zài zhēnguì de cèyè lǐ"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "nàxiē cèyè, shì bèi chēngyáng de, shì bèi dí jìng de"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "shì zài xǔduō shūjì de shǒu lǐ de"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "nàxiē shūjì, shì zūnguì de, shì shànliáng de"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Gāisǐ de rén! Tā shì héděng de wàng'ēn"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ān lā céng yòng shénme chuàngzào tā de ne"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Shì yòng jīngyè. Tā céng chuàngzào tā, bìng yùdìng tā fāyù de chéngxù"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Ránhòu, tā shǐ tā de dàolù píngyì"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Ránhòu, tā shǐ tā sǐ, bìng ānzàng tā"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ránhòu, dāng tā yìyù de shíhòu, tā shǐ tā fùhuó"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jué bùrán, tā hái méiyǒu fèngxíng tāsuǒ mìnglìng tā de shìwù"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Jiào rén guānchá zìjǐ de shíwù ba"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Wǒ jiāng yǔshuǐ dà liáng dì qīngzhù xiàlái"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Ránhòu, wǒ shǐ dìmiàn qíyì dì liè kāi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "wǒ zài dà dìshàng shēngchǎn bǎi gǔ"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "yǔ pútáo hé mùxu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yǔ gǎnlǎn hé hǎi zǎo"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "yǔ màomì de yuánpǔ"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "shuǐguǒ hé mùcǎo"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "yǐ gōng nǐmen hé nǐmen de shēngchù xiǎngshòu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Dāng zhèn'ěryùlóng de hōng shēng láilín de shíhòu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "zài nà rì, gè rén jiāng táobì zìjǐ de dìxiōng"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "zìjǐ de fùmǔ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "zìjǐ de qī zǐ er nǚ"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "zài nà rì, gè rén jiāng zìgùbùxiá"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "zài nà rì, xǔduō miànmù shì guānghuá de"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "shì xǐ xiào de, shì yúkuài de"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "zài nà rì, xǔduō miànmù shàng, jiāng yǒu huīchén"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "líhēi jiāng méngbì tā"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Zhè děng rén, shì bù xìndào de, shì huāngyín de"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Dāng tàiyáng àn chù de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "dāng xīngsù língluò de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "dāng shānluán bēngkuì de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "dāng yùn tuó bèi pāoqì de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Dāng yěshòu bèi jíhé de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "dāng hǎiyáng péngpài de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "dāng línghún bèi pèihé de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "dāng bèi huómái de nǚhái bèi xúnwèn de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Tā wèishéme zuìguo ér zāo shāhài ne"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Dāng gōngguò báo bèi zhǎnkāi de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "dàngtiān pí bèi jiē qù de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "dāng huǒ yù bèi ránzhe de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "dāng lèyuán bèi sòng jìn de shíhòu"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "měi gèrén dōu zhīdào tā suǒzuòguò de shàn è"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Wǒ shì yǐ yùnxíng de zhòng xīng"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "mòluò de xíngxīng"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "hé shìqù shí de hēiyè"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "zhàoyào shí de zǎochén"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "zhè què shì yīgè zūnguì de shǐzhě de yáncí"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "tā zài bǎozuò de zhǔ nàlǐ, shì yǒu quánlì de, shì yǒu dìwèi de"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "shì zhòngwàngsuǒguī, érqiě zhōngyú zhíshǒu de"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Nǐmen de péngyǒu, bùshì yīgè fēng rén"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "tā què yǐ kànjiàn nàgè tiānshǐ zài míngxiǎn de tiānbiān"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "tā duì yōuxuán bùshì lìn jiào de"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Zhè bùshì bèi fàngzhú de èmó de yáncí"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "nǐmen jiāng wǎng nǎlǐ qù ne"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Zhè zhǐshì duìyú zhòng shìjiè de jiàohuì——"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "duì yú nǐmen zhōng yù xúnguīdǎojǔ zhě de jiàohuì"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "nǐmen bù yù xúnguīdǎojǔ, chúfēi ān lā——zhòng shìjiè de zhǔ——yìyù de shíhòu"
        }
    ]
}