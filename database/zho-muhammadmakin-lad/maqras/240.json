{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nándào tā bù zhīdào ma? Dāng fén zhōng de xiǔ gǔ bèi jiēfā"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "xiōngzhōng de mìmì bèi xiǎnshì de shíhòu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "zài nà rì, tāmen de zhǔ, què shì chè zhī tāmen de"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Dà nàn"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "dà nàn shì shénme"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "nǐ zěn néng zhīdào dà nàn shì shénme"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Zài nà rì, zhòngrén jiāng shì fēnsàn de fēi é"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "shānyuè jiāng shì shūsōng de cǎi róng"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Zhìyú shàngōng de fènliàng jiào zhòng zhě"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "jiàng zài mǎnyì de shēnghuó zhōng"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "zhìyú shàngōng de fènliàng jiào qīng zhě"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tā de guīsù shì shēn kēng"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Nǐ zěn néng zhīdào shēn kēng li yǒu shé me"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Yǒu lièhuǒ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Jìngsài fùshù, yǐ shǐ nǐmen shūhū"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "zhídào nǐmen qù yóu féndì"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Zhēn de, nǐmen jiānglái jiù zhīdàole"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Zhēn de, nǐmen jiānglái jiù zhīdàole"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Zhēn de, jiǎruò nǐmen yǒu zhēnzhīzhuójiàn, (nǐmen bì bù shūhū)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "nǐmen bìdìng kànjiàn huǒ yù"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ránhòu, nǐmen bì qīnyǎn kànjiàn tā"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zài nà rì, nǐmen bì wèi ēnzé ér bèi shěnwèn"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Yǐ shíguāng méngshì"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "rén què shì zài kuī shé zhī zhōng"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "wéi xìndào érqiě xíngshàn, bìng yǐ zhēnlǐ xiāngquàn, yǐ jiānrěn xiāng miǎn de rén zé bùrán"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Shāng zāi! Měi gè fěibàng zhě, dǐhuǐ zhě"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "tā jùjī cáichǎn, ér dàng zuò wǔqì"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tā yǐwéi tā de cáichǎn, néng shǐ tā bù miè"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jué bùrán, tā bìdìng yào bèi tóu zài huǐmiè kēng zhōng"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Nǐ zěn néng zhīdào huǐmiè kēng shì shénme"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Shì ān lā de ránzhe de lièhuǒ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Néng shēng dào rén de xīn shàng"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Tāmen bìdìng yào bèi guān zài lièhuǒ zhōng"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "diào zài xǔduō gāo zhùzi shàng"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Nándào nǐ bù zhīdào nǐ de zhǔ zěnyàng chǔzhì xiàng de zhǔrén ma"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Nándào tā méiyǒu shǐ tāmen de jìmóu, biàn chéng wúyì de ma"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Tā céng pàiqiǎn chéng qún de niǎo qù shāng tāmen"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "yǐ niántǔ shí shèjí tāmen"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "shǐ tāmen biàn chéng chī shèng de gāncǎo yīyàng"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Yīnwèi bǎohù gǔlái shì"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yīnwèi zài dōngjì hé xiàjì de lǚxíng zhōng bǎohù tāmen"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "gù jiào tāmen chóngjìng zhè tiān fáng de zhǔ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "tā céng wèi jīhuang ér zhènjì tāmen, céng wèi kǒngbù ér bǎoyòu tāmen"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Nǐ céng jiàn fǒurèn bàoyìng rì de rén ma"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tā jiùshì nàgè hēchì gū'ér"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "qiě bù miǎnlì rén zhènjì pínmín de rén"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Shāng zāi! Lǐbài de rénmen"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "tāmen shì shūhū bài gōng de"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "tāmen shì gūmíngdiàoyù de"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "tāmen shì bù kěn jiè rén shíwù de"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Wǒ què cì nǐ duō fú"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "gù nǐ yīngdāng wèi nǐ de zhǔ ér lǐbài, bìng zǎi xīshēng"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Yuànhèn nǐ zhě, què shì juéhòu de"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Nǐ shuō:“Bù xìndào de rénmen a!”"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Wǒ bù chóngbài nǐmen suǒ chóngbài de"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nǐmen yě bù chóngbài wǒ suǒ chóngbài de"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "wǒ bù huì chóngbài nǐmen suǒ chóngbài de"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nǐmen yě bù huì chóngbài wǒ suǒ chóngbài de"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "nǐmen yǒu nǐmen de zōngjiào, wǒ yǒu wǒ de zōngjiào.”"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Dāng ān lā de yuánzhù hé shènglì jiànglín"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "dāng nǐ kànjiàn zhòngrén chéng qún jié duì de chóngfèng ān lā de zōngjiào shí"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Nǐ yīngdāng zànsòng nǐ de zhǔ chāojué wànwù, bìngqiě xiàng tā qiúráo, tā què shì zhì yòu de"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Yuàn yàn fù liǎngshǒu shòushāng! Tā bìdìng shòushāng"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "tā de cáichǎn, hé tāsuǒ huòdé de, jiāng wú bì yú tā"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "tā jiàng rù yǒu yàn de lièhuǒ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "tā de dān chái de qīzi, yě jiàng rù lièhuǒ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "tā de jǐng shàng xìzhe yītiáo jiānshí de zōng shéng"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Nǐ shuō:“Tā shì ān lā, shì dú yī de zhǔ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ān lā shì wànwù suǒ yǎnglài de"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "tā wèi shēngyù, yě wèi bèi shēngyù"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Méiyǒu rènhé wù kěyǐ zuò tā de pǐdí.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Nǐ shuō:“Wǒ qiú bì yú shǔguāng de zhǔ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "miǎn zāo tāsuǒ chuàngzào zhě de dúhài"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "miǎn zāo hēiyè lóngzhào shí de dúhài"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "miǎn zāo chuī pò jiānjué de zhǔyì zhě de dúhài"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "miǎn zāo jídù zhě jídù shí de dúhài.”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Nǐ shuō:“Wǒ qiú bì yú shìrén de yǎng zhǔ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "shìrén de jūnzhǔ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "shìrén de zhǔzǎi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "miǎn zāo qiánfú de jiàosuō zhě de dúhài"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "tā zài shìrén de xiōngzhōng jiàosuō"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "tā shì shǔyú jīnglíng hé rénlèi de"
        }
    ]
}