{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Suratu Al-Maun. Viste quem desmente o Dia do Juízo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Esse é o que repele o órfão"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "E não incitava ninguém a alimentar o necessitado"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Então, ai dos orantes"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Que são distraídos de suas orações"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Que, por ostentação, só querem ser vistos orando"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "E impedem o adjutório"
        }
    ]
}