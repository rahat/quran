    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Suratu Al-Ikhlas. Dize: \"Ele é Allah, Único"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah é O Solicitado"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Não gerou e não foi gerado"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "E não há ninguém igual a Ele"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Suratu Al-Falaq. Dize: \"Refugio-me no Senhor da Alvorada"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Contra o mal daquilo que Ele criou"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "E contra o mal da noitequando entenebrece"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "E contra o mal das sopradoras dos nós"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "E contra o mal do invejoso, quando inveja"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Suratu An-Nass. Dize: \"Refugio-me nO Senhor dos homens"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "O Rei dos homens"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "O Deus dos homens"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Contra o mal do sussurrador, o absconso"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Que sussurra perfídias nos peitos dos homens"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Seja ele dos jinns, seja ele dos homens"
        }
    ]
