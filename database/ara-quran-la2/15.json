{
    "chapter": [
        {
            "chapter": 15,
            "verse": 1,
            "text": "Alif-lam-ra tilka ayatualkitabi waqur-anin mubeen"
        },
        {
            "chapter": 15,
            "verse": 2,
            "text": "Rubama yawaddu allatheenakafaroo law kanoo muslimeen"
        },
        {
            "chapter": 15,
            "verse": 3,
            "text": "Tharhum ya/kuloo wayatamattaAAoowayulhihimu al-amalu fasawfa yaAAlamoon"
        },
        {
            "chapter": 15,
            "verse": 4,
            "text": "Wama ahlakna min qaryatin illawalaha kitabun maAAloom"
        },
        {
            "chapter": 15,
            "verse": 5,
            "text": "Ma tasbiqu min ommatin ajalahawama yasta/khiroon"
        },
        {
            "chapter": 15,
            "verse": 6,
            "text": "Waqaloo ya ayyuha allatheenuzzila AAalayhi aththikru innaka lamajnoon"
        },
        {
            "chapter": 15,
            "verse": 7,
            "text": "Law ma ta/teena bilmala-ikatiin kunta mina assadiqeen"
        },
        {
            "chapter": 15,
            "verse": 8,
            "text": "Ma nunazzilu almala-ikata illabilhaqqi wama kanoo ithan munthareen"
        },
        {
            "chapter": 15,
            "verse": 9,
            "text": "Inna nahnu nazzalna aththikrawa-inna lahu lahafithoon"
        },
        {
            "chapter": 15,
            "verse": 10,
            "text": "Walaqad arsalna min qablika feeshiyaAAi al-awwaleen"
        },
        {
            "chapter": 15,
            "verse": 11,
            "text": "Wama ya/teehim min rasoolin illakanoo bihi yastahzi-oon"
        },
        {
            "chapter": 15,
            "verse": 12,
            "text": "Kathalika naslukuhu fee quloobialmujrimeen"
        },
        {
            "chapter": 15,
            "verse": 13,
            "text": "La yu/minoona bihi waqad khalatsunnatu al-awwaleen"
        },
        {
            "chapter": 15,
            "verse": 14,
            "text": "Walaw fatahna AAalayhim babanmina assama-i fathalloo feehiyaAArujoon"
        },
        {
            "chapter": 15,
            "verse": 15,
            "text": "Laqaloo innama sukkirat absarunabal nahnu qawmun mashooroon"
        },
        {
            "chapter": 15,
            "verse": 16,
            "text": "Walaqad jaAAalna fee assama-iburoojan wazayyannaha linnathireen"
        },
        {
            "chapter": 15,
            "verse": 17,
            "text": "Wahafithnahamin kulli shaytanin rajeem"
        },
        {
            "chapter": 15,
            "verse": 18,
            "text": "Illa mani istaraqa assamAAafaatbaAAahu shihabun mubeen"
        },
        {
            "chapter": 15,
            "verse": 19,
            "text": "Wal-arda madadnahawaalqayna feeha rawasiya waanbatnafeeha min kulli shay-in mawzoon"
        },
        {
            "chapter": 15,
            "verse": 20,
            "text": "WajaAAalna lakum feeha maAAayishawaman lastum lahu biraziqeen"
        },
        {
            "chapter": 15,
            "verse": 21,
            "text": "Wa-in min shay-in illa AAindanakhaza-inuhu wama nunazziluhu illa biqadarinmaAAloom"
        },
        {
            "chapter": 15,
            "verse": 22,
            "text": "Waarsalna arriyaha lawaqihafaanzalna mina assama-i maanfaasqaynakumoohu wama antum lahu bikhazineen"
        },
        {
            "chapter": 15,
            "verse": 23,
            "text": "Wa-inna lanahnu nuhyeewanumeetu wanahnu alwarithoon"
        },
        {
            "chapter": 15,
            "verse": 24,
            "text": "Walaqad AAalimna almustaqdimeenaminkum walaqad AAalimna almusta/khireen"
        },
        {
            "chapter": 15,
            "verse": 25,
            "text": "Wa-inna rabbaka huwa yahshuruhuminnahu hakeemun AAaleem"
        },
        {
            "chapter": 15,
            "verse": 26,
            "text": "Walaqad khalaqna al-insana minsalsalin min hama-in masnoon"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Waljanna khalaqnahumin qablu min nari assamoom"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Wa-ith qala rabbuka lilmala-ikatiinnee khaliqun basharan min salsalin min hama-inmasnoon"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fa-itha sawwaytuhu wanafakhtu feehimin roohee faqaAAoo lahu sajideen"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fasajada almala-ikatu kulluhumajmaAAoon"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illa ibleesa aba an yakoonamaAAa assajideen"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qala ya ibleesu ma lakaalla takoona maAAa assajideen"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qala lam akun li-asjuda libasharinkhalaqtahu min salsalin min hama-in masnoon"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qala fakhruj minhafa-innaka rajeem"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wa-inna AAalayka allaAAnata ila yawmiaddeen"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qala rabbi faanthirneeila yawmi yubAAathoon"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qala fa-innaka mina almunthareen"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila yawmi alwaqti almaAAloom"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qala rabbi bima aghwaytaneelaozayyinanna lahum fee al-ardi walaoghwiyannahumajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa AAibadaka minhumualmukhlaseen"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qala hatha siratunAAalayya mustaqeem"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna AAibadee laysa laka AAalayhimsultanun illa mani ittabaAAaka mina alghaween"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wa-inna jahannama lamawAAiduhum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Laha sabAAatu abwabin likullibabin minhum juz-on maqsoom"
        },
        {
            "chapter": 15,
            "verse": 45,
            "text": "Inna almuttaqeena fee jannatinwaAAuyoon"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Odkhulooha bisalamin amineen"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "WanazaAAna ma fee sudoorihimmin ghillin ikhwanan AAala sururin mutaqabileen"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La yamassuhum feeha nasabunwama hum minha bimukhrajeen"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nabbi/ AAibadee annee anaalghafooru arraheem"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Waanna AAathabee huwa alAAathabual-aleem"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wanabbi/hum AAan dayfi ibraheem"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Ith dakhaloo AAalayhi faqaloosalaman qala inna minkum wajiloon"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qaloo la tawjal innanubashshiruka bighulamin AAaleem"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qala abashshartumoonee AAalaan massaniya alkibaru fabima tubashshiroon"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qaloo bashsharnaka bilhaqqifala takun mina alqaniteen"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qala waman yaqnatu min rahmatirabbihi illa addalloon"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qala fama khatbukumayyuha almursaloon"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qaloo inna orsilna ilaqawmin mujrimeen"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ala lootin innalamunajjoohum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illa imraatahu qaddarna innahalamina alghabireen"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "Falamma jaa ala lootinalmursaloon"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Qala innakum qawmun munkaroon"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Qaloo bal ji/naka bimakanoo feehi yamtaroon"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Waataynaka bilhaqqiwa-inna lasadiqoon"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Faasri bi-ahlika biqitAAin minaallayli wattabiAA adbarahum wala yaltafitminkum ahadun wamdoo haythutu/maroon"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Waqadayna ilayhi thalikaal-amra anna dabira haola-i maqtooAAunmusbiheen"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Wajaa ahlu almadeenati yastabshiroon"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Qala inna haola-i dayfeefala tafdahoon"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Wattaqoo Allaha walatukhzoon"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Qaloo awa lam nanhaka AAani alAAalameen"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala haola-i banateein kuntum faAAileen"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "LaAAamruka innahum lafee sakratihimyaAAmahoona"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Faakhathat-humu assayhatumushriqeen"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "FajaAAalna AAaliyaha safilahawaamtarna AAalayhim hijaratan minsijjeel"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna fee thalika laayatinlilmutawassimeen"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa-innaha labisabeelin muqeem"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna fee thalika laayatanlilmu/mineen"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa-in kana as-habual-aykati lathalimeen"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fantaqamna minhum wa-innahumalabi-imamin mubeen"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Walaqad kaththaba as-habualhijri almursaleen"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Waataynahum ayatinafakanoo AAanha muAArideen"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wakanoo yanhitoona mina aljibalibuyootan amineen"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Faakhathat-humu assayhatumusbiheen"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama aghna AAanhum ma kanooyaksiboon"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wama khalaqna assamawatiwal-arda wama baynahuma illabilhaqqi wa-inna assaAAata laatiyatunfasfahi assafhaaljameel"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna rabbaka huwa alkhallaqualAAaleem"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Walaqad ataynaka sabAAan minaalmathanee walqur-ana alAAatheem"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La tamuddanna AAaynayka ila mamattaAAna bihi azwajan minhum wala tahzanAAalayhim wakhfid janahaka lilmu/mineen"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Waqul innee ana annatheerualmubeen"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama anzalna AAalaalmuqtasimeen"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Allatheena jaAAaloo alqur-anaAAideen"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika lanas-alannahum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "AAamma kanoo yaAAmaloon"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FasdaAA bima tu/maruwaaAArid AAani almushrikeen"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna kafaynaka almustahzi-een"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Allatheena yajAAaloona maAAa Allahiilahan akhara fasawfa yaAAlamoon"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Walaqad naAAlamu annaka yadeequ sadrukabima yaqooloon"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih bihamdi rabbika wakunmina assajideen"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WaAAbud rabbaka hattaya/tiyaka alyaqeen"
        }
    ]
}