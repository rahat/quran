{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wannajmi itha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalla sahibukum wamaghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wama yantiqu AAani alhawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huwa illa wahyun yooha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "AAallamahu shadeedu alquwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Thoo mirratin fastawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wahuwa bil-ofuqi al-aAAla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Faawha ila AAabdihi maawha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kathaba alfu-adu maraa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumaroonahu AAala mayara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Walaqad raahu nazlatan okhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "AAinda sidrati almuntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "AAindaha jannatu alma/wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ith yaghsha assidratama yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagha albasaru wamatagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad raa min ayatirabbihi alkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afaraaytumu allata walAAuzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wamanata aththalithataal-okhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu aththakaru walahual-ontha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka ithan qismatun deeza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiya illa asmaonsammaytumooha antum waabaokum maanzala Allahu biha min sultanin inyattabiAAoona illa aththanna wamatahwa al-anfusu walaqad jaahum min rabbihimu alhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am lil-insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi al-akhiratu wal-oola"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Wakam min malakin fee assamawatila tughnee shafaAAatuhum shay-an illa minbaAAdi an ya/thana Allahu liman yashao wayarda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Inna allatheena la yu/minoonabil-akhirati layusammoona almala-ikatatasmiyata al-ontha"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Wama lahum bihi min AAilmin inyattabiAAoona illa aththanna wa-innaaththanna la yughnee mina alhaqqishay-a"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "FaaAArid AAan man tawalla AAanthikrina walam yurid illa alhayataaddunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Thalika mablaghuhum mina alAAilmiinna rabbaka huwa aAAlamu biman dalla AAan sabeelihiwahuwa aAAlamu bimani ihtada"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Walillahi ma fee assamawatiwama fee al-ardi liyajziya allatheena asaoobima AAamiloo wayajziya allatheena ahsanoobilhusna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Allatheena yajtaniboona kaba-iraal-ithmi walfawahisha illa allamama innarabbaka wasiAAu almaghfirati huwa aAAlamu bikum ithanshaakum mina al-ardi wa-ith antum ajinnatun feebutooni ommahatikum fala tuzakkoo anfusakumhuwa aAAlamu bimani ittaqa"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Afaraayta allathee tawalla"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "WaaAAta qaleelan waakda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "aAAindahu AAilmu alghaybi fahuwa yara"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Am lam yunabba/ bima fee suhufimoosa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Wa-ibraheema allathee waffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Alla taziru waziratun wizraokhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Waan laysa lil-insani illa masaAAa"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Waanna saAAyahu sawfa yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Thumma yujzahu aljazaa al-awfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Waanna ila rabbika almuntaha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Waannahu huwa adhaka waabka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Waannahu huwa amata waahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Waannahu khalaqa azzawjayni aththakarawal-ontha"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfatin itha tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Waanna AAalayhi annash-ata al-okhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Waannahu huwa aghna waaqna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Waannahu huwa rabbu ashshiAAra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Waannahu ahlaka AAadan al-oola"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Wathamooda fama abqa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Waqawma noohin min qablu innahum kanoohum athlama waatgha"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Walmu/tafikata ahwa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Faghashshaha ma ghashsha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fabi-ayyi ala-i rabbika tatamara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hatha natheerun mina annuthurial-oola"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Azifati al-azifat"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Laysa laha min dooni Allahi kashifat"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Afamin hatha alhadeethitaAAjaboon"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Watadhakoona wala tabkoon"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Waantum samidoon"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fasjudoo lillahi waAAbudoo"
        }
    ]
}