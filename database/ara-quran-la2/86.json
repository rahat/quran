{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Wassama-i wattariq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wama adraka ma attariq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Annajmu aththaqib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsin lamma AAalayha hafith"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Falyanthuri al-insanumimma khuliq"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa min ma-in dafiq"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju min bayni assulbi wattara-ib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahu AAala rajAAihi laqadir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tubla assara-ir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fama lahu min quwwatin wala nasir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Wassama-i thati arrajAA"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Wal-ardi thati assadAA"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahu laqawlun fasl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wama huwa bilhazl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kayda"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Waakeedu kayda"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhili alkafireena amhilhum ruwayda"
        }
    ]
}