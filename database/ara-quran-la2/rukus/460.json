{
    "rukus": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wannajmi itha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalla sahibukum wamaghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wama yantiqu AAani alhawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huwa illa wahyun yooha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "AAallamahu shadeedu alquwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Thoo mirratin fastawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wahuwa bil-ofuqi al-aAAla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Faawha ila AAabdihi maawha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kathaba alfu-adu maraa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumaroonahu AAala mayara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Walaqad raahu nazlatan okhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "AAinda sidrati almuntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "AAindaha jannatu alma/wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ith yaghsha assidratama yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagha albasaru wamatagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad raa min ayatirabbihi alkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afaraaytumu allata walAAuzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wamanata aththalithataal-okhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu aththakaru walahual-ontha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka ithan qismatun deeza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiya illa asmaonsammaytumooha antum waabaokum maanzala Allahu biha min sultanin inyattabiAAoona illa aththanna wamatahwa al-anfusu walaqad jaahum min rabbihimu alhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am lil-insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi al-akhiratu wal-oola"
        }
    ]
}