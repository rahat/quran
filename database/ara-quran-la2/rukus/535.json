{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Wadduha"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Wallayli itha saja"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ma waddaAAaka rabbuka wama qala"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Walal-akhiratu khayrun laka minaal-oola"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Walasawfa yuAAteeka rabbuka fatarda"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Alam yajidka yateeman faawa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Wawajadaka dallan fahada"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Wawajadaka AAa-ilan faaghna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Faamma alyateema fala taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Waamma assa-ila falatanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Waamma biniAAmati rabbika fahaddith"
        }
    ]
}