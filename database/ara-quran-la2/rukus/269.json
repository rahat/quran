{
    "rukus": [
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam tara anna arsalna ashshayateenaAAala alkafireena taozzuhum azza"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Fala taAAjal AAalayhim innamanaAAuddu lahum AAadda"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma nahshuru almuttaqeena ilaarrahmani wafda"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wanasooqu almujrimeena ila jahannamawirda"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "La yamlikoona ashshafaAAatailla mani ittakhatha AAinda arrahmaniAAahda"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Waqaloo ittakhatha arrahmanuwalada"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad ji/tum shay-an idda"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takadu assamawatuyatafattarna minhu watanshaqqu al-ardu watakhirrualjibalu hadda"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An daAAaw lirrahmaniwalada"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wama yanbaghee lirrahmanian yattakhitha walada"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In kullu man fee assamawatiwal-ardi illa atee arrahmaniAAabda"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad ahsahum waAAaddahum AAadda"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wakulluhum ateehi yawma alqiyamatifarda"
        },
        {
            "chapter": 19,
            "verse": 96,
            "text": "Inna allatheena amanoowaAAamiloo assalihati sayajAAalu lahumu arrahmanuwudda"
        },
        {
            "chapter": 19,
            "verse": 97,
            "text": "Fa-innama yassarnahu bilisanikalitubashshira bihi almuttaqeena watunthira bihi qawmanludda"
        },
        {
            "chapter": 19,
            "verse": 98,
            "text": "Wakam ahlakna qablahum min qarnin haltuhissu minhum min ahadin aw tasmaAAu lahum rikza"
        }
    ]
}