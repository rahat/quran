{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "AAabasa watawalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "An jaahu al-aAAma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Wama yudreeka laAAallahu yazzakka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Aw yaththakkaru fatanfaAAahu aththikra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Amma mani istaghna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Faanta lahu tasadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Wama AAalayka alla yazzakka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Waamma man jaaka yasAAa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Wahuwa yakhsha"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Faanta AAanhu talahha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kalla innaha tathkira"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Faman shaa thakarah"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fee suhufin mukarrama"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "MarfooAAatin mutahhara"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi-aydee safara"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kiramin barara"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qutila al-insanu ma akfarah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min ayyi shay-in khalaqah"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min nutfatin khalaqahu faqaddarah"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Thumma assabeela yassarah"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Thumma amatahu faaqbarah"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Thumma itha shaa ansharah"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kalla lamma yaqdi maamarah"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Falyanthuri al-insanuila taAAamih"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Anna sababna almaasabba"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Thumma shaqaqna al-arda shaqqa"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Faanbatna feeha habba"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "WaAAinaban waqadba"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Wazaytoonan wanakhla"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Wahada-iqa ghulba"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Wafakihatan waabba"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "MataAAan lakum wali-anAAamikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fa-itha jaati assakhkha"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yawma yafirru almaro min akheeh"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Waommihi waabeeh"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Wasahibatihi wabaneeh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Likulli imri-in minhum yawma-ithinsha/nun yughneeh"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wujoohun yawma-ithin musfira"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Dahikatun mustabshira"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wawujoohun yawma-ithin AAalayhaghabara"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tarhaquha qatara"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ola-ika humu alkafaratu alfajara"
        }
    ]
}