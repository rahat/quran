{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "Walaqad kaththaba as-habualhijri almursaleen"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Waataynahum ayatinafakanoo AAanha muAArideen"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wakanoo yanhitoona mina aljibalibuyootan amineen"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Faakhathat-humu assayhatumusbiheen"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama aghna AAanhum ma kanooyaksiboon"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wama khalaqna assamawatiwal-arda wama baynahuma illabilhaqqi wa-inna assaAAata laatiyatunfasfahi assafhaaljameel"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna rabbaka huwa alkhallaqualAAaleem"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Walaqad ataynaka sabAAan minaalmathanee walqur-ana alAAatheem"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La tamuddanna AAaynayka ila mamattaAAna bihi azwajan minhum wala tahzanAAalayhim wakhfid janahaka lilmu/mineen"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Waqul innee ana annatheerualmubeen"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama anzalna AAalaalmuqtasimeen"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Allatheena jaAAaloo alqur-anaAAideen"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika lanas-alannahum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "AAamma kanoo yaAAmaloon"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FasdaAA bima tu/maruwaaAArid AAani almushrikeen"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna kafaynaka almustahzi-een"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Allatheena yajAAaloona maAAa Allahiilahan akhara fasawfa yaAAlamoon"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Walaqad naAAlamu annaka yadeequ sadrukabima yaqooloon"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih bihamdi rabbika wakunmina assajideen"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WaAAbud rabbaka hattaya/tiyaka alyaqeen"
        }
    ]
}