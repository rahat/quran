{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "Saala sa-ilun biAAathabin waqiAA"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkafireena laysa lahu dafiAA"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Mina Allahi thee almaAAarij"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "TaAAruju almala-ikatu warroohuilayhi fee yawmin kana miqdaruhu khamseena alfasana"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir sabran jameela"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Innahum yarawnahu baAAeeda"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wanarahu qareeba"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma takoonu assamao kalmuhl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Watakoonu aljibalu kalAAihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wala yas-alu hameemun hameema"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassaroonahum yawaddu almujrimu lawyaftadee min AAathabi yawmi-ithin bibaneeh"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wasahibatihi waakheeh"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wafaseelatihi allatee tu/weeh"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Waman fee al-ardi jameeAAan thummayunjeeh"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kalla innaha latha"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "NazzaAAatan lishshawa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "TadAAoo man adbara watawalla"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "WajamaAAa faawAAa"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inna al-insana khuliqa halooAAa"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Itha massahu ashsharrujazooAAa"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Wa-itha massahu alkhayru manooAAa"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illa almusalleen"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Allatheena hum AAala salatihimda-imoon"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Wallatheena fee amwalihimhaqqun maAAloom"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lissa-ili walmahroom"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Wallatheena yusaddiqoonabiyawmi addeen"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Wallatheena hum min AAathabirabbihim mushfiqoon"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna AAathaba rabbihim ghayru ma/moon"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Wallatheena hum lifuroojihim hafithoon"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illa AAala azwajihim awma malakat aymanuhum fa-innahum ghayru maloomeen"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famani ibtagha waraa thalikafaola-ika humu alAAadoon"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Wallatheena hum li-amanatihimwaAAahdihim raAAoon"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Wallatheena hum bishahadatihimqa-imoon"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Wallatheena hum AAala salatihimyuhafithoon"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ola-ika fee jannatin mukramoon"
        }
    ]
}