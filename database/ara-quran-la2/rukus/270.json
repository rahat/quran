{
    "rukus": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "Ta-ha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "Ma anzalna AAalayka alqur-analitashqa"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "Illa tathkiratan liman yakhsha"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "Tanzeelan mimman khalaqa al-arda wassamawatialAAula"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "Arrahmanu AAalaalAAarshi istawa"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "Lahu ma fee assamawatiwama fee al-ardi wama baynahuma wamatahta aththara"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "Wa-in tajhar bilqawli fa-innahuyaAAlamu assirra waakhfa"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "Allahu la ilaha illahuwa lahu al-asmao alhusna"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "Wahal ataka hadeethu moosa"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "Ith raa naran faqalali-ahlihi omkuthoo innee anastu naran laAAallee ateekumminha biqabasin aw ajidu AAala annarihuda"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "Falamma ataha noodiya yamoosa"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "Innee ana rabbuka fakhlaAAnaAAlayka innaka bilwadi almuqaddasi tuwa"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "Waana ikhtartuka fastamiAAlima yooha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innanee ana Allahu lailaha illa ana faAAbudnee waaqimi assalatalithikree"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna assaAAata atiyatunakadu okhfeeha litujza kullu nafsin bimatasAAa"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fala yasuddannaka AAanhaman la yu/minu biha wattabaAAa hawahufatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wama tilka biyameenika ya moosa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qala hiya AAasaya atawakkaoAAalayha waahushshu biha AAala ghanameewaliya feeha maaribu okhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qala alqiha ya moosa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Faalqaha fa-itha hiya hayyatuntasAAa"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qala khuthha walatakhaf sanuAAeeduha seerataha al-oola"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Wadmum yadaka ila janahikatakhruj baydaa min ghayri soo-in ayatan okhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka min ayatinaalkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Ithhab ila firAAawna innahu tagha"
        }
    ]
}