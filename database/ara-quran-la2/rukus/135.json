{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "Qala almalao min qawmi firAAawnainna hatha lasahirun AAaleem"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "Yureedu an yukhrijakum min ardikumfamatha ta/muroon"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "Qaloo arjih waakhahu waarsilfee almada-ini hashireen"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "Ya/tooka bikulli sahirin AAaleem"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "Wajaa assaharatufirAAawna qaloo inna lana laajran in kunnanahnu alghalibeen"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "Qala naAAam wa-innakum laminaalmuqarrabeen"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "Qaloo ya moosa immaan tulqiya wa-imma an nakoona nahnu almulqeen"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "Qala alqoo falamma alqaw saharooaAAyuna annasi wastarhaboohum wajaoobisihrin AAatheem"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "Waawhayna ila moosaan alqi AAasaka fa-itha hiya talqafu maya/fikoon"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "FawaqaAAa alhaqqu wabatala makanoo yaAAmaloon"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "Faghuliboo hunalika wanqalaboosaghireen"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "Waolqiya assaharatu sajideen"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "Qaloo amanna birabbialAAalameen"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "Rabbi moosa waharoon"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "Qala firAAawnu amantum bihiqabla an athana lakum inna hatha lamakrunmakartumoohu fee almadeenati litukhrijoo minha ahlahafasawfa taAAlamoon"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "LaoqatiAAanna aydiyakum waarjulakummin khilafin thumma laosallibannakum ajmaAAeen"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "Qaloo inna ila rabbinamunqaliboon"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "Wama tanqimu minna illaan amanna bi-ayati rabbinalamma jaatna rabbana afrigh AAalaynasabran watawaffana muslimeen"
        }
    ]
}