{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "Aantum ashaddu khalqan ami assamaobanaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "RafaAAa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Waaghtasha laylaha waakhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Wal-arda baAAda thalikadahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja minha maahawamarAAaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Waljibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "MataAAan lakum wali-anAAamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fa-itha jaati attammatualkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma yatathakkaru al-insanu masaAAa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Waburrizati aljaheemu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Faamma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Waathara alhayata addunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fa-inna aljaheema hiya alma/wa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Waama man khafa maqamarabbihi wanaha annafsa AAani alhawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fa-inna aljannata hiya alma/wa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yas-aloonaka AAani assaAAatiayyana mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Feema anta min thikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innama anta munthiru manyakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Kaannahum yawma yarawnaha lamyalbathoo illa AAashiyyatan aw duhaha"
        }
    ]
}