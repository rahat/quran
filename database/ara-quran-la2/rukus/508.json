{
    "rukus": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "Ya ayyuha almuzzammil"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "Qumi allayla illa qaleela"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "Nisfahu awi onqus minhu qaleela"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "Aw zid AAalayhi warattili alqur-anatarteela"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "Inna sanulqee AAalayka qawlan thaqeela"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "Inna nashi-ata allayli hiya ashaddu wat-anwaaqwamu qeela"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "Inna laka fee annahari sabhantaweela"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "Wathkuri isma rabbikawatabattal ilayhi tabteela"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "Rabbu almashriqi walmaghribi lailaha illa huwa fattakhithhu wakeela"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "Wasbir AAala mayaqooloona wahjurhum hajran jameela"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "Watharnee walmukaththibeenaolee annaAAmati wamahhilhum qaleela"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "Inna ladayna ankalan wajaheema"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "WataAAaman tha ghussatinwaAAathaban aleema"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "Yawma tarjufu al-ardu waljibaluwakanati aljibalu katheeban maheela"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "Inna arsalna ilaykum rasoolanshahidan AAalaykum kama arsalna ilafirAAawna rasoola"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "FaAAasa firAAawnu arrasoolafaakhathnahu akhthan wabeela"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "Fakayfa tattaqoona in kafartum yawmanyajAAalu alwildana sheeba"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "Assamao munfatirunbihi kana waAAduhu mafAAoola"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "Inna hathihi tathkiratun famanshaa ittakhatha ila rabbihi sabeela"
        }
    ]
}