{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Thullatun mina al-awwaleen"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Wathullatun mina al-akhireen"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Waas-habu ashshimalima as-habu ashshimal"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fee samoomin wahameem"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Wathillin min yahmoom"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "La baridin wala kareem"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innahum kanoo qabla thalikamutrafeen"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Wakanoo yusirroona AAalaalhinthi alAAatheem"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Wakanoo yaqooloona a-itha mitnawakunna turaban waAAithamana-inna lamabAAoothoon"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Awa abaona al-awwaloon"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Qul inna al-awwaleena wal-akhireen"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "LamajmooAAoona ila meeqatiyawmin maAAloom"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Thumma innakum ayyuha addalloonaalmukaththiboon"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Laakiloona min shajarin min zaqqoom"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Famali-oona minha albutoon"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fashariboona AAalayhi mina alhameem"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fashariboona shurba alheem"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hatha nuzuluhum yawma addeen"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nahnu khalaqnakum falawlatusaddiqoon"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Afaraaytum ma tumnoon"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Aantum takhluqoonahu am nahnu alkhaliqoon"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nahnu qaddarna baynakumualmawta wama nahnu bimasbooqeen"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "AAala an nubaddila amthalakumwanunshi-akum fee ma la taAAlamoon"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Walaqad AAalimtumu annash-ata al-oolafalawla tathakkaroon"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Afaraaytum ma tahruthoon"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Aantum tazraAAoonahu am nahnu azzariAAoon"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Law nashao lajaAAalnahu hutamanfathaltum tafakkahoon"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Inna lamughramoon"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bal nahnu mahroomoon"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Afaraaytumu almaa allatheetashraboon"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Aantum anzaltumoohu mina almuzni am nahnualmunziloon"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Law nashao jaAAalnahu ojajanfalawla tashkuroon"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Afaraaytumu annara allateetooroon"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Aantum ansha/tum shajarataha am nahnualmunshi-oon"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nahnu jaAAalnaha tathkiratanwamataAAan lilmuqween"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fasabbih bismi rabbika alAAatheem"
        }
    ]
}