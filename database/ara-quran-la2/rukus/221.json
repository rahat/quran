{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "Inna almuttaqeena fee jannatinwaAAuyoon"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Odkhulooha bisalamin amineen"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "WanazaAAna ma fee sudoorihimmin ghillin ikhwanan AAala sururin mutaqabileen"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La yamassuhum feeha nasabunwama hum minha bimukhrajeen"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nabbi/ AAibadee annee anaalghafooru arraheem"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Waanna AAathabee huwa alAAathabual-aleem"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wanabbi/hum AAan dayfi ibraheem"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Ith dakhaloo AAalayhi faqaloosalaman qala inna minkum wajiloon"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qaloo la tawjal innanubashshiruka bighulamin AAaleem"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qala abashshartumoonee AAalaan massaniya alkibaru fabima tubashshiroon"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qaloo bashsharnaka bilhaqqifala takun mina alqaniteen"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qala waman yaqnatu min rahmatirabbihi illa addalloon"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qala fama khatbukumayyuha almursaloon"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qaloo inna orsilna ilaqawmin mujrimeen"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ala lootin innalamunajjoohum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illa imraatahu qaddarna innahalamina alghabireen"
        }
    ]
}