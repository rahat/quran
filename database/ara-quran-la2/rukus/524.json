{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Itha assamao infatarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Wa-itha alkawakibu intatharat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Wa-itha albiharu fujjirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Wa-itha alqubooru buAAthirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "AAalimat nafsun ma qaddamatwaakhkharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya ayyuha al-insanu magharraka birabbika alkareem"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Allathee khalaqaka fasawwakafaAAadalak"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fee ayyi sooratin ma shaarakkabak"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kalla bal tukaththiboona biddeen"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Wa-inna AAalaykum lahafitheen"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiraman katibeen"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "YaAAlamoona ma tafAAaloon"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Inna al-abrara lafee naAAeem"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Wa-inna alfujjara lafee jaheem"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslawnaha yawma addeen"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Wama hum AAanha bigha-ibeen"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Wama adraka ma yawmu addeeni"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Thumma ma adraka mayawmu addeen"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yawma la tamliku nafsun linafsinshay-an wal-amru yawma-ithin lillah"
        }
    ]
}