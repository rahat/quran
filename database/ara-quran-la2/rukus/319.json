{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almada-ini hashireen"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haola-i lashirthimatunqaleeloon"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa-innahum lana lagha-ithoon"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa-inna lajameeAAun hathiroon"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatinwaAAuyoon"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareem"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnahabanee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeen"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma taraa aljamAAaniqala as-habu moosa innalamudrakoon"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala kalla inna maAAiya rabbeesayahdeen"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Faawhayna ila moosaani idrib biAAasaka albahra fanfalaqafakana kullu firqin kattawdi alAAatheem"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Waazlafna thamma al-akhareen"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Waanjayna moosa waman maAAahuajmaAAeen"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma aghraqna al-akhareen"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Watlu AAalayhim nabaa ibraheem"
        }
    ]
