        {
            "chapter": 81,
            "verse": 22,
            "text": "Wama sahibukum bimajnoon"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Walaqad raahu bilofuqialmubeen"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wama huwa AAala alghaybi bidaneen"
