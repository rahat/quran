{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fala oqsimu bima tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wama la tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu laqawlu rasoolin kareem"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wama huwa biqawli shaAAirinqaleelan ma tu/minoon"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wala biqawli kahinin qaleelanma tathakkaroon"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzeelun min rabbi alAAalameen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Walaw taqawwala AAalayna baAAdaal-aqaweel"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Laakhathna minhu bilyameen"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma laqataAAna minhualwateen"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fama minkum min ahadin AAanhu hajizeen"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa-innahu latathkiratun lilmuttaqeen"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa-inna lanaAAlamu anna minkum mukaththibeen"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa-innahu lahasratun AAala alkafireen"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa-innahu lahaqqu alyaqeen"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbih bismi rabbika alAAatheem"
        }
    ]
