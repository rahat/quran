{
    "rukus": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "Ha-meem"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "Walkitabi almubeen"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "Inna anzalnahu fee laylatin mubarakatininna kunna munthireen"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "Feeha yufraqu kullu amrin hakeem"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "Amran min AAindina inna kunnamursileen"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "Rahmatan min rabbika innahu huwa assameeAAualAAaleem"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "Rabbi assamawati wal-ardiwama baynahuma in kuntum mooqineen"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "La ilaha illa huwa yuhyeewayumeetu rabbukum warabbu aba-ikumu al-awwaleen"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "Bal hum fee shakkin yalAAaboon"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "Fartaqib yawma ta/tee assamaobidukhanin mubeen"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "Yaghsha annasa hathaAAathabun aleem"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "Rabbana ikshif AAanna alAAathabainna mu/minoon"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "Anna lahumu aththikrawaqad jaahum rasoolun mubeen"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "Thumma tawallaw AAanhu waqaloomuAAallamun majnoon"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "Inna kashifoo alAAathabiqaleelan innakum AAa-idoon"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "Yawma nabtishu albatshataalkubra inna muntaqimoon"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "Walaqad fatanna qablahum qawmafirAAawna wajaahum rasoolun kareem"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "An addoo ilayya AAibada Allahiinnee lakum rasoolun ameen"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "Waan la taAAloo AAala Allahiinnee ateekum bisultanin mubeen"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "Wa-innee AAuthtu birabbee warabbikuman tarjumoon"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "Wa-in lam tu/minoo lee faAAtaziloon"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "FadaAAa rabbahu anna haola-iqawmun mujrimoon"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "Faasri biAAibadee laylan innakummuttabaAAoon"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "Watruki albahra rahwaninnahum jundun mughraqoon"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "Kam tarakoo min jannatin waAAuyoon"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "WazurooAAin wamaqamin kareem"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "WanaAAmatin kanoo feeha fakiheen"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "Kathalika waawrathnahaqawman akhareen"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "Fama bakat AAalayhimu assamaowal-ardu wama kanoo munthareen"
        }
    ]
}