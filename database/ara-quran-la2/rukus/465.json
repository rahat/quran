{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "Walaqad jaa ala firAAawna annuthur"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Kaththaboo bi-ayatinakulliha faakhathnahum akhthaAAazeezin muqtadir"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Akuffarukum khayrun min ola-ikumam lakum baraatun fee azzubur"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Am yaqooloona nahnu jameeAAun muntasir"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "Sayuhzamu aljamAAu wayuwalloona addubur"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Bali assaAAatu mawAAiduhum wassaAAatuadha waamar"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Inna almujrimeena fee dalalinwasuAAur"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yawma yushaboona fee annariAAala wujoohihim thooqoo massa saqar"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Inna kulla shay-in khalaqnahubiqadar"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "Wama amruna illa wahidatunkalamhin bilbasar"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Walaqad ahlakna ashyaAAakumfahal min muddakir"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Wakullu shay-in faAAaloohu fee azzubur"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Wakullu sagheerin wakabeerin mustatar"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Inna almuttaqeena fee jannatinwanahar"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Fee maqAAadi sidqin AAinda maleekinmuqtadir"
        }
    ]
}