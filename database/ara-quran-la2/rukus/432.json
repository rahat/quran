{
    "rukus": [
        {
            "chapter": 44,
            "verse": 30,
            "text": "Walaqad najjayna banee isra-eelamina alAAathabi almuheen"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "Min firAAawna innahu kana AAaliyanmina almusrifeen"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "Walaqadi ikhtarnahum AAalaAAilmin AAala alAAalameen"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "Waataynahum mina al-ayatima feehi balaon mubeen"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "Inna haola-i layaqooloon"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "In hiya illa mawtatuna al-oolawama nahnu bimunshareen"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "Fa/too bi-aba-ina inkuntum sadiqeen"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "Ahum khayrun am qawmu tubbaAAin wallatheenamin qablihim ahlaknahum innahum kanoo mujrimeen"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "Wama khalaqna assamawatiwal-arda wama baynahuma laAAibeen"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "Ma khalaqnahuma illabilhaqqi walakinna aktharahum layaAAlamoon"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "Inna yawma alfasli meeqatuhumajmaAAeen"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "Yawma la yughnee mawlan AAan mawlanshay-an wala hum yunsaroon"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "Illa man rahima Allahuinnahu huwa alAAazeezu arraheem"
        }
    ]
}