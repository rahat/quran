{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa-inna yoonusa lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhoon"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeen"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu alhootu wahuwamuleem"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana minaalmusabbiheen"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ilayawmi yubAAathoon"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bilAAara-iwahuwa saqeem"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan minyaqteen"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila mi-ati alfinaw yazeedoon"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ilaheen"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim alirabbika albanatuwalahumu albanoon"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almala-ikata inathanwahum shahidoon"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloon"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wa-innahum lakathiboon"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAalaalbaneen"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lakum kayfa tahkumoon"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala tathakkaroon"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am lakum sultanun mubeen"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa/too bikitabikum in kuntum sadiqeen"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WajaAAaloo baynahu wabayna aljinnatinasaban walaqad AAalimati aljinnatu innahum lamuhdaroon"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi AAammayasifoon"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa AAibada Allahialmukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa-innakum wama taAAbudoon"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma antum AAalayhi bifatineen"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa man huwa sali aljaheem"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wama minna illa lahumaqamun maAAloom"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa-inna lanahnu assaffoon"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa-inna lanahnu almusabbihoon"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa-in kanoo layaqooloon"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law anna AAindana thikranmina al-awwaleen"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna AAibada Allahialmukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaroo bihi fasawfa yaAAlamoon"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Walaqad sabaqat kalimatuna liAAibadinaalmursaleen"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum lahumu almansooroon"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa-inna jundana lahumu alghaliboon"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla AAanhum hatta heen"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Waabsirhum fasawfa yubsiroon"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "AfabiAAathabina yastaAAjiloon"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa-itha nazala bisahatihimfasaa sabahu almunthareen"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Watawalla AAanhum hatta heen"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Waabsir fasawfa yubsiroon"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhana rabbika rabbi alAAizzatiAAamma yasifoon"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wasalamun AAala almursaleen"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Walhamdu lillahirabbi alAAalameen"
        }
    ]
}