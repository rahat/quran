{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kaththabat qawmu lootinalmursaleen"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Ith qala lahum akhoohum lootunala tattaqoon"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Innee lakum rasoolun ameen"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Wama as-alukum AAalayhi min ajrin inajriya illa AAala rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Ata/toona aththukranamina alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Watatharoona ma khalaqa lakumrabbukum min azwajikum bal antum qawmun AAadoon"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Qaloo la-in lam tantahi yalootu latakoonanna mina almukhrajeen"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Qala innee liAAamalikum mina alqaleen"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi najjinee waahlee mimmayaAAmaloon"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fanajjaynahu waahlahu ajmaAAeen"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa AAajoozan fee alghabireen"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Thumma dammarna al-akhareen"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Waamtarna AAalayhim mataranfasaa mataru almunthareen"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        }
    ]
}