{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Walaqad khalaqna al-insana minsalsalin min hama-in masnoon"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Waljanna khalaqnahumin qablu min nari assamoom"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Wa-ith qala rabbuka lilmala-ikatiinnee khaliqun basharan min salsalin min hama-inmasnoon"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fa-itha sawwaytuhu wanafakhtu feehimin roohee faqaAAoo lahu sajideen"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fasajada almala-ikatu kulluhumajmaAAoon"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illa ibleesa aba an yakoonamaAAa assajideen"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qala ya ibleesu ma lakaalla takoona maAAa assajideen"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qala lam akun li-asjuda libasharinkhalaqtahu min salsalin min hama-in masnoon"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qala fakhruj minhafa-innaka rajeem"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wa-inna AAalayka allaAAnata ila yawmiaddeen"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qala rabbi faanthirneeila yawmi yubAAathoon"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qala fa-innaka mina almunthareen"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila yawmi alwaqti almaAAloom"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qala rabbi bima aghwaytaneelaozayyinanna lahum fee al-ardi walaoghwiyannahumajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa AAibadaka minhumualmukhlaseen"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qala hatha siratunAAalayya mustaqeem"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna AAibadee laysa laka AAalayhimsultanun illa mani ittabaAAaka mina alghaween"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wa-inna jahannama lamawAAiduhum ajmaAAeen"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Laha sabAAatu abwabin likullibabin minhum juz-on maqsoom"
        }
    ]
}