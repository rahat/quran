{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Fala oqsimu bimawaqiAAi annujoom"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Wa-innahu laqasamun law taAAlamoona AAatheem"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Innahu laqur-anun kareem"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Fee kitabin maknoon"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "La yamassuhu illa almutahharoon"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Tanzeelun min rabbi alAAalameen"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Afabihatha alhadeethi antummudhinoon"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "WatajAAaloona rizqakum annakum tukaththiboon"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Falawla itha balaghati alhulqoom"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Waantum heena-ithin tanthuroona"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Wanahnu aqrabu ilayhi minkum walakinla tubsiroon"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Falawla in kuntum ghayra madeeneen"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "TarjiAAoonaha in kuntum sadiqeen"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Faamma in kana minaalmuqarrabeen"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Farawhun warayhanun wajannatunaAAeem"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Waamma in kana min as-habialyameen"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Fasalamun laka min as-habialyameen"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Waamma in kana mina almukaththibeenaaddalleen"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Fanuzulun min hameem"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Watasliyatu jaheem"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Inna hatha lahuwa haqqualyaqeen"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Fasabbih bismi rabbika alAAatheem"
        }
    ]
