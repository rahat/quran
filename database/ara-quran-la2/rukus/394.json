{
    "rukus": [
        {
            "chapter": 38,
            "verse": 41,
            "text": "Wathkur AAabdanaayyooba ith nada rabbahu annee massaniya ashshaytanubinusbin waAAathab"
        },
        {
            "chapter": 38,
            "verse": 42,
            "text": "Orkud birijlika hathamughtasalun baridun washarab"
        },
        {
            "chapter": 38,
            "verse": 43,
            "text": "Wawahabna lahu ahlahu wamithlahummaAAahum rahmatan minna wathikrali-olee al-albab"
        },
        {
            "chapter": 38,
            "verse": 44,
            "text": "Wakhuth biyadika dighthan fadribbihi wala tahnath inna wajadnahu sabiranniAAma alAAabdu innahu awwab"
        },
        {
            "chapter": 38,
            "verse": 45,
            "text": "Wathkur AAibadanaibraheema wa-ishaqa wayaAAqooba olee al-aydee wal-absar"
        },
        {
            "chapter": 38,
            "verse": 46,
            "text": "Inna akhlasnahum bikhalisatinthikra addar"
        },
        {
            "chapter": 38,
            "verse": 47,
            "text": "Wa-innahum AAindana lamina almustafaynaal-akhyar"
        },
        {
            "chapter": 38,
            "verse": 48,
            "text": "Wathkur ismaAAeelawa-ilyasaAAa watha alkifli wakullun mina al-akhyar"
        },
        {
            "chapter": 38,
            "verse": 49,
            "text": "Hatha thikrun wa-innalilmuttaqeena lahusna maab"
        },
        {
            "chapter": 38,
            "verse": 50,
            "text": "Jannati AAadnin mufattahatanlahumu al-abwab"
        },
        {
            "chapter": 38,
            "verse": 51,
            "text": "Muttaki-eena feeha yadAAoona feehabifakihatin katheeratin washarab"
        },
        {
            "chapter": 38,
            "verse": 52,
            "text": "WaAAindahum qasiratu attarfiatrab"
        },
        {
            "chapter": 38,
            "verse": 53,
            "text": "Hatha ma tooAAadoona liyawmialhisab"
        },
        {
            "chapter": 38,
            "verse": 54,
            "text": "Inna hatha larizquna malahu min nafad"
        },
        {
            "chapter": 38,
            "verse": 55,
            "text": "Hatha wa-inna littagheenalasharra maab"
        },
        {
            "chapter": 38,
            "verse": 56,
            "text": "Jahannama yaslawnaha fabi/saalmihad"
        },
        {
            "chapter": 38,
            "verse": 57,
            "text": "Hatha falyathooqoohu hameemunwaghassaq"
        },
        {
            "chapter": 38,
            "verse": 58,
            "text": "Waakharu min shaklihi azwaj"
        },
        {
            "chapter": 38,
            "verse": 59,
            "text": "Hatha fawjun muqtahimunmaAAakum la marhaban bihim innahum saloo annar"
        },
        {
            "chapter": 38,
            "verse": 60,
            "text": "Qaloo bal antum la marhabanbikum antum qaddamtumoohu lana fabi/sa alqarar"
        },
        {
            "chapter": 38,
            "verse": 61,
            "text": "Qaloo rabbana man qaddama lanahatha fazidhu AAathaban diAAfan fee annar"
        },
        {
            "chapter": 38,
            "verse": 62,
            "text": "Waqaloo ma lana lanara rijalan kunna naAAudduhum mina al-ashrar"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "Attakhathnahum sikhriyyan am zaghatAAanhumu al-absar"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "Inna thalika lahaqqun takhasumuahli annar"
        }
    ]
}