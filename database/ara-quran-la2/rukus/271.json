{
    "rukus": [
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qala rabbi ishrah lee sadree"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wayassir lee amree"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Wahlul AAuqdatan min lisanee"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahoo qawlee"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "WajAAal lee wazeeran min ahlee"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Haroona akhee"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Oshdud bihi azree"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Waashrik-hu fee amree"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay nusabbihaka katheera"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wanathkuraka katheera"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka kunta bina baseera"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qala qad ooteeta su/laka yamoosa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Walaqad mananna AAalayka marratanokhra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "Ith awhayna ilaommika ma yooha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "Ani iqthifeehi fee attabootifaqthifeehi fee alyammi falyulqihi alyammu bissahiliya/khuthhu AAaduwwun lee waAAaduwwun lahu waalqaytuAAalayka mahabbatan minnee walitusnaAAa AAalaAAaynee"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "Ith tamshee okhtuka fataqoolu haladullukum AAala man yakfuluhu farajaAAnaka ilaommika kay taqarra AAaynuha wala tahzanawaqatalta nafsan fanajjaynaka mina alghammi wafatannakafutoonan falabithta sineena fee ahli madyana thumma ji/ta AAalaqadarin ya moosa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "WastanaAAtuka linafsee"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "Ithhab anta waakhooka bi-ayateewala taniya fee thikree"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "Ithhaba ila firAAawnainnahu tagha"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "Faqoola lahu qawlan layyinanlaAAallahu yatathakkaru aw yakhsha"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "Qala rabbana innananakhafu an yafruta AAalayna aw an yatgha"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "Qala la takhafainnanee maAAakuma asmaAAu waara"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "Fa/tiyahu faqoola innarasoola rabbika faarsil maAAana banee isra-eelawala tuAAaththibhum qad ji/naka bi-ayatinmin rabbika wassalamu AAala mani ittabaAAaalhuda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "Inna qad oohiya ilaynaanna alAAathaba AAala man kaththaba watawalla"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "Qala faman rabbukuma yamoosa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "Qala rabbuna allatheeaAAta kulla shay-in khalqahu thumma hada"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "Qala fama balualqurooni al-oola"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "Qala AAilmuha AAinda rabbeefee kitabin la yadillu rabbee walayansa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "Allathee jaAAala lakumu al-ardamahdan wasalaka lakum feeha subulan waanzala mina assama-imaan faakhrajna bihi azwajan min nabatinshatta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "Kuloo warAAaw anAAamakum innafee thalika laayatin li-olee annuha"
        }
    ]
}