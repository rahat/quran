{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kaththabat qawmu noohinalmursaleen"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Ith qala lahum akhoohum noohunala tattaqoon"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innee lakum rasoolun ameen"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wama as-alukum AAalayhi min ajrin inajriya illa AAala rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qaloo anu/minu laka wattabaAAakaal-arthaloon"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala wama AAilmee bimakanoo yaAAmaloon"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hisabuhum illa AAalarabbee law tashAAuroon"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wama ana bitaridialmu/mineen"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In ana illa natheerunmubeen"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qaloo la-in lam tantahi yanoohu latakoonanna mina almarjoomeen"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala rabbi inna qawmee kaththaboon"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faftah baynee wabaynahum fathanwanajjinee waman maAAiya mina almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Faanjaynahu waman maAAahu feealfulki almashhoon"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma aghraqna baAAdu albaqeen"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        }
    ]
}