        {
            "chapter": 26,
            "verse": 116,
            "text": "Qaloo la-in lam tantahi yanoohu latakoonanna mina almarjoomeen"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala rabbi inna qawmee kaththaboon"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faftah baynee wabaynahum fathanwanajjinee waman maAAiya mina almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Faanjaynahu waman maAAahu feealfulki almashhoon"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma aghraqna baAAdu albaqeen"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kaththabat AAadun almursaleen"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Ith qala lahum akhoohumhoodun ala tattaqoon"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Innee lakum rasoolun ameen"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wama as-alukum AAalayhi min ajrin inajriya illa AAala rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnoona bikulli reeAAin ayatantaAAbathoon"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Watattakhithoona masaniAAalaAAallakum takhludoon"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa-itha batashtum batashtumjabbareen"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wattaqoo allathee amaddakumbima taAAlamoon"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum bi-anAAamin wabaneen"
