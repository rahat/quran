{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka ayatu alkitabialmubeen"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LaAAallaka bakhiAAun nafsaka allayakoonoo mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In nasha/ nunazzil AAalayhim mina assama-iayatan fathallat aAAnaquhum lahakhadiAAeen"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wama ya/teehim min thikrin minaarrahmani muhdathin illa kanooAAanhu muAArideen"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad kaththaboo fasaya/teehim anbaoma kanoo bihi yastahzi-oon"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awa lam yaraw ila al-ardi kamanbatna feeha min kulli zawjin kareem"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa-ith nada rabbukamoosa ani i/ti alqawma aththalimeen"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma firAAawna ala yattaqoon"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qala rabbi innee akhafu anyukaththiboon"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wayadeequ sadree walayantaliqu lisanee faarsil ila haroon"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Walahum AAalayya thanbun faakhafuan yaqtuloon"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qala kalla fathhababi-ayatina inna maAAakum mustamiAAoon"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa/tiya firAAawna faqoola innarasoolu rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An arsil maAAana banee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qala alam nurabbika feenawaleedan walabithta feena min AAumurika sineen"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WafaAAalta faAAlataka allatee faAAaltawaanta mina alkafireen"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "Qala faAAaltuha ithanwaana mina addalleen"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "Fafarartu minkum lamma khiftukumfawahaba lee rabbee hukman wajaAAalanee mina almursaleen"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "Watilka niAAmatun tamunnuha AAalayyaan AAabbadta banee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "Qala firAAawnu wama rabbu alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "Qala rabbu assamawatiwal-ardi wama baynahuma in kuntummooqineen"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "Qala liman hawlahu alatastamiAAoon"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "Qala rabbukum warabbu aba-ikumual-awwaleen"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "Qala inna rasoolakumu allatheeorsila ilaykum lamajnoon"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "Qala rabbu almashriqi walmaghribiwama baynahuma in kuntum taAAqiloon"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "Qala la-ini ittakhathta ilahanghayree laajAAalannaka mina almasjooneen"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "Qala awa law ji/tuka bishay-in mubeen"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "Qala fa/ti bihi in kunta mina assadiqeen"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "Faalqa AAasahu fa-ithahiya thuAAbanun mubeen"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "WanazaAAa yadahu fa-itha hiya baydaolinnathireen"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "Qala lilmala-i hawlahu inna hathalasahirun AAaleem"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "Yureedu an yukhrijakum min ardikumbisihrihi famatha ta/muroon"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "Qaloo arjih waakhahu wabAAathfee almada-ini hashireen"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "Ya/tooka bikulli sahharin AAaleem"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "FajumiAAa assaharatu limeeqatiyawmin maAAloom"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "Waqeela linnasi hal antummujtamiAAoon"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "LaAAallana nattabiAAu assaharatain kanoo humu alghalibeen"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma jaa assaharatuqaloo lifirAAawna a-inna lana laajran in kunnanahnu alghalibeen"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala naAAam wa-innakum ithanlamina almuqarrabeen"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala lahum moosa alqoo maantum mulqoon"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Faalqaw hibalahum waAAisiyyahumwaqaloo biAAizzati firAAawna inna lanahnualghaliboon"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Faalqa moosa AAasahufa-itha hiya talqafu ma ya/fikoon"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Faolqiya assaharatu sajideen"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qaloo amanna birabbialAAalameen"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi moosa waharoon"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala amantum lahu qabla an athanalakum innahu lakabeerukumu allathee AAallamakumu assihrafalasawfa taAAlamoona laoqattiAAanna aydiyakum waarjulakummin khilafin walaosallibannakum ajmaAAeen"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qaloo la dayra innaila rabbina munqaliboon"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna natmaAAu an yaghfira lanarabbuna khatayana an kunnaawwala almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Waawhayna ila moosaan asri biAAibadee innakum muttabaAAoon"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almada-ini hashireen"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haola-i lashirthimatunqaleeloon"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa-innahum lana lagha-ithoon"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa-inna lajameeAAun hathiroon"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatinwaAAuyoon"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareem"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnahabanee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeen"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma taraa aljamAAaniqala as-habu moosa innalamudrakoon"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala kalla inna maAAiya rabbeesayahdeen"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Faawhayna ila moosaani idrib biAAasaka albahra fanfalaqafakana kullu firqin kattawdi alAAatheem"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Waazlafna thamma al-akhareen"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Waanjayna moosa waman maAAahuajmaAAeen"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma aghraqna al-akhareen"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Watlu AAalayhim nabaa ibraheem"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Ith qala li-abeehi waqawmihi mataAAbudoon"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qaloo naAAbudu asnamanfanathallu laha AAakifeen"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala hal yasmaAAoonakum ithtadAAoon"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw yanfaAAoonakum aw yadurroon"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qaloo bal wajadna abaanakathalika yafAAaloon"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala afaraaytum ma kuntumtaAAbudoon"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum waabaokumu al-aqdamoon"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa-innahum AAaduwwun lee illa rabbaalAAalameen"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Allathee khalaqanee fahuwa yahdeen"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Wallathee huwa yutAAimuneewayasqeen"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa-itha maridtu fahuwayashfeen"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Wallathee yumeetunee thummayuhyeen"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Wallathee atmaAAu anyaghfira lee khatee-atee yawma addeen"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi hab lee hukman waalhiqneebissaliheen"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "WajAAal lee lisana sidqinfee al-akhireen"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "WajAAalnee min warathati jannati annaAAeem"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Waghfir li-abee innahu kanamina addalleen"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wala tukhzinee yawma yubAAathoon"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma la yanfaAAu malun walabanoon"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa man ata Allahabiqalbin saleem"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Waozlifati aljannatu lilmuttaqeen"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Waburrizati aljaheemu lilghaween"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Waqeela lahum ayna ma kuntumtaAAbudoon"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min dooni Allahi hal yansuroonakumaw yantasiroon"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkiboo feeha hum walghawoon"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wajunoodu ibleesa ajmaAAoon"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qaloo wahum feeha yakhtasimoon"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Tallahi in kunna lafeedalalin mubeen"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Ith nusawweekum birabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wama adallana illaalmujrimoon"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fama lana min shafiAAeen"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wala sadeeqin hameem"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw anna lana karratan fanakoonamina almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kaththabat qawmu noohinalmursaleen"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Ith qala lahum akhoohum noohunala tattaqoon"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innee lakum rasoolun ameen"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wama as-alukum AAalayhi min ajrin inajriya illa AAala rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fattaqoo Allaha waateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qaloo anu/minu laka wattabaAAakaal-arthaloon"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala wama AAilmee bimakanoo yaAAmaloon"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hisabuhum illa AAalarabbee law tashAAuroon"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wama ana bitaridialmu/mineen"
        },
        {
            "chapter": 26,
            "verse": 115,
