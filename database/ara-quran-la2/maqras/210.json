{
    "hizbs": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wayatoofu AAalayhim ghilmanunlahum kaannahum lu/luon maknoon"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Waaqbala baAAduhum AAala baAAdinyatasaaloon"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qaloo inna kunna qablufee ahlina mushfiqeen"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allahu AAalayna wawaqanaAAathaba assamoom"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Inna kunna min qablu nadAAoohuinnahu huwa albarru arraheem"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "Fathakkir fama anta biniAAmatirabbika bikahin wala majnoon"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "Am yaqooloona shaAAirun natarabbasubihi rayba almanoon"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "Qul tarabbasoo fa-innee maAAakum minaalmutarabbiseen"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "Am ta/muruhum ahlamuhum bihathaam hum qawmun taghoon"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "Am yaqooloona taqawwalahu bal layu/minoon"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "Falya/too bihadeethin mithlihi in kanoosadiqeena"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "Am khuliqoo min ghayri shay-in am humu alkhaliqoon"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "Am khalaqoo assamawatiwal-arda bal la yooqinoon"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "Am AAindahum khaza-inu rabbika amhumu almusaytiroon"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "Am lahum sullamun yastamiAAoona feehifalya/ti mustamiAAuhum bisultanin mubeen"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "Am lahu albanatu walakumu albanoon"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "Am tas-aluhum ajran fahum min maghraminmuthqaloon"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "Am AAindahumu alghaybu fahum yaktuboon"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "Am yureedoona kaydan fallatheenakafaroo humu almakeedoon"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "Am lahum ilahun ghayru Allahisubhana Allahi AAamma yushrikoon"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "Wa-in yaraw kisfan mina assama-isaqitan yaqooloo sahabun markoom"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "Fatharhum hatta yulaqooyawmahumu allathee feehi yusAAaqoon"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "Yawma la yughnee AAanhum kayduhumshay-an wala hum yunsaroon"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "Wa-inna lillatheena thalamooAAathaban doona thalika walakinna aktharahumla yaAAlamoon"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "Wasbir lihukmi rabbikafa-innaka bi-aAAyunina wasabbih bihamdirabbika heena taqoom"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "Wamina allayli fasabbihhu wa-idbaraannujoom"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wannajmi itha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalla sahibukum wamaghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wama yantiqu AAani alhawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huwa illa wahyun yooha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "AAallamahu shadeedu alquwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Thoo mirratin fastawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wahuwa bil-ofuqi al-aAAla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Faawha ila AAabdihi maawha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kathaba alfu-adu maraa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumaroonahu AAala mayara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Walaqad raahu nazlatan okhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "AAinda sidrati almuntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "AAindaha jannatu alma/wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ith yaghsha assidratama yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagha albasaru wamatagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad raa min ayatirabbihi alkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afaraaytumu allata walAAuzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wamanata aththalithataal-okhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu aththakaru walahual-ontha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka ithan qismatun deeza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiya illa asmaonsammaytumooha antum waabaokum maanzala Allahu biha min sultanin inyattabiAAoona illa aththanna wamatahwa al-anfusu walaqad jaahum min rabbihimu alhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am lil-insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi al-akhiratu wal-oola"
        }
    ]
}