        {
            "chapter": 88,
            "verse": 13,
            "text": "Feeha sururun marfooAAa"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Waakwabun mawdooAAa"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Wanamariqu masfoofa"
