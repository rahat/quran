{
    "hizbs": [
        {
            "chapter": 50,
            "verse": 27,
            "text": "Qala qareenuhu rabbana maatghaytuhu walakin kana fee dalalinbaAAeed"
        },
        {
            "chapter": 50,
            "verse": 28,
            "text": "Qala la takhtasimooladayya waqad qaddamtu ilaykum bilwaAAeed"
        },
        {
            "chapter": 50,
            "verse": 29,
            "text": "Ma yubaddalu alqawlu ladayya wamaana bithallamin lilAAabeed"
        },
        {
            "chapter": 50,
            "verse": 30,
            "text": "Yawma naqoolu lijahannama hali imtala/tiwataqoolu hal min mazeed"
        },
        {
            "chapter": 50,
            "verse": 31,
            "text": "Waozlifati aljannatu lilmuttaqeena ghayrabaAAeed"
        },
        {
            "chapter": 50,
            "verse": 32,
            "text": "Hatha ma tooAAadoona likulliawwabin hafeeth"
        },
        {
            "chapter": 50,
            "verse": 33,
            "text": "Man khashiya arrahmanabilghaybi wajaa biqalbin muneeb"
        },
        {
            "chapter": 50,
            "verse": 34,
            "text": "Odkhulooha bisalamin thalikayawmu alkhulood"
        },
        {
            "chapter": 50,
            "verse": 35,
            "text": "Lahum ma yashaoona feehawaladayna mazeed"
        },
        {
            "chapter": 50,
            "verse": 36,
            "text": "Wakam ahlakna qablahum min qarnin humashaddu minhum batshan fanaqqaboo fee albiladi halmin mahees"
        },
        {
            "chapter": 50,
            "verse": 37,
            "text": "Inna fee thalika lathikraliman kana lahu qalbun aw alqa assamAAawahuwa shaheed"
        },
        {
            "chapter": 50,
            "verse": 38,
            "text": "Walaqad khalaqna assamawatiwal-arda wama baynahuma fee sittatiayyamin wama massana min lughoob"
        },
        {
            "chapter": 50,
            "verse": 39,
            "text": "Fasbir AAala mayaqooloona wasabbih bihamdi rabbika qabla tulooAAiashshamsi waqabla alghuroob"
        },
        {
            "chapter": 50,
            "verse": 40,
            "text": "Wamina allayli fasabbihhu waadbaraassujood"
        },
        {
            "chapter": 50,
            "verse": 41,
            "text": "WastamiAA yawma yunadi almunadimin makanin qareeb"
        },
        {
            "chapter": 50,
            "verse": 42,
            "text": "Yawma yasmaAAoona assayhatabilhaqqi thalika yawmu alkhurooj"
        },
        {
            "chapter": 50,
            "verse": 43,
            "text": "Inna nahnu nuhyeewanumeetu wa-ilayna almaseer"
        },
        {
            "chapter": 50,
            "verse": 44,
            "text": "Yawma tashaqqaqu al-ardu AAanhum siraAAanthalika hashrun AAalayna yaseer"
        },
        {
            "chapter": 50,
            "verse": 45,
            "text": "Nahnu aAAlamu bima yaqooloonawama anta AAalayhim bijabbarin fathakkir bilqur-animan yakhafu waAAeed"
        },
        {
            "chapter": 51,
            "verse": 1,
            "text": "Waththariyati tharwa"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Falhamilati wiqra"
