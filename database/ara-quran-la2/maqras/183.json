{
    "hizbs": [
        {
            "chapter": 38,
            "verse": 52,
            "text": "WaAAindahum qasiratu attarfiatrab"
        },
        {
            "chapter": 38,
            "verse": 53,
            "text": "Hatha ma tooAAadoona liyawmialhisab"
        },
        {
            "chapter": 38,
            "verse": 54,
            "text": "Inna hatha larizquna malahu min nafad"
        },
        {
            "chapter": 38,
            "verse": 55,
            "text": "Hatha wa-inna littagheenalasharra maab"
        },
        {
            "chapter": 38,
            "verse": 56,
            "text": "Jahannama yaslawnaha fabi/saalmihad"
        },
        {
            "chapter": 38,
            "verse": 57,
            "text": "Hatha falyathooqoohu hameemunwaghassaq"
        },
        {
            "chapter": 38,
            "verse": 58,
            "text": "Waakharu min shaklihi azwaj"
        },
        {
            "chapter": 38,
            "verse": 59,
            "text": "Hatha fawjun muqtahimunmaAAakum la marhaban bihim innahum saloo annar"
        },
        {
            "chapter": 38,
            "verse": 60,
            "text": "Qaloo bal antum la marhabanbikum antum qaddamtumoohu lana fabi/sa alqarar"
        },
        {
            "chapter": 38,
            "verse": 61,
            "text": "Qaloo rabbana man qaddama lanahatha fazidhu AAathaban diAAfan fee annar"
        },
        {
            "chapter": 38,
            "verse": 62,
            "text": "Waqaloo ma lana lanara rijalan kunna naAAudduhum mina al-ashrar"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "Attakhathnahum sikhriyyan am zaghatAAanhumu al-absar"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "Inna thalika lahaqqun takhasumuahli annar"
        },
        {
            "chapter": 38,
            "verse": 65,
            "text": "Qul innama ana munthirunwama min ilahin illa Allahu alwahidualqahhar"
        },
        {
            "chapter": 38,
            "verse": 66,
            "text": "Rabbu assamawati wal-ardiwama baynahuma alAAazeezu alghaffar"
        },
        {
            "chapter": 38,
            "verse": 67,
            "text": "Qul huwa nabaon AAatheem"
        },
        {
            "chapter": 38,
            "verse": 68,
            "text": "Antum AAanhu muAAridoon"
        },
        {
            "chapter": 38,
            "verse": 69,
            "text": "Ma kana liya min AAilmin bilmala-ial-aAAla ith yakhtasimoon"
        },
        {
            "chapter": 38,
            "verse": 70,
            "text": "In yooha ilayya illa annamaana natheerun mubeen"
        },
        {
            "chapter": 38,
            "verse": 71,
            "text": "Ith qala rabbuka lilmala-ikatiinnee khaliqun basharan min teen"
        },
        {
            "chapter": 38,
            "verse": 72,
            "text": "Fa-itha sawwaytuhu wanafakhtu feehimin roohee faqaAAoo lahu sajideen"
        },
        {
            "chapter": 38,
            "verse": 73,
            "text": "Fasajada almala-ikatu kulluhumajmaAAoon"
        },
        {
            "chapter": 38,
            "verse": 74,
            "text": "Illa ibleesa istakbara wakanamina alkafireen"
        },
        {
            "chapter": 38,
            "verse": 75,
            "text": "Qala ya ibleesu mamanaAAaka an tasjuda lima khalaqtu biyadayya astakbarta amkunta mina alAAaleen"
        },
        {
            "chapter": 38,
            "verse": 76,
            "text": "Qala ana khayrun minhukhalaqtanee min narin wakhalaqtahu min teen"
        },
        {
            "chapter": 38,
            "verse": 77,
            "text": "Qala fakhruj minhafa-innaka rajeem"
        },
        {
            "chapter": 38,
            "verse": 78,
            "text": "Wa-inna AAalayka laAAnatee ila yawmiaddeen"
        },
        {
            "chapter": 38,
            "verse": 79,
            "text": "Qala rabbi faanthirneeila yawmi yubAAathoon"
        },
        {
            "chapter": 38,
            "verse": 80,
            "text": "Qala fa-innaka mina almunthareen"
        },
        {
            "chapter": 38,
            "verse": 81,
            "text": "Ila yawmi alwaqti almaAAloom"
        },
        {
            "chapter": 38,
            "verse": 82,
            "text": "Qala fabiAAizzatika laoghwiyannahumajmaAAeen"
        },
        {
            "chapter": 38,
            "verse": 83,
            "text": "Illa AAibadaka minhumualmukhlaseen"
        },
        {
            "chapter": 38,
            "verse": 84,
            "text": "Qala falhaqqu walhaqqaaqool"
        },
        {
            "chapter": 38,
            "verse": 85,
            "text": "Laamlaanna jahannama minka wamimmantabiAAaka minhum ajmaAAeen"
        },
        {
            "chapter": 38,
            "verse": 86,
            "text": "Qul ma as-alukum AAalayhi min ajrinwama ana mina almutakallifeen"
        },
        {
            "chapter": 38,
            "verse": 87,
            "text": "In huwa illa thikrun lilAAalameen"
        },
        {
            "chapter": 38,
            "verse": 88,
            "text": "WalataAAlamunna nabaahu baAAda heen"
        },
        {
            "chapter": 39,
            "verse": 1,
            "text": "Tanzeelu alkitabi mina AllahialAAazeezi alhakeem"
        },
        {
            "chapter": 39,
            "verse": 2,
            "text": "Inna anzalna ilayka alkitababilhaqqi faAAbudi Allaha mukhlisanlahu addeen"
        },
        {
            "chapter": 39,
            "verse": 3,
            "text": "Ala lillahi addeenualkhalisu wallatheena ittakhathoomin doonihi awliyaa ma naAAbuduhum illaliyuqarriboona ila Allahi zulfa innaAllaha yahkumu baynahum fee ma hum feehiyakhtalifoona inna Allaha la yahdee man huwa kathibunkaffar"
        },
        {
            "chapter": 39,
            "verse": 4,
            "text": "Law arada Allahu an yattakhithawaladan lastafa mimma yakhluqu mayashao subhanahu huwa Allahu alwahidualqahhar"
        },
        {
            "chapter": 39,
            "verse": 5,
            "text": "Khalaqa assamawati wal-ardabilhaqqi yukawwiru allayla AAala annahariwayukawwiru annahara AAala allayliwasakhkhara ashshamsa walqamara kullun yajreeli-ajalin musamman ala huwa alAAazeezu alghaffar"
        },
        {
            "chapter": 39,
            "verse": 6,
            "text": "Khalaqakum min nafsin wahidatin thummajaAAala minha zawjaha waanzala lakum mina al-anAAamithamaniyata azwajin yakhluqukum fee butooniommahatikum khalqan min baAAdi khalqin fee thulumatinthalathin thalikumu Allahu rabbukum lahualmulku la ilaha illa huwa faanna tusrafoon"
        },
        {
            "chapter": 39,
            "verse": 7,
            "text": "In takfuroo fa-inna Allaha ghaniyyunAAankum wala yarda liAAibadihi alkufra wa-intashkuroo yardahu lakum wala taziru waziratunwizra okhra thumma ila rabbikum marjiAAukumfayunabbi-okum bima kuntum taAAmaloona innahu AAaleemun bithatiassudoor"
        }
    ]
}