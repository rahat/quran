{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 181,
            "text": "Awfoo alkayla wala takoonoo minaalmukhsireen"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Wazinoo bilqistasialmustaqeem"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Wala tabkhasoo annasaashyaahum wala taAAthaw fee al-ardimufsideen"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Wattaqoo allathee khalaqakumwaljibillata al-awwaleen"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qaloo innama anta mina almusahhareen"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wama anta illa basharunmithluna wa-in nathunnuka lamina alkathibeen"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Faasqit AAalayna kisafan minaassama-i in kunta mina assadiqeen"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala rabbee aAAlamu bimataAAmaloon"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakaththaboohu faakhathahumAAathabu yawmi aththullati innahu kanaAAathaba yawmin AAatheem"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa-innahu latanzeelu rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala bihi arroohu al-ameen"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "AAala qalbika litakoona mina almunthireen"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin AAarabiyyin mubeen"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa-innahu lafee zuburi al-awwaleen"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awa lam yakun lahum ayatan anyaAAlamahu AAulamao banee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Walaw nazzalnahu AAala baAAdial-aAAjameen"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqaraahu AAalayhim ma kanoobihi mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kathalika salaknahu feequloobi almujrimeen"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yu/minoona bihi hattayarawoo alAAathaba al-aleem"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya/tiyahum baghtatan wahum layashAAuroon"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqooloo hal nahnu muntharoon"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "AfabiAAathabina yastaAAjiloon"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afaraayta in mattaAAnahum sineen"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma jaahum ma kanooyooAAadoon"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma aghna AAanhum ma kanooyumattaAAoon"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wama ahlakna min qaryatin illalaha munthiroon"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Thikra wama kunnathalimeen"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wama tanazzalat bihi ashshayateen"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wama yanbaghee lahum wamayastateeAAoon"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum AAani assamAAilamaAAzooloona"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fala tadAAu maAAa Allahi ilahanakhara fatakoona mina almuAAaththabeen"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Waanthir AAasheerataka al-aqrabeen"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Wakhfid janahakalimani ittabaAAaka mina almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fa-in AAasawka faqul innee baree-onmimma taAAmaloon"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Watawakkal AAala alAAazeezi arraheem"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Allathee yaraka heenataqoom"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wataqallubaka fee assajideen"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu huwa assameeAAu alAAaleem"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal onabbi-okum AAala man tanazzaluashshayateen"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu AAala kulli affakinatheem"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulqoona assamAAa waaktharuhum kathiboon"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "WashshuAAaraoyattabiAAuhumu alghawoon"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam tara annahum fee kulli wadinyaheemoon"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Waannahum yaqooloona ma layafAAaloon"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illa allatheena amanoowaAAamiloo assalihati wathakaroo Allahakatheeran wantasaroo min baAAdi ma thulimoowasayaAAlamu allatheena thalamoo ayyamunqalabin yanqaliboon"
        }
    ]
}