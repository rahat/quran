{
    "hizbs": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa-inna min sheeAAatihi la-ibraheem"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Ith jaa rabbahu biqalbinsaleem"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Ith qala li-abeehi waqawmihi mathataAAbudoon"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A-ifkan alihatan doona Allahitureedoon"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama thannukum birabbialAAalameen"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanathara nathratanfee annujoom"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala innee saqeem"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw AAanhu mudbireen"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha ila alihatihimfaqala ala ta/kuloon"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lakum la tantiqoon"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha AAalayhim darban bilyameen"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Faaqbaloo ilayhi yaziffoon"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala ataAAbudoona ma tanhitoon"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Wallahu khalaqakum wamataAAmaloon"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qaloo ibnoo lahu bunyananfaalqoohu fee aljaheem"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Faaradoo bihi kaydan fajaAAalnahumual-asfaleen"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Waqala innee thahibun ilarabbee sayahdeen"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi hablee mina assaliheen"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabashsharnahu bighulamin haleem"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma balagha maAAahu assaAAyaqala ya bunayya innee ara fee almanamiannee athbahuka fanthur mathatara qala ya abati ifAAal ma tu/marusatajidunee in shaa Allahu mina assabireen"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahuliljabeen"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheem"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta arru/yainna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalaoalmubeen"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhinAAatheem"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheem"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu bi-ishaqanabiyyan mina assaliheen"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAalaishaqa wamin thurriyyatihima muhsinunwathalimun linafsihi mubeen"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosawaharoon"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahumamina alkarbi alAAatheem"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoohumu alghalibeen"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitabaalmustabeen"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma assirataalmustaqeem"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosawaharoon"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa-inna ilyasa lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi alatattaqoon"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsanaalkhaliqeen"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba aba-ikumual-awwaleen"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fa-innahum lamuhdaroon"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahialmukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseen"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa-inna lootan lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahuajmaAAeen"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireen"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna al-akhareen"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa-innakum latamurroona AAalayhim musbiheen"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabillayli afala taAAqiloon"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa-inna yoonusa lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhoon"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeen"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu alhootu wahuwamuleem"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana minaalmusabbiheen"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ilayawmi yubAAathoon"
        }
    ]
}