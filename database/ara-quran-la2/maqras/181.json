        {
            "chapter": 38,
            "verse": 1,
            "text": "Sad walqur-ani theeaththikr"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "Bali allatheena kafaroo fee AAizzatinwashiqaq"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "Kam ahlakna min qablihim min qarninfanadaw walata heena manas"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "WaAAajiboo an jaahum munthirunminhum waqala alkafiroona hatha sahirunkaththab"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "AjaAAala al-alihata ilahan wahidaninna hatha lashay-on AAujab"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "Wantalaqa almalao minhum aniimshoo wasbiroo AAala alihatikuminna hatha lashay-on yurad"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "Ma samiAAna bihatha feealmillati al-akhirati in hatha illa ikhtilaq"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "Aonzila AAalayhi aththikru minbaynina bal hum fee shakkin min thikree bal lammayathooqoo AAathab"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "Am AAindahum khaza-inu rahmatirabbika alAAazeezi alwahhab"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "Am lahum mulku assamawatiwal-ardi wama baynahuma falyartaqoofee al-asbab"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "Jundun ma hunalika mahzoomunmina al-ahzab"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "Kaththabat qablahum qawmu noohinwaAAadun wafirAAawnu thoo al-awtad"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "Wathamoodu waqawmu lootin waas-habual-aykati ola-ika al-ahzab"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "In kullun illa kaththaba arrusulafahaqqa AAiqab"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "Wama yanthuru haola-iilla sayhatan wahidatan ma lahamin fawaq"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "Waqaloo rabbana AAajjil lanaqittana qabla yawmi alhisab"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "Isbir AAala mayaqooloona wathkur AAabdana dawooda thaal-aydi innahu awwab"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "Inna sakhkharna aljibalamaAAahu yusabbihna bilAAashiyyi wal-ishraq"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "Wattayra mahshooratankullun lahu awwab"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "Washadadna mulkahu waataynahualhikmata wafasla alkhitab"
