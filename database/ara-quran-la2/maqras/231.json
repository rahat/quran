    "hizbs": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "La oqsimu biyawmi alqiyamat"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wala oqsimu binnafsi allawwamat"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabu al-insanu allannajmaAAa AAithamah"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bala qadireena AAala annusawwiya bananah"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal yureedu al-insanu liyafjura amamah"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas-alu ayyana yawmu alqiyama"
