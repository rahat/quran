        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa-itha bariqa albasar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Wakhasafa alqamar"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "WajumiAAa ashshamsu walqamar"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqoolu al-insanu yawma-ithinayna almafar"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kalla la wazar"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila rabbika yawma-ithinalmustaqar"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabbao al-insanu yawma-ithinbima qaddama waakhkhar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Bali al-insanu AAala nafsihibaseera"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Walaw alqa maAAatheerah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La tuharrik bihi lisanakalitaAAjala bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna AAalayna jamAAahu waqur-anah"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa-itha qara/nahu fattabiAAqur-anah"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Thumma inna AAalayna bayanah"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kalla bal tuhibboona alAAajila"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Watatharoona al-akhira"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujoohun yawma-ithin nadira"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ila rabbiha nathira"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wawujoohun yawma-ithin basira"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tathunnu an yufAAala bihafaqira"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kalla itha balaghati attaraqiy"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Waqeela man raq"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wathanna annahu alfiraq"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Waltaffati assaqu bilssaq"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ila rabbika yawma-ithin almasaq"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fala saddaqa wala salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Walakin kaththaba watawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Thumma thahaba ila ahlihiyatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Thumma awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayahsabu al-insanu an yutrakasuda"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam yaku nutfatan min manayyin yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Thumma kana AAalaqatan fakhalaqafasawwa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "FajaAAala minhu azzawjayni aththakarawal-ontha"
