        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hal ata AAala al-insani heenunmina addahri lam yakun shay-an mathkoora"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Inna khalaqna al-insanamin nutfatin amshajin nabtaleehi fajaAAalnahusameeAAan baseera"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Inna hadaynahu assabeelaimma shakiran wa-imma kafoora"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Inna aAAtadna lilkafireenasalasila waaghlalan wasaAAeera"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Inna al-abrara yashraboona min ka/sinkana mizajuha kafoora"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "AAaynan yashrabu biha AAibaduAllahi yufajjiroonaha tafjeera"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yoofoona binnathri wayakhafoonayawman kana sharruhu mustateera"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "WayutAAimona attaAAamaAAala hubbihi miskeenan wayateeman waaseera"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innama nutAAimukum liwajhi Allahila nureedu minkum jazaan wala shukoora"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Inna nakhafu min rabbinayawman AAaboosan qamtareera"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fawaqahumu Allahu sharra thalikaalyawmi walaqqahum nadratan wasuroora"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Wajazahum bima sabaroojannatan wahareera"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttaki-eena feeha AAala al-ara-ikila yarawna feeha shamsan wala zamhareera"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Wadaniyatan AAalayhim thilaluhawathullilat qutoofuha tathleela"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Wayutafu AAalayhim bi-aniyatinmin fiddatin waakwabin kanat qawareera"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Qawareera min fiddatinqaddarooha taqdeera"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Wayusqawna feeha ka/san kanamizajuha zanjabeela"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "AAaynan feeha tusammasalsabeela"
        }
    ]
