        {
            "chapter": 95,
            "verse": 2,
            "text": "Watoori seeneen"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Wahatha albaladi al-ameen"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Laqad khalaqna al-insana fee ahsanitaqweem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Thumma radadnahu asfala safileen"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Illa allatheena amanoowaAAamiloo assalihati falahum ajrun ghayrumamnoon"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Fama yukaththibuka baAAdu biddeen"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alaysa Allahu bi-ahkami alhakimeen"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Iqra/ bi-ismi rabbika allathee khalaq"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Khalaqa al-insana min AAalaq"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Iqra/ warabbuka al-akram"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Allathee AAallama bilqalam"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "AAallama al-insana ma lamyaAAlam"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kalla inna al-insana layatgha"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "An raahu istaghna"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Inna ila rabbika arrujAAa"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Araayta allathee yanha"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "AAabdan itha salla"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Araayta in kana AAala alhuda"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Aw amara bittaqwa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Araayta in kaththaba watawalla"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Alam yaAAlam bi-anna Allaha yara"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kalla la-in lam yantahi lanasfaAAanbinnasiya"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nasiyatin kathibatin khati-a"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "FalyadAAu nadiyah"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "SanadAAu azzabaniya"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kalla la tutiAAhu wasjudwaqtarib"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Inna anzalnahu fee laylatialqadr"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Wama adraka ma laylatualqadr"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Laylatu alqadri khayrun min alfi shahr"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tanazzalu almala-ikatu warroohufeeha bi-ithni rabbihim min kulli amr"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Salamun hiya hatta matlaAAialfajr"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Lam yakuni allatheena kafaroo min ahlialkitabi walmushrikeena munfakkeena hattata/tiyahumu albayyina"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Rasoolun mina Allahi yatloo suhufanmutahhara"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Feeha kutubun qayyima"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Wama tafarraqa allatheena ootooalkitaba illa min baAAdi ma jaat-humualbayyina"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Wama omiroo illa liyaAAbudooAllaha mukhliseena lahu addeena hunafaawayuqeemoo assalata wayu/too azzakatawathalika deenu alqayyima"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Inna allatheena kafaroo min ahli alkitabiwalmushrikeena fee nari jahannama khalideenafeeha ola-ika hum sharru albariyya"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Inna allatheena amanoowaAAamiloo assalihati ola-ika humkhayru albariyya"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Jazaohum AAinda rabbihim jannatuAAadnin tajree min tahtiha al-anharu khalideenafeeha abadan radiya Allahu AAanhum waradooAAanhu thalika liman khashiya rabbah"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Itha zulzilati al-ardu zilzalaha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Waakhrajati al-ardu athqalaha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Waqala al-insanu ma laha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yawma-ithin tuhaddithu akhbaraha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi-anna rabbaka awha laha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yawma-ithin yasduru annasuashtatan liyuraw aAAmalahum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Faman yaAAmal mithqala tharratinkhayran yarah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Waman yaAAmal mithqala tharratinsharran yarah"
        },
        {
