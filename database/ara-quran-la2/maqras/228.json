{
    "hizbs": [
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inna al-insana khuliqa halooAAa"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Itha massahu ashsharrujazooAAa"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Wa-itha massahu alkhayru manooAAa"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illa almusalleen"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Allatheena hum AAala salatihimda-imoon"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Wallatheena fee amwalihimhaqqun maAAloom"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lissa-ili walmahroom"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Wallatheena yusaddiqoonabiyawmi addeen"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Wallatheena hum min AAathabirabbihim mushfiqoon"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna AAathaba rabbihim ghayru ma/moon"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Wallatheena hum lifuroojihim hafithoon"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illa AAala azwajihim awma malakat aymanuhum fa-innahum ghayru maloomeen"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famani ibtagha waraa thalikafaola-ika humu alAAadoon"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Wallatheena hum li-amanatihimwaAAahdihim raAAoon"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Wallatheena hum bishahadatihimqa-imoon"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Wallatheena hum AAala salatihimyuhafithoon"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ola-ika fee jannatin mukramoon"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Famali allatheena kafarooqibalaka muhtiAAeen"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "AAani alyameeni waAAani ashshimaliAAizeen"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "AyatmaAAu kullu imri-in minhum anyudkhala jannata naAAeem"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kalla inna khalaqnahummimma yaAAlamoon"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "Fala oqsimu birabbi almashariqiwalmagharibi inna laqadiroon"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "AAala an nubaddila khayran minhum wamanahnu bimasbooqeen"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "Fatharhum yakhoodoowayalAAaboo hatta yulaqoo yawmahumu allatheeyooAAadoon"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "Yawma yakhrujoona mina al-ajdathi siraAAankaannahum ila nusubin yoofidoon"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "KhashiAAatan absaruhumtarhaquhum thillatun thalika alyawmu allatheekanoo yooAAadoon"
        },
        {
            "chapter": 71,
            "verse": 1,
            "text": "Inna arsalna noohan ilaqawmihi an anthir qawmaka min qabli an ya/tiyahum AAathabunaleem"
        },
        {
            "chapter": 71,
            "verse": 2,
            "text": "Qala ya qawmi innee lakum natheerunmubeen"
        },
        {
            "chapter": 71,
            "verse": 3,
            "text": "Ani oAAbudoo Allaha wattaqoohuwaateeAAoon"
        },
        {
            "chapter": 71,
            "verse": 4,
            "text": "Yaghfir lakum min thunoobikumwayu-akhkhirkum ila ajalin musamman inna ajala Allahiitha jaa la yu-akhkharu law kuntumtaAAlamoon"
        },
        {
            "chapter": 71,
            "verse": 5,
            "text": "Qala rabbi innee daAAawtu qawmeelaylan wanahara"
        },
        {
            "chapter": 71,
            "verse": 6,
            "text": "Falam yazidhum duAAa-ee illafirara"
        },
        {
            "chapter": 71,
            "verse": 7,
            "text": "Wa-innee kullama daAAawtuhumlitaghfira lahum jaAAaloo asabiAAahum fee athanihimwastaghshaw thiyabahum waasarroo wastakbarooistikbara"
        },
        {
            "chapter": 71,
            "verse": 8,
            "text": "Thumma innee daAAawtuhum jihara"
        },
        {
            "chapter": 71,
            "verse": 9,
            "text": "Thumma innee aAAlantu lahum waasrartu lahumisrara"
        },
        {
            "chapter": 71,
            "verse": 10,
            "text": "Faqultu istaghfiroo rabbakum innahu kanaghaffara"
        },
        {
            "chapter": 71,
            "verse": 11,
            "text": "Yursili assamaa AAalaykummidrara"
        },
        {
            "chapter": 71,
            "verse": 12,
            "text": "Wayumdidkum bi-amwalin wabaneenawayajAAal lakum jannatin wayajAAal lakum anhara"
        },
        {
            "chapter": 71,
            "verse": 13,
            "text": "Ma lakum la tarjoona lillahiwaqara"
        },
        {
            "chapter": 71,
            "verse": 14,
            "text": "Waqad khalaqakum atwara"
        },
        {
            "chapter": 71,
            "verse": 15,
            "text": "Alam taraw kayfa khalaqa AllahusabAAa samawatin tibaqa"
        },
        {
            "chapter": 71,
            "verse": 16,
            "text": "WajaAAala alqamara feehinna nooran wajaAAalaashshamsa siraja"
        },
        {
            "chapter": 71,
            "verse": 17,
            "text": "Wallahu anbatakum mina al-ardinabata"
        },
        {
            "chapter": 71,
            "verse": 18,
            "text": "Thumma yuAAeedukum feehawayukhrijukum ikhraja"
        },
        {
            "chapter": 71,
            "verse": 19,
            "text": "Wallahu jaAAala lakumu al-ardabisata"
        },
        {
            "chapter": 71,
            "verse": 20,
            "text": "Litaslukoo minha subulan fijaja"
        },
        {
            "chapter": 71,
            "verse": 21,
            "text": "Qala noohun rabbi innahum AAasawneewattabaAAoo man lam yazidhu maluhu wawaladuhu illakhasara"
        },
        {
            "chapter": 71,
            "verse": 22,
            "text": "Wamakaroo makran kubbara"
        },
        {
            "chapter": 71,
            "verse": 23,
            "text": "Waqaloo la tatharunna alihatakumwala tatharunna waddan wala suwaAAanwala yaghootha wayaAAooqa wanasra"
        },
        {
            "chapter": 71,
            "verse": 24,
            "text": "Waqad adalloo katheeran walatazidi aththalimeena illa dalala"
        },
        {
            "chapter": 71,
            "verse": 25,
            "text": "Mimma khatee-atihimoghriqoo faodkhiloo naran falam yajidoo lahum min dooniAllahi ansara"
        },
        {
            "chapter": 71,
            "verse": 26,
            "text": "Waqala noohun rabbi latathar AAala al-ardi mina alkafireenadayyara"
        },
        {
            "chapter": 71,
            "verse": 27,
            "text": "Innaka in tatharhum yudillooAAibadaka wala yalidoo illa fajirankaffara"
        },
        {
            "chapter": 71,
            "verse": 28,
            "text": "Rabbi ighfir lee waliwalidayyawaliman dakhala baytiya mu/minan walilmu/mineena walmu/minatiwala tazidi aththalimeena illatabaran"
        }
    ]
}