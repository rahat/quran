{
    "pages": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka ayatu alkitabialmubeen"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LaAAallaka bakhiAAun nafsaka allayakoonoo mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In nasha/ nunazzil AAalayhim mina assama-iayatan fathallat aAAnaquhum lahakhadiAAeen"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wama ya/teehim min thikrin minaarrahmani muhdathin illa kanooAAanhu muAArideen"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad kaththaboo fasaya/teehim anbaoma kanoo bihi yastahzi-oon"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awa lam yaraw ila al-ardi kamanbatna feeha min kulli zawjin kareem"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna fee thalika laayatan wamakana aktharuhum mu/mineen"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu arraheem"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa-ith nada rabbukamoosa ani i/ti alqawma aththalimeen"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma firAAawna ala yattaqoon"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qala rabbi innee akhafu anyukaththiboon"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wayadeequ sadree walayantaliqu lisanee faarsil ila haroon"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Walahum AAalayya thanbun faakhafuan yaqtuloon"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qala kalla fathhababi-ayatina inna maAAakum mustamiAAoon"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa/tiya firAAawna faqoola innarasoolu rabbi alAAalameen"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An arsil maAAana banee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qala alam nurabbika feenawaleedan walabithta feena min AAumurika sineen"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WafaAAalta faAAlataka allatee faAAaltawaanta mina alkafireen"
        }
    ]
}