        {
            "chapter": 68,
            "verse": 30,
            "text": "Faaqbala baAAduhum AAala baAAdinyatalawamoon"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qaloo ya waylana innakunna tagheen"
