{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma lakum la tanasaroon"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal humu alyawma mustaslimoon"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Waaqbala baAAduhum AAala baAAdinyatasaaloon"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qaloo innakum kuntum ta/toonanaAAani alyameen"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qaloo bal lam takoonoo mu/mineen"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wama kana lanaAAalaykum min sultanin bal kuntum qawman tagheen"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fahaqqa AAalayna qawlu rabbinainna latha-iqoon"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Faaghwaynakum inna kunnaghaween"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fa-innahum yawma-ithin fee alAAathabimushtarikoon"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kathalika nafAAalu bilmujrimeen"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum kanoo itha qeela lahumla ilaha illa Allahu yastakbiroon"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wayaqooloona a-inna latarikoo alihatinalishaAAirin majnoon"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal jaa bilhaqqi wasaddaqaalmursaleen"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum latha-iqoo alAAathabial-aleem"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wama tujzawna illa makuntum taAAmaloon"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa AAibada Allahialmukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ola-ika lahum rizqun maAAloom"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawakihu wahum mukramoon"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fee jannati annaAAeem"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "AAala sururin mutaqabileen"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu AAalayhim bika/sin min maAAeen"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Baydaa laththatin lishsharibeen"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La feeha ghawlun walahum AAanha yunzafoon"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "WaAAindahum qasiratu attarfiAAeen"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Kaannahunna baydun maknoon"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Faaqbala baAAduhum AAala baAAdinyatasaaloon"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qala qa-ilun minhum innee kanalee qareen"
        }
    ]
