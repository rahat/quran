{
    "pages": [
        {
            "chapter": 36,
            "verse": 55,
            "text": "Inna as-haba aljannati alyawmafee shughulin fakihoon"
        },
        {
            "chapter": 36,
            "verse": 56,
            "text": "Hum waazwajuhum fee thilalinAAala al-ara-iki muttaki-oon"
        },
        {
            "chapter": 36,
            "verse": 57,
            "text": "Lahum feeha fakihatun walahumma yaddaAAoon"
        },
        {
            "chapter": 36,
            "verse": 58,
            "text": "Salamun qawlan min rabbin raheem"
        },
        {
            "chapter": 36,
            "verse": 59,
            "text": "Wamtazoo alyawma ayyuhaalmujrimoon"
        },
        {
            "chapter": 36,
            "verse": 60,
            "text": "Alam aAAhad ilaykum ya banee adamaan la taAAbudoo ashshaytana innahu lakumAAaduwwun mubeen"
        },
        {
            "chapter": 36,
            "verse": 61,
            "text": "Waani oAAbudoonee hatha siratunmustaqeem"
        },
        {
            "chapter": 36,
            "verse": 62,
            "text": "Walaqad adalla minkum jibillankatheeran afalam takoonoo taAAqiloon"
        },
        {
            "chapter": 36,
            "verse": 63,
            "text": "Hathihi jahannamu allatee kuntumtooAAadoon"
        },
        {
            "chapter": 36,
            "verse": 64,
            "text": "Islawha alyawma bimakuntum takfuroon"
        },
        {
            "chapter": 36,
            "verse": 65,
            "text": "Alyawma nakhtimu AAala afwahihimwatukallimuna aydeehim watashhadu arjuluhum bima kanooyaksiboon"
        },
        {
            "chapter": 36,
            "verse": 66,
            "text": "Walaw nashao latamasnaAAala aAAyunihim fastabaqoo assiratafaanna yubsiroon"
        },
        {
            "chapter": 36,
            "verse": 67,
            "text": "Walaw nashao lamasakhnahumAAala makanatihim fama istataAAoo mudiyyanwala yarjiAAoon"
        },
        {
            "chapter": 36,
            "verse": 68,
            "text": "Waman nuAAammirhu nunakkis-hu fee alkhalqiafala yaAAqiloon"
        },
        {
            "chapter": 36,
            "verse": 69,
            "text": "Wama AAallamnahu ashshiAArawama yanbaghee lahu in huwa illa thikrunwaqur-anun mubeen"
        },
        {
            "chapter": 36,
            "verse": 70,
            "text": "Liyunthira man kana hayyanwayahiqqa alqawlu AAala alkafireen"
        }
    ]
}