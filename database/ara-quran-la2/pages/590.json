{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Wassama-i thatialburooj"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Walyawmi almawAAood"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Washahidin wamashhood"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Qutila as-habu alukhdood"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Annari thati alwaqood"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ith hum AAalayha quAAood"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Wahum AAala ma yafAAaloona bilmu/mineenashuhood"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Wama naqamoo minhum illa anyu/minoo billahi alAAazeezi alhameed"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Allathee lahu mulku assamawatiwal-ardi wallahu AAala kullishay-in shaheed"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inna allatheena fatanoo almu/mineenawalmu/minati thumma lam yatooboo falahum AAathabujahannama walahum AAathabu alhareeq"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inna allatheena amanoowaAAamiloo assalihati lahum jannatuntajree min tahtiha al-anharu thalikaalfawzu alkabeer"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inna batsha rabbika lashadeed"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innahu huwa yubdi-o wayuAAeed"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Wahuwa alghafooru alwadood"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Thoo alAAarshi almajeed"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "FaAAAAalun lima yureed"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hal ataka hadeethu aljunood"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "FirAAawna wathamood"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Bali allatheena kafaroo fee taktheeb"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Wallahu min wara-ihimmuheet"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bal huwa qur-anun majeed"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fee lawhin mahfooth"
        }
    ]
}