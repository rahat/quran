        {
            "chapter": 86,
            "verse": 1,
            "text": "Wassama-i wattariq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wama adraka ma attariq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Annajmu aththaqib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsin lamma AAalayha hafith"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Falyanthuri al-insanumimma khuliq"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa min ma-in dafiq"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju min bayni assulbi wattara-ib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahu AAala rajAAihi laqadir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tubla assara-ir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fama lahu min quwwatin wala nasir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Wassama-i thati arrajAA"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Wal-ardi thati assadAA"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahu laqawlun fasl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wama huwa bilhazl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kayda"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Waakeedu kayda"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhili alkafireena amhilhum ruwayda"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbihi isma rabbika al-aAAla"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Allathee khalaqa fasawwa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wallathee qaddara fahada"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wallathee akhraja almarAAa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "FajaAAalahu ghuthaan ahwa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri-oka fala tansa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illa ma shaa Allahuinnahu yaAAlamu aljahra wama yakhfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wanuyassiruka lilyusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fathakkir in nafaAAati aththikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sayaththakkaru man yakhsha"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wayatajannabuha al-ashqa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Allathee yasla annaraalkubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Thumma la yamootu feeha walayahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad aflaha man tazakka"
        },
        {
