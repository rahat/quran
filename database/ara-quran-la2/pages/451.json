{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fa-innahum lamuhdaroon"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahialmukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseen"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa-inna lootan lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahuajmaAAeen"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireen"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna al-akhareen"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa-innakum latamurroona AAalayhim musbiheen"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabillayli afala taAAqiloon"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa-inna yoonusa lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhoon"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeen"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu alhootu wahuwamuleem"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana minaalmusabbiheen"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ilayawmi yubAAathoon"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bilAAara-iwahuwa saqeem"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan minyaqteen"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila mi-ati alfinaw yazeedoon"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ilaheen"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim alirabbika albanatuwalahumu albanoon"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almala-ikata inathanwahum shahidoon"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloon"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wa-innahum lakathiboon"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAalaalbaneen"
        }
    ]
}