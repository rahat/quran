{
    "pages": [
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahuliljabeen"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheem"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta arru/yainna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalaoalmubeen"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhinAAatheem"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheem"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu bi-ishaqanabiyyan mina assaliheen"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAalaishaqa wamin thurriyyatihima muhsinunwathalimun linafsihi mubeen"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosawaharoon"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahumamina alkarbi alAAatheem"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoohumu alghalibeen"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitabaalmustabeen"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma assirataalmustaqeem"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee al-akhireen"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosawaharoon"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineen"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadinaalmu/mineen"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa-inna ilyasa lamina almursaleen"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi alatattaqoon"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsanaalkhaliqeen"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba aba-ikumual-awwaleen"
        }
    ]
}