{
    "pages": [
        {
            "chapter": 51,
            "verse": 7,
            "text": "Wassama-i thati alhubuk"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Innakum lafee qawlin mukhtalif"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Yu/faku AAanhu man ofik"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Qutila alkharrasoon"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Allatheena hum fee ghamratin sahoon"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Yas-aloona ayyana yawmu addeen"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Yawma hum AAala annariyuftanoon"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Thooqoo fitnatakum hatha allatheekuntum bihi tastaAAjiloon"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Inna almuttaqeena fee jannatinwaAAuyoon"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Akhitheena ma atahumrabbuhum innahum kanoo qabla thalika muhsineen"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Kanoo qaleelan mina allayli mayahjaAAoon"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Wabil-ashari humyastaghfiroon"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Wafee amwalihim haqqun lissa-iliwalmahroom"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Wafee al-ardi ayatunlilmooqineen"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Wafee anfusikum afala tubsiroon"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Wafee assama-i rizqukum wamatooAAadoon"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Fawarabbi assama-i wal-ardiinnahu lahaqqun mithla ma annakum tantiqoon"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "Hal ataka hadeethu dayfiibraheema almukrameen"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "Ith dakhaloo AAalayhi faqaloosalaman qala salamun qawmun munkaroon"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "Faragha ila ahlihi fajaabiAAijlin sameen"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "Faqarrabahu ilayhim qala alata/kuloon"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "Faawjasa minhum kheefatan qaloo latakhaf wabashsharoohu bighulamin AAaleem"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "Faaqbalati imraatuhu fee sarratin fasakkatwajhaha waqalat AAajoozun AAaqeem"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "Qaloo kathaliki qalarabbuki innahu huwa alhakeemu alAAaleem"
        }
    ]
}