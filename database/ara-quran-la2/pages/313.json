{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Waana ikhtartuka fastamiAAlima yooha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innanee ana Allahu lailaha illa ana faAAbudnee waaqimi assalatalithikree"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna assaAAata atiyatunakadu okhfeeha litujza kullu nafsin bimatasAAa"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fala yasuddannaka AAanhaman la yu/minu biha wattabaAAa hawahufatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wama tilka biyameenika ya moosa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qala hiya AAasaya atawakkaoAAalayha waahushshu biha AAala ghanameewaliya feeha maaribu okhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qala alqiha ya moosa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Faalqaha fa-itha hiya hayyatuntasAAa"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qala khuthha walatakhaf sanuAAeeduha seerataha al-oola"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Wadmum yadaka ila janahikatakhruj baydaa min ghayri soo-in ayatan okhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka min ayatinaalkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qala rabbi ishrah lee sadree"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wayassir lee amree"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Wahlul AAuqdatan min lisanee"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahoo qawlee"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "WajAAal lee wazeeran min ahlee"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Haroona akhee"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Oshdud bihi azree"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Waashrik-hu fee amree"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay nusabbihaka katheera"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wanathkuraka katheera"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka kunta bina baseera"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qala qad ooteeta su/laka yamoosa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Walaqad mananna AAalayka marratanokhra"
        }
    ]
}