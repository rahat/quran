{
    "pages": [
        {
            "chapter": 26,
            "verse": 40,
            "text": "LaAAallana nattabiAAu assaharatain kanoo humu alghalibeen"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma jaa assaharatuqaloo lifirAAawna a-inna lana laajran in kunnanahnu alghalibeen"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala naAAam wa-innakum ithanlamina almuqarrabeen"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala lahum moosa alqoo maantum mulqoon"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Faalqaw hibalahum waAAisiyyahumwaqaloo biAAizzati firAAawna inna lanahnualghaliboon"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Faalqa moosa AAasahufa-itha hiya talqafu ma ya/fikoon"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Faolqiya assaharatu sajideen"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qaloo amanna birabbialAAalameen"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi moosa waharoon"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala amantum lahu qabla an athanalakum innahu lakabeerukumu allathee AAallamakumu assihrafalasawfa taAAlamoona laoqattiAAanna aydiyakum waarjulakummin khilafin walaosallibannakum ajmaAAeen"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qaloo la dayra innaila rabbina munqaliboon"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna natmaAAu an yaghfira lanarabbuna khatayana an kunnaawwala almu/mineen"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Waawhayna ila moosaan asri biAAibadee innakum muttabaAAoon"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almada-ini hashireen"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haola-i lashirthimatunqaleeloon"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa-innahum lana lagha-ithoon"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa-inna lajameeAAun hathiroon"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatinwaAAuyoon"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareem"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnahabanee isra-eel"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeen"
        }
    ]
}