{
    "pages": [
        {
            "chapter": 23,
            "verse": 90,
            "text": "Bal ataynahum bilhaqqiwa-innahum lakathiboon"
        },
        {
            "chapter": 23,
            "verse": 91,
            "text": "Ma ittakhatha Allahumin waladin wama kana maAAahu min ilahin ithanlathahaba kullu ilahin bima khalaqa walaAAalabaAAduhum AAala baAAdin subhana AllahiAAamma yasifoon"
        },
        {
            "chapter": 23,
            "verse": 92,
            "text": "AAalimi alghaybi washshahadatifataAAala AAamma yushrikoon"
        },
        {
            "chapter": 23,
            "verse": 93,
            "text": "Qul rabbi imma turiyannee mayooAAadoon"
        },
        {
            "chapter": 23,
            "verse": 94,
            "text": "Rabbi fala tajAAalnee fee alqawmi aththalimeen"
        },
        {
            "chapter": 23,
            "verse": 95,
            "text": "Wa-inna AAala an nuriyaka manaAAiduhum laqadiroon"
        },
        {
            "chapter": 23,
            "verse": 96,
            "text": "IdfaAA billatee hiya ahsanu assayyi-atanahnu aAAlamu bima yasifoon"
        },
        {
            "chapter": 23,
            "verse": 97,
            "text": "Waqul rabbi aAAoothu bika min hamazatiashshayateen"
        },
        {
            "chapter": 23,
            "verse": 98,
            "text": "WaaAAoothu bika rabbi an yahduroon"
        },
        {
            "chapter": 23,
            "verse": 99,
            "text": "Hatta itha jaa ahadahumualmawtu qala rabbi irjiAAoon"
        },
        {
            "chapter": 23,
            "verse": 100,
            "text": "LaAAallee aAAmalu salihanfeema taraktu kalla innaha kalimatun huwa qa-iluhawamin wara-ihim barzakhun ila yawmi yubAAathoon"
        },
        {
            "chapter": 23,
            "verse": 101,
            "text": "Fa-itha nufikha fee assoorifala ansaba baynahum yawma-ithin walayatasaaloon"
        },
        {
            "chapter": 23,
            "verse": 102,
            "text": "Faman thaqulat mawazeenuhu faola-ikahumu almuflihoon"
        },
        {
            "chapter": 23,
            "verse": 103,
            "text": "Waman khaffat mawazeenuhu faola-ikaallatheena khasiroo anfusahum fee jahannama khalidoon"
        },
        {
            "chapter": 23,
            "verse": 104,
            "text": "Talfahu wujoohahumu annaruwahum feeha kalihoon"
        }
    ]
