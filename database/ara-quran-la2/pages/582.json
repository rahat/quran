{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "AAamma yatasaaloon"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "AAani annaba-i alAAatheem"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Allathee hum feehi mukhtalifoon"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kalla sayaAAlamoon"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Thumma kalla sayaAAlamoon"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Alam najAAali al-arda mihada"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Waljibala awtada"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Wakhalaqnakum azwaja"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "WajaAAalna nawmakum subata"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "WajaAAalna allayla libasa"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "WajaAAalna annaharamaAAasha"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Wabanayna fawqakum sabAAan shidada"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "WajaAAalna sirajan wahhaja"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Waanzalna mina almuAAsiratimaan thajjaja"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Linukhrija bihi habban wanabata"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Wajannatin alfafa"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inna yawma alfasli kana meeqata"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yawma yunfakhu fee assoorifata/toona afwaja"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Wafutihati assamao fakanatabwaba"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Wasuyyirati aljibalu fakanatsaraba"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inna jahannama kanat mirsada"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Littagheena maaba"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Labitheena feeha ahqaba"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "La yathooqoona feehabardan wala sharaba"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illa hameeman waghassaqa"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jazaan wifaqa"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innahum kanoo la yarjoona hisaba"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Wakaththaboo bi-ayatinakiththaba"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wakulla shay-in ahsaynahu kitaba"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fathooqoo falan nazeedakum illaAAathaba"
        }
    ]
