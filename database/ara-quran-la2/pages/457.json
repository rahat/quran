{
    "pages": [
        {
            "chapter": 38,
            "verse": 62,
            "text": "Waqaloo ma lana lanara rijalan kunna naAAudduhum mina al-ashrar"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "Attakhathnahum sikhriyyan am zaghatAAanhumu al-absar"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "Inna thalika lahaqqun takhasumuahli annar"
        },
        {
            "chapter": 38,
            "verse": 65,
            "text": "Qul innama ana munthirunwama min ilahin illa Allahu alwahidualqahhar"
        },
        {
            "chapter": 38,
            "verse": 66,
            "text": "Rabbu assamawati wal-ardiwama baynahuma alAAazeezu alghaffar"
        },
        {
            "chapter": 38,
            "verse": 67,
            "text": "Qul huwa nabaon AAatheem"
        },
        {
            "chapter": 38,
            "verse": 68,
            "text": "Antum AAanhu muAAridoon"
        },
        {
            "chapter": 38,
            "verse": 69,
            "text": "Ma kana liya min AAilmin bilmala-ial-aAAla ith yakhtasimoon"
        },
        {
            "chapter": 38,
            "verse": 70,
            "text": "In yooha ilayya illa annamaana natheerun mubeen"
        },
        {
            "chapter": 38,
            "verse": 71,
            "text": "Ith qala rabbuka lilmala-ikatiinnee khaliqun basharan min teen"
        },
        {
            "chapter": 38,
            "verse": 72,
            "text": "Fa-itha sawwaytuhu wanafakhtu feehimin roohee faqaAAoo lahu sajideen"
        },
        {
            "chapter": 38,
            "verse": 73,
            "text": "Fasajada almala-ikatu kulluhumajmaAAoon"
        },
        {
            "chapter": 38,
            "verse": 74,
            "text": "Illa ibleesa istakbara wakanamina alkafireen"
        },
        {
            "chapter": 38,
            "verse": 75,
            "text": "Qala ya ibleesu mamanaAAaka an tasjuda lima khalaqtu biyadayya astakbarta amkunta mina alAAaleen"
        },
        {
            "chapter": 38,
            "verse": 76,
            "text": "Qala ana khayrun minhukhalaqtanee min narin wakhalaqtahu min teen"
        },
        {
            "chapter": 38,
            "verse": 77,
            "text": "Qala fakhruj minhafa-innaka rajeem"
        },
        {
            "chapter": 38,
            "verse": 78,
            "text": "Wa-inna AAalayka laAAnatee ila yawmiaddeen"
        },
        {
            "chapter": 38,
            "verse": 79,
            "text": "Qala rabbi faanthirneeila yawmi yubAAathoon"
        },
        {
            "chapter": 38,
            "verse": 80,
            "text": "Qala fa-innaka mina almunthareen"
        },
        {
            "chapter": 38,
            "verse": 81,
            "text": "Ila yawmi alwaqti almaAAloom"
        },
        {
            "chapter": 38,
            "verse": 82,
            "text": "Qala fabiAAizzatika laoghwiyannahumajmaAAeen"
        },
        {
            "chapter": 38,
            "verse": 83,
            "text": "Illa AAibadaka minhumualmukhlaseen"
        }
    ]
}