{
    "pages": [
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kalla inna kitaba alfujjarilafee sijjeen"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Wama adraka ma sijjeen"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitabun marqoom"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Waylun yawma-ithin lilmukaththibeen"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Allatheena yukaththiboonabiyawmi addeen"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Wama yukaththibu bihi illakullu muAAtadin atheem"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Itha tutla AAalayhi ayatunaqala asateeru al-awwaleen"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kalla bal rana AAalaquloobihim ma kanoo yaksiboon"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kalla innahum AAan rabbihim yawma-ithinlamahjooboon"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Thumma innahum lasaloo aljaheem"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Thumma yuqalu hatha allatheekuntum bihi tukaththiboon"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kalla inna kitaba al-abrarilafee AAilliyyeen"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Wama adraka maAAilliyyoon"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitabun marqoom"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yashhaduhu almuqarraboon"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Inna al-abrara lafee naAAeem"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "AAala al-ara-iki yanthuroon"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "TaAArifu fee wujoohihim nadrata annaAAeem"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yusqawna min raheeqin makhtoom"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Khitamuhu miskun wafee thalikafalyatanafasi almutanafisoon"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Wamizajuhu min tasneem"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "AAaynan yashrabu biha almuqarraboon"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Inna allatheena ajramoo kanoomina allatheena amanoo yadhakoon"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Wa-itha marroo bihim yataghamazoon"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Wa-itha inqalaboo ila ahlihimuinqalaboo fakiheen"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Wa-itha raawhum qaloo inna haola-iladalloon"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Wama orsiloo AAalayhim hafitheen"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Falyawma allatheena amanoomina alkuffari yadhakoon"
        }
    ]
}