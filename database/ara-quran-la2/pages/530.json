    "pages": [
        {
            "chapter": 54,
            "verse": 28,
            "text": "Wanabbi/hum anna almaa qismatunbaynahum kullu shirbin muhtadar"
        },
        {
            "chapter": 54,
            "verse": 29,
            "text": "Fanadaw sahibahum fataAAatafaAAaqar"
        },
        {
            "chapter": 54,
            "verse": 30,
            "text": "Fakayfa kana AAathabee wanuthur"
        },
        {
            "chapter": 54,
            "verse": 31,
            "text": "Inna arsalna AAalayhim sayhatanwahidatan fakanoo kahasheemi almuhtathir"
        },
        {
            "chapter": 54,
            "verse": 32,
            "text": "Walaqad yassarna alqur-ana liththikrifahal min muddakir"
        },
        {
            "chapter": 54,
            "verse": 33,
            "text": "Kaththabat qawmu lootin binnuthur"
        },
        {
            "chapter": 54,
            "verse": 34,
            "text": "Inna arsalna AAalayhim hasibanilla ala lootin najjaynahum bisahar"
        },
        {
            "chapter": 54,
            "verse": 35,
            "text": "NiAAmatan min AAindina kathalikanajzee man shakar"
        },
        {
            "chapter": 54,
            "verse": 36,
            "text": "Walaqad antharahum batshatanafatamaraw binnuthur"
        },
        {
            "chapter": 54,
            "verse": 37,
            "text": "Walaqad rawadoohu AAan dayfihifatamasna aAAyunahum fathooqoo AAathabeewanuthur"
        },
        {
            "chapter": 54,
            "verse": 38,
            "text": "Walaqad sabbahahum bukratanAAathabun mustaqirr"
        },
        {
            "chapter": 54,
            "verse": 39,
            "text": "Fathooqoo AAathabee wanuthur"
        },
        {
            "chapter": 54,
            "verse": 40,
            "text": "Walaqad yassarna alqur-ana liththikrifahal min muddakir"
        },
        {
            "chapter": 54,
            "verse": 41,
            "text": "Walaqad jaa ala firAAawna annuthur"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Kaththaboo bi-ayatinakulliha faakhathnahum akhthaAAazeezin muqtadir"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Akuffarukum khayrun min ola-ikumam lakum baraatun fee azzubur"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Am yaqooloona nahnu jameeAAun muntasir"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "Sayuhzamu aljamAAu wayuwalloona addubur"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Bali assaAAatu mawAAiduhum wassaAAatuadha waamar"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Inna almujrimeena fee dalalinwasuAAur"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yawma yushaboona fee annariAAala wujoohihim thooqoo massa saqar"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Inna kulla shay-in khalaqnahubiqadar"
