{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Qad aflaha almu/minoon"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Allatheena hum fee salatihimkhashiAAoon"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Wallatheena hum AAani allaghwimuAAridoon"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Wallatheena hum lizzakatifaAAiloon"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Wallatheena hum lifuroojihim hafithoon"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Illa AAala azwajihim awma malakat aymanuhum fa-innahum ghayru maloomeen"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Famani ibtagha waraa thalikafaola-ika humu alAAadoon"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Wallatheena hum li-amanatihimwaAAahdihim raAAoon"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Wallatheena hum AAala salawatihimyuhafithoon"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Ola-ika humu alwarithoon"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Allatheena yarithoona alfirdawsa humfeeha khalidoon"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Walaqad khalaqna al-insana minsulalatin min teen"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Thumma jaAAalnahu nutfatan feeqararin makeen"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Thumma khalaqna annutfataAAalaqatan fakhalaqna alAAalaqata mudghatanfakhalaqna almudghata AAithamanfakasawna alAAithama lahmanthumma ansha/nahu khalqan akhara fatabarakaAllahu ahsanu alkhaliqeen"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Thumma innakum baAAda thalikalamayyitoon"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Thumma innakum yawma alqiyamatitubAAathoon"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Walaqad khalaqna fawqakum sabAAa tara-iqawama kunna AAani alkhalqi ghafileen"
        }
    ]
}