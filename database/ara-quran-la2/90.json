{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "La oqsimu bihatha albalad"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Waanta hillun bihatha albalad"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Wawalidin wama walad"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Laqad khalaqna al-insana feekabad"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ayahsabu an lan yaqdira AAalayhi ahad"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yaqoolu ahlaktu malan lubada"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ayahsabu an lam yarahu ahad"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Alam najAAal lahu AAaynayn"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Walisanan washafatayn"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Wahadaynahu annajdayn"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fala iqtahama alAAaqaba"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Wama adraka maalAAaqaba"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fakku raqaba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Aw itAAamun fee yawmin theemasghaba"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yateeman tha maqraba"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Aw miskeenan tha matraba"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Thumma kana mina allatheena amanoowatawasaw bissabri watawasaw bilmarhama"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ola-ika as-habualmaymana"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Wallatheena kafaroo bi-ayatinahum as-habu almash-ama"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "AAalayhim narun mu/sada"
        }
    ]
}