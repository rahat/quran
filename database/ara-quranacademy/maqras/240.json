{
    "hizbs": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞ أَفَلَا یَعۡلَمُ إِذَا بُعۡثِرَ مَا فِی ٱلۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِی ٱلصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ یَوۡمَئِذࣲ لَّخَبِیرُۢ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "مَا ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "یَوۡمَ یَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَ ٰزِینُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فَهُوَ فِی عِیشَةࣲ رَّاضِیَةࣲ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَ ٰزِینُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فَأُمُّهُۥ هَاوِیَةࣱ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا هِیَهۡ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نَارٌ حَامِیَةُۢ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡیَقِینِ"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لَتَرَوُنَّ ٱلۡجَحِیمَ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَیۡنَ ٱلۡیَقِینِ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثُمَّ لَتُسۡءَلُنَّ یَوۡمَئِذٍ عَنِ ٱلنَّعِیمِ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَٱلۡعَصۡرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ ٱلۡإِنسَـٰنَ لَفِی خُسۡرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "إِلَّا ٱلَّذِینَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ وَتَوَاصَوۡا۟ بِٱلۡحَقِّ وَتَوَاصَوۡا۟ بِٱلصَّبۡرِ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَیۡلࣱ لِّكُلِّ هُمَزَةࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ٱلَّذِی جَمَعَ مَالࣰا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "یَحۡسَبُ أَنَّ مَالَهُۥۤ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَیُنۢبَذَنَّ فِی ٱلۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ٱلَّتِی تَطَّلِعُ عَلَى ٱلۡأَفۡءِدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَیۡهِم مُّؤۡصَدَةࣱ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِی عَمَدࣲ مُّمَدَّدَةِۭ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "أَلَمۡ تَرَ كَیۡفَ فَعَلَ رَبُّكَ بِأَصۡحَـٰبِ ٱلۡفِیلِ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمۡ یَجۡعَلۡ كَیۡدَهُمۡ فِی تَضۡلِیلࣲ"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرۡسَلَ عَلَیۡهِمۡ طَیۡرًا أَبَابِیلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرۡمِیهِم بِحِجَارَةࣲ مِّن سِجِّیلࣲ"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمۡ كَعَصۡفࣲ مَّأۡكُولِۭ"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لِإِیلَـٰفِ قُرَیۡشٍ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "إِۦلَـٰفِهِمۡ رِحۡلَةَ ٱلشِّتَاۤءِ وَٱلصَّیۡفِ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَلۡیَعۡبُدُوا۟ رَبَّ هَـٰذَا ٱلۡبَیۡتِ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "ٱلَّذِیۤ أَطۡعَمَهُم مِّن جُوعࣲ وَءَامَنَهُم مِّنۡ خَوۡفِۭ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "أَرَءَیۡتَ ٱلَّذِی یُكَذِّبُ بِٱلدِّینِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَ ٰلِكَ ٱلَّذِی یَدُعُّ ٱلۡیَتِیمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِینِ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَیۡلࣱ لِّلۡمُصَلِّینَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ٱلَّذِینَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ٱلَّذِینَ هُمۡ یُرَاۤءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَیَمۡنَعُونَ ٱلۡمَاعُونَ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "إِنَّاۤ أَعۡطَیۡنَـٰكَ ٱلۡكَوۡثَرَ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَٱنۡحَرۡ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "إِنَّ شَانِئَكَ هُوَ ٱلۡأَبۡتَرُ"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قُلۡ یَـٰۤأَیُّهَا ٱلۡكَـٰفِرُونَ"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لَاۤ أَعۡبُدُ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "وَلَاۤ أَنتُمۡ عَـٰبِدُونَ مَاۤ أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "وَلَاۤ أَنَا۠ عَابِدࣱ مَّا عَبَدتُّمۡ"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "وَلَاۤ أَنتُمۡ عَـٰبِدُونَ مَاۤ أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لَكُمۡ دِینُكُمۡ وَلِیَ دِینِ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "إِذَا جَاۤءَ نَصۡرُ ٱللَّهِ وَٱلۡفَتۡحُ"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "وَرَأَیۡتَ ٱلنَّاسَ یَدۡخُلُونَ فِی دِینِ ٱللَّهِ أَفۡوَاجࣰا"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فَسَبِّحۡ بِحَمۡدِ رَبِّكَ وَٱسۡتَغۡفِرۡهُۚ إِنَّهُۥ كَانَ تَوَّابَۢا"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتۡ یَدَاۤ أَبِی لَهَبࣲ وَتَبَّ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَاۤ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَیَصۡلَىٰ نَارࣰا ذَاتَ لَهَبࣲ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِی جِیدِهَا حَبۡلࣱ مِّن مَّسَدِۭ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ٱللَّهُ ٱلصَّمَدُ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لَمۡ یَلِدۡ وَلَمۡ یُولَدۡ"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "وَلَمۡ یَكُن لَّهُۥ كُفُوًا أَحَدُۢ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِن شَرِّ مَا خَلَقَ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِن شَرِّ ٱلنَّفَّـٰثَـٰتِ فِی ٱلۡعُقَدِ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "إِلَـٰهِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ٱلَّذِی یُوَسۡوِسُ فِی صُدُورِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ"
        }
    ]
}