{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞ قَالُوۤا۟ أَنُؤۡمِنُ لَكَ وَٱتَّبَعَكَ ٱلۡأَرۡذَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلۡمِی بِمَا كَانُوا۟ یَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنۡ حِسَابُهُمۡ إِلَّا عَلَىٰ رَبِّیۖ لَوۡ تَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَاۤ أَنَا۠ بِطَارِدِ ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنۡ أَنَا۠ إِلَّا نَذِیرࣱ مُّبِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُوا۟ لَئِن لَّمۡ تَنتَهِ یَـٰنُوحُ لَتَكُونَنَّ مِنَ ٱلۡمَرۡجُومِینَ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوۡمِی كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَٱفۡتَحۡ بَیۡنِی وَبَیۡنَهُمۡ فَتۡحࣰا وَنَجِّنِی وَمَن مَّعِیَ مِنَ ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَیۡنَـٰهُ وَمَن مَّعَهُۥ فِی ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغۡرَقۡنَا بَعۡدُ ٱلۡبَاقِینَ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِیعٍ ءَایَةࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُم بَطَشۡتُمۡ جَبَّارِینَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَٱتَّقُوا۟ ٱلَّذِیۤ أَمَدَّكُم بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُم بِأَنۡعَـٰمࣲ وَبَنِینَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّـٰتࣲ وَعُیُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّیۤ أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ عَظِیمࣲ"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُوا۟ سَوَاۤءٌ عَلَیۡنَاۤ أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ ٱلۡوَ ٰعِظِینَ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنۡ هَـٰذَاۤ إِلَّا خُلُقُ ٱلۡأَوَّلِینَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِینَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهۡلَكۡنَـٰهُمۡۚ إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كَذَّبَتۡ ثَمُودُ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ صَـٰلِحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "أَتُتۡرَكُونَ فِی مَا هَـٰهُنَاۤ ءَامِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "فِی جَنَّـٰتࣲ وَعُیُونࣲ"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وَزُرُوعࣲ وَنَخۡلࣲ طَلۡعُهَا هَضِیمࣱ"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وَتَنۡحِتُونَ مِنَ ٱلۡجِبَالِ بُیُوتࣰا فَـٰرِهِینَ"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "وَلَا تُطِیعُوۤا۟ أَمۡرَ ٱلۡمُسۡرِفِینَ"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "ٱلَّذِینَ یُفۡسِدُونَ فِی ٱلۡأَرۡضِ وَلَا یُصۡلِحُونَ"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قَالُوۤا۟ إِنَّمَاۤ أَنتَ مِنَ ٱلۡمُسَحَّرِینَ"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "مَاۤ أَنتَ إِلَّا بَشَرࣱ مِّثۡلُنَا فَأۡتِ بِءَایَةٍ إِن كُنتَ مِنَ ٱلصَّـٰدِقِینَ"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قَالَ هَـٰذِهِۦ نَاقَةࣱ لَّهَا شِرۡبࣱ وَلَكُمۡ شِرۡبُ یَوۡمࣲ مَّعۡلُومࣲ"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "وَلَا تَمَسُّوهَا بِسُوۤءࣲ فَیَأۡخُذَكُمۡ عَذَابُ یَوۡمٍ عَظِیمࣲ"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فَعَقَرُوهَا فَأَصۡبَحُوا۟ نَـٰدِمِینَ"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فَأَخَذَهُمُ ٱلۡعَذَابُۚ إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "كَذَّبَتۡ قَوۡمُ لُوطٍ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ لُوطٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "أَتَأۡتُونَ ٱلذُّكۡرَانَ مِنَ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وَتَذَرُونَ مَا خَلَقَ لَكُمۡ رَبُّكُم مِّنۡ أَزۡوَ ٰجِكُمۚ بَلۡ أَنتُمۡ قَوۡمٌ عَادُونَ"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قَالُوا۟ لَئِن لَّمۡ تَنتَهِ یَـٰلُوطُ لَتَكُونَنَّ مِنَ ٱلۡمُخۡرَجِینَ"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قَالَ إِنِّی لِعَمَلِكُم مِّنَ ٱلۡقَالِینَ"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رَبِّ نَجِّنِی وَأَهۡلِی مِمَّا یَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فَنَجَّیۡنَـٰهُ وَأَهۡلَهُۥۤ أَجۡمَعِینَ"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "إِلَّا عَجُوزࣰا فِی ٱلۡغَـٰبِرِینَ"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثُمَّ دَمَّرۡنَا ٱلۡءَاخَرِینَ"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وَأَمۡطَرۡنَا عَلَیۡهِم مَّطَرࣰاۖ فَسَاۤءَ مَطَرُ ٱلۡمُنذَرِینَ"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "كَذَّبَ أَصۡحَـٰبُ لۡءَیۡكَةِ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "إِذۡ قَالَ لَهُمۡ شُعَیۡبٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        }
    ]
}