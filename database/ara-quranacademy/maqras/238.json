{
    "hizbs": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "۞ لَاۤ أُقۡسِمُ بِهَـٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَـٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَـٰنَ فِی كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَیَحۡسَبُ أَن لَّن یَقۡدِرَ عَلَیۡهِ أَحَدࣱ"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "یَقُولُ أَهۡلَكۡتُ مَالࣰا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَیَحۡسَبُ أَن لَّمۡ یَرَهُۥۤ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَیۡنَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانࣰا وَشَفَتَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَیۡنَـٰهُ ٱلنَّجۡدَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكُّ رَقَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ إِطۡعَـٰمࣱ فِی یَوۡمࣲ ذِی مَسۡغَبَةࣲ"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "یَتِیمࣰا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِینࣰا ذَا مَتۡرَبَةࣲ"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ ٱلَّذِینَ ءَامَنُوا۟ وَتَوَاصَوۡا۟ بِٱلصَّبۡرِ وَتَوَاصَوۡا۟ بِٱلۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُو۟لَـٰۤئِكَ أَصۡحَـٰبُ ٱلۡمَیۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَٱلَّذِینَ كَفَرُوا۟ بِءَایَـٰتِنَا هُمۡ أَصۡحَـٰبُ ٱلۡمَشۡءَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَیۡهِمۡ نَارࣱ مُّؤۡصَدَةُۢ"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَٱلَّیۡلِ إِذَا یَغۡشَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَٱلسَّمَاۤءِ وَمَا بَنَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسࣲ وَمَا سَوَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَاۤ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡیَـٰهَا"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَیۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا یَخَافُ عُقۡبَـٰهَا"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَٱلَّیۡلِ إِذَا یَغۡشَىٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰۤ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعۡیَكُمۡ لَشَتَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُیَسِّرُهُۥ لِلۡیُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُیَسِّرُهُۥ لِلۡعُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا یُغۡنِی عَنۡهُ مَالُهُۥۤ إِذَا تَرَدَّىٰۤ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَیۡنَا لَلۡهُدَىٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلۡءَاخِرَةَ وَٱلۡأُولَىٰ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرۡتُكُمۡ نَارࣰا تَلَظَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا یَصۡلَىٰهَاۤ إِلَّا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ٱلَّذِی كَذَّبَ وَتَوَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَیُجَنَّبُهَا ٱلۡأَتۡقَى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ٱلَّذِی یُؤۡتِی مَالَهُۥ یَتَزَكَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةࣲ تُجۡزَىٰۤ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا ٱبۡتِغَاۤءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوۡفَ یَرۡضَىٰ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَٱلضُّحَىٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَٱلَّیۡلِ إِذَا سَجَىٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡءَاخِرَةُ خَیۡرࣱ لَّكَ مِنَ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ یُعۡطِیكَ رَبُّكَ فَتَرۡضَىٰۤ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمۡ یَجِدۡكَ یَتِیمࣰا فَءَاوَىٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَاۤلࣰّا فَهَدَىٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَاۤئِلࣰا فَأَغۡنَىٰ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا ٱلۡیَتِیمَ فَلَا تَقۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا ٱلسَّاۤئِلَ فَلَا تَنۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
        }
    ]
}