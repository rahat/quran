{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَٱلنَّـٰزِعَـٰتِ غَرۡقࣰا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَٱلنَّـٰشِطَـٰتِ نَشۡطࣰا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَٱلسَّـٰبِحَـٰتِ سَبۡحࣰا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَٱلسَّـٰبِقَـٰتِ سَبۡقࣰا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَٱلۡمُدَبِّرَ ٰتِ أَمۡرࣰا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "یَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوبࣱ یَوۡمَئِذࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَـٰرُهَا خَـٰشِعَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "یَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِی ٱلۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَءِذَا كُنَّا عِظَـٰمࣰا نَّخِرَةࣰ"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُوا۟ تِلۡكَ إِذࣰا كَرَّةٌ خَاسِرَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِیَ زَجۡرَةࣱ وَ ٰحِدَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِیثُ مُوسَىٰۤ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰۤ أَن تَزَكَّىٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِیَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَرَىٰهُ ٱلۡءَایَةَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصَىٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ یَسۡعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادَىٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡءَاخِرَةِ وَٱلۡأُولَىٰۤ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِی ذَ ٰلِكَ لَعِبۡرَةࣰ لِّمَن یَخۡشَىٰۤ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَاۤءُۚ بَنَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَأَغۡطَشَ لَیۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَ ٰلِكَ دَحَىٰهَاۤ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "أَخۡرَجَ مِنۡهَا مَاۤءَهَا وَمَرۡعَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَـٰعࣰا لَّكُمۡ وَلِأَنۡعَـٰمِكُمۡ"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَإِذَا جَاۤءَتِ ٱلطَّاۤمَّةُ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "یَوۡمَ یَتَذَكَّرُ ٱلۡإِنسَـٰنُ مَا سَعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ ٱلۡجَحِیمُ لِمَن یَرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَأَمَّا مَن طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَءَاثَرَ ٱلۡحَیَوٰةَ ٱلدُّنۡیَا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَإِنَّ ٱلۡجَحِیمَ هِیَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَإِنَّ ٱلۡجَنَّةَ هِیَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "یَسۡءَلُونَكَ عَنِ ٱلسَّاعَةِ أَیَّانَ مُرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِیمَ أَنتَ مِن ذِكۡرَىٰهَاۤ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَاۤ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "إِنَّمَاۤ أَنتَ مُنذِرُ مَن یَخۡشَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَأَنَّهُمۡ یَوۡمَ یَرَوۡنَهَا لَمۡ یَلۡبَثُوۤا۟ إِلَّا عَشِیَّةً أَوۡ ضُحَىٰهَا"
        }
    ]
}