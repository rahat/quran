{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "كَلَّا وَٱلۡقَمَرِ"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "وَٱلَّیۡلِ إِذۡ أَدۡبَرَ"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "وَٱلصُّبۡحِ إِذَاۤ أَسۡفَرَ"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "نَذِیرࣰا لِّلۡبَشَرِ"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "لِمَن شَاۤءَ مِنكُمۡ أَن یَتَقَدَّمَ أَوۡ یَتَأَخَّرَ"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِینَةٌ"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "إِلَّاۤ أَصۡحَـٰبَ ٱلۡیَمِینِ"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "فِی جَنَّـٰتࣲ یَتَسَاۤءَلُونَ"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "عَنِ ٱلۡمُجۡرِمِینَ"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "مَا سَلَكَكُمۡ فِی سَقَرَ"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "قَالُوا۟ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّینَ"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِینَ"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَاۤئِضِینَ"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "وَكُنَّا نُكَذِّبُ بِیَوۡمِ ٱلدِّینِ"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "حَتَّىٰۤ أَتَىٰنَا ٱلۡیَقِینُ"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "فَمَا تَنفَعُهُمۡ شَفَـٰعَةُ ٱلشَّـٰفِعِینَ"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِینَ"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "كَأَنَّهُمۡ حُمُرࣱ مُّسۡتَنفِرَةࣱ"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "فَرَّتۡ مِن قَسۡوَرَةِۭ"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بَلۡ یُرِیدُ كُلُّ ٱمۡرِئࣲ مِّنۡهُمۡ أَن یُؤۡتَىٰ صُحُفࣰا مُّنَشَّرَةࣰ"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "كَلَّاۖ بَل لَّا یَخَافُونَ ٱلۡءَاخِرَةَ"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "كَلَّاۤ إِنَّهُۥ تَذۡكِرَةࣱ"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "فَمَن شَاۤءَ ذَكَرَهُۥ"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "وَمَا یَذۡكُرُونَ إِلَّاۤ أَن یَشَاۤءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱلتَّقۡوَىٰ وَأَهۡلُ ٱلۡمَغۡفِرَةِ"
        }
    ]
}