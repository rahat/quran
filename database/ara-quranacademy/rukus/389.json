{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّیۡنَـٰهُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَرۡبِ ٱلۡعَظِیمِ"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرۡنَـٰهُمۡ فَكَانُوا۟ هُمُ ٱلۡغَـٰلِبِینَ"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَیۡنَـٰهُمَا ٱلۡكِتَـٰبَ ٱلۡمُسۡتَبِینَ"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَیۡنَـٰهُمَا ٱلصِّرَ ٰطَ ٱلۡمُسۡتَقِیمَ"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكۡنَا عَلَیۡهِمَا فِی ٱلۡءَاخِرِینَ"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَ ٰلِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلۡیَاسَ لَمِنَ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذۡ قَالَ لِقَوۡمِهِۦۤ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدۡعُونَ بَعۡلࣰا وَتَذَرُونَ أَحۡسَنَ ٱلۡخَـٰلِقِینَ"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ٱللَّهَ رَبَّكُمۡ وَرَبَّ ءَابَاۤئِكُمُ ٱلۡأَوَّلِینَ"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡءَاخِرِینَ"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَـٰمٌ عَلَىٰۤ إِلۡ یَاسِینَ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَ ٰلِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوطࣰا لَّمِنَ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذۡ نَجَّیۡنَـٰهُ وَأَهۡلَهُۥۤ أَجۡمَعِینَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوزࣰا فِی ٱلۡغَـٰبِرِینَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرۡنَا ٱلۡءَاخَرِینَ"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَیۡهِم مُّصۡبِحِینَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِٱلَّیۡلِۚ أَفَلَا تَعۡقِلُونَ"
        }
    ]
