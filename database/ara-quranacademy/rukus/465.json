{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "وَلَقَدۡ جَاۤءَ ءَالَ فِرۡعَوۡنَ ٱلنُّذُرُ"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "كَذَّبُوا۟ بِءَایَـٰتِنَا كُلِّهَا فَأَخَذۡنَـٰهُمۡ أَخۡذَ عَزِیزࣲ مُّقۡتَدِرٍ"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "أَكُفَّارُكُمۡ خَیۡرࣱ مِّنۡ أُو۟لَـٰۤئِكُمۡ أَمۡ لَكُم بَرَاۤءَةࣱ فِی ٱلزُّبُرِ"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "أَمۡ یَقُولُونَ نَحۡنُ جَمِیعࣱ مُّنتَصِرࣱ"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "سَیُهۡزَمُ ٱلۡجَمۡعُ وَیُوَلُّونَ ٱلدُّبُرَ"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "بَلِ ٱلسَّاعَةُ مَوۡعِدُهُمۡ وَٱلسَّاعَةُ أَدۡهَىٰ وَأَمَرُّ"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "إِنَّ ٱلۡمُجۡرِمِینَ فِی ضَلَـٰلࣲ وَسُعُرࣲ"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "یَوۡمَ یُسۡحَبُونَ فِی ٱلنَّارِ عَلَىٰ وُجُوهِهِمۡ ذُوقُوا۟ مَسَّ سَقَرَ"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "إِنَّا كُلَّ شَیۡءٍ خَلَقۡنَـٰهُ بِقَدَرࣲ"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "وَمَاۤ أَمۡرُنَاۤ إِلَّا وَ ٰحِدَةࣱ كَلَمۡحِۭ بِٱلۡبَصَرِ"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "وَلَقَدۡ أَهۡلَكۡنَاۤ أَشۡیَاعَكُمۡ فَهَلۡ مِن مُّدَّكِرࣲ"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "وَكُلُّ شَیۡءࣲ فَعَلُوهُ فِی ٱلزُّبُرِ"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "وَكُلُّ صَغِیرࣲ وَكَبِیرࣲ مُّسۡتَطَرٌ"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "إِنَّ ٱلۡمُتَّقِینَ فِی جَنَّـٰتࣲ وَنَهَرࣲ"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "فِی مَقۡعَدِ صِدۡقٍ عِندَ مَلِیكࣲ مُّقۡتَدِرِۭ"
        }
    ]
}