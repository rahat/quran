{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِیعٍ ءَایَةࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُم بَطَشۡتُمۡ جَبَّارِینَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَٱتَّقُوا۟ ٱلَّذِیۤ أَمَدَّكُم بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُم بِأَنۡعَـٰمࣲ وَبَنِینَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّـٰتࣲ وَعُیُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّیۤ أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ عَظِیمࣲ"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُوا۟ سَوَاۤءٌ عَلَیۡنَاۤ أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ ٱلۡوَ ٰعِظِینَ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنۡ هَـٰذَاۤ إِلَّا خُلُقُ ٱلۡأَوَّلِینَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِینَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهۡلَكۡنَـٰهُمۡۚ إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        }
    ]
}