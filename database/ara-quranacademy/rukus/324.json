{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "كَذَّبَتۡ قَوۡمُ لُوطٍ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ لُوطٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "إِنِّی لَكُمۡ رَسُولٌ أَمِینࣱ"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِیعُونِ"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وَمَاۤ أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِیَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "أَتَأۡتُونَ ٱلذُّكۡرَانَ مِنَ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وَتَذَرُونَ مَا خَلَقَ لَكُمۡ رَبُّكُم مِّنۡ أَزۡوَ ٰجِكُمۚ بَلۡ أَنتُمۡ قَوۡمٌ عَادُونَ"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قَالُوا۟ لَئِن لَّمۡ تَنتَهِ یَـٰلُوطُ لَتَكُونَنَّ مِنَ ٱلۡمُخۡرَجِینَ"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قَالَ إِنِّی لِعَمَلِكُم مِّنَ ٱلۡقَالِینَ"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رَبِّ نَجِّنِی وَأَهۡلِی مِمَّا یَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فَنَجَّیۡنَـٰهُ وَأَهۡلَهُۥۤ أَجۡمَعِینَ"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "إِلَّا عَجُوزࣰا فِی ٱلۡغَـٰبِرِینَ"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثُمَّ دَمَّرۡنَا ٱلۡءَاخَرِینَ"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وَأَمۡطَرۡنَا عَلَیۡهِم مَّطَرࣰاۖ فَسَاۤءَ مَطَرُ ٱلۡمُنذَرِینَ"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "إِنَّ فِی ذَ ٰلِكَ لَءَایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        }
    ]
}