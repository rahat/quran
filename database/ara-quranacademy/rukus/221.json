{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "إِنَّ ٱلۡمُتَّقِینَ فِی جَنَّـٰتࣲ وَعُیُونٍ"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "ٱدۡخُلُوهَا بِسَلَـٰمٍ ءَامِنِینَ"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "وَنَزَعۡنَا مَا فِی صُدُورِهِم مِّنۡ غِلٍّ إِخۡوَ ٰنًا عَلَىٰ سُرُرࣲ مُّتَقَـٰبِلِینَ"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "لَا یَمَسُّهُمۡ فِیهَا نَصَبࣱ وَمَا هُم مِّنۡهَا بِمُخۡرَجِینَ"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "۞ نَبِّئۡ عِبَادِیۤ أَنِّیۤ أَنَا ٱلۡغَفُورُ ٱلرَّحِیمُ"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "وَأَنَّ عَذَابِی هُوَ ٱلۡعَذَابُ ٱلۡأَلِیمُ"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "وَنَبِّئۡهُمۡ عَن ضَیۡفِ إِبۡرَ ٰهِیمَ"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "إِذۡ دَخَلُوا۟ عَلَیۡهِ فَقَالُوا۟ سَلَـٰمࣰا قَالَ إِنَّا مِنكُمۡ وَجِلُونَ"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "قَالُوا۟ لَا تَوۡجَلۡ إِنَّا نُبَشِّرُكَ بِغُلَـٰمٍ عَلِیمࣲ"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "قَالَ أَبَشَّرۡتُمُونِی عَلَىٰۤ أَن مَّسَّنِیَ ٱلۡكِبَرُ فَبِمَ تُبَشِّرُونَ"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "قَالُوا۟ بَشَّرۡنَـٰكَ بِٱلۡحَقِّ فَلَا تَكُن مِّنَ ٱلۡقَـٰنِطِینَ"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "قَالَ وَمَن یَقۡنَطُ مِن رَّحۡمَةِ رَبِّهِۦۤ إِلَّا ٱلضَّاۤلُّونَ"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "قَالَ فَمَا خَطۡبُكُمۡ أَیُّهَا ٱلۡمُرۡسَلُونَ"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "قَالُوۤا۟ إِنَّاۤ أُرۡسِلۡنَاۤ إِلَىٰ قَوۡمࣲ مُّجۡرِمِینَ"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "إِلَّاۤ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمۡ أَجۡمَعِینَ"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "إِلَّا ٱمۡرَأَتَهُۥ قَدَّرۡنَاۤ إِنَّهَا لَمِنَ ٱلۡغَـٰبِرِینَ"
        }
    ]
}