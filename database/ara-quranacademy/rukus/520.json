{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَٱلنَّـٰزِعَـٰتِ غَرۡقࣰا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَٱلنَّـٰشِطَـٰتِ نَشۡطࣰا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَٱلسَّـٰبِحَـٰتِ سَبۡحࣰا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَٱلسَّـٰبِقَـٰتِ سَبۡقࣰا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَٱلۡمُدَبِّرَ ٰتِ أَمۡرࣰا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "یَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوبࣱ یَوۡمَئِذࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَـٰرُهَا خَـٰشِعَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "یَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِی ٱلۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَءِذَا كُنَّا عِظَـٰمࣰا نَّخِرَةࣰ"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُوا۟ تِلۡكَ إِذࣰا كَرَّةٌ خَاسِرَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِیَ زَجۡرَةࣱ وَ ٰحِدَةࣱ"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِیثُ مُوسَىٰۤ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰۤ أَن تَزَكَّىٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِیَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَرَىٰهُ ٱلۡءَایَةَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصَىٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ یَسۡعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادَىٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡءَاخِرَةِ وَٱلۡأُولَىٰۤ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِی ذَ ٰلِكَ لَعِبۡرَةࣰ لِّمَن یَخۡشَىٰۤ"
        }
    ]
}