{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَٱلۡعَـٰدِیَـٰتِ ضَبۡحࣰا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَٱلۡمُورِیَـٰتِ قَدۡحࣰا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَٱلۡمُغِیرَ ٰتِ صُبۡحࣰا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡعࣰا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ ٱلۡإِنسَـٰنَ لِرَبِّهِۦ لَكَنُودࣱ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَ ٰلِكَ لَشَهِیدࣱ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَیۡرِ لَشَدِیدٌ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞ أَفَلَا یَعۡلَمُ إِذَا بُعۡثِرَ مَا فِی ٱلۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِی ٱلصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ یَوۡمَئِذࣲ لَّخَبِیرُۢ"
        }
    ]
}