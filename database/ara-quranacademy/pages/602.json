            "text": "لِإِیلَـٰفِ قُرَیۡشٍ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "إِۦلَـٰفِهِمۡ رِحۡلَةَ ٱلشِّتَاۤءِ وَٱلصَّیۡفِ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَلۡیَعۡبُدُوا۟ رَبَّ هَـٰذَا ٱلۡبَیۡتِ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "ٱلَّذِیۤ أَطۡعَمَهُم مِّن جُوعࣲ وَءَامَنَهُم مِّنۡ خَوۡفِۭ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "أَرَءَیۡتَ ٱلَّذِی یُكَذِّبُ بِٱلدِّینِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَ ٰلِكَ ٱلَّذِی یَدُعُّ ٱلۡیَتِیمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِینِ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَیۡلࣱ لِّلۡمُصَلِّینَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ٱلَّذِینَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ٱلَّذِینَ هُمۡ یُرَاۤءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَیَمۡنَعُونَ ٱلۡمَاعُونَ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "إِنَّاۤ أَعۡطَیۡنَـٰكَ ٱلۡكَوۡثَرَ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَٱنۡحَرۡ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "إِنَّ شَانِئَكَ هُوَ ٱلۡأَبۡتَرُ"
