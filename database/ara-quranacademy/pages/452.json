{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "مَا لَكُمۡ كَیۡفَ تَحۡكُمُونَ"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "أَفَلَا تَذَكَّرُونَ"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "أَمۡ لَكُمۡ سُلۡطَـٰنࣱ مُّبِینࣱ"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "فَأۡتُوا۟ بِكِتَـٰبِكُمۡ إِن كُنتُمۡ صَـٰدِقِینَ"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "وَجَعَلُوا۟ بَیۡنَهُۥ وَبَیۡنَ ٱلۡجِنَّةِ نَسَبࣰاۚ وَلَقَدۡ عَلِمَتِ ٱلۡجِنَّةُ إِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "سُبۡحَـٰنَ ٱللَّهِ عَمَّا یَصِفُونَ"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "فَإِنَّكُمۡ وَمَا تَعۡبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "مَاۤ أَنتُمۡ عَلَیۡهِ بِفَـٰتِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "إِلَّا مَنۡ هُوَ صَالِ ٱلۡجَحِیمِ"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "وَمَا مِنَّاۤ إِلَّا لَهُۥ مَقَامࣱ مَّعۡلُومࣱ"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "وَإِنَّا لَنَحۡنُ ٱلصَّاۤفُّونَ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "وَإِنَّا لَنَحۡنُ ٱلۡمُسَبِّحُونَ"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "وَإِن كَانُوا۟ لَیَقُولُونَ"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "لَوۡ أَنَّ عِندَنَا ذِكۡرࣰا مِّنَ ٱلۡأَوَّلِینَ"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "فَكَفَرُوا۟ بِهِۦۖ فَسَوۡفَ یَعۡلَمُونَ"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "وَلَقَدۡ سَبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "إِنَّهُمۡ لَهُمُ ٱلۡمَنصُورُونَ"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "وَإِنَّ جُندَنَا لَهُمُ ٱلۡغَـٰلِبُونَ"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "فَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِینࣲ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "وَأَبۡصِرۡهُمۡ فَسَوۡفَ یُبۡصِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "أَفَبِعَذَابِنَا یَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "فَإِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَاۤءَ صَبَاحُ ٱلۡمُنذَرِینَ"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "وَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِینࣲ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "وَأَبۡصِرۡ فَسَوۡفَ یُبۡصِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "سُبۡحَـٰنَ رَبِّكَ رَبِّ ٱلۡعِزَّةِ عَمَّا یَصِفُونَ"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "وَسَلَـٰمٌ عَلَى ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "وَٱلۡحَمۡدُ لِلَّهِ رَبِّ ٱلۡعَـٰلَمِینَ"
        }
    ]
}