{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "كَذَ ٰلِكَ مَاۤ أَتَى ٱلَّذِینَ مِن قَبۡلِهِم مِّن رَّسُولٍ إِلَّا قَالُوا۟ سَاحِرٌ أَوۡ مَجۡنُونٌ"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "أَتَوَاصَوۡا۟ بِهِۦۚ بَلۡ هُمۡ قَوۡمࣱ طَاغُونَ"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فَتَوَلَّ عَنۡهُمۡ فَمَاۤ أَنتَ بِمَلُومࣲ"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وَذَكِّرۡ فَإِنَّ ٱلذِّكۡرَىٰ تَنفَعُ ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وَمَا خَلَقۡتُ ٱلۡجِنَّ وَٱلۡإِنسَ إِلَّا لِیَعۡبُدُونِ"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "مَاۤ أُرِیدُ مِنۡهُم مِّن رِّزۡقࣲ وَمَاۤ أُرِیدُ أَن یُطۡعِمُونِ"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "إِنَّ ٱللَّهَ هُوَ ٱلرَّزَّاقُ ذُو ٱلۡقُوَّةِ ٱلۡمَتِینُ"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فَإِنَّ لِلَّذِینَ ظَلَمُوا۟ ذَنُوبࣰا مِّثۡلَ ذَنُوبِ أَصۡحَـٰبِهِمۡ فَلَا یَسۡتَعۡجِلُونِ"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فَوَیۡلࣱ لِّلَّذِینَ كَفَرُوا۟ مِن یَوۡمِهِمُ ٱلَّذِی یُوعَدُونَ"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "وَٱلطُّورِ"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وَكِتَـٰبࣲ مَّسۡطُورࣲ"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "فِی رَقࣲّ مَّنشُورࣲ"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "وَٱلۡبَیۡتِ ٱلۡمَعۡمُورِ"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "وَٱلسَّقۡفِ ٱلۡمَرۡفُوعِ"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "وَٱلۡبَحۡرِ ٱلۡمَسۡجُورِ"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "إِنَّ عَذَابَ رَبِّكَ لَوَ ٰقِعࣱ"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "مَّا لَهُۥ مِن دَافِعࣲ"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "یَوۡمَ تَمُورُ ٱلسَّمَاۤءُ مَوۡرࣰا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وَتَسِیرُ ٱلۡجِبَالُ سَیۡرࣰا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فَوَیۡلࣱ یَوۡمَئِذࣲ لِّلۡمُكَذِّبِینَ"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "ٱلَّذِینَ هُمۡ فِی خَوۡضࣲ یَلۡعَبُونَ"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "یَوۡمَ یُدَعُّونَ إِلَىٰ نَارِ جَهَنَّمَ دَعًّا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هَـٰذِهِ ٱلنَّارُ ٱلَّتِی كُنتُم بِهَا تُكَذِّبُونَ"
        }
    ]
}