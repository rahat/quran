{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "إِنَّهُۥ لَقُرۡءَانࣱ كَرِیمࣱ"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "فِی كِتَـٰبࣲ مَّكۡنُونࣲ"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "لَّا یَمَسُّهُۥۤ إِلَّا ٱلۡمُطَهَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "تَنزِیلࣱ مِّن رَّبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "أَفَبِهَـٰذَا ٱلۡحَدِیثِ أَنتُم مُّدۡهِنُونَ"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "فَلَوۡلَاۤ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "وَأَنتُمۡ حِینَئِذࣲ تَنظُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "وَنَحۡنُ أَقۡرَبُ إِلَیۡهِ مِنكُمۡ وَلَـٰكِن لَّا تُبۡصِرُونَ"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "فَلَوۡلَاۤ إِن كُنتُمۡ غَیۡرَ مَدِینِینَ"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "تَرۡجِعُونَهَاۤ إِن كُنتُمۡ صَـٰدِقِینَ"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "فَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُقَرَّبِینَ"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "فَرَوۡحࣱ وَرَیۡحَانࣱ وَجَنَّتُ نَعِیمࣲ"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "وَأَمَّاۤ إِن كَانَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "فَسَلَـٰمࣱ لَّكَ مِنۡ أَصۡحَـٰبِ ٱلۡیَمِینِ"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "وَأَمَّاۤ إِن كَانَ مِنَ ٱلۡمُكَذِّبِینَ ٱلضَّاۤلِّینَ"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "فَنُزُلࣱ مِّنۡ حَمِیمࣲ"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "وَتَصۡلِیَةُ جَحِیمٍ"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "إِنَّ هَـٰذَا لَهُوَ حَقُّ ٱلۡیَقِینِ"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِیمِ"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "سَبَّحَ لِلَّهِ مَا فِی ٱلسَّمَـٰوَ ٰتِ وَٱلۡأَرۡضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِیمُ"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "لَهُۥ مُلۡكُ ٱلسَّمَـٰوَ ٰتِ وَٱلۡأَرۡضِۖ یُحۡیِۦ وَیُمِیتُۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءࣲ قَدِیرٌ"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "هُوَ ٱلۡأَوَّلُ وَٱلۡءَاخِرُ وَٱلظَّـٰهِرُ وَٱلۡبَاطِنُۖ وَهُوَ بِكُلِّ شَیۡءٍ عَلِیمٌ"
        }
    ]
}