{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "وَٱلسَّمَاۤءِ وَٱلطَّارِقِ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "إِن كُلُّ نَفۡسࣲ لَّمَّا عَلَیۡهَا حَافِظࣱ"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فَلۡیَنظُرِ ٱلۡإِنسَـٰنُ مِمَّ خُلِقَ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خُلِقَ مِن مَّاۤءࣲ دَافِقࣲ"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "یَخۡرُجُ مِنۢ بَیۡنِ ٱلصُّلۡبِ وَٱلتَّرَاۤئِبِ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرࣱ"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "یَوۡمَ تُبۡلَى ٱلسَّرَاۤئِرُ"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فَمَا لَهُۥ مِن قُوَّةࣲ وَلَا نَاصِرࣲ"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "وَٱلسَّمَاۤءِ ذَاتِ ٱلرَّجۡعِ"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "إِنَّهُۥ لَقَوۡلࣱ فَصۡلࣱ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وَمَا هُوَ بِٱلۡهَزۡلِ"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "إِنَّهُمۡ یَكِیدُونَ كَیۡدࣰا"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "وَأَكِیدُ كَیۡدࣰا"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فَمَهِّلِ ٱلۡكَـٰفِرِینَ أَمۡهِلۡهُمۡ رُوَیۡدَۢا"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "۞ سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ٱلَّذِی خَلَقَ فَسَوَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَٱلَّذِی قَدَّرَ فَهَدَىٰ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَٱلَّذِیۤ أَخۡرَجَ ٱلۡمَرۡعَىٰ"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهُۥ غُثَاۤءً أَحۡوَىٰ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰۤ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "إِلَّا مَا شَاۤءَ ٱللَّهُۚ إِنَّهُۥ یَعۡلَمُ ٱلۡجَهۡرَ وَمَا یَخۡفَىٰ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُیَسِّرُكَ لِلۡیُسۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَیَذَّكَّرُ مَن یَخۡشَىٰ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَیَتَجَنَّبُهَا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ٱلَّذِی یَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا یَمُوتُ فِیهَا وَلَا یَحۡیَىٰ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ"
        }
    ]
