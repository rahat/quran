{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "مَا لَكُمۡ لَا تَنَاصَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "بَلۡ هُمُ ٱلۡیَوۡمَ مُسۡتَسۡلِمُونَ"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "وَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضࣲ یَتَسَاۤءَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "قَالُوۤا۟ إِنَّكُمۡ كُنتُمۡ تَأۡتُونَنَا عَنِ ٱلۡیَمِینِ"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "قَالُوا۟ بَل لَّمۡ تَكُونُوا۟ مُؤۡمِنِینَ"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "وَمَا كَانَ لَنَا عَلَیۡكُم مِّن سُلۡطَـٰنِۭۖ بَلۡ كُنتُمۡ قَوۡمࣰا طَـٰغِینَ"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "فَحَقَّ عَلَیۡنَا قَوۡلُ رَبِّنَاۤۖ إِنَّا لَذَاۤئِقُونَ"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "فَأَغۡوَیۡنَـٰكُمۡ إِنَّا كُنَّا غَـٰوِینَ"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "فَإِنَّهُمۡ یَوۡمَئِذࣲ فِی ٱلۡعَذَابِ مُشۡتَرِكُونَ"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "إِنَّا كَذَ ٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِینَ"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "إِنَّهُمۡ كَانُوۤا۟ إِذَا قِیلَ لَهُمۡ لَاۤ إِلَـٰهَ إِلَّا ٱللَّهُ یَسۡتَكۡبِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "وَیَقُولُونَ أَئِنَّا لَتَارِكُوۤا۟ ءَالِهَتِنَا لِشَاعِرࣲ مَّجۡنُونِۭ"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "بَلۡ جَاۤءَ بِٱلۡحَقِّ وَصَدَّقَ ٱلۡمُرۡسَلِینَ"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "إِنَّكُمۡ لَذَاۤئِقُوا۟ ٱلۡعَذَابِ ٱلۡأَلِیمِ"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "وَمَا تُجۡزَوۡنَ إِلَّا مَا كُنتُمۡ تَعۡمَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "أُو۟لَـٰۤئِكَ لَهُمۡ رِزۡقࣱ مَّعۡلُومࣱ"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "فَوَ ٰكِهُ وَهُم مُّكۡرَمُونَ"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "فِی جَنَّـٰتِ ٱلنَّعِیمِ"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "عَلَىٰ سُرُرࣲ مُّتَقَـٰبِلِینَ"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "یُطَافُ عَلَیۡهِم بِكَأۡسࣲ مِّن مَّعِینِۭ"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "بَیۡضَاۤءَ لَذَّةࣲ لِّلشَّـٰرِبِینَ"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "لَا فِیهَا غَوۡلࣱ وَلَا هُمۡ عَنۡهَا یُنزَفُونَ"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "وَعِندَهُمۡ قَـٰصِرَ ٰتُ ٱلطَّرۡفِ عِینࣱ"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "كَأَنَّهُنَّ بَیۡضࣱ مَّكۡنُونࣱ"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضࣲ یَتَسَاۤءَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "قَالَ قَاۤئِلࣱ مِّنۡهُمۡ إِنِّی كَانَ لِی قَرِینࣱ"
        }
    ]
}