{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَٱلۡفَجۡرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَیَالٍ عَشۡرࣲ"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَٱلَّیۡلِ إِذَا یَسۡرِ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِی ذَ ٰلِكَ قَسَمࣱ لِّذِی حِجۡرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "أَلَمۡ تَرَ كَیۡفَ فَعَلَ رَبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "ٱلَّتِی لَمۡ یُخۡلَقۡ مِثۡلُهَا فِی ٱلۡبِلَـٰدِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُودَ ٱلَّذِینَ جَابُوا۟ ٱلصَّخۡرَ بِٱلۡوَادِ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِی ٱلۡأَوۡتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ٱلَّذِینَ طَغَوۡا۟ فِی ٱلۡبِلَـٰدِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَأَكۡثَرُوا۟ فِیهَا ٱلۡفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَیۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَأَمَّا ٱلۡإِنسَـٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَیَقُولُ رَبِّیۤ أَكۡرَمَنِ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَأَمَّاۤ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَیۡهِ رِزۡقَهُۥ فَیَقُولُ رَبِّیۤ أَهَـٰنَنِ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡیَتِیمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا تَحَـٰۤضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِینِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلࣰا لَّمࣰّا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبࣰّا جَمࣰّا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّاۤۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكࣰّا دَكࣰّا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَجَاۤءَ رَبُّكَ وَٱلۡمَلَكُ صَفࣰّا صَفࣰّا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِا۟یۤءَ یَوۡمَئِذِۭ بِجَهَنَّمَۚ یَوۡمَئِذࣲ یَتَذَكَّرُ ٱلۡإِنسَـٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ"
        }
    ]
