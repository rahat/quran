{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Aspan jarılgan kezde"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "ari aspan deneleri togilgen kezde"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "jane tenizder / tasıp / , bir-birine quyılgan kezde"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "jane qabirler awdarılgan kezde"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "sonda arbir jan ilgeri ne jiberilgenin, artına ne qaldırılganın biledi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ey, adam balası! Al-Karim / asa Jomart / Rabbın jaylı seni ne aldadı"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ol / Allah / seni jarattı, / dene muselerin uylesimdi etip / tenestirdi jane / tulaboyındı / tuzwlep"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Ozi qalagan beynede seni qurastırdı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Joq, soytse de sender Esep kunin otirikke sıgarasındar"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Al, sın maninde, senderdin usterinnen kuzetip turwsılar bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "qurmetti jazıp turwsılar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Olar ne istep jatqandarındı biledi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Sın maninde, izgiler, sozsiz igilikte boladı"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "al buzıqtar, mindetti turde Jaximda boladı"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Olar ogan Esep kuni kiredi"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "ari olar odan joq bolıp ta qutıla almaydı"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Al, Esep kuninin ne ekenin sagan ne bildirdi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Sosın tagı da, Esep kuninin ne ekenin sagan ne bildirdi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Sol kuni jan / ozge / jan usin es narse istey almaydı. Ari ol kuni amir berw - Allahqa gana tan"
        }
    ]
}