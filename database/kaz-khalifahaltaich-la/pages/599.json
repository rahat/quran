    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Rabbısı aldındagı olardın sıyları - astınan ozender agıp jatatın ’Adn baqtarı. Olar onda mangi qalwsılar. Allah olardan razı, olar da Odan razı. Mine, bul - Rabbısınan qorıqqandar usin"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkinwimen silkingen kezde"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "ari isindegi jugin sıgargan kezde"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "jane adam: «Ogan ne boldı?» - degen kezde"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "sol Kuni ol, oz xabarın aytadı"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbınnın ogan waxi etip bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Mine, sol Kuni, adamdar top-tobımen amaldarı korsetilwi usin sıgadı"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim / zattın / en kiskentay bolseginin salmagınday jaqsılıq istegen bolsa, ol onı koredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Jane kim / zattın / en kiskentay bolseginin salmagınday jamandıq istese, ol da onı koredi"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ant etemin, qattı entigip sabatın"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ari / tuyaqtarınan / usqın sıgaratın"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ari tan ata sabwıldaytın"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "onımen burqırata san koteretin"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ari onımen / jawdın / ortasına top bo­lıp kiretindermen"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Aqiqatında, adam Rabbısına sukirlik etpewsi / Onın bergen igilikterin moyındamawsı"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "jane anıgında, ol osıgan kwa"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ari sın maninde, ol qayırlı narselerdi suyude tım qattı"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al, sonda ol bilmey me, qabirdegiler sıgarılgan kezde"
        }
    ]
