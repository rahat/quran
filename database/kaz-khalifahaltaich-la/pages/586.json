{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kun oratılıp buktelgen kezde"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "ari juldızdar togilgen kezde"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "jane tawlar / ornınan / qozgaltılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "jane on aylıq bwaz tuyeler qarawsız qalgan kezde"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "ari andar jiılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "jane tenizder otqa aynalgan kezde"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jane jandar toptastırılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "ari tiridey komilgen qız suralgan kezde"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "«Qay kunasi usin oltirildi?» - / dep"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Aman / jazbaları taratılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "jane aspan sıdırılıp alıngan kezde"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "jane Jaxim (Tozaq) jandırılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "ari Jannat jaqındatılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "arbir jan neni dayındap, alıp kelgenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq! Qaytwsılarmen ant etemin"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "qozgalwsı, jasırınwsı"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "ari qarangılıgı tuse bastagan tunmen"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "jane tınıs alıp, agarabastagan tanmen"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Anıgında, bul - ardaqtı Elsinin sozi"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "kusti jane Arsı Iesinin aldında martebeli"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "ogan ol jaqta / peristeler / bagınatın, senimdi"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Senderdin joldastarın jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ol, onı anıq kokjiekte kordi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ari ol gayıptı / waxidı / jetkizwde sarandıq / kemitwsilik / istemedi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "jane bul - qwılgan saytannın sozi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Sonda, qayda barasındar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ol / Quran / - alemder usin eske salw gana"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "senderden twralıqtı ustanwdı qalagandar usin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Alemderdin Rabbısı Allah qalamayınsa, sender qalay almaysındar"
        }
    ]
}