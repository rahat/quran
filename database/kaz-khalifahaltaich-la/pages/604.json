    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Ol, Allah - Axad / Jalgız"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah - as-Samad / Muqtajsız, al barlıq jaratılıs Ogan muqtaj"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ol twmagan jane twılmagan"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ari Ogan eskim ten emes"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Tannın Rabbısına / Iesine / siınıp, qorgawın tileymin"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "jaratqan narselerinin jamandıgınan"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ari qarangılıqtın jamandıgınan, ol japqan kezde"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "jane tuyinderge urlewsilerdin jamandıgınan"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ari xasadsının qızgangan / xasad qılgan / kezdegi jamandıgınan"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Adamdardın Rabbısına / Iesine / siınıp, qorgawın tileymin"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "adamdardın sın Bilewsisine / al-Malik"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "adamdardın qudayına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Allah eske alınganda / seginwsi azgıratınnın / waswaswl xannastın / jamandıgınan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "sonday, adamdardın kokirekterine waswasa (azgırw) salatın"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jındar jane adamdardan»"
        }
    ]
