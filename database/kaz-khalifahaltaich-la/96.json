{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ey, Muxammed! / Oqı, jaratqan Rabbınnın atımen"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "adam balasın uyıgan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı! Ari senin Rabbın asa Jomart"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ol qalam arqılı uyretti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Adamga, ol bilmegendi uyretti"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Joq, kumansiz, adam balası sekten sıgadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ozin bay, muqtajsız sanaganınan"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Al, anıgında, qaytw - Rabbına"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kordin be, kedergi jasagandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "bir qulga, ol namaz oqıgan kezde"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Aytsı, eger ol twra jolda bolgan bolsa"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "nemese taqwalıqtı / Allahtın jazasınan qorqıp, saqtanwdı / buyırgan bolsa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kordin be, onın otirikke sıgarganın jane teris aynalganın"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "sonda ol Allahtın korip turganın bilmey me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq, olay emes! Eger ol osılay istegenin toqtatpasa, sozsiz Biz kekilden ustaymız"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Otiriksi, bile tura qateleswsi kekilden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Qane, saqırsın ol jaqtastarın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz de saqsılardı saqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes! / Ey, Muxammedi / Sen ogan bagınba. / Rabbına / sajde qıl jane jaqınday tus"
        }
    ]
}