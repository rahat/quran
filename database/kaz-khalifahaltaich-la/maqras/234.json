{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ol qabagı tusip, burıldı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ogan soqır kisinin kelgendiginen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ey, Muxammed! / Sen qaydan bilesin, mumkin ol tazaratın sıgar"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Nemese eske alar, soytip eske salw ogan payda berer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al, qajetsinbeytin birew bolsa"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "sen ogan konil bolesin"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Al, onın tazarmawında turgan sagan es narse joq"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al birew sagan ıntalanıp kelse"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ari / Rabbısınan / qorıqqan xalde"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "sen odan / basqaga / konil awdarasın"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Joq, olay emes! Anıgında, bul - bir eske salw"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kim qalasa, onı esine aladı"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "qurmetti jazbalarda / swxwftarda"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "jogarı bagalangan tap-taza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "elsilerdin / peristelerdin / qolında"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "qadirli, igi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Senbegen / adamnın janı sıqsın! Kupirlik etwde / igilikterdi moyındamawda / ol qanday qattı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ol / Allah / onı ne narseden jarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamsıdan jarattı, ari belgiledi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sodan son ogan joldı onaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Odan keyin onın janın alıp, / murdesin / qabirge qoydırdı"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Keyin onı Ozi qalagan kezde qayta tiriltedi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Joq! Ol bolsa, Onın buyrıgın orındamadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Al, endi adam ozinin tamagına, bir koz salıp qarasın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz swdı mol etip quydıq / jawdırdıq"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "sodan keyin jerdi tilimdep ayırdıq"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Soytip odan dandi / daqıldar / osirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ari juzim men jonısqanı"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "jane zaytun men qurmanı"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "jane qalın baw-baqtardı"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "sonday-aq, jemister men sopterdi"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "senderdin paydalanwlarına ari maldarın usin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Al endi Soxxa kelgen kezde"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sol kuni adam, twgan bawırınan qasadı"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "jane anası men akesinen"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "ari ayeli men balalarınan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ol kuni arbir adamdı alek etetin oz isi bar"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ol kuni / keybir / juzder jarqın"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "kulimsiregen, qwanıstı"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ari ol kuni / keybir / juzderdi san basıp"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "olar qaraygan boladı"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Mine, solar - kapirler, buzıqtar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kun oratılıp buktelgen kezde"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "ari juldızdar togilgen kezde"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "jane tawlar / ornınan / qozgaltılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "jane on aylıq bwaz tuyeler qarawsız qalgan kezde"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "ari andar jiılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "jane tenizder otqa aynalgan kezde"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jane jandar toptastırılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "ari tiridey komilgen qız suralgan kezde"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "«Qay kunasi usin oltirildi?» - / dep"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Aman / jazbaları taratılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "jane aspan sıdırılıp alıngan kezde"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "jane Jaxim (Tozaq) jandırılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "ari Jannat jaqındatılgan kezde"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "arbir jan neni dayındap, alıp kelgenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq! Qaytwsılarmen ant etemin"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "qozgalwsı, jasırınwsı"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "ari qarangılıgı tuse bastagan tunmen"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "jane tınıs alıp, agarabastagan tanmen"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Anıgında, bul - ardaqtı Elsinin sozi"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "kusti jane Arsı Iesinin aldında martebeli"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "ogan ol jaqta / peristeler / bagınatın, senimdi"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Senderdin joldastarın jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ol, onı anıq kokjiekte kordi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ari ol gayıptı / waxidı / jetkizwde sarandıq / kemitwsilik / istemedi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "jane bul - qwılgan saytannın sozi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Sonda, qayda barasındar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ol / Quran / - alemder usin eske salw gana"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "senderden twralıqtı ustanwdı qalagandar usin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Alemderdin Rabbısı Allah qalamayınsa, sender qalay almaysındar"
        }
    ]
}