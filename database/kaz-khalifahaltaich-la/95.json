{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ant etemin anjir jane zaytunmen"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "jane Sinin tawımen"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "jane osı qawipsiz qalamen"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Aqiqatında, adamdı korkem beynede jarattıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Keyin onı tomenderdin tomenine qaytardıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Al, sonday imanga kelgen jane izgi is istegender - basqa. Olarga tawsılmaytın sıy bar"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Osıdan keyin Esep berwdi otirik dewge seni ne itermeleydi"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah ukim sıgarwsılardın en adil ukim berwsisi emes pe"
        }
    ]
}