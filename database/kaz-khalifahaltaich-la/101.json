{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Al-Qaria / ulken apat"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Al-Qaria degen ne"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Al-Qaria ne ekenin sagan ne bildirdi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ol kuni adam balası bıtırap taralgan kobelektey boladı"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ari tawlar tutilgen jundey boladı"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sonda kimnin tarazısı awır tartsa"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "onda ol -razı bolatın bir omirde boladı"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al, kimnin tarazısı jenil tartsa"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "onda onın «anası» / baratın ornının negizi / Hawiya boladı"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Al onın ne ekenin sagan ne bildirdi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ol - jalındagan ot"
        }
    ]
}