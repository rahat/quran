{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ey, Muxammed! / Biz sagan kokiregindi aspadıq pa"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ari senen jugindi almadıq pa"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "arqana awırtpalıq tusirgen"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ari senin eske alınwındı jogarılatpadıq pa"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Negizinde, qiınsılıqpen birge jenildik bar"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Sın maninde, qiınsılıqpen birge jenildik bar"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Al, endi bosay qalsan, tırıs / areket et"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "jane Rabbına qaray umtıl"
        }
    ]
}