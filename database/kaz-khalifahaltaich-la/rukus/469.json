{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Al-Waqia / Qiyamet / bolgan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "onın bolwın esnarse joqqa sıgarmaydı"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ol tomendetedi, jogarılatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer silkinip qozgaltılgan kezde"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "jane tawlar qirap, talqandalıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "ari sasılgan tozanga aynalgan kezde"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "sonda sender us topqa bolinesinder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Biri on jaqta boladı. On jaqtagılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Al, birewler sol jaqta boladı. Sol jaqtagılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ari ozwsılar, / olar / - ozwsılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine, solar - jaqındatılgandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Naim (Jannattın bir atı. Magnası - koptegen nıgmet, igilik) baqtarında boladı"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Olar / awelgilerden kobirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "songılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezendirilgen, koterinki jaylı orındarda"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "ogan suyenip, bir-birine qaragan kuyde otıradı"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olardı mangi bozbalalar aynalıp, qızmet etip juredi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "qaynar bulaqtan / toltırılgan / koze, qumıralar jane keselermen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "odan olardın bastarı awırmaydı ari aqıl-esterin jogaltpaydı"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "ari olar tandagan jemisterimen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "jane qalagan kus etimen"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ari Olarga janarları ulken de ademi / xwrlar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "beyne bir / qabıgı asılmagan / saqtawlı injw sekildi / jubaylar bar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bular - istegen amaldarının qaytarım sıyı"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ol jerde olar bos sozdi de, kunali sozdi de estimeydi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Tek: «Salam! Salam! / Beybitsilik bol­sın! Amandıq bolsın! / » - degen sozdi gana / estidi"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Al, on jaqtagılar. On jaqtagılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tikenegi joq lotos / as-sidr / osimdikteri"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jane jemisteri sogırlangan banan agastarı arasında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ari jayılıp jatqan kolenkede"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "jane sarqırap aqqan sw"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "mol jemister arasında"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "sonday/ esqasan tawsılmaytın jane tıyım salınbaytın"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "ari jogarı koterilgen jumsaq tosekterde / boladı"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Aqiqatında, Biz olardı basqa jaratwmen jarattıq"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "ari olardı er kormegen pak qızdar ettik"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "kuyewlerine suyikti, qurbılas"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Bul / on jaqtagılar usin"
        }
    ]
}