{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Abw Lahabtın eki qolı qurısın ari ozi de qurıdı"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ogan duniesi men tapqandarı payda bermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ol jaqında jalındagan otqa kiredi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "jane otın koterip onın ayeli de"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Onın moynında - esilgen arqan / boladı"
        }
    ]
}