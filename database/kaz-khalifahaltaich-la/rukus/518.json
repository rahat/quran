{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Olar bir-birinen ne twralı surasıp jatır"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ulı xabar twralı"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "ozderi ol joninde qarama-qaysılıqta"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Joq, olay emes! Olar jaqında biledi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Keyin, tipti olay emes! Olar ali-aq biledi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Biz jerdi tosenis etpedik pe"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "al tawlardı qazıq"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ari Biz senderdi juptar etip jarattıq"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Al uyqılarındı tınıgw ettik"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "ari tundi jamılgı etip jasadıq"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "jane kundizdi tirsilik / waqıtı / etip jasadıq"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Ari usterinnen jeti berikti qurdıq"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "jane lawlagan sıraq jasadıq"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ari sıgılwsılardan / bulttardan / mol sw tusirdik"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "onımen dan jane osimdikti sıgarw usin"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "jane nw baqtardı da"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Aqiqatında, Ayırw kuni - belgilengen bir merzim"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Ol kuni sur urlenedi de sender top-top bolıp kelesinder"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Ari aspan asılıp, qaqpalarga aynaladı"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "al tawlar jurgizilip, sagımga aynaladı"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Aqiqatında, Jahannam - andıp, kutip turwsı"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "sekten sıgwsılar usin qaytar orın"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Olar onda gasırlar boyı qaladı"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Ol jerde olar salqındıqtı da, sw sın da tatpaydı"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "tek qaynap turgan sw men irinnen basqa"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "isterine layıqtı qaytarım retinde"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Anıgında, olar, esepke tartılwdı «umit» etpegen / qorıqpagan / edi"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "ari ayattarımızdı mulde otirikke sıgargan edi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Al, Biz barlıq narseni jazıp, eseptep qoydıq"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Endi, tatındar. Senderge azaptan basqa esnarseni arttırmaymız"
        }
    ]
}