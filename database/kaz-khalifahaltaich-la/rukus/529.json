{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Ey, Muxammed! / Asa jogarı Rabbınnın esimin pak dep daripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol jaratıp, / orın-ornımen / tenestirgen"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "jane aldın ala belgilep, bagıttagan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ari jayılımdardı sıgargan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "keyin onı qwrap, qaraygan qoqımga aynaldırgan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ey, Muxammed! / Biz sagan / Qurandı / oqıtamız, sen esinnen sıgarmaysın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "tek Allahtın qalaganın gana / umıtasın / . Sın maninde, Ol jariyanı da jane jasırılgandı da biledi"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ari Biz sagan en jenilin onaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Esterine salw payda beretin bolsa, esterine sal"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Allahtın jazasınan / qorıqqan esine aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Al, odan en baqıtsız gana ozin awlaq ustaydı"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ulken otta janatın"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Keyin ol onda olmeydi de, omir de surmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Sozsiz, muratına jetti - ozin tazartqan"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "ari Rabbısının esimin eske algan jane namaz oqıgan"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Alayda sender osı omirdi artıq koresinder"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Al, songı, mangilik omir / aqiret / qayırlı ari turaqtı"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Anıgında, bul awelgi jazbalarda bar"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ibrahim jane Musanın jazbalarında / olarga tusirilgen swxwftarda"
        }
    ]
}