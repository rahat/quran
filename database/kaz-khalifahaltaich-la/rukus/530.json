{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ey, Muxammed! / Sagan Fasiyanın xabarı keldi me"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ol kuni / keybir / juzder qorlangan boladı"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "amal etwsi, sarsagan"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "olar / ıstıgı qattı otqa janadı"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "qattı qaynap turgan bastawdan iskiziledi"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Olarga «dariden» / ulı tikennen / basqa esbir tamaq joq"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ol onaltpaydı da, astıqtı da baspaydı"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Sol kuni / keybir / juzder sattıqta"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "amal, areketterine razı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "biik baqtarda boladı"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Olar ol jerde bos soz estimeydi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ol jerde agıp jatqan bulaq bar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Onda jogarı koterilgen jaylı orındar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "ari qoyılgan keseler"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "jane tizilgen jastıqtar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "jayılgan kilemder bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Olar tuyege, onın qalay jaratılganına nazar salmay ma"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "ari aspanga, onın qalay koterilgenine"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "jane tawlarga, onın qalay turgızılganına"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ari jerge, onın qalay toselgenine"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ey, Muxammed! / Eske sal, anıgında sen tek eske salwsısın"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen olarga kuspen ustemdik etwsi emessin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Alayda kim teris burılsa jane kupirlik etse / qarsı kelse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "onda Allah onı en ulken azappen qinaydı"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Aqiqatında, olardın qaytwı - Bizge"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sodan keyin, kumansiz, olardın esebi - Bizde"
        }
    ]
}