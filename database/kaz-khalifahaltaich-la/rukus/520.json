{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Qattı tartıp, swırıp alwsılarmen"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "jenil etip sıgarıp alwsılarmen"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "jane jıldam juzwsilermen"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "solay, ekpindi ozwsılarmen"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "buyrıqtı orındawsılarmen ant etemin"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol kuni silkinwsi silkinedi"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "ogan artınan kelesisi jalgasadı"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol kuni jurekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "kozder qorlana bagınıp, tomen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Olar / senbewsiler / : «Biz artqa qaytarılamız ba"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Eger biz ugitilgen suyekke aynalsaq ta ma?» - deydi"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Olar: «Endese, ol tiimsiz bir qaytw eken», - deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Aqiqatında, ol - bar bolganı bir gana qattı dawıs"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Sonda olar jer betinde / tirilip / turadı"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sagan Musanın xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kezinde qasietti Twwa alqabında ogan Rabbısı undedi"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "«Firawnga bar, oytkeni ol sekten sıqtı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Ari ogan ayt: «Sen tazarwdı qalaysın ba"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sagan Rabbına / aparatın / joldı korseteyin. Sonda sen / Onın jazasınan / qorqatın bolasın», - / dep"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Soytip ogan ulken bir belgi-mugjizanı korsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "biraq / Firawn / onı otirikke sıgardı jane qarsı sıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Sodan keyin ol teris aynalıp, areketke kiristi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Ari / xalqın / jinap, jar saldı"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "«Men senderdin en jogargı ielerinmin», - dep"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Allah, onı songı, mangilik omir / aqiret / jane osı omir azabımen jazaladı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Kumansiz, munda qorqatındar usin bir gibrat bar"
        }
    ]
}