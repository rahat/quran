{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkinwimen silkingen kezde"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "ari isindegi jugin sıgargan kezde"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "jane adam: «Ogan ne boldı?» - degen kezde"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "sol Kuni ol, oz xabarın aytadı"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbınnın ogan waxi etip bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Mine, sol Kuni, adamdar top-tobımen amaldarı korsetilwi usin sıgadı"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim / zattın / en kiskentay bolseginin salmagınday jaqsılıq istegen bolsa, ol onı koredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Jane kim / zattın / en kiskentay bolseginin salmagınday jamandıq istese, ol da onı koredi"
        }
    ]
}