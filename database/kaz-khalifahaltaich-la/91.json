{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Kun jane onın jarıgımen"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "ari onın sonınan ilesken kezdegi aymen"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "jane onı / kundi / anıq korsetken kezdegi kundizben"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "ari onı japqan kezdegi tunmen"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspan jane onı turgızganmen"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "ari jermen jane onı Jayganmen"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "jane janmen ari onı layıqtap Jasaganmen"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "soytip ogan / janga / buzıqtıqtı ari taqwalıqtı / Allahtın jazasınan qorqıp, saqtanwdı / Bildirwsimen ant etemin"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Onı / jandı / tazartqan muratına jetti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Al, onı / kunaga / batırgan ziyanga usıradı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Sekten sıgwları sebepti Samud eli / Elsini / otiriksige sıgardı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Sonda olardın en baqıtsızı qozgalganda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahtın elsisi / Salix / olarga: «Allahtın tuyesine ari onın swarılwına / tiispender / », - dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Al, olar bolsa onı / Elsini / otiriksige sıgarıp, onın / tuyenin / tirsegin kesti / oltirdi / . Sonda, Rabbısı olardın qılmıstarı usin barine birdey apat jiberdi"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ari Ol / Allah / onın saldarınan qawiptenbeydi"
        }
    ]
}