{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "بگو: به پروردگار مردم پناه مى‌برم،"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "فرمانرواى مردم،"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "خداى مردم،"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "از شر وسوسه وسوسه‌گر نهانى،"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "آن كه در دلهاى مردم وسوسه مى‌كند،"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "خواه از جنيان باشد يا از آدميان"
        }
    ]
}