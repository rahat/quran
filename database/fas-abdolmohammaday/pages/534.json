{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "در آن دو، ميوه هست و نخل هست و انار هست"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "پس كدام يك از نعمتهاى پروردگارتان را انكار مى‌كنيد؟"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "در آنجا زنانى است نيك‌سيرت و زيباروى"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "پس كدام يك از نعمتهاى پروردگارتان را انكار مى‌كنيد؟"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "حورانى مستور در خيمه‌ها"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "پس كدام يك از نعمتهاى پروردگارتان را انكار مى‌كنيد؟"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "پيش از بهشتيان هيچ آدمى و جنى به آنها دست نزده است"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "پس كدام يك از نعمتهاى پروردگارتان را انكار مى‌كنيد؟"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "بر بالشهاى سبز و فرشهاى نيكو تكيه مى‌زنند"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "پس كدام يك از نعمتهاى پروردگارتان را انكار مى‌كنيد؟"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "بزرگ است نام پروردگار تو آن صاحب جلالت و اكرام"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "چون قيامت واقع شود،"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "كه در واقع‌شدنش هيچ دروغ نيست،"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "گروهى را خواركننده است و گروهى را برافرازنده"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "آنگاه كه زمين به سختى بلرزد،"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "و كوه‌ها به تمامى متلاشى شوند،"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "و چون غبارى پراكنده گردند،"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "شما سه گروه باشيد"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "يكى اهل سعادت. اهل سعادت چه حال دارند؟"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "ديگر، اهل شقاوت. اهل شقاوت چه حال دارند؟"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "سه ديگر، آنها كه سبقت جسته بودند و اينك پيش افتاده‌اند"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "اينان مقرّبانند،"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "در بهشتهاى پرنعمت"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "گروهى از پيشينيان،"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "و اندكى از آنها كه از پى آمده‌اند،"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "بر تختهايى مرصّع،"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "روبروى هم بر آنها تكيه زده‌اند"
        }
    ]
}