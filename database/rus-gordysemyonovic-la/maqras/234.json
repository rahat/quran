{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Potomu chto k nemu prishel slepoy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "O yesli by chto nibud' vrazumilo tebya v tom, chto on mozhet byt' chistym"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ili, primet nastavleniye, i nastavleniye eto prineset yemu pol'zu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kto bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Togo ty prinimayesh' laskovo"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Togda kak ne ot tebya zavisit, yesli on ne sdelayetsya chistym"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Kto zhe k tebe prikhodit s userdiyem"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "I buduchi bogoboyazliv"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Na togo ty ne obrashchayesh' vnimaniye"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne tak. Eto tebe predosterezheniye"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kto zakhochet, tot budet pomnit' yego"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "On na svitkakh, dostochtimykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Vysokikh, chistykh"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Nachertannykh rukami pistsev"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "sanovitykh, pravednykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Da budet porazhen chelovek! Kak neblagodaren on"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Iz chego On tvorit yego"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Iz semeni. On tvorit yego, dayet sorazmernost' yego chlenam"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Za tem oblegchayet yemu put'"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potom umershchvlyayet yego i pogrebayet yego"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potom kogda zakhochet, voskresit yego"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "A on ne ispolnyayet togo, chto povelel On yemu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' obratit chelovek vzor svoy na to, chem pitayetsya on"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "My prolivayem vodu livnyami"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Potom zastavlyayem zemlyu rastreskivat'sya treshchinami"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Vyrashchayem na ney khleb"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Vinograd, ovoshchi"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Masliny, pal'my"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Sady, obil'nyye derevami"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Plody i zlaki"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "V prodovol'stviye vam i skotu vashemu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "No kogda nastanet oglushitel'nyy trubnyy zvuk"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "V tot den', kogda chelovek pobezhit ot svoyego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Svoyey materi i svoyego ottsa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Svoyey suprugi i svoikh synovey"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "V etot den' dlya kazhdogo iz nikh budet zabota, kotoraya otyagotit yego"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V etot den' u nekotorykh litsa budut svetlyye"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Smeyushchiyesya, veselyye"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "V etot den' u nekotorykh litsa budut zapylennyye"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Pokrytyye prakhom"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Takimi budut neveruyushchiye, razvratnyye"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogda solntse obov'yetsya mrakom"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Kogda zvezdy pomerknut"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Kogda gory s mest svoikh sdvinutsya"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kogda devyat' mesyatsev suzherebyye verblyuditsy okazhutsya prazdnymi"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Kogda zveri stolpyatsya"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kogda morya zakipyat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "kogda dushi sopryagutsya"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kogda pokhoronennaya zhivoyu budet sproshena"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakoy grekh ona ubita"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kogda svitki razov'yutsya"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Kogda nebo, kak pokrov, snimetsya"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Kogda ad razgoritsya"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "I kogda ray priblizitsya"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Togda dusha uznayet, chto zagotovila ona sebe"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Klyanus' planetami"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Skryvayushchimisya kometami"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Klyanus' noch'yu, kogda ona temneyet"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Klyanus' utrom, kogda ono prokhladno veyet"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Deystvitel'no, on - slovo poslannika, slavnogo"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Sil'nogo pred Vlastitelem prestola, polnomochnogo"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Pokorno chtimogo, vernogo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I sograzhdanin vash ne besnuyushchiysya"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "On nekogda videl yego na svetlom nebosklone"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "U nego net nedoumeniya o tainstvennom"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "On ne yest' slovo satany, progonyayemogo kamnyami"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Za chem zhe vy idete proch'"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "On - uveshchaniye miram"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Tem iz vas, kotoryye khotyat idti pryamo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "No vy mozhete khotet' tol'ko togo, chego khochet Bog, Gospod' mirov"
        }
    ]
}