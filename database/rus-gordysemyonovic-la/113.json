        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Ishchu ubezhishcha u Gospoda rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ot zlotvornosti togo, chto sotvoril On"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ot zlotvornosti nochnoy temnoty, kogda ona vse pokryvayet"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ot zlotvornosti zhenshchin, duyushchikh na uzly"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ot zlotvornosti zavistnika, kogda on zaviduyet"
        }
    ]
}