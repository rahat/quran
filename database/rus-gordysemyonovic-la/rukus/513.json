{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "On ne imel uverennosti i ne molilsya"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "A veril lzhi i udalilsya"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Ushel k svoim i razvelichalsya"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Gore tebe, i yeshche gore"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "I za tem, gore tebe, i yeshche gore"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Uzhe li chelovek dumayet, chto on ostayetsya bez prismotra"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Ne byl li on vlagoy, kakoyu izlivayetsya semya muzha"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Ne byl on potom kuskom sgustivsheysya krovi? I On ustroil yego i dal yemu sorazmernost' chlenov"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "On proizvodit iz nego chetu - muzhchinu i zhenshchinu"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Ne tak zhe li mozhet On ozhivit' i mertvykh"
        }
    ]
}