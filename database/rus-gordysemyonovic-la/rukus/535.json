{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Klyanus' utrenney svetozarnost'yu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Klyanus' noch'yu, kogda ona temneyet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospod' tvoy ne ostavil tebya, i ne prezrel"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Budushcheye dlya tebya luchshe nastoyashchego"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Gospod' tvoy skoro ushchedrit tebya, i ty budesh' dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ne sirotoy li On nashel tebya, i - priyutil"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ne bluzhdayushchim li On nashel tebya, i - na pryamoy put' postanovil"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "On nashel tebya bednym, i - obogatil"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "A potomu, siroty ne pritesnyay"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nishchego ot sebya krikom ne otgonyay"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "No blagost' Gospoda tvoyego im proyavlyay"
        }
    ]
}