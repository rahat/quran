{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Klyanus' posylayemymi poocheredno"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Nesushchimisya bystro"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Klyanus' pokazyvayushchimi yasno"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Razlichayushchimi verno"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Peredayushchimi nastavleniya"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "I proshcheniye i ugrozu"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Predvozveshchennoye vam uzhe gotovo sovershit'sya"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Kogda zvezdy unichtozhatsya"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Kogda nebo raskoletsya"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Kogda gory sdvinutsya s osnovaniy"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Kogda proroki predstanut v opredelennoye vremya"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Togda do kakogo dnya budet yeshcho otsrocheno"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Do dnya razdeleniya"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "O yesli by ty uznal, chto takoye den' razdeleniya"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Ne pogubili li My prezhnikh"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "I potom ne zamenili li ikh drugimi"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Takovy nashi deystviya s zakonoprestupnikami"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Ne tvorim li My vas iz prezrennoy vlagi"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "A potom pomeshchayem yeye v nadezhnom meste"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Do opredelennoy cheredy"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Tak My uchredili: a potomu kakiye My prevoskhodnyye uchrediteli"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Ne sdelali li My zemli pomestilishchem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Dlya zhivykh i mertvykh"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "My postavili na ney vysokiye gornyye tverdyni i poim vas vkusnoyu vodoyu"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Idite k tomu, chto schitali vy vymyslom"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Idite vo t'mu, izvergayushchuyu tri stolba dyma"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Kotoryy ni teni ne dast, ni ot plameni ne zashchitit"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "On budet izvergat' iskry, velichinoy kak bashni"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Tsvetom kak zheltyye verblyudy"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Eto takoy den', v kotoryy ne vygovoryat slova"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "V kotoryy ne budet pozvoleno im opravdyvat'sya"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "V tot den' gore veruyushchim v lozh'"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Takov den' razdeleniya. Togda My soberem i vas i prezhnikh"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Yesli u vas yest' kakaya khitrost', to ukhitrites' protiv Menya"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "V tot den' gore veruyushchim v lozh'"
        }
    ]
}