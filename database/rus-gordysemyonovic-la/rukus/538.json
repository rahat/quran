{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Chitay vo imya Gospoda tvoyego, kotoryy sozdayet"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Sozdayet cheloveka iz sgustivsheysya krovi"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Chitay - vseblagoy Gospod' tvoy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kotoryy dal poznaniya o pis'mennoy trosti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Dayet cheloveku znaniye o tom, o chem u nego ne bylo znaniya"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Istinno, chelovek stanovitsya buynym"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kogda vidit sebya razbogatevshim"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Istinno, budet vozvrashcheniye ko Gospodu tvoyemu"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidish' li, kakov tot, kotoryy pretit"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Rabu Moyemu molit'sya"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Vidish' li, kakov by on byl, yesli by on byl na pryamom puti"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ili vnushal by blagochestiye"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Vidish' li, kakov on, kogda predayetsya lzhi, i otstupayet ot istiny"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Uzhe li on ne znayet, chto Bog vidit"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Da, yesli on ne uderzhitsya, to My skhvatim i uvlechem yego za peredniye volosy"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Za peredniye volosy yego golovy, lzhivoy, greshnoy"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Togda zovi on svoikh soobshchnikov"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My prizovem etikh groznykh strazhey"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Net, ty yemu ne podchinyaysya: predo Mnoy preklonyaysya, ko Mne priblizhaysya"
        }
    ]
}