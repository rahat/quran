{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "O zakutavshiysya v odezhdu"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Vstan' i pouchay"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Gospoda tvoyego - velichay"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Odezhdy svoi - ochishchay"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Merzosti - ubegay"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Delaya dobro, ne bud' korystolyubiv"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Radi Gospoda tvoyego bud' terpeliv"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Vot, vostrubit truba"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "I den' etot budet den' tyagostnyy"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Dlya nevernykh bezotradnyy"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ostav' Menya vmeste s tem, kotorogo YA sdelal yedinstvennym"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Nadelil yego velikim bogatstvom"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Synami, zhivushchimi pri nem v dovol'stve"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Ustroil vse dlya nego nailuchshim stroyem"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "I posle sego on zhelayet yeshche bol'shego"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Net, etogo ne budet yemu za to, chto on byl protiv Nashikh znameniy"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Verno, istomlyu yego velikim stradaniyem"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Za to, chto umyslil on, chto ustroil on"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Da budet porazhen on sootvetstvenno tomu, chto ustroil on"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "I yeshche: da budet porazhen on sootvetstvenno tomu, chto ustroil on"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "On oziralsya krugom"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Nakhmurilsya, nasupilsya"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Potom otvorotilsya, vstal velichavo"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "I skazal: \"On - odno charodeystvo, prinyatoye po predaniyu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "On - tol'ko chelovecheskoye slovo"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Ognyu adskomu velyu zhech' yego"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "O yesli by kto dal ponyat' tebe, kakov ogon' adskiy"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "On, ne ostanavlivayas', ne prekrashchayas'"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Zhzhet chelovecheskoye telo"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Pri nem devyatnadtsat'"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "V strazhi ada My opredelili tol'ko angelov, i chislo ikh opredelili tol'ko dlya togo, chtoby ispytat' nevernykh, vyvedat' tekh, kotorym dano Pisaniye, chtoby uvelichit' veru v veruyushchikh, i chtoby ne somnevalis' te, kotorym dano Pisaniye, i veruyushchiye; a te, u kotorykh v serdtse yest' bolezn', i neveruyushchiye, chtoby govorili: \"Chto ukazat' khochet Bog etoy pritchey?\" Tak Bog vvodit v zabluzhdeniye, kogo khochet, ili vedet pryamo, kogo khochet. Nikto ne znayet voinstv Gospoda tvoyego, krome Yego samogo. Eto skazano tol'ko v nazidaniye lyudyam"
        }
    ]
}