{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Klyanus' solntsem i utrenney svetozarnost'yu yego"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "I lunoyu, kogda ona posleduyet za nim"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Klyanus' dnem, kogda on otkryvayet blesk yego"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "I noch'yu, kogda ona zakryvayet yego"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Klyanus' nebom i Tem, kto ustroil yego"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "I zemleyu i Tem, kto rasproster yeye"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Klyanus' dusheyu i Tem, kto obrazoval yeye"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Vdokhnul yey nechestiye yeye i blagochestiye yeye"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Blazhen tot, kto ochistit yeye"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Neschasten tot, kto oskudit yeye"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Femudyane predalis' lzhi; pri svoyevol'stve ikh"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kogda vosstal nechestiveyshiy iz nikh"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Togda poslannik Bozhiy skazal im: \"I verblyuditsu Bozhiyu i pit'ye yey"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "No oni lzhetsom pochli yego, i pererezali zhily v nogakh yeye. Togda Gospod' ikh porazil ikh kazn'yu za grekh ikh, dlya vsekh uravnyav yeye"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ne trevozhas' za posledstviya yeyo"
        }
    ]
}