{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Khvali imya Vsevyshnego Gospoda tvoyego"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Kotoryy sozdal vse i uravnovesil"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Kotoryy raspredelil vse, i pravit im"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Kotoryy vozrashchayet pastbishcha"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "A potom obrashchayet ikh v chernoye byliye"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My velim tebe chitat', i ty zabudesh'"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Tol'ko to, chto zakhochet Bog. On znayet proiznosimoye gromko, i to, chto utaivayetsya"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My budem spospeshestvovat' k uspekhu tvoyemu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "A potomu uchi, yesli polezno budet eto ucheniye"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Kto bogoboyazliv, tot naverno budet uchitsya"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A otvratitsya ot nego tot neschastnyy"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Kotoryy budet goret' v velikom ogne"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "On v nem ne umret, no i ne budet zhit'"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Blazhen, kto chist"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Vspominayet imya Gospoda svoyego i molitsya"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Vysoko tsenite vy zdeshnyuyu zhizn'"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "No budushchaya luchshe, potomu chto vechna"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Tak i v prezhnikh svitkakh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "V svitkakh Avraama i Moiseya"
        }
    ]
}