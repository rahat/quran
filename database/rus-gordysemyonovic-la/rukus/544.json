{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Privyazannost' k bogatstvu ozabochivayet vas"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Dotole, pokuda ne ukhodite gostit' v mogilakh"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Deystvitel'no, uznayete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I yeshche, deystvitel'no, vy uznayete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "O yesli by znali vy eto teper' vernym znaniyem"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Vy uvidite plamen' ada"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Uvidite yego ochami uverennosti"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "V tot den' ot vas sprosyat otvetstvennosti za privyazannost' k udovol'stviyam"
        }
    ]
}