{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Klyanus' tem, chto vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "I tem, chego ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "On, deystvitel'no yest' slovo dostochtimogo poslannika"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "On - ne slovo poeta (iz vas malo veruyushchikh etomu)"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ne slovo gadatelya (iz vas malo razmyshlyayushchikh ob etom)"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "On - otkroveniye Gospoda mirov"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Yesli by on vydumal o Nas kakiye-libo vydumki"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "To My skhvatili by yego za pravuyu ruku"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Peresekli by zhily v nem"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "I ni odin by iz vas ne zagradil by yego"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "On - uveshchaniye blagochestivym"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "I My deystvitel'no znayem, chto nekotoryye iz vas schitayut yego lzhivym"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "I v samom dele, on - bedstven dlya nevernykh"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Togda kak on yest' vernaya istina"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Khvali imya velikogo Gospoda tvoyego"
        }
    ]
}