{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "My byli blagouvetlivy k Moiseyu i Aaronu"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Spasli ikh i narod ikh ot velikogo bedstviya"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "My pomogli im, i oni sdelalis' pobeditelyami"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "My dali oboim im yasnuyu knigu"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "I vodili ikh po pryamomu puti"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "My ostavili v potomstve privet im"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Mir Moiseyu i Aaronu"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Tak nagrazhdayem My dobrodetel'nykh"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Oba oni byli iz veruyushchikh rabov nashikh"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Iliya byl odin iz poslannikov"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Vot, on skazal svoyemu narodu: \"Uzheli vy ne budete bogoboyazlivy"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Uzheli budete molit'sya Vaalu, i ostavite nailuchshego iz tvortsov"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Boga, Gospoda vashego i Gospoda vashikh praottsev"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Oni pochli yego lzhetsom. So vremenem oni yavyatsya predo Mnoy"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Krome blagochestivykh rabov Boga"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "My ostavili v potomstve privet yemu"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Mir Il'yasinu"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Tak nagrazhdayem My dobrodetel'nykh"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "On byl odin iz veruyushchikh rabov nashikh"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Lot takzhe byl iz poslannikov"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "My spasli yego i vse yego semeystvo"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Krome staroy zheny yego, zamedlivshey sredi ostavshikhsya pozadi"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "A prochikh My istrebili"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Vy prokhodite i utrom"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "I noch'yu po tem mestam, gde oni byli: kak zhe vy ne rassuditel'ny"
        }
    ]
}