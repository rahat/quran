{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Togda Faraon poslal v goroda sborshchikov voyska"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Oni - nemnogolyudnaya tolpa"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "khotya i razdrazheny protiv nas"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "a my narod mnogochislennyy, ves' na litse"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Togda My zastavili ikh ostavit' sady, istochniki"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Dragotsennosti, prekrasnyye zhilishcha"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Sdelavshi tak, My otdali ikh v naslediye synam Izrailevym"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Podnyavshis' pri voskhode solnechnom, oni pognalis' za nimi"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "I kogda oba eti opolcheniya uvideli odno drugoye, togda spodvizhniki Moiseya skazali: \"Nas nastigli"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "On skazal: \"Net; so mnoyu Gospod' moy, On ukazhet mne put'"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Togda My v otkrovenii Moiseyu skazali: \"Udar' zhezlom tvoim po moryu\". I ono razdelilos' i kazhdaya storona yego stala kak velikaya gora"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Potom My dali priblizit'sya i drugim"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "My spasli Moiseya i vsekh, kto byl s nim"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "I potopili drugikh"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Istinno, v etom znameniye; no mnogiye iz nikh ne delayutsya veruyushchimi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Istinno, Gospod' tvoy silen, miloserd"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Prochitay im povest' ob Avraame"
        }
    ]
}