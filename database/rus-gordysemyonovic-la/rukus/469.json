{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogda postignet postigayushchiy"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ne budet ni odnoy dushi, eto postizheniye lozhnym schitayushchey"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "odnogo unizhaya, drugogo vozvyshaya"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kogda zemlya, koleblyas', zakolebletsya"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "kogda gory, rassypayas', rassypyatsya"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i sdelayutsya razvevayushchimsya prakhom"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "togda vy razdelyaytes' na tri otdela"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "stoyashchiye na pravoy storone (kto oni, stoyashchiye na pravoy storone)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i stoyashchiye na levoy storone (kto oni, stoyashchiye na levoy storone)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i stoyashchiye na peredi (kto oni, stoyashchiye na peredi)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Eti, - priblizhennyye k Bogu"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Oni budut v sadakh sladosti"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "eto sonm lyudey iz drevnikh"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i nemnogiye iz poslednikh"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na ukrashennykh dorogimi kamnyami sedalishchakh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblokotivshis' na nikh i sidya odni protiv drugikh"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Vechno yunyye otroki budut nosit' vokrug ikh"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Kubki, bratiny, chashi s napitkom"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ot kotorogo oni ne odureyut, ni op'yaneyut"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "S plodami, kakiye zakhotyat vybrat'"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "S myasami ptits, kakikh pozhelayut"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Chernozenichnyye, veleokiye"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "podobnyye khranimym zhemchuzhinam"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Budut im vozdayaniyem za to, chto oni prezhde delali"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tam oni ne uslyshat ni prazdnosloviya, ni vozbuzhdeniya ko grekhu"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "A tol'ko slova: \"mir! Mir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Stoyashchiye po pravoy storone - Kto oni stoyashchiye na pravoy storone"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Oni budut sredi lotosov, ne imeyushchikh shchipov"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Sredi bananov, na kotorykh visyat ryady plodov"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "V teni, shiroko rasstilayushcheysya"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Sredi vody razlivayushcheysya"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Sredi obil'nykh plodov"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Kotoryye ne prekrashchayutsya, i nikogda ne zapreshchayutsya"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Budut na vysokikh lozhakh"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "A oni - sozdany nami osobennym sozdaniyem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "My im polozhili byt' devami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Muzh'yam milymi, po vozrastu ravnymi"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oni - dlya stoyashchikh na pravoy storone"
        }
    ]
}