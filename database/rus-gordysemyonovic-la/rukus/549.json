{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Razmyshlyal li ty o tom, kotoryy lozhnoy schitayet etu veru"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "A potomu on gonit ot sebya sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ne privetit k svoyemu stolu bednotu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore tem molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kotoryye vo vremya svoyey molitvy neradivy"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "I otkazyvayutsya prizrit' drugikh"
        }
    ]
}