{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Derevo zakkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Budet pishcheyu greshnikam"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kak rasplavlennaya med', ona budet kipet' vo chreve"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Kak kipit kipyashchaya voda"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Skhvatite yego i bros'te v sredinu adskogo plameni"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "leyte na golovu yemu muku ot kipyashchey vody"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Nasladis'! Ty silen, znaten"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Eto to, o chem vy somnevayetes'"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Blagochestivyye budut v bezopasnom zhilishche"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Sredi sadov i istochnikov"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Budut odety v atlas i v shelk, vykazyvaya sebya pred ochami drug druga"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Tak; i My sochetayem ikh s chernoglazymi, ochistymi"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Tam oni kakikh ne sprosyat plodov, vsyakiye poluchat naverno"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Tam oni ne vkusyat drugoy smerti, posle pervoy smerti; i On sokhranit ikh ot muki v adskom plameni"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Tak budet po shchedrotam Gospoda tvoyego; eto - velikoye blazhenstvo"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Istinno, My sdelali yego legkim, izlozhiv yego na yazyke tvoyem, s tem, chtoby oni ponimali"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "I tak, zhdi, takzhe kak i oni zhdut"
        }
    ]
}