{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Klyanus' bystrobegushchimi s pykhan'yem"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "I vybivayushchimi iskry"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Puskayushchimisya v nabeg s rannego utra"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Podnimayushchego vo vremya yego pyl'"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Vryvayushchimisya vo vremya yego v seredinu tolpy"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Istinno, chelovek pred Gospodom svoim neblagodaren"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "On sam tomu svidetel'"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "v lyubvi k zdeshnim blagam on krepok"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "uzhe li ne znayut, chto, kogda vstrevozhitsya to, chto v grobakh"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i obnaruzhitsya to, chto v serdtsakh"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Gospod' ikh v tot den' uvedayet ikh"
        }
    ]
}