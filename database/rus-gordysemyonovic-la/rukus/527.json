{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanus' nebom, ukrashennym sozvezdiyami zodiaka"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "I dnem predvozveshchennym"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Klyanus' svidetel'stvuyushchim, i tem, o chem oni svidetel'stvuyut"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Gibel' iskopavshim rov"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Razzhigavshim v nem ogon', podderzhivavshim yego rastopkoyu"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Vot, oni sideli pri nem"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "I sami byli svidetelyami tomu, chto delali veruyushchim"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Oni muchili ikh tol'ko za to, chto verovali oni v Boga, sil'nogo, slavnogo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "V Togo, u kogo vlast' nad nebesami i zemleyu: Bog svidetel' vsemu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Tem, ot kogo podvergayutsya mucheniyu veruyushchiye i veruyushchiya, i kotoryye posle togo ne pokayutsya, - tem muka geyennskaya, tem muka v plameni goryashchem"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "A kotoryye uverovali i delayut dobroye, tem sady rayskiye, po kotorym tekut reki. Eto - velikoye blazhenstvo"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Sila Gospoda tvoyego krepka"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "On proizvodit i unichtozhayet"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On, proshchayushchiy, lyubyashchiy"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vladyka prestola, slavnyy"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Yest' sovershitel' togo, chto zakhochet"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Razve ne dokhodil do tebya rasskaz ob etikh voyskakh"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "O Faraone, o Femudyanakh"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "No nevernyye v lozhnoy vere"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Bog otovsyudu okruzhayet ikh"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Eto ucheniye yest' slavnyy Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Na khranimoy skrizhali"
        }
    ]
}