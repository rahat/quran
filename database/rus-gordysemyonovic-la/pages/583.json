{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "A dlya blagochestivykh blazhennoye zhilishche"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Sady i vinogradniki"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Polnogrudyye, ravnyye im po letam"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "I polnyye chashi"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tam ne uslyshat oni ne pustosloviya, ni lzhi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Eto v nagradu im ot Gospoda tvoyego, v dar i vozdayaniye"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Ot Gospoda nebes, zemli i togo, chto mezhdu nimi, ot Milostivogo. Oni ne vozmogut togda obratit' k Nemu rechi svoyey"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "V etot den', v kotoryy dukh i angely stanut chinami, budut govorit' tol'ko te, kotorym pozvolit Milostivyy, i oni skazhut tol'ko vernoye"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Etot den' yest' nesomnennyy den'. Kto zakhochet, tot izberet put' ko Gospodu svoyemu"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "My dayem vam znat' o muke, uzhe blizkoy; o dne, v kotoryy chelovek uvidit to, chto prigotovili ruki yego, i v kotoryy nechestivyy skazhet: \"O yesli by ya sdelalsya prakhom"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Klyanus' istorgayushchimi nasil'no"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Vynimayushchimi krotko"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Klyanus' plavayushchimi bystro"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Operezhivayushchimi daleko"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Ispolnyayushchimi poveleniya"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Kogda potryasayushchaya potryaset"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "I povtoritsya za nim posleduyushchiy"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "V tot den' serdtsa zatrepeshchut"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Vzory poniknut"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Oni govoryat: \"Uzhe li my, soshedshi v mogilu"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Uzhe li i togda, kak sdelayemsya sognivshimi kostyami"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Oni govoryat: \"Da, eto - bedstvennaya pora"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Togda - tol'ko odin groznyy krik"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "I - vot, oni uzhe v neusypayushchem"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Ne dokhodil li do tebya rasskaz o Moiseye"
        }
    ]
}