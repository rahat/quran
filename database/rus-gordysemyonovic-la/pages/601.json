{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Klyanus' vechernim vremenem"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Rod chelovecheskiy - v zabluzhdenii"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Krome tekh, kotoryye veruyut, delayut dobryye dela, uchat drug druga istine, uchat drug druga terpeniyu"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore kazhdomu khulitelyu, ponositelyu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Kotoryy sobirayet imushchestvo i kopit yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dumaya, chto yego imushchestvo uvekovechit yego"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Net, on vverzhen budet v Khotamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "O yesli by kto vrazumil tebya, chto takoye Khotama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Eto - razzhennyy ogon' Bozhiy"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Kotoryy vzvivayetsya vkrug serdets"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "I nad nimi smykayetsya v svod"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Na vysokikh stolbakh"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ne vspominal li ty, kak postupil Gospod' tvoy s soputnikami slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ne obratil li On umysla ikh k ikh zhe pogibeli"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "On poslal na nikh stai ptits"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Oni brosali v nikh kamni iz obozhennykh glinyanykh glyb"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Tak, On sravnyal ikh s potravlennoy nivoy"
        }
    ]
}