{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogda solntse obov'yetsya mrakom"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Kogda zvezdy pomerknut"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Kogda gory s mest svoikh sdvinutsya"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kogda devyat' mesyatsev suzherebyye verblyuditsy okazhutsya prazdnymi"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Kogda zveri stolpyatsya"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kogda morya zakipyat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "kogda dushi sopryagutsya"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kogda pokhoronennaya zhivoyu budet sproshena"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakoy grekh ona ubita"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kogda svitki razov'yutsya"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Kogda nebo, kak pokrov, snimetsya"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Kogda ad razgoritsya"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "I kogda ray priblizitsya"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Togda dusha uznayet, chto zagotovila ona sebe"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Klyanus' planetami"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Skryvayushchimisya kometami"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Klyanus' noch'yu, kogda ona temneyet"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Klyanus' utrom, kogda ono prokhladno veyet"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Deystvitel'no, on - slovo poslannika, slavnogo"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Sil'nogo pred Vlastitelem prestola, polnomochnogo"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Pokorno chtimogo, vernogo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I sograzhdanin vash ne besnuyushchiysya"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "On nekogda videl yego na svetlom nebosklone"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "U nego net nedoumeniya o tainstvennom"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "On ne yest' slovo satany, progonyayemogo kamnyami"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Za chem zhe vy idete proch'"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "On - uveshchaniye miram"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Tem iz vas, kotoryye khotyat idti pryamo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "No vy mozhete khotet' tol'ko togo, chego khochet Bog, Gospod' mirov"
        }
    ]
}