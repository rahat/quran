{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Vysoko tsenite vy zdeshnyuyu zhizn'"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "No budushchaya luchshe, potomu chto vechna"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Tak i v prezhnikh svitkakh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "V svitkakh Avraama i Moiseya"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Dokhodila li do tebya vest' o nakryvayushchem"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "V etot den' nekotoryye budut s litsami ponikshimi"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Tomnymi, unylymi"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Opalyayemyye zhguchim ognem"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Pit'ye im budet iz sil'no kipyashchego istochnika"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Pishcheyu im budet tol'ko tern dzarig"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "On ne utuchnit, da i goloda ne utolit"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "V etot den' nekotoryye budut s litsami radostnymi"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Udovletvoryayas' svoimi podvigami"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "V gornem raye"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "V nem ne uslyshish' prazdnosloviya"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "V nem istochnik protochnyy"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "V nem sedalishcha vysokiye"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Kubki rasstavlennyye"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Podushki, ryadami nalozhennyye"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Kovry razostlannyye"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Uzhe li oni ne obrashchayut vzora na verblyudov, kak oni sozdany"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Na nebo, kak ono vozvysheno"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Na gory, kak oni postavleny"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Na zemlyu, kak ona razostlana"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Uchi ikh; ty ikh uchitel'"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nad nimi ty ne nadziratel'"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Tekh zhe, kotoryye otvorotyatsya i budut neveruyushchimi"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Bog nakazhet samym velikim nakazaniyem"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Istinno, k Nam budet vozvrashcheniye ikh"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Togda nashim delom budet rasschitat' ikh"
        }
    ]
}