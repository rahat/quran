{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Koreyshane, za soglas svoy"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Za soglas mezhdu soboy v otpravlenii karavanov zimoy i letom"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Da poklanyayutsya Gospodu sego doma"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Pitayushchemu ikh vo vremya glada, okhranyayushchemu ikh ot strakha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Razmyshlyal li ty o tom, kotoryy lozhnoy schitayet etu veru"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "A potomu on gonit ot sebya sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ne privetit k svoyemu stolu bednotu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore tem molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kotoryye vo vremya svoyey molitvy neradivy"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "I otkazyvayutsya prizrit' drugikh"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "My dali tebe kevser"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Potomu molis' Gospodu tvoyemu i zakoli zhertvu"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Nenavidyashchiy tebya ischeznet"
        }
    ]
}