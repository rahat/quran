    "chapter": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Koreyshane, za soglas svoy"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Za soglas mezhdu soboy v otpravlenii karavanov zimoy i letom"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Da poklanyayutsya Gospodu sego doma"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Pitayushchemu ikh vo vremya glada, okhranyayushchemu ikh ot strakha"
        }
    ]
}