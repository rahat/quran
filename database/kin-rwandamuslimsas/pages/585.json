{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Intumwa Muhamadi) yazinze umunya inatera umugongo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ubwo umuntu utabona (Abdullah bin Umm-Maktum) yari aje ayisanga (igihe yigishaga bamwe mu bikomerezwa by’Abakurayishi)"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ese wabibwirwa n’iki? Wenda arashaka kwiyeza"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Cyangwa akakira urwibutso, hanyuma urwibutso rukamugirira akamaro"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Naho wa wundi wirata yumva ko adakeneye (kuyoboka)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ni we witaho"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Kandi nta cyo wabazwa aramutse atiyejeje (ngo ave mu buhakanyi, kuko wowe icyo ushinzwe ari ugusohoza ubutumwa bwa Allah gusa)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ariko wa wundi uje akugana yihuta"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Kandi akaba atinya (Allah n’ibihano bye)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Uramwirengagiza (ukita ku bandi)"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Oya ntibikwiye! Mu by’ukuri (iyi Qur’an) ni urwibutso"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Bityo, ubishaka nayizirikane"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Iri) mu nyandiko zirinzwe mu cyubahiro (Lawuhul Mahfudh)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Zubahitse kandi zejejwe"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ziri mu maboko y’abanditsi (abamalayika bajyana ubutumwa ku bantu)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "B’abanyacyubahiro kandi b’inyangamugayo"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Umuntu (w’umuhakanyi) navumwe! Mbega ukuntu ari umuhakanyi (w’indashima)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ese (Allah) yamuremye mu ki"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Yamuremye mu ntanga, arangije amugenera uko azabaho"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Nuko amworohereza inzira (y’imibereho ye)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Nyuma amugenera urupfu no gushyingurwa"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Kandi igihe azabishakira azamuzura"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Oya! Nyamara (umuntu) ntarakora ibyo yamutegetse"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ngaho umuntu niyitegereze amafunguro ye"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Uko tumanura amazi (mu kirere) yisuka ari menshi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Nuko tugasatura ubutaka uko bikwiye (kugira ngo amazi abashe kubwinjiramo n’imbuto zimere)"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Maze tukabumezamo impeke"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "N’imizabibu n’imboga"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "N’imizeti n’imitende"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "N’imirima igizwe n’ibiti by’inzitane"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "N’imbuto n’ubwatsi"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Kugira ngo bibagirire akamaro, mwe n’amatungo yanyu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ariko ubwo urusaku ruziba amatwi (impanda y’imperuka) ruzaza"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Uwo munsi umuntu azahunga umuvandimwe we"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "(Ahunge) nyina na se"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "N’umugore we n’abana be"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kuri uwo munsi, buri muntu azaba afite ibimuhangayikishije (bituma atita ku bandi)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Kuri uwo munsi, uburanga (bw’abemeramana) buzaba bwererana"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Bamwenyura, bishimiye inkuru nziza bahawe (y’Ijuru)"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Naho uburanga (bw’abahakanyi) kuri uwo munsi buzaba bwuzuye umukungugu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Bwarenzwe n’umwijima"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Abo ni bo bahakanyi b’inkozi z’ibibi"
        }
    ]
}