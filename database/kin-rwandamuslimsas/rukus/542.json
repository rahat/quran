{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ndahiye amafarasi yiruka cyane avuza imirindi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Akubita (ibinono) bigatarutsa ibishashi by’umuriro"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "N’amafarasi agaba igitero mu rukerera"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Akanatumura umukungugu"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Akinjira hagati mu ngabo z’umwanzi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Mu by’ukuri umuntu ni indashima kuri Nyagasani we"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Kandi rwose ibyo yabibera umuhamya"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ndetse akunda imitungo bikabije"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ese ntazi ko hari igihe abari mu mva bazazurwa"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "N’ibiri mu bituza (by’abantu) bigashyirwa ahagaragara"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Mu by’ukuri uwo munsi Nyagasani wabo azaba azi neza ibyabo"
        }
    ]
}