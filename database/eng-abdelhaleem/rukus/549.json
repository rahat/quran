    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "[Prophet], have you considered the person who denies the Judgement"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "It is he who pushes aside the orphan"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "and does not urge others to feed the needy"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "So woe to those who pray"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "but are heedless of their prayer"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "those who are all show"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "and forbid common kindnesses"
        }
    ]
