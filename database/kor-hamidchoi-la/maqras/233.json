{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "geudeul-eun mueos-e gwanhayeo seolo-ege mudneun geos-inga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "widaehan sosig-e gwanhaeseoni"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "geudeul-eun geue gwanhae uigyeon-eul dallihago issnola"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "geuleohji anhnola. geudeul-eun god alge doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "dasi hanbeon, geuleohji anhnola. geudeul-eun god alge doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "naega daejileul chimsang-eulo duji anh-assdeonga"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "ttohan san-eul malttug-eulo duji anh-assdeonga"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "naneun geudaedeul-eul namnyeolo changjohayeoss-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "geudaedeul-ui jam-eul danjeol-ui sigan-eulo dueossnola"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "geuligo bam-eul os-eulo dueossgo"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "naj-eul saeng-gyelo dueossnola"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "geuligo geudaedeul wie ilgob gaeui ganghan geos(haneul)eul jieoss-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "chanlanhan deungbul(taeyang)eul dueossnola"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "geuligo bisbang-ul-eul jjanaeneun guleum-eseo peobusneun bileul naelyeoss-euni"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "geugeos-eulosseo gogmulgwa yacholeul naego"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "namuga ugeojin jeong-wondo naenola"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "sillo bunliui nal-eun giyagdoeeo issnola"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "geunal napal-i bullimyeon geudaedeul-eun ttejieo ol geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "haneul-eun yeollieo mun(mun)cheoleom doego"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "sandeul-eun heulleonaelieo singilucheoleom doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "sillo jiog-eun jamboghae iss-euni"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "gyeong-gyeleul neom-eoseon jadeul-eul wihan gwichagjila"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "geugos-eseo geudeul-eun yeong-guui sigan-eul meomul geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "geudeul-eun geugos-eseo siwonhamdo, eumlyodo masboji moshal geos-ini"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "ojig bugeulgeolineun mulgwa goleum ppun-ila"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "ineun sang-eunghaneun jingbeolloseo"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "sillo geudeul-eun simpan-eul gaeuichi anh-ass-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "naui jingpyoleul gojibseule bu-inhayeossnola"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "modeun geos-eul naneun gilogbue gijaehayeossnola"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "geuleoni neohuineun masbola, naega neohuiege jeung-gasikil geos-eun ojig beol ppun-ila"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "sillo (hananim-eul) gyeong-oehaneun jadeul-egeneun seong-gong-ui jangsoga jueojil geos-ini"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "tteulgwa podonamu"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "bong-geushan gaseum-ui dong-gabnaegi yeoseong"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "gadeug chan suljan-i jueojil geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "geudeul-eun geugos-eseo sil-eobsneun maldo, geojismaldo deudji anh-eul geos-ila"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "geudae junim-eulobuteoui bosang-euloseo bepulm-ija gyesan-ui gyeolgwaloseo"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "haneulgwa daeji geuligo geu saie jonjaehaneun geos-ui junim, gajang jabilousin bun-egeseo on geos-ila. geudeul-eun geubunkke gwanhayeo eotteohan bal-eongwondo gaj-ji moshal geos-ila"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yeonghon(gabeuliel cheonsa)gwa cheonsadeul-i daeyeol-eul jieo seoneun geunal geudeul-eun mal-eul haji moshal geos-ina, gajang jabilousin bunkkeseo heolaghasin janeun ye-oelo geuneun jinsil-eul malhal geos-ila"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "igeos-iyamallo jinliui nal-ila. geuleoni wonhaneun janeun geuui junimkkelo gwichagjileul dula"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "sillo naneun geudaedeul-ege gakkaun beol-eul gyeong-gohayeossnola. geunal ingan-eun jasin-ui du son-i naenoh-eun geos-eul bol geos-imyeo bulsinjaneun “naega heulg-ieossdamyeon joh-ass-eul geos-eul!” ilago malhage doel geos-ila"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "sanabge kkeujib-eonaeneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "yuyeonhi ppob-anaeneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "nallyeobhi yuyeonghaneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "gyeongjaenghadeus apseoganeun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "il-eul gyeoljeongjisneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "geunal yodongchineun geos-i yodongchigo"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "dwiseoneun geos-i geugeos-eul dwittaleul geos-ila"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "geunal ma-eum-eun tteollimyeo"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "geudeul-ui siseon-eun tteolgueojil geos-ila"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "geudeul-i malhani “jinsillo uliga ijeon sangtaelo doedol-agandan mal-inga?”"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "“uliga buseuleojineun ppyeoga doeeossneundedo geuleohdan mal-inga?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "geudeul-i malhagil “geugeos-eun geuleohdamyeon sonhaega doel gwihwan-ila”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "sillo geugeos-eun hanbeon-ui goham soliil ppun-ila"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "bola, ije geudeul-eun wongi gadeughan daeji wie issnola"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "geudae(muhammadeu)ege mose-ui iyagiga dolaehayeossneunga"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "geuui junimkkeseo seongseuleoun goljjagiin ‘ttuwa’ eseo geuleul bulleossnola"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“geudaeneun palao-egelo gala, sillo geuneun gyeong-gyeleul neom-eoseossnola”"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "“geuligo malhala : ‘geudae(palao)neun kkaekkeushaejigil balaneunga?’ ”"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“ ‘naega geudaeleul geudae junim-eulo indohalyeo hani, geuleohge geudaeneun (hananim-eul) gyeong-oehage doel geos-ila’ “"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "geu hu geu(mose)neun geu(palao)ege eomahan jingpyoleul boyeojueoss-euna"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "geuneun bu-inhago bulboghayeossnola"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "geu hu geuneun kkongmunileul ppaemyeo (geoyeogham-e) maejinhani"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(salamdeul-eul) moa bulleossnola"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "geu hu geuga malhagil “jim-eun geudaedeul-ui gajang jigohan junim-ila”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "geulihayeo hananimkkeseoneun naesewa hyeonse-ui jing-gyeloseo geuleul umkyeojab-eusyeossnola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "jinsillo igeos-eneun dulyeowohaneun jaleul wihan gyohun-i issnola"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "changjoga deo eolyeoun geos-eun geudaedeul-inga animyeon haneul-inga? geubunkkeseo geugeos(haneul)eul jieusyeossnola"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "geubunkkeseoneun geugeos(haneul)ui kkogdaegileul ollisyeossgo geu hu geugeos-eul gyundeunghage dusyeoss-eumyeo"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "geugeos-ui bam-eul eodubge hasyeossgo geugeos-ui achimbich-eul naesyeossnola"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "ttang-eun geu hue pyeolchisyeossgo"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "geugeos(ttang)eulobuteo geugeos-ui mulgwa mogcho(mogcho)leul naesyeoss-eumyeo"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "san-eun dandanhi gojeongsikisyeoss-euni"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "geudaedeulgwa geudaedeul gachug-eul wihan pyeon-uila"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "geulihayeo eomcheongnan daejaeang-i dolaehayeoss-eul ttae"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "geunal ingan-eun jasin-ui nolyeog-eul tteoollil geos-ila"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "boneun ja-ege jiog-ui moseub-i deuleonal ttae"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "gyeong-gyeleul neom-eoseomyeonseo"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "hyeonse-ui salm-eul seonhohadeon janeun"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "sillo taoleuneun buljiog, geugos-i geojujiga doel geos-ila"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "geuleona jasin-ui junim ap-e seoneun il-eul dulyeowohamyeo jasin-ui yogguleul jeoljehadeon janeun"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "sillo cheongug, geugos-i geojujiga doel geos-ila"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "geudeul-i geudae(muhammadeu)ege jongmal-ui sigan-e gwanhae mul-euni “geugeos-eun eonje gaesidoendan mal-inga?”"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "geugeos(gaesi sigan)ui eongeub-e gwanhayeo geudaega eotteon gwanlyeon-i issdan mal-inga"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "geugeos-ui jisig-eun geudae junimkkeman issneun geos-ila"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "geudaeneun ojig geugeos(jongmal-ui sigan)eul dulyeowohaneun jaleul wihan gyeong-gojail ppun-ila"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "geugeos(jongmal-ui sigan)eul boneun nal geudeul-eun machi (hyeonseeseo) ohuna achimnajeolman-eul meomulleossda saeng-gaghal geos-ila"
        }
    ]
}