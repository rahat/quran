{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "sillo (hananim-eul) gyeong-oehaneun jadeul-egeneun seong-gong-ui jangsoga jueojil geos-ini"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "tteulgwa podonamu"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "bong-geushan gaseum-ui dong-gabnaegi yeoseong"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "gadeug chan suljan-i jueojil geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "geudeul-eun geugos-eseo sil-eobsneun maldo, geojismaldo deudji anh-eul geos-ila"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "geudae junim-eulobuteoui bosang-euloseo bepulm-ija gyesan-ui gyeolgwaloseo"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "haneulgwa daeji geuligo geu saie jonjaehaneun geos-ui junim, gajang jabilousin bun-egeseo on geos-ila. geudeul-eun geubunkke gwanhayeo eotteohan bal-eongwondo gaj-ji moshal geos-ila"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yeonghon(gabeuliel cheonsa)gwa cheonsadeul-i daeyeol-eul jieo seoneun geunal geudeul-eun mal-eul haji moshal geos-ina, gajang jabilousin bunkkeseo heolaghasin janeun ye-oelo geuneun jinsil-eul malhal geos-ila"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "igeos-iyamallo jinliui nal-ila. geuleoni wonhaneun janeun geuui junimkkelo gwichagjileul dula"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "sillo naneun geudaedeul-ege gakkaun beol-eul gyeong-gohayeossnola. geunal ingan-eun jasin-ui du son-i naenoh-eun geos-eul bol geos-imyeo bulsinjaneun “naega heulg-ieossdamyeon joh-ass-eul geos-eul!” ilago malhage doel geos-ila"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "sanabge kkeujib-eonaeneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "yuyeonhi ppob-anaeneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "nallyeobhi yuyeonghaneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "gyeongjaenghadeus apseoganeun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "il-eul gyeoljeongjisneun cheonsaleul dugo maengseko"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "geunal yodongchineun geos-i yodongchigo"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "dwiseoneun geos-i geugeos-eul dwittaleul geos-ila"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "geunal ma-eum-eun tteollimyeo"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "geudeul-ui siseon-eun tteolgueojil geos-ila"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "geudeul-i malhani “jinsillo uliga ijeon sangtaelo doedol-agandan mal-inga?”"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "“uliga buseuleojineun ppyeoga doeeossneundedo geuleohdan mal-inga?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "geudeul-i malhagil “geugeos-eun geuleohdamyeon sonhaega doel gwihwan-ila”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "sillo geugeos-eun hanbeon-ui goham soliil ppun-ila"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "bola, ije geudeul-eun wongi gadeughan daeji wie issnola"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "geudae(muhammadeu)ege mose-ui iyagiga dolaehayeossneunga"
        }
    ]
}