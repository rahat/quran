{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "achim-eul dugo maengsehago"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "eodum-i jit-eojineun bam-eul dugo maengsehasa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "junimkkeseo geudaeleul beoliji anihaess-eumyeo miwohajido anh-eusinila"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "sillo geudaeegeneun naesega hyeonseboda na-eulini"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "junimkkeseo geudaeege eunhyeleul bepulgeos-imae geudaeneun ilohayeo gippeohalila"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "geubun-eun goain geudaeleul balgyeonhayeo geudaeleul bohohaji ani haessdeonyo"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "geubun-eun banghwanghaneun geudaeleul balgyeonhayeo geudaeleul indohaji ani haessdeo nyo"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "geubun-eun gananhaessdeon geudaeleul buyuhage hayeo juji ani haessdeonyo"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "geuleomeulo goadeul-eul geochilge daehajimalmyeo"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "guhaneun ja-ege geojeolhaji malgo"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "geuligo junim-ui eunhyeleul iyagi hala"
        }
    ]
