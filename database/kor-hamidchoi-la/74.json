{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "oetuleul deop-eun jayeo"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "il-eonaseo gyeong-gohala"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "geudae junim-ui widaeham-eul chan-yanghago"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "geudaeui os-eun jeonghwahamyeo"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "usang-eun meollihala"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "geuligo deo manh-i bad-eulyeo eunhyeleul bepulji malmyeo"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "geudae junim-eul wihae innaehala"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "geulihayeo napal-i bullil ttae"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "sillo geunal-eun eolyeoun nal-ila"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "bulsinjadeul-ege swibji anh-eun nal-ila"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "geudaeneun nawa naega holmom-eulo changjohan jaleul noh-adula"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "naneun geuleul wihae jeulbihan jaesan-eul dueossgo"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "hamkke haneun jasondeul-eul dueossnola"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "geuligo naneun geuleul wihae manh-eun geos-eul pyeolchyeo jueossnola"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "geu huedo geuneun naega deohae jugileul huimanghadeola"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "geuleohji anhnola. sillo geuneun naui jingpyoe daehae gojibbulineun jayeossnola"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "naneun geuege godoen beol-i deopchige hal geos-ila"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "sillo geuneun saeng-gaghago jaedeola"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "geuleoni geuga jaeneun bangbeob-e jeojuga issnola"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "dasi hanbeon, geuga jaeneun bangbeob-e jeojuga issnola"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "geu hu geuneun boassgo"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "geu hu geuneun eolgul-eul jjipuligo jjing-geulyeossnola"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "geu hu geuneun kkongmunileul ppaemyeo geomanhae hayeossnola"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "geulihayeo geuga malhagil : \"igeos-eun ojig jeonhaejyeo naelyeooneun jusul-il ppun-ila"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "igeos-eun ojig ingan-ui mal-il ppun-ila"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "naneun geuleul tteugeoun buljiog-eulo taeul geos-ila"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "mueos-i geudaeege tteugeoun buljiog-i mueos-injileul allyeo jul geos-inga"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "geugeos-eun namgyeodujido anh-eumyeo naebeolyeodujido anh-eumyeo"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "pibuleul guwobeolinola"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "yeol-ahob-i geugeos-eul chaeg-imjinola"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "naneun buljiog-ui gwanlijadeul-eul ojig cheonsadeulloman dueoss-eumyeo, naneun geudeul-ui susjaleul ojig bulsinhaneun jadeul-e daehan siheom-eulo dueossnola. geulihayeo seongseoleul bad-eun jadeul-i hwagsin-eul eodgo, midneun jadeul-i mid-eum-i jeung-gadoemyeo, seongseoleul bad-eun jadeulgwa sin-ang-indeul-i uisimhaji anhgi wiham-ila. geuleona ma-eum sog-e byeong-i issneun jadeulgwa bulsinjadeul-eun 'i biyulosseo hananim-i wonhaneun geos-eun mueos-inga?' lago malhadeola. geuleohge hananimkkeseoneun geubunkkeseo wonhasineun jaleul banghwangsikisigo geubunkkeseo wonhasineun jaleul indohasinola. ojig geubunman-i geudae junim-ui gundaeleul algo gyesinola. geugeos(buljiog)eun daleum anin ingan-eul wihan tteoollim-ila"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "geuleohji anhnola. dal-eul dugo maengseko"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "jachwileul gamchul ttaeui bam-eul dugo maengseko"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "balg-aol ttaeui achim-eul dugo maengseko"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "jinsillo geugeos(buljiog)eun eomahan geos jung-ui hanaimyeo"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ingan-eul wihan gyeong-goini"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "geudaedeul jung-eseo ap-eulo naagageona hog-eun dwilo mulleoseogoja haneun jaleul wihan geos-ila"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "modeun yeonghon-eun geuga eod-eun geos-eulo jeondang jabhyeo issnola"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "geuleona upyeon-ui geojujadeul-eun ye-oela"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "cheongug-eseo geudeul-eun seolo mul-euni"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "aghaeng-eul jeojileun jadeul-e gwanhaeseola"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "mueos-i geudaedeul-eul tteugeoun buljiog-e deulge hayeossneunga"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "geudeul-i malhagil : \"ulineun yebae deulineun jadeul-i anieossgo"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "bul-uhan jadeul-ege eumsig-eul jujido anh-assso"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "ulineun nodag-ineun jadeulgwa hamkke nodag-yeossgo"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "simpan-ui nal-eul bujeonghayeoss-euni‚Äù"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "uliege jug-eum-i chaj-aol ttaekkaji geuleohayeossso"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "geuleoni jungjaejadeul-ui jungjaedo geudeul-egeneun soyong-i eobsnola"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "geuleom-edo geudeul-eun tteoollim-e deung-eul dollineun jadeul-ini i eojji doen il-inga"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "geudeul-eun machi dal-eumjilchineun dangnagwideulgwa gat-euni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "sajalobuteo domangchineun dangnagwideul-ila"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "sillo geudeul jung-ui modeun ineun pyeolchyeojin chaegjaleul badgil wonhadeola"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "gyeolko geuleohji anhnola! geudeul-eun naeseleul dulyeowohaji anhnola"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "geuleohji anhnola. sillo geugeos(kkulan)eun tteoollim-ila"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "geuleohgie, wonhaneun janeun geugeos(kkulan)eul tteoollinola"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "geudeul-eun ojig hananimkkeseo wonhasil ttaeman tteoollil geos-ila. geubunkkeseoneun gyeong-oesim-eul bad-eusil bun-imyeo yongseoui gwonlileul gajisineun bun-ila"
        }
    ]
}