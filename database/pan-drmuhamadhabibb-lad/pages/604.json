{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Ākhō, ki uha alāha ika hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alāha kisē cīza dā lōṛavada nahīṁ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nā usa dī kō'ī aulāda hai atē nā uha kisē dī aulāda"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Atē kō'ī usa dē barābara nahīṁ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Ākhō, maiṁ śaraṇa magadā hāṁ, savēra dē raba dī"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Hara cīza dī burā'ī tōṁ jihaṛī usa nē paidā kītī"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Hanērē dī burā'ī tōṁ jadōṁ uha (hanērā) chā jāvē"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Atē gaḍhāṁ tē (paṛha-paṛha kē) phūkāṁ mārana vāli'āṁ dī burā'ī tōṁ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Atē īrakhā karana vālē'dī burā'ī tōṁ, jadōṁ uha īrakhā karē"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Ākhō, maiṁ śaraṇa magadā hāṁ, lōkāṁ dē raba dī"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lōkāṁ dē pātaśāha dī"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lōkāṁ dē pūjanīka dī"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Śaitāna) vasavasā (māṛā ḵẖi'āla) pā'uṇa vālē dī burā'ī tōṁ, jihaṛā (alāha dā nāma suṇa kē) luka jāndā hai"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jihaṛā lōkāṁ dē dilāṁ vica vasavasā (māṛā khi'āla) pā'undā hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jināṁ vicōṁ jāṁ manukhāṁ vicōṁ"
        }
    ]
}