{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Ākhō, maiṁ śaraṇa magadā hāṁ, lōkāṁ dē raba dī"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lōkāṁ dē pātaśāha dī"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lōkāṁ dē pūjanīka dī"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Śaitāna) vasavasā (māṛā ḵẖi'āla) pā'uṇa vālē dī burā'ī tōṁ, jihaṛā (alāha dā nāma suṇa kē) luka jāndā hai"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jihaṛā lōkāṁ dē dilāṁ vica vasavasā (māṛā khi'āla) pā'undā hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jināṁ vicōṁ jāṁ manukhāṁ vicōṁ"
        }
    ]
}