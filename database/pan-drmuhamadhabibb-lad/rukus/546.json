{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vināśa hai harēka vi'aga kasaṇa vālē atē nukasa kaḍhaṇa vālē la'ī"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Jisa nē jā'idāda nū ikaṭhā kītā atē giṇa-giṇa kē rakhi'ā"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Uha sōcadā hai ki usa dī jā'idāda hamēśā usa dē nāla rahēgī"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kadē vī nahīṁ, uha raundaṇa vālī jag'hā vica suṭi'ā jāvēgā"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Atē tusīṁ kī jāṇō ki uha raundaṇa vālī jag'hā kī hai"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Alāha dī bhaṭakā'ī hō'ī aga"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Jihaṛī dilāṁ taka pūjēgī"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Uha unhāṁ la'ī bada kara ditī jāvēgī"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ucē-ucē khabi'āṁ vica"
        }
    ]
}