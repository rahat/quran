{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Jadōṁ hōṇa vālī ghaṭanā hō jāvēgī"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Usa dē vāparana vica kō'ī jhūṭha nahīṁ"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Uha nīnvā atē ucā karana vālī hōvēgī"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jadōṁ dharatī hilā ditī jāvēgī"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Atē pahāṛa ṭuṭa kē cūrā cūrā hō jāṇagē"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Phira uha khilarī hō'ī dhūṛa saṇa kē rahi jāṇagē"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Atē tusīṁ lōka tina tar'hāṁ dē hō jāvēṅgē"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Phira sajē hatha vālē, kī khūba hana"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Atē khabē hatha vālē (aphasōsa), khabē hatha vālē kihō jihē būrē hana"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Atē agē vālē tāṁ agē vālē hī hana"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Uha nēṛē rahiṇa vālē lōka hana"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ni'amatāṁ dē bāġāṁ vica"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Unhāṁ dī vaḍī sakhi'āṁ pahilē lōkāṁ vica hōvēgī"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Atē thōṛhē pichali'āṁ vicōṁ hōṇagē"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Hīrē mōtī'āṁ nāla) jaṛā'ū taḵẖatāṁ tē"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Sir'hāṇā lā kē āhamaṇē sāhamaṇē baiṭhē hōṇagē"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Unhāṁ dē kōla hamēśāṁ rahiṇa vālē bacē (naujavāna) ghuma rahē hōṇagē"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Kapa atē jaga lai kē atē vadhī'ā śarāba dā pi'ālā"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Usa nāla nā sira dukhēgā nā budhī vica vikāra āvēgā"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Atē fala jihaṛē uha cāhuṇa zuṇa laiṇa"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Atē pachī'āṁ dā uha māsa jihaṛā unhāṁ nū pasada hōvē"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Atē vaḍī'āṁ akhāṁ vālī'āṁ (khūbasūrata) hūrāṁ"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Jivēṁ ḍhakē hō'ē mōtī dē dāṇē"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Fala unhāṁ karamāṁ dā jihaṛē uha karadē sana"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Usa vica uha kō'ī vi'aratha atē pāpa dī gala nahīṁ suṇanagē"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Paratū sirafa unhāṁ nū salāma hōvēgā"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Atē sajē vālē, kī khūba hana sajē vālē"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bēra dē rukhāṁ vica jinhāṁ vica kaḍā nahīṁ"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Atē kēli'āṁ dē guchē"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "atē phailī hō'ī chāṁ"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Atē vagadā hō'i'ā pāṇī"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Atē bhānta bhānta dē fala"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Jihaṛē nā ḵẖatama hōṇagē. Atē nā unhāṁ nū kō'ī rōka ṭōka hōvēgī"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Atē ucē ba̔isatarē"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Asīṁ unhāṁ auratāṁ nū khāsa taura tē baṇā'i'ā"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Phira unhāṁ nū kavārī'āṁ rakhi'ā hai"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Manamōhana vālī'āṁ atē ika samāna umara vālī'āṁ"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Sajē vāli'āṁ la'ī"
        }
    ]
}