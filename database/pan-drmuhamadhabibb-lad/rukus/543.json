{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Khaṛa-khaṛā'uṇa vālī"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kī hai khaṛa-khaṛā'uṇa vālī"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Atē tusīṁ kī jāṇō, ki kī hai, uha khaṛa-khaṛā'uṇa vālī"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Jisa dina lōka patagi'ā dī tarāṁ khil'harē hō'ē hōṇagē"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Atē pahāṛa pijī hō'ī ragalī una dī tarāṁ hō jāṇagē"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Phira jisa badē dā palaṛā bhārī hōvēgā"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Uha mana bhā'undē ārāma vica hōvēgā"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Atē jisa badē dā palaṛā haulā hōvēgā"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Tāṁ usa dā ṭikāṇā khā'ī (naraka) hai"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Atē tusīṁ kī jāṇō, ki uha kī hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Bhaṭakadī hō'ī aga. Śurū alāha dē nāma nāla jihaṛā ati'ata miharabāna atē rahima faramā'uṇa vālā hai"
        }
    ]
}