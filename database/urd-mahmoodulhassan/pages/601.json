{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "قسم ہے عصر کی [۱]"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "مقرر انسان ٹوٹے میں ہے [۲]"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "مگر جو لوگ کہ یقین لائے اور کئے بھلے کام اور آپس میں تاکید کرتے رہے سچے دین کی اور آپس میں تاکید کرتے رہے تحمل کی [۳]"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "خرابی ہے ہر طعنہ دینے والے عیب چننے والے کی [۱]"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "جس نے سمیٹا مال اور گن گن کر رکھا [۲]"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "خیال رکھتا ہے کہ اُس کا مال سدا کو رہے گا اُسکے ساتھ [۳]"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "کوئی نہیں وہ پھینکا جائے گا اُس روندنے والی میں [۴]"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "اور تو کیا سمجھا کون ہے وہ روندنے والی"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ایک آگ ہے اللہ کی سلگائی ہوئی"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "وہ جھانک لیتی ہے دل کو [۵]"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "اُنکو اُس میں موند دیا ہے [۶]"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "لنبے لنبے ستونوں میں [۷]"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "کیا تو نے نہ دیکھا کیسا کیا تیرے رب نے ہاتھی والوں کے ساتھ [۱]"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "کیا نہیں کر دیا اُن کا داؤ غلط [۲]"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "اور بھیجے اُن پر اڑتے جانور ٹکڑیاں ٹکڑیاں"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "پھینکتے تھے اُن پر پتھریاں کنکر کی [۳]"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "پھر کر ڈالا اُنکو جیسے بھس کھایا ہوا[۴]"
        }
    ]
}