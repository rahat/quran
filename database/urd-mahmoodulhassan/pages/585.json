{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "[۱] تیوری چڑھائی اور منہ موڑھا"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "اس بات سے کہ آیا اُسکے پاس اندھا [۲]"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "اور تجھ کو کیا خبر ہے شاید کہ وہ سنورتا"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "یا سوچتا تو کام آتا اُسکے سمجھانا [۳]"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "وہ جو پروا نہں کرتا"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "سو تو اُسکی فکر میں ہے"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "تور تجھ پر کچھ الزام نہیں کہ وہ نہیں درست ہوتا [۴]"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "اور وہ جو آیا تیرے پاس دوڑتا"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "اور ہو ڈرتا ہے [۵]"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "سو تو اُس سے تغافل کرتا ہے [۶]"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "یوں نہیں یہ تو نصیحت ہے"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "پھر جو کوئی چاہے اسکو پڑھے [۷]"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "لکھا ہے عزت کے ورقوں میں"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "اونچے رکھے ہوئے نہایت ستھرے [۸]"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ہاتھوں میں لکھنے والوں کے"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "جو بڑے درجہ والے نیک کار ہیں [۹]"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "مارا جائیو آدمی کیسا نا شکرا ہے [۱۰]"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "کس چیز سے بنایا اُسکو"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "ایک بوند سے [۱۱] بنایا اسکو پھر اندازہ پر رکھا اُسکو [۱۲]"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "پھر راہ آسان کر دی اُسکو [۱۳]"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "پھر اُسکو مردہ کیا پھر قبر میں رکھوا دیا اُسکو [۱۴]"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "پھر جب چاہا اٹھا نکالا اُسکو [۱۵]"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "ہرگز نہیں پورا نہ کیا جو اُسکو فرمایا [۱۶]"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "اب دیکھ لے آدمی اپنے کھانے کو [۱۷]"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "کہ ہم نے ڈالا پانی اوپر سے گرتا ہوا"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "پھر چیرا زمین کو پھاڑ کر [۱۸]"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "پھر اگایا اُس میں اناج"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "اور انگور اور ترکاری"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "اور زیتون اور کھجوریں"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "اور گھن کے باغ"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "اور میوہ اورگھاس"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "کام چلانے کو تمہارے اور تمہارے چوپایوں کے [۱۹]"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "پھر جب آئے وہ کان پھوڑنے والی [۲۰]"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "جس دن کے بھاگے مرد اپنے بھائی سے"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "اور اپنی ماں اور اپنے باپ سے"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "اور اپنی ساتھ والی سے اور اپنے بیٹوں سے"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ہر مرد کو اُن میں سے اُس دن ایک فکر لگا ہوا ہے جو اُسکے لئے کافی ہے [۲۱]"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "کتنے منہ اُس دن روشن ہیں"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ہنستے خوشیاں کرتے [۲۲]"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "اور کتنے منہ اُس دن اُن پر گرد پڑی ہے"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "چڑھی آتی ہے اُن پر سیاہی [۲۳]"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "یہ لوگ وہی ہیں جو منکر ہیں ڈھیٹھ [۲۴]"
        }
    ]
}