{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "قسم ہے آسمان کی اور اندھیری میں آنے والے کی"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "اور تو نے کیا سمجھاکیا ہے اندھیرے میں آنے والا"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "وہ تارا چمکتا ہوا"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "کوئی جی نہیں جس پر نہیں ایک نگہبان"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "اب دیکھ لے آدمی کہ کاہے سے بنا ہے [۱]"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "بنا ہے ایک اچھلتے ہوئے پانی سے [۲]"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "جو نکلتا ہے پیٹھ کے بیچ سے اور چھاتی کے بیچ سے [۳]"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "بیشک وہ اُسکو پھیر لا سکتا ہے [۴]"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "جس دن جانچے جائیں بھید [۵]"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "تو کچھ نہ ہو گا اُسکو زور اور نہ کوئی مدد کرنے والا [۶]"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "قسم ہے آسمان چکر مارنے والے کی [۷]"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "اور زمین پھوٹ نکلنے والی کی [۸]"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "بیشک یہ بات ہے دوٹوک"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "اور نہیں یہ بات ہنسی کی [۹]"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "البتہ وہ لگے ہوئے ہیں ایک داؤ کرنے میں"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "اور میں لگا ہوا ہوں ایک داؤ کرنے میں"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "سو ڈھیل دے منکروں کو ڈھیل دے اُنکو تھوڑے دنوں [۱۰]"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "پاکی بیان کر اپنے رب کے نام کی جو سب سے اوپر [۱]"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "جس نے بنایا پھر ٹھیک کیا [۲]"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "اور جس نے ٹھہرا دیا پھر راہ بتلائی [۳]"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "اور جس نے نکالا چارا"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "پھر کر ڈالا اُسکو کُوڑا سیاہ [۴]"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "البتہ ہم پڑھائیں گے تجھ کو پھر تو نہ بھولے گا"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "مگر جو چاہے اللہ [۵] وہ جانتا ہے پکارنے کو اور جو چھپا ہوا ہے [۶]"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "اور سہج سہج پہنچائیں گے ہم تجھ کو آسانی تک [۷]"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "سو تو سمجھا دے اگر فائدہ کرے سمجھانا [۸]"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سمجھ جائے گا جس کو ڈر ہو گا [۹]"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "اور یکسو رہے گا اُس سے بڑا بدقسمت"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "وہ جو داخل ہو گا بڑی آگ میں [۱۰]"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "پھر نہ مرے گا اُس میں اور نہ جئے گا [۱۱]"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "بیشک بھلا ہوا اُس کا جو سنورا [۱۲]"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "اورلیا اُس نے نام اپنے رب کا پھر نماز پڑھی [۱۳]"
        }
    ]
}