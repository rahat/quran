{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "قسم ہے چلتی ہواؤں کی دل کو خوش آتی"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "پھر جھونکا دینے والیوں کی زور سے [۱]"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "پھر ابھارنے والیوں کی اٹھا کر"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "پھر پھاڑنے والیوں کی بانٹ کر [۲]"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "پھر فرشتوں کی جو اتار کر لائیں وحی [۳]"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "الزام اتارنے کو یا ڈر سنانے کو [۴]"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "مقرر جو تم سے وعدہ ہوا وہ ضرور ہونا ہے [۵]"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "پھر جب تارے مٹائے جائیں"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "اور جب آسمان میں جھروکے پڑ جائیں [۶]"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "اور جب پہاڑ اڑا دیے جائیں [۷]"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "اور جب رسولوں کا وقت مقرر ہو جائے [۸]"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "کس دن کے واسطے ان چیزوں میں دیر ہے"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "اس فیصلے کے دن کے واسطے [۹]"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "اور تو نے کیا بوجھا کیا ہے فیصلے کا دن"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "خرابی ہے اس دن جھٹلانے والوں کی [۱۰]"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "کیا ہم نے نہیں مار کھپایا پہلوں کو"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "پھر اُنکے پیچھے بھیجتے ہیں پچھلوں کو"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "ہم ایسا ہی کیا کرتے ہیں گنہگاروں کے ساتھ [۱۱]"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۱۲]"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "کیا ہم نے نہیں بنایا تمکو ایک بے قدر پانی سے"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "پھر رکھا اُسکو ایک جمے ہوئے ٹھکانے میں [۱۳]"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "ایک وعدہ مقرر تک [۱۴]"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "پھر ہم اُسکو پورا کر سکے سو ہم کیا خوب سکت والے ہیں [۱۵]"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "خرابی ہے اس دن جھٹلانے والوں کی [۱۶]"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "کیا ہم نے نہیں بنائی زمین سمیٹنے والی"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "زندوں کو اور مردوں کو [۱۷]"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "اور رکھے ہم نے زمین میں بوجھ کے لئے پہاڑ اونچے اور پلایا ہم نے تمکو پانی میٹھا پیاس بھجانے والا [۱۸]"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۱۹]"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "چل کر دیکھو جس چیز کو تم جھٹلاتے تھے [۲۰]"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "چلو ایک چھاؤں میں جسکی تین پھانکیں ہیں [۲۱]"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "نہ گہری چھاؤں اور نہ کچھ کام آئے تپش میں [۲۲]"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "وہ آگ پھینکتی ہے چنگاریاں جیسے محل [۲۳]"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "گویا وہ اونٹ ہیں زرد [۲۴]"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "خرابی ہے اس دن جھٹلانے والوں کی [۲۵]"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "یہ وہ دن ہے کہ نہ بولیں گے [۲۶]"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "اور نہ اُنکو حکم ہو کہ توبہ کریں [۲۷]"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۲۸]"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "یہ ہے دن فیصلے کا جمع کیا ہم نے تمکو اور اگلوں کو [۲۹]"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "پھر اگر کچھ داؤ ہے تمہارا تو چلا لو مجھ پر [۳۰]"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "خرابی ہے اس دن جھٹلانے والوں کی [۳۱]"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "البتہ جو ڈرنے والے ہیں وہ سایہ میں ہیں [۳۲] اور نہروں میں"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "اور میوے جس قسم کے وہ چاہیں"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "کھاؤ اور پیو مزے سے بدلا اُن کاموں کا جو تم نے کئے تھے [۳۳]"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "ہم یونہی دیتے ہیں بدلا نیکی والوں کو"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۳۴]"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "کھا لو اور برت لو تھوڑے دنوں بیشک تم گنہگار ہو [۳۵]"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۳۶]"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "اور جب کہیے اُن کو کہ جھک جاؤ نہیں جھکتے [۳۷]"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "خرابی ہے اُس دن جھٹلانے والوں کی [۳۸]"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "اب کس بات پر اُسکے بعد یقین لائیں گے [۳۹]"
        }
    ]
}