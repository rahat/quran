{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "سو قسم کھاتا ہوں اُن چیزوں کی جو دیکھتے ہو"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "اور جو چیزیں کہ تم نہیں دیکھتے"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "کہ کہا ہے ایک پیغام لانے والے سردار کا [۲۳]"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "اور نہیں ہے یہ کہا کسی شاعر کا تم تھوڑا یقین کرتے ہو [۲۴]"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "اور نہیں ہے کہا پریوں والے کا تم بہت کم دھیان کرتے ہو [۲۵]"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "یہ اتارا ہوا ہے جہان کے رب کا [۲۶]"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "اور اگر یہ بنا لاتا ہم پر کوئی بات"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "تو ہم پکڑ لیتے اس کا داہنا ہاتھ"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "پھر کاٹ ڈالتے اُسکی گردن"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "پھر تم میں کوئی ایسا نہیں جو اُس سے بچا لے [۲۷]"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "اور یہ نصیحت ہے ڈرنے والوں کو"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "اور ہمکو معلوم ہے کہ تم میں بعضے جھٹلاتے ہیں"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "اور وہ جو ہے پچتاوا ہے منکروں پر [۲۸]"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "اور وہ جو ہے یقین کرنے کے قابل ہے"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "اب بول پاکی اپنے رب کے نام کی جو ہے سب سے بڑا [۲۹]"
        }
    ]
}