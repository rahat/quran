{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "تو کہہ اے منکرو [۱]"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "میں نہیں پوجتا جسکو تم پوجتے ہو"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "اور نہ تم پوجو جسکو میں پوجوں [۲]"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "اور نہ مجھ کو پوجنا ہے اُس کا جسکو تم نے پوجا"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "اور نہ تم کو پوجنا ہے اُس کا جس کو میں پوجوں [۳]"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "تم کو تمہاری راہ اور مجھ کو میری راہ [۴]"
        }
    ]
}