{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "مقرر درخت سیہنڈ کا"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "کھانا ہے گنہگار کا [۳۵]"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "جیسے پگھلا ہوا تانبا کھولتا ہے پیٹوں میں"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "جیسے کھولتا ہوا پانی"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "پکڑو اسکو اور دھکیل کر لیجاؤ بیچوں بیچ دوزخ کے [۳۶]"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "پھر ڈالو اُسکے سر پر جلتے پانی کا عذاب [۳۷]"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "یہ چکھ تو ہی ہے بڑا عزت والا سردار [۳۸]"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "یہ وہی ہے جس میں تم دھوکے میں پڑے تھے [۳۹]"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "بیشک ڈرنے والے گھر میں ہیں چین کے [۴۰]"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "باغوں میں اور چشموں میں"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "پہنتے ہیں پوشاک ریشمی پتلی اور گاڑھی ایک دوسرے کے سامنے [۴۱]"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "اسی طرح ہو گا اور بیاہ دیں گے ہم اُنکو حوریں بڑی آنکھوں والیاں [۴۲]"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "منگوائیں گے وہاں ہر میوہ دل جمعی سے [۴۳]"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "نہ چکھیں گے وہاں موت مگر جو پہلے آچکی [۴۴] اور بچایا انکو دوزخ کے عذاب سے"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "فضل سے تیرے رب کے یہی ہے بڑی مراد ملنی [۴۵]"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "سو یہ قرآن آسان کیا ہم نے اسکو تیری بولی میں تاکہ وہ یاد رکھیں [۴۶]"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "اب تو راہ دیکھ وہ بھی راہ تکتے ہیں [۴۷]"
        }
    ]
}