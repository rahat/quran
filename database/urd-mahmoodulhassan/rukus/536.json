{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "کیا ہم نے نہیں کھول دیا تیرا سینہ [۱]"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "اور اتار رکھا ہم نے تجھ پر سے بوجھ تیرا"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "جس نے جھکا دی تھی پیٹھ تیری [۲]"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "اور بلند کیا ہم نے مذکور تیرا [۳]"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "سو البتہ مشکل کے ساتھ آسانی ہے"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "البتہ مشکل کے ساتھ آسانی ہے [۴]"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "پھر جب تو فارغ ہو تو محنت کر"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "اور اپنے رب کی طرف دل لگا [۵]"
        }
    ]
}