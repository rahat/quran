{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "سچ کہتا ہوں اور قسم ہے چاند کی"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "اور رات کی جب پیٹھ پھیرے"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "اور صبح کی جب روشن ہووے"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "وہ ایک ہے بڑی چیزوں میں کی [۲۹]"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ڈرانے والی ہے لوگوں کو"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "جو کوئی چاہے تم میں سے کہ آگے بڑھے یا پیچھے رہے [۳۰]"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "ہر ایک جی اپنے کئے کاموں میں پھنسا ہوا ہے"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "مگر داہنی طرف والے"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "باغوں میں ہیں مل کر پوچھتے ہیں"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "گنہگاروں کا حال [۳۱]"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "تم کاہے سے جا پڑے دوزخ میں [۳۲]"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "وہ بولے ہم نہ تھے نماز پڑھتے"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "اور نہ تھے کھانا کھلاتے محتاج کو"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "اور ہم تھے باتوں میں دھنستے دھنسنے والوں کے ساتھ"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "اور ہم تھے جھٹلاتے انصاف کے دن کو"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "یہاں تک یہ آ پہنچی ہم پر وہ یقینی بات [۳۳]"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "پھر کام نہ آئے گی اُنکے سفارش سفارش کرنے والوں کی [۳۴]"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "پھر کیا ہوا ہے انکو کہ نصیحت سے منہ موڑتے ہیں [۳۵]"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "گویا کہ وہ گدھے ہیں بدکنے والے"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "بھاگے ہیں غل مچانے سے [۳۶]"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بلکہ چاہتا ہے ہر ایک مرد اُن میں کا کہ ملیں اُسکو ورق کھلے ہوئے [۳۷]"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "ہرگز نہیں [۳۸] پر وہ ڈرتے نہیں آخرت سے [۳۹]"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "کوئی نہیں یہ تو نصیحت ہے [۴۰]"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "پھر جو کوئی چاہے اُسکو یاد کرے [۴۱]"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "اور وہ یاد جبھی کریں کہ چاہے اللہ [۴۲] وہی ہے جس سے ڈرناچاہیے اور وہی ہے بخشنے کے لائق [۴۳]"
        }
    ]
}