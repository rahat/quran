{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "ٹوٹ گئے ہاتھ ابی لہب کے اور ٹوٹ گیا وہ آپ [۱]"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "کام نہ آیا اُسکو مال اُسکا اور نہ جو اُس نے کمایا [۲]"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "ڈیگ مارتی آگ میں [۳]"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "اور اُسکی جورو جو سر پر لئے پھرتی ہے ایندھن [۴]"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "اُسکی گردن میں رسی ہے مونجھ کی [۵]"
        }
    ]
}