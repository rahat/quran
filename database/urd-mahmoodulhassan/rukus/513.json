{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "پھر نہ یقین لایا اور نہ نماز پڑھی"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "پھر جھٹلایا اور منہ موڑا"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "پھر گیا اپنے گھر کو اکڑتا ہوا [۲۲]"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "خرابی تیری خرابی پر خرابی تیری"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "پھر خرابی تیری خرابی پر خرابی تیری [۲۳]"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "کیا خیال رکھتا ہے آدمی کہ چھوٹا رہے گا بے قید [۲۴]"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "بھلا نہ تھا وہ ایک بوند منی کی جو ٹپکی [۲۵]"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "پھر تھا لہو جما ہوا پھر اُس نے بنایا اور ٹھیک کر اٹھایا"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "پھر کیا اُس میں جوڑا نر اور مادہ"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "کیا یہ خدا زندہ نہیں کر سکتا مردوں کو [۲۶]"
        }
    ]
}