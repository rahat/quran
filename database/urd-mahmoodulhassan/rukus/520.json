{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "قسم ہے گھسیٹ لانے والوں کی غوطہ لگا کر [۱]"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "اور بند چھڑا دینے والوں کی کھول کر [۲]"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "اور پیرنے والوں کی تیزی سے"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "پھر آگے بڑھنے والوں کی دوڑ کر [۳]"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "پھر کام بنانے والوں کی حکم سے [۴]"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "جس دن کانپے کانپنے والی [۵]"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "اُسکے پیچھے آئے دوسری [۶]"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "کتنے دل اُس دن دھڑکتے ہیں"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "اُنکی آنکھیں جھک رہی ہیں [۷]"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "لوگ کہتے ہیں کیا ہم پھر آئیں گے الٹے پاؤں"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "کیا جب ہم ہو چکیں ہڈیاں کھوکھری"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "بولے تو تو یہ پھر آنا ہے ٹوٹے کا [۸]"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "سو وہ تو صرف ایک جھڑکی ہے"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "پھر تبھی وہ آ رہیں میدان میں [۹]"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "کیا پہنچی ہے تجھ کو بات موسٰی کی [۱۰]"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "جب پکارا اُس کو اُسکے رب نے پاک میدان میں جس کا نام طویٰ ہے [۱۱]"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "جا فرعون کے پاس اُس نے سر اٹھایا"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "پھر کہہ تیرا جی چاہتا ہے کہ تو سنور جائے"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "اور راہ بتلاؤں تجھ کو تیرے رب کی طرف پھر تجھ کو ڈر ہو [۱۲]"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "پھر دکھلائی اسکو وہ بڑی نشانی [۱۳]"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "پھر جھٹلایا اُس نے اور نہ مانا"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "پھر چلا پیٹھ پھیر کر تلاش کرتا ہوا [۱۴]"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "پھر سبکو جمع کیا پھر پکارا"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "تو کہا میں ہوں رب تمہارا سب سے اوپر [۱۵]"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "پھر پکڑا اسکو اللہ نے سزا میں آخرت کی اور دنیا کی [۱۶]"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "بیشک اس میں سوچنے کی جگہ ہے جس کے دل میں ڈر ہے [۱۷]"
        }
    ]
}