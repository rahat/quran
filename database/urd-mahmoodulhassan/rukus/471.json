{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "سو میں قسم کھاتا ہوں تاروں کے ڈوبنے کی [۴۲]"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "اور یہ قسم ہے اگر سمجھو تو بڑی قسم"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "بیشک یہ قرآن ہے عزت والا"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "لکھا ہوا ہے ایک پوشیدہ کتاب میں"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "اُس کو وہی چھوتے ہیں جو پاک بنائے گئے ہیں [۴۳]"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "اتارا ہوا ہے اپنے پروردگار عالم کی طرف سے [۴۴]"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "اب کیا اس بات میں تم سستی کرتے ہو"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "اور اپنا حصہ تم یہی لیتے ہو کہ اُسکو جھٹلاتے ہو [۴۵]"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "پھر کیوں نہیں جس وقت جان پہنچے حلق کو"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "اور تم اُس وقت دیکھ رہے ہو"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "اور ہم اُسکے پاس ہیں تم سے زیادہ پر تم نہیں دیکھتے"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "پھر کیوں نہیں اگر تم نہیں ہو کسی کے حکم میں"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "تو کیوں نہیں پھیر لیتے اُس روح کو اگر ہو تم سچے [۴۶]"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "سو جو اگر وہ مردہ ہوا مقرب لوگوں میں"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "تو راحت ہے اور روزی ہے اور باغ نعمت کا"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "اور جو اگر وہ ہوا داہنے والوں میں"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "تو سلامتی پہنچے تجھ کو داہنے والوں سے [۴۷]"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "اور جو اگر وہ ہوا جھٹلانے والوں بہکنے والوں میں سے"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "تو مہمانی ہے جلتا پانی"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "اور ڈالنا آگ میں [۴۸]"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "بیشک یہ بات یہی ہے لائق یقین کے [۴۹]"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "سو بول پاکی اپنے رب کے نام سے جو سب سے بڑا ہے [۵۰]"
        }
    ]
}