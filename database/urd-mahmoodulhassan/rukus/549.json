{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "تو نے دیکھا اُسکو جو جھٹلاتا ہے انصاف ہونے کو [۱]"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "سو یہ وہی ہے جو دھکے دیتا ہے یتیم کو [۲]"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "اور نہیں تاکید کرتا محتاج کے کھانے پر [۳]"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "پھر خرابی ہے اُن نمازیوں کی"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "جو اپنی نماز سے بے خبر ہیں [۴]"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "وہ جو دکھلاوا کرتے ہیں [۵]"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "اور مانگے نہ دیویں برتنے کی چیز [۶]"
        }
    ]
}