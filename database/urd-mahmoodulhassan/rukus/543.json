{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "وہ کھڑکھڑا ڈالنے والی"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "کیا ہے وہ کھڑ کھڑا ڈالنے والی"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "اور تو کیا سمجھا کیا ہے وہ کھڑ کھڑا ڈالنے والی [۱]"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "جس دن ہوویں لوگ جیسے پتنگے بکھرے ہوئے [۲]"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "اور ہوویں پہاڑ جیسے رنگی ہوئی اون دُھنی ہوئی [۳]"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "سو جسکی بھاری ہوئیں تولیں"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "تو وہ رہے گا من مانتے گذران میں [۴]"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "اور جسکی ہلکی ہوئیں تولیں"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "تو اُس کا ٹھکانہ گڑھا ہے"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "اور تو کیا سمجھا وہ کیا ہے"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "آگ ہے دہکتی ہوئی [۵]"
        }
    ]
}