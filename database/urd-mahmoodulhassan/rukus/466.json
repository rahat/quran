{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "رحمٰن نے"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "سکھلایا قرآن [۱]"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "بنایا آدمی"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "پھر سکھلایا اُسکو بات کرنا [۲]"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "سورج اور چاند کیلئے ایک حساب ہے [۳]"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "اور جھاڑ اور درخت مشغول ہیں سجدہ میں [۴]"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "اور آسمان کو اونچا کیا اور رکھی ترازو"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "کہ زیادتی نہ کرو ترازو میں"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "اور سیدھی ترازو تولو انصاف سے اور مت گھٹاؤ تول کو [۵]"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "اور زمین کو بچھایا واسطے خلق کے [۶]"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "اس میں میوہ ہے اور کھجوریں جنکے میوہ پر غلاف"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "اور اُس میں اناج ہے جسکے ساتھ بھُس ہے اور پھول خوشبودار [۷]"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "پھر کیا کیا نعمتیں رب اپنے کی جھٹلاؤ گے تم دونوں [۸]"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "بنایا آدمی کو کھنکھناتی مٹی سے جیسے ٹھیکرا"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "اور بنایا جن کو آگ کی لپٹ سے [۹]"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "پھر کیا کیا نعمتیں رب اپنے کی جھٹلاؤ گے تم دونوں [۱۰]"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "مالک دو مشرق کا اور مالک دو مغرب کا [۱۱]"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "پھر کیا کیا نعمتیں اپنے رب کی جھٹلاؤ گے"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "دو دریا مل کر چلنے والے"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "اُن دونوں میں ہے ایک پر وہ جو ایک دوسرے پر زیادتی نہ کرے [۱۲]"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "پھر کیا کیا نعمتیں اپنے رب کی جھٹلاؤ گے"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "نکلتا ہے اُن دونوں سے موتی اورمونگا"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "پھر کیا کیا نعمتیں اپنے رب کی جھٹلاؤ گے"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "اور اُسی کے ہیں جہاز اونچے کھڑے دریا میں جیسے پہاڑ [۱۳]"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "پھر کیا کیا نعمتیں اپنے رب کی جھٹلاؤ گے"
        }
    ]
}