{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "قسم رات کی جب چھا جائے"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "اور دن کی جب روشن ہو"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "اور اُسکی جو اُس نے پیدا کئے نر اور مادہ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "تمہاری کمائی طرح طرح پر ہے [۱]"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "سو جس نے دیا اور ڈرتا رہا"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "اور سچ جانا بھلی بات کو"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "تو اُسکو ہم سہج سہج پہنچا دیں گے آسانی میں [۲]"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "اور جس نے نہ دیا اور بے پروا رہا"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "اور جھوٹ جانا بھلی بات کو"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "سو اُسکو ہم سہج سہج پہنچا دیں گے سختی میں [۳]"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "اور کام نہ آئے گا اُسکے مال اُس کا جب گڑھے میں گرے گا [۴]"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "ہمارا ذمہ ہے راہ سجھا دینا"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "اور ہمارے ہاتھ میں ہے آخرت اور دنیا [۵]"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "سو میں نے سنا دی تم کو خبر ایک بھڑکتی ہوئی آگ کی [۶]"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "اُس میں وہی گرے گا جو بڑا بدبخت ہے"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "جس نے جھٹلایا اور منہ پھیرا [۷]"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "اور بچا دیں گے اُس سے بڑے ڈرنے والے کو [۸]"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "جو دیتا ہے اپنا مال دل پاک کرنے کو [۹]"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "اور نہیں کسی کا اُس پر احسان جس کا بدلا دے"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "مگر واسطے چاہنے مرضی اپنے رب کی جو سب سے برتر ہے"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "اور آگے وہ راضی ہو گا [۱۰]"
        }
    ]
}