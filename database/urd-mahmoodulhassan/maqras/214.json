{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "جب ہو پڑے ہو پڑنے والی"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "نہیں ہے اُسکے ہو پڑے میں کچھ جھوٹ [۱]"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "پست کرنے والی ہے بلند کرنے والی [۲]"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "جب لرزے زمین کپکپا کر"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "اور ریزہ ریزہ ہوں پہاڑ ٹوٹ پھوٹ کر"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "پھر ہو جائیں غبار اڑتا ہوا [۳]"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "اور ہو جاؤ تین قسم پر [۴]"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "پھر داہنے والے کیا خوب ہیں داہنے والے [۵]"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "اور بائیں والے کیا برے لوگ ہیں بائیں والے [۶]"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "اور اگاڑی والے تو اگاڑی والے"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "وہ لوگ ہیں مقرب"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "باغوں میں نعمت کے [۷]"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "انبوہ ہے پہلوں میں سے"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "اور تھوڑے ہیں پچھلوں میں سے [۸]"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "بیٹھے ہیں جڑاؤ تختوں پر [۹]"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "تکیہ لگائےاُن پر ایک دوسرے کے سامنے [۱۰]"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "لئے پھرتے ہیں اُنکے پاس لڑکے سدا رہنے والے [۱۱]"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "آبخورے اور کوزے اور پیالہ نتھری شراب کا"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "جس سے نہ سر دکھے اور نہ بکواس لگے [۱۲]"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "اور میوہ جونسا پسند کر لیں"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "اور گوشت اڑتے جانوروں کا جس قسم کو جی چاہے [۱۳]"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "اور عورتیں گوری بڑی آنکھوں والیاں"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "جیسے موتی کے دانے اپنے غلاف کے اندر [۱۴]"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "بدلہ اُن کاموں کا جو کرتے تھے"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "نہیں سنیں گے وہاں بکواس اور گناہ کی بات"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "مگر ایک بولنا سلام سلام [۱۵]"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "اور داہنے والے کیا کہنے داہنے والوں کے"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "رہتے ہیں بیری کے درختوں میں جن میں کانٹا نہیں [۱۶]"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "اور کیلے تہہ پر تہہ"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "اور سایہ لنبا [۱۷]"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "اور پانی بہتا ہوا"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "اور میوہ بہت"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "نہ اُس میں سے ٹوٹا اور نہ روکا ہوا [۱۸]"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "اور بچھونے اونچے [۱۹]"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "ہم نے اٹھایا اُن عورتوں کو ایک اچھے اٹھان پر"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "پھر کیا اُنکو کنواریاں"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "پیار دلانے والیاں ہم عمر"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "واسطے داہنے والوں کے [۲۰]"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "انبوہ ہے پہلوں میں سے"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "اور انبوہ ہے پچھلوں میں سے [۲۱]"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "اور بائیں والے کیسے بائیں والے"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "تیز بھاپ میں اور جلتے پانی میں"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "اور سایہ میں دھوئیں کے"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "نہ ٹھنڈا اور نہ عزت کا [۲۲]"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "وہ لوگ تھے اس سے پہلے خوشحال"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "اور ضد کرتے تھے اُس بڑے گناہ پر [۲۳]"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "اور کہا کرتے تھے کیا جب ہم مر گئے اور ہو چکے مٹی اور ہڈیاں کیا ہم پھر اٹھائے جائیں گے"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "اور کیا ہمارے اگلے باپ دادے بھی [۲۴]"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "تو کہہ دے کہ اگلے اور پچھلے"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "سب اکٹھے ہونے والے ہیں ایک دن مقرر کے وقت پر [۲۵]"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "پھر تو جو ہو اے بہکے ہوؤ جھٹلانے والو"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "البتہ کھاؤ گے ایک درخت سینڈکے سے"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "پھر بھرو گے اس سے پیٹ [۲۶]"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "پھر پیو گے اُس پر ایک جلتا پانی"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "پھر پیو گے جیسے پئیں اونٹ تونسے ہوئے [۲۷]"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "یہ مہمانی ہے اُنکی انصاف کے دن [۲۸]"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "ہم نے تم کو بنایا پھر کیوں نہیں سچ مانتے [۲۹]"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "بھلا دیکھو تو جو پانی تم ٹپکاتے ہو"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "اب تم اُسکو بناتے ہو یا ہم ہیں بنانے والے [۳۰]"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "ہم ٹھہرا چکے تم میں مرنا [۳۱] اور ہم عاجز نہیں"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "اس بات سے کہ بدلے میں لے آئیں تمہاری طرح کے لوگ اور اٹھا کھڑا کریں تم کو وہاں جہاں تم نہیں جانتے [۳۲]"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "اور تم جان چکے ہو پہلا اٹھان پھر کیوں نہیں یاد کرتے [۳۳]"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "بھلا دیکھو تو جو تم بوتے ہو"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "کیا تم اُسکو کر تے ہو کھیتی یا ہم ہیں کھیتی کر دینے والے [۳۴]"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "اگر ہم چاہیں تو کر ڈالیں اُسکو روندا ہوا گھانس پھر تم سارے دن رہو باتیں بناتے"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "ہم تو قرضدار رہ گئے"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بلکہ ہم بے نصیب ہو گئے [۳۵]"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "بھلا دیکھو تو پانی کو جو تم پیتے ہو"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "کیا تم نے اتارا اُسکو بادل سے ہا ہم ہیں اتارنے والے [۳۶]"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "اگر ہم چاہیں کر دیں اُسکو کھارا پھر کیوں نہیں احسان مانتے [۳۷]"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "بھلا دیکھو تو آگ جس کو تم سلگاتے ہو"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "کیا تم نے پیدا کیا اُس کا درخت یا ہم ہیں پیدا کرنے والے [۳۸]"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "ہم نے ہی تو بنایا وہ درخت یاد دلانے کو [۳۹] اور برتنے کو جنگل والوں کے [۴۰]"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "سو بول پاکی اپنے رب کے نام کی جو سب سے بڑا [۴۱]"
        }
    ]
}