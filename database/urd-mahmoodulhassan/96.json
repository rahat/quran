{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "پڑھ اپنے رب کے نام سے [۱] جو سب کا بنانے والا ہے [۲]"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "بنایا آدمی کو جمے ہوئے لہو سے [۳]"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "پڑھ اور تیرا رب بڑا کریم ہے [۴]"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "جس نے علم سکھایا قلم سے [۵]"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "سکھلایا آدمی کو جو وہ نہ جانتا تھا [۶]"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "کوئی نہیں آدمی سر چڑھتا ہے اس سے"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "کہ دیکھے اپنے آپکو بے پروا [۷]"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "بیشک تیرے رب کی طرف پھر جانا ہے [۸]"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "تو نے دیکھا اُسکو جو منع کرتا ہے"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "ایک بندہ کو جب وہ نماز پڑھے [۹]"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "بھلا دیکھ تو اگر ہوتا نیک راہ پر"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "یا سکھلاتا ڈر کے کام"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "بھلا دیکھ تو اگر جھٹلایا اور منہ موڑا [۱۰]"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "یہ نہ جانا کہ اللہ دیکھتا ہے [۱۱]"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "کوئی نہیں اگر باز نہ آئے گا ہم گھسیٹیں گے چوٹی پکڑ کر [۱۲]"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "کیسی چوٹی جھوٹی گنہگار [۱۳]"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "اب بلا لیوے اپنے مجلس والوں کو"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "ہم بھی بلاتے ہیں پیادے سیاست کرنے کو [۱۴]"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "کوئی نہیں مت مان اُسکا کہا اور سجدہ کر اور نزدیک ہو [۱۵]"
        }
    ]
}