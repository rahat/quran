{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Biyya (Makkaa) kanaanin kakadha"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ati biyyi kun (lolaan banuun) siif eeyyamamaadha"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Abbaafi waan inni dhaleenis (nan kakadha)"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Dhugumatti, Nuti nama cinqii[] keessatti uumne"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Sila inni tokkolleen[] isa irratti hin danda'uu yaadaa"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "“Qabeenya guddaan balleesse\" jedha"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Sila inni tokkolleen isa hin arguu yaadaa"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Sila Nuti ija lama isaaf hin taasifnee"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Arrabaafi hidhii lamas"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Karaa lama (gaariifi badaa) isaaf ibsine"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Sila maaliif tulluu hin dabrine"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Tulluttiin maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(Inni) Gabra bilisoomsuudha"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Yookiin guyyaa beelaa keessa nyaachisuudha"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yatiima fira ta’an"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yookiin hiyyeessa harkaan lafa qabate (deege)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Ergasii warra amananii, obsa waliif dhaamaniifi rahmata waliif dhaaman irraa maaliif hin taane"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Isaan sun warra mirgaati"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Isaan keeyyattoota keenyatti kafaran immoo isaan warra bitaati"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Isaan irratti ibiddi cufamtuudha"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Aduufi ifa isheetiinin (kakadha)"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ji’aanis; yeroo inni ishee hordofe"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Guyyaanis yeroo inni ishee (aduu) mul’ise"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Halkaniinis yeroo inni ishee haguuge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Samiifi kan ishee ijaareenis"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Dachiifi kan ishee diriirseenis"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Lubbuufi kan ishee uumeenis"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Kan jal’ina isheefi qajeeluma ishee ishee barsiise"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Namni ishee qulqulleesse dhugumatti milkaa’e"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Namni ishee xureesses dhugaatti hoonga’e"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Samuud daangaa dabruu isheetiin kijibsiifte"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Yeroo irra jallaan ishee ka’u (kijibsiifte)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Ergamaan Rabbiis isaaniin “gaala Rabbiitiifi dhugaatii ishee (eeggadhaa)” jedhe"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Isa kijibsiisanii ishee waraananSababa badii isaanii isaan balleessee wal qixxeesse"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Haaloo ishee hin sodaatu"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Halkaniin (nan kakadha); yeroo (inni) haguuge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Guyyaanis yeroo inni mul'ate"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Isa kormaafi dhalaa uumeenis"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Dhugumatti, dalagaan keessan adda adda (wal dhaba)"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Namni kennatee Rabbiin sodaate"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Islaamummaas dhugoomse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "(Karaa) laaftuuf isa laaffisuuf jirra"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Nama don’oomee of ga’e"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Islaamummaa sobsiise immoo"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "(Karaa) jabduuf isa laaffisuuf jirra"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Yeroo inni (ibiddatti) kufu qabeenyi isaa isa hin fayyadu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Dhugumatti, karaa qajeelaa ifa galchuun (mul'isuun) Nurra jira"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Dhugumatti, Aakhiraafi addunyaan keenya"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ibidda qabsiifamaa ta'e isin sodaachise"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Hoonga’aa malee ishee hin seenu"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Kan sobsiisee gara gale (malee)"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Namni Rabbiin sodaatu immoo ishee irraa fageeffamuuf jira"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Kan qabeenya isaa qulqulleeffachuuf kennu"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Haala isa bira nama tokkoof galanni galfamu hin jirreen (kan kennatu)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Fuula Gooftaa isaa ol aanaa ta'e barbaachaaf malee (hin kennu)"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Dhugumatti, inni fuulduratti jaalachuuf jira"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Waareenin (kakadha)"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Halkaniinis yeroo inni callise"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gooftaan kee si hin dhiifne; si hin jibbines"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Dhugumatti, Aakhiraatu addunyaa irra siif caala"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Dhugumatti, Rabbiin kee fuula dura (kenna guddaa) siif kenna; ni jaallattas"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Sila yatiima ta’uu si argee si hin hirkifnee"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Dogoggoraas ta’uu si argee si qajeelche"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Hiyyeessas taatee si argee si duroomse"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yatiima hin hacuucin"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Kadhataas hin ifatin"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ni'imaa (qananii) Gooftaa keetiis odeessi"
        }
    ]
}