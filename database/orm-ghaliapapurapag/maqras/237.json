{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Maqaa Gooftaa kee olaanaa ta'ee qulqulleessi"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Isa (waan hunda) uumee guute"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Isa murteessee qajeelche"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Isa marga baase"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Goggogaa gurraacha isa taasise"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Si qaraasisuuf jirraa hin dagattu"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Waan Rabbiin fedhe maleeInni mul’ataafi dhokataas ni beeka"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Nuti karaa laaftuu taateef si laaffifna"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Yoo yaadachiisni kan fayyadu ta’e yaadachiisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Namni Rabbiin sodaatu gorfamuuf jira"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Hoonga'aan ishee irraa fagaata"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Inni ibidda guddoo seenu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Ergasii ishee keessatti hin du'u; hin jiraatus"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Dhugumatti, namni of qulqulleeffate milkaa’e"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Maqaa Gooftaa isaa dubbatee kan salaate"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Inumaa isin jiruu addunyaa filattu"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Odoo Aakhiraan caaltuufi haftuu taatee jirtuu [addunyaa filattu]"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Dhugumatti, kun barreeffamoota duraanii keessa jira"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Barreeffamoota Ibraahiimifi Muusaa [keessa jira]"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Sila oduun haguugduu[] (Qiyaamaa) sitti dhufeeraa"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Fuulli gariin guyyaa san xiqqaattuudha"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Hojjattuu ifaajjuudha"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ibidda gubduu seenti"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Burqituu danfituu irraa obaafamti"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Adaamii malee nyaanni isaaniif hin jiru"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Hin gabbisu; beela irraas hin duroomsu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Fuulli gariin guyyaa san qananiituudha"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Dalagaa ishee jaalattuudha"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Jannata ol fuudhamtuu keessa jiraatti"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ishee keessatti waca hin dhaga'anu"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ishee keessa burqituu yaatutu jirti"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Ishee keessa sireewwan ol fuudhamtuutu jirti"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Kubbaayyaawwan kaa’amtuutu jirti"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Boraatiiwwan tarree galfamantu (isaaniif jira)"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Afaawwan diriirfamanis (isaaniif jirti)"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Sila gara gaalaa akkamitti akka uumamte hin ilaalanuu"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Gara samii akkamitti akka ol fuudhamte (hin ilaalanuu)"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Gara gaarreenis akkamitti akka dhaabamte (hin ilaalanuu)"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Gara dachiis akkamitti akka diriirfamte (hin ilaalanuu)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Yaadachiisi; ati yaadachiisaa qofa"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Isaan irratti dirqisiisaa hin taane"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Garuu nama gara galee kafare"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Rabbiin adabbii guddaa isa adaba"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Deebiin isaanii garuma Keenya"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Ergasii qormaanni isaanii Nurra jira"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Bariinin[] (kakadha)"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Halkan kurnaniini"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Cimdiifi qarattis"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Halkanittis; yeroo inni deeme"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Sila sana keessa abbootii sammuutiif kakuun quubsaan hin jiruu? (ni jira)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Sila akkam akka Gooftaan kee Aaditti hojjate hin arginee"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iramiin warra dhedheerootti"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Ishee fakkiin ishee biyyoota keessatti hin uumamin san"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Samuudittis warra laga (Hijri) keessatti dhagaa uran (mana irraa tolfatan)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Fir'awniinis abbaa shikaalaatti"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Isaan biyyoota keessatti daangaa dabran"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Kan ishee keessatti badii baay'isan"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Gooftaan kee akaakuu adabbii isaan irratti buuse"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Dhugumatti, Gooftaan kee qeexaadha (beekaadha)"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Namni yeroo gooftaan isaa isa kabajuun qanansiisee isa qorate “Gooftaan kiyya na kabaje” jedha"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Yeroo inni hiree isaa itti xiqqeessee isa qorate immoo “Gooftaan kiyya na xiqqeesse” jedha"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Haa dhorgamu! Dhugumatti, isin yatiima hin kabajjan"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Hiyyeessa nyaachisuu irrattis hin kakaaftan"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Qabeenya dhaalaa, nyaata [halaalaafi haraama] walitti qabate nyaattu"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Jaalala baay’ee qabeenya jaalattu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Dhorgamaa! Yeroo dachiin waliin dhawamiinsa jabaa waliin dhawamte"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "(Yeroo) Gooftaan kee dhufee maleykonnis hiriira hiriiran [dhufan]"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Guyyaa san jahannamis ni fidamaGuyyaa san namni ni yaadataYaadannoon (guyyaa sanii) maal isaaf godha ree"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Ni jedha: \"odoon jiruu kiyyaaf dabarfadheen hawwa"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Guyyaa san adabbii Isaa eenyunuu hin adabu"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Hidhaa Isaas eenyunuu hin hidhu"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "“Yaa lubbuu tasgabbooftuu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Jaallataa, jaallatamaa gara Gooftaa keetii deebi'i"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Gabroota kiyya keessas seeni"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Jannata kiyyas seeni” (jedhamti)"
        }
    ]
}