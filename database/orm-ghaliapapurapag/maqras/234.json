{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Fuula guuree gara gale"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Sababa jaamaan isatti dhufeef"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Maaltu si beeksise? Qulqullaa’uun isaa ni kajeelama"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yookiin gorfameeti gorsi isa fayyaddi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Nama duroome immoo"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ati [akka amananuuf jecha] gara isaa gara galta"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Haala qulqullaa'uu dhiisuun isaa homaa sirra hin jirreen"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Nama fiigaa sitti dhufe immoo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Kan Rabbiin sodaatu ta’ee"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ati isa irraa dagatta"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Dhorgami! Isheen gorsa"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Nama fedhetu isa (Qur'aana) yaadata"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Galmeewwan kabajamtuu keessa jira"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Ol fuudhamtuu, qulqulleeffamtuu (keessa jira)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Harka barressitootaa (maleykotaa) jira"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kabajamoo, ajajamtoota (harka jira)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Namni abaarame! Sila maaltu isa kafarsiise"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "(Rabbiin) waan akkamii irraa isa uume? (hin xiinxaluu)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bishaan saalaa irraa isa uumeeti, ergasii isa sirreesse"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Ergasii karaa (inni ittiin dhalatu) isaaf laaffise"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Ergasii isa ajjeeseti isa awwaalchise"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ergasii yeroo fedhe isa kaasa"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Dhugumatti, (namni) waan Rabbiin isa ajaje ammallee hin raawwanne"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Namni gara nyaata isaa haa ilaalu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Nuti bishaan buusiinsa baay’inaan buufne"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Ergasii dachii dhodhoosiinsa dhodhoofne"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Achumaan Ishee keessatti firii magarsine"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Inabaafi margas"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytunaafi muka timiraas"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ashaakiltiilee muka baay’atanis"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Fuduraafi kaloo [akka nuti uumne haa ilaalu]"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Isiniifi beylada keessanis qanansiisuuf"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Yeroo gurra duuchituun (Qiyaamaan)dhufte"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Guyyaa namni obboleessa isaa irraa dheessu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Haadha isaatiifi abbaa isaa irraas"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Niitii isaatiifi ijoollee isaa irraas (baqatu san dhufti)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Nama Isaan irraa ta’e hundaaf guyyaa sana dhimma isa duroomsutu jira"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Fuulli gariin guyyaa san ni ifti"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Kolfituu, gammadduudha"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Fuulli gariin immoo guyyaa san dhukkeetu (quttootu) irra jira"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Gurraachinni ishee haguuga"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Isaan sun warra Rabbitti kafaranii, balleessa hojjataniidha"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Yeroo aduun maramte"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Yeroo urjiin harcaate"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Yeroo gaarreen bakkaa oofamte"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Yeroo gaalli rimaan baatii kudhanii dhiifamte (tiksitee malee hafte)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Yeroo bineensonni bosonaa walitti qabamte"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Yeroo galaanni qabsiifamte (akka ibiddaatti boba'an)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Yeroo lubbuun hundi qindeeffamte"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Yeroo durbi fayyaan awwaalamte gaafatamte"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Balleessa kamiif akka ajjeefamte"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Yeroo galmeen (hojii) raabsamte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Yeroo samiin buqqifamtee maramte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Yeroo ibiddi qabsiifamte"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Yeroo jannanni dhiheeffamte"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Lubbuun waan dalagatte beekteetti"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "(Urjiiwwan) guyyaa badanii halkan mul'ataniinin kakadha"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(Urjiiwwan) yaa’aa seenaa ta’aniinin (kakadha)"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Halkaniinis yeroo inni dukkanaa’en (kakadha)"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Bariittillee yeroo inni diriiren (kakadha)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Dhugumatti, inni (Qur'aanni) kun jecha Ergamaa kabajamaati"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "(Ergamaa) abbaa humnaa ta'e, kan Gooftaa Arshii biratti iddoo jabaa qabu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Bakka sanatti ajajni isaa fudhatamaafi amanamaa kan ta'e"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Saahibni keessan (Muhammad SAW!) maraataa miti"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Dhugumatti, inni moggaa ol aanaa (samii) ifa galaa (gara Bahaa) keessatti isa argee jira"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Inni (Nabiyyiin) beekumsa fagoo irratti don'aa miti (hin dhoksu)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Innis (Qur'aanni) jecha sheeyxaana darbamaa miti"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Mee eessuma deemtu"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Inni aalama hundaaf gorsa malee homaa miti"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Nama isin irraa qajeeluma fedheef (gorsa ta'a)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Yoo Rabbiin aalama hundaa fedhe malee isin homaa hin feetanu"
        }
    ]
}