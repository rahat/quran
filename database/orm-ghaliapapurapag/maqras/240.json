{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Sila inni yeroo wanti qabroota keessa jiran hundi baafaman sana hin beekuu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Yeroo wanti qoma keessa jiran (hundi) ifa baafaman (hin beekuu)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Dhugumatti, Gooftan isaanii guyyaa san isaan beekaadha"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qalbii rukuttuun (Qiyaamaan)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Qalbii rukuttuun maali"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Qalbii rukuttuun maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Guyyaa namni akka billaacha faffacaafamee ta’u"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "(Guyyaa) gaarreen akka jirbii iddamee (qaasasame) ta'u"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Namni madaalli (hojii) isaa ulfaatte"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Inni jireenya jaalatamtuu keessa ta'a"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Namni madaalli (hojii) isaa salphatte immoo"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Haati isaa “Haawiyaa[]”dha"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Isheen (Haawiyaan) maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Isheen) ibidda gubduudha"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Baay’ifachuun (ibaadaa irraa) isin jaataneesse (shaagale)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hanga awwaala dhaqxanitti (duutanitti)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Dhorgamaa! Fuula dura beekuuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ergasiis dhorgamaa! Fuula dura beekuuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Dhugumatti, odoo beekumsa mirkanaa’aa beektanii (hin shaagalamtanu ture)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dhugumatti ibidda ni argitu"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ergasii argaa mirkanaa’aa arguuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Ergasii dhugumatti guyyaa san qananii irraa gaafatamuuf jirtu"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Yeroonin kakadha"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Dhugumatti, namni hunduu hoongoo keessa jira"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Isaan amanan, gaggaarii dalagan, haqa walii dhaamaniifi obsa walii dhaaman malee (warri hafan hoongoo guddaa keessa jiru)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hamattuufi arrabsituu hundaaf badiin mirkanaa’e"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Isa qabeenya walitti qabee lakkaa'e"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Qabeenyi isaa isa hambisuu kan yaadu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Haa dhorgamu! [Ibidda] caccabsituu keessatti darbamuuf jira"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Caccabsituun maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Isheen) ibidda Rabbii kan qabsiifamtuudha"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ishee onnee irratti (laalaan ishee) mul’attuudha"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Dhugumatti, isheen isaan irratti cufamtuudha"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Utubaalee diriirfamaa keessatti (itti cufama)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Sila Gooftan kee Abbootii Arbaa irratti akkam akka dalage hin arginee"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Sila tooftaa isaanii dhabama hin goonee? (godheera)"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Simbira garee gareedhaan isaanitti erge"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Dhagaalee xuubii (suphee gubamte) irraa taate isaanitti darbatti"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Akka alalaa (okaa) nyaatamee isaan taasise"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qurayshota barsiisuuf"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Fe’annaa gannaafi bonaa isaan barsiisuuf jecha (warra arbaa balleesse)"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Kanaafuu Gooftaa mana kanaa haa gabbaranu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Isa beela irraa isaan nyachiseefi sodaa irraa isaan tasgabbeesse"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Isa qormaata sobsiisu argitee"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Sun isa yatiima humnaan deebisu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Hiyyeessa nyaachisuu irrattis (namoota) hin kakaafne"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Warra salaataniif badiin mirkanaa’e"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Isaan sun warra salaata isaanii irraa dagatan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Isaan warra (dalagaa namatti) agarsiifataniif"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Isaan meeshaa ergisaa dhorgataniif (badiin mirkanaa’e)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Nuti dhugumatti, tola guddaa siif kennineerra"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Gooftaa keetiif sagadii isaaf qali"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Dhugumatti, kan si jibbu isuma kan qaccee cite (kheyrii hunda irraa cite)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Jedhi: \"yaa warra Rabbitti kafartan"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ani waan isin gabbartan hin gabbaru"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Isinis waan ani gabbaru kan gabbartan hin taane"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Anis waan isin gabbartan (fuula duras) kan gabbaruu miti"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Isinis waan ani gabbaru (fuula duras) kan gabbartanii miti"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Isiniif amantiin keessan jira; anaafis amantiin kiyya jira"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Yeroo tumsi Rabbiitiifi banamiinsi (Makkaa) dhufe"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Namootas garee gareedhaan diinii Rabbii keessa kan seenan argite"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Gooftaa kee faarsuu wajjin qulqulleessi; araaramas Isa irra barbaadInni akkaan tawbaa qeebalaadha"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Harki Abuu Lahab lamaan badde[Innis]bade"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Qabeenyi isaatiifi wanti inni hores isa hin fayyanne"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ibidda abbaa bobayaa (belbelaa) taate seenuuf jira"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Niitiin isaas baadhattuu qoraanii taatee (seenti)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Morma ishee keessa haada quncee temiraa irraa ta'etu jira"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Jedhi: \"Inni Allaah (gabbaramaa dhugaa) tokkicha"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allaahn hirkoo waa hundaati"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hin dhalle hin dhalannes"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Hiriyaan tokkoos isaaf hin taane.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Jedhi: \"Gooftaa bariittin maganfadha"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Hamtuu waan Inni uumee irraa"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Hamtuu dukkanaa yeroo dukkanaa’e irraas"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Hamtuu guduunfaa keessatti tuftu irraas"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Hamtuu haasidii yeroo inni hassadu (waanyu) irraas (Rabbittin maganfadha).”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Jedhi: \"Gooftaa namaattin maganfadha"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mootii namaatti"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Gabbaramaa namaatti"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Hamtuu waswaasii dhokatuufi mul’atuu irraa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Isa qoma namaa keessa waswaasu irraa"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jinniifi nama irraa kan ta’e (irraan maganfadha).”"
        }
    ]
}