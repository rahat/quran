{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Yeroo samiin dhodhoote"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "(Yeroo isheen ajaja) Gooftaa ishee dhaggeeffatte; (wanti kana godhu waan itti argameef) dhugaa qabdi"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Yeroo dachiinis diriirfamte"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Waan ishee keessa jiru darbitee, duwwaas taate"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "(Yeroo isheen ajaja) Gooftaa ishee dhaggeeffatte; (wanti kana godhu waan itti argameef) dhugaa qabdi"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Yaa namootaa! Dhugumatti, ati gara Rabbii keetii (dhaqqabuuf) hojiin qabsaa’aadhaa, isa (hojii kee) qunnamaadhas"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Namni galmee isaa mirga isaatiin kenname"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Qormaata salphaa qoramuuf jira"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Gammadaa gara maatii isaa deebi’a"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Namni galmee isaa dugda duubaan kenname immoo"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Badii kadhachuuf jira"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ibidda boba’aa seena"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Dhugumatti, inni maatii isaa keessatti gammadaa ture"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Dhugamatti, inni (gama Rabbii) deebi’uu dhabuu yaade"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Lakki! (ni deebi’a)Gooftaan isaa isa argaadha"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Haa dhorgamu! Diimina galgalaattin kakadha"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Halkaniifi waan inni walitti qabeenis (nan kakadha)"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Ji'attis yeroo inni guuteenin (kakadha)"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Dhugumatti, isin sadarkaa tokko irraa gara sadarkaa biraatti ceetu"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Maaltu isaaniif jiraayi hin amanne"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Yeroo Qur'aanni isaan irratti qara'amus sujuuda hin bu'an"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Dhugumaatti, warri kafaran ni sobsiisu"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Rabbiin irra beekaa waan isaan (onnee isaanii keessatti) walitti qabaniiti"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Adaba laalessaadhaan isaan gammachiisi"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Garuu isaan amananii gaggaarii hojjatan isaaniif mindaa hin cinnetu jira"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Samii abbaa orbitii urjiitiin (nan kakadha)"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Guyyaa beellamamaanis"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ragaa ba'aafi warra ragaan itti ba'amuunis (nan kakadhaa)"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Warri boollaa abaaraman"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "(Warri) ibidda boba’aa"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Yeroo isaan ishee irra taa'an"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Isaan waan mu'imintootatti hojjatan irratti hirmaattota"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Rabbii injifataa, faarfamaatti amanuu malee homaa isaan irraa haaloo hin ba’anne"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "(Rabbiin) Isa mootummaan samiifi dachii Isaaf ta'eedhaRabbiin waan hunda irratti ragaadha"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Dhugumatti, isaan mu'imintoota dhiiraafi dubartootaa rakkisanii ergasii gara Rabbii isaanii kan hin deebi'Islaamni (hin tawbatin) isaaniif adabbii jahannamtu jira; ammas isaaniif adabbii gubaatu jira"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Dhugumatti, isaan amananii gaggaarii hojjatan, isaaniif ashaakiltiiwwan laggeen jala ishee yaatutu jiraSun milkii guddaadha"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Dhugumatti, qabaan Gooftaa keetii daran cimaadha"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Inni Isumatu [uumama] jalqaba; (du'a boodas) ni deebisa"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Inni akkaan araaramaa, akkaan jaalataadha"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "(Inni) abbaa Arshiiti; guddaadha"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Waan fedhe akkaan hojjataadha"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Sila oduun humna waraanaa si ga'eeraa"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "(Waraana) Fir'awniifi Samuud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Dhugumatti, isaan kafaran kijibsiisuu keessa jiru"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Rabbiin immoo duuba isaanii irraan marsaadha"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Dhugumatti, inni Qur'aana guddaadha"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Luuhii tiikfamaa keessa jira"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Samiifi (urjii) halkan dhufaa ta’eenin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Urjii) halkan dhufaa ta’e maaltu si beeksise"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Inni) urjii ifti isaa dukkana uraa ta’eedha"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Lubbuun hundi tiksaan ishee irra jiraatu malee hin hafu"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Namni maal irraa akka uumame haa ilaalu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bishaan utaalaa irraa uumame"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Jidduu dugdaatiifi qomaatii kan ba’u"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Inni isa deebisuu irratti danda’aadha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Guyyaa iccitiiwwan ifa ba'an"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Gaafas) humnas ta’ee tumsaan isaaf hin jiru"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Samii abbaa deddeebiitiinin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Dachii dhodhootuunin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dhugumatti, inni (Qur'aanni) jecha adda baasaadha"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Inni qoosaa hin taane"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Dhugumatti, isaan tooftaa baasu"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Anis tooftaan baasa"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kaafirootaaf yeroo kenniAmmas xinnoo yeroo kenniif"
        }
    ]
}