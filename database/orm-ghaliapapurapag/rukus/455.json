{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "(Qilleensa) bubbisuu bubbiftuunin kakadha"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "(Duumessa bishaan) ulfaataa baadhattuunis"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "(Dooniwwan) haala salphaan (bishaan gubbaa) yaa’aniinis"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "(Maleykota) ajaja (Rabbii) raabsaniinis"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Wanti isin beellamamtan dhugaadha"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "(Guyyaan) murtii dhugumatti argamaadha"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Ammas samii abbaa miidhaginaatiin kakadhe"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Dhugumatti, isin yaada wal dhadhabaa keessa jirtu"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Kan namni haqa irraa garagalfamu ittiin garagalfamuudha"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Sobdoonni abaaramaniiru"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Isaan sun wallaaluma keessatti kan of dagataniidha"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "“Guyyaan mindaa yoomi?” (jechuun) gaafatu"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "(Inni) Guyyaa isaan ibidda irra fidamaniidha"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "“Adaba keessan kan ariifataa turtan dhandhamaa” (isaaniin jedhama)"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Isaan Rabbiin sodaatan, jannataafi burqituuwwan keessa jiraatu"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Waan Gooftaan isaanii isaaniif kenne fudhachaa (jiraatu)Isaan sana dura toltuu hojjattoota turan"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Halkan irraa xiqqaa qofa kan rafan turan"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Yeroo hiraabaa (obboroo) isaan araarama kadhataniidha"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Kan qabeenya isaanii keessa kadhataafi kan hin kadhanneefis haqni jiruudha"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Dachii keessas isaan dhugaan amananiif mallattooleetu jiru"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Lubbuu keessan keessas (akkasuma)Sila hin argitanuu"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Sooranni keessaaniifi wanti isin waadaa galamtanis samii keessa jira"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Gooftaa samiifi dachiitiin kakadhee! Inni dhugaa fakkiidhuma waan isinuu dubbattanuuti"
        }
    ]
}