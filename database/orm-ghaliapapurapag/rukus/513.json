{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Hin amanne; hin salaanne"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Garuu kijibsiisee irraa gara gale"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Ergasii boonaa gara maatii isaa deeme"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Badiin siif haa ta’u! Ammas badiin"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Sana booda badiin siif haa ta’u! Ammas badiin"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Sila namni akkanumaan dhiifamuu yaadaa"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Sila inni bishaan saalaa kan dhangalaafamu irraa ta'ee hin turree"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Ergasii dhiiga ititaa ta'eeti (nama godhee) uumee guute"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Isa irraas cimdii lama, dhiiraafi dhalaa, taasise"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Sila (Rabbiin akkas godhe) sun du'aa jiraachisuu irratti danda'aa mitii"
        }
    ]
}