{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Maqaa Gooftaa keetii Isa (waan hunda) uumeetiin dubbisi"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "(Isa) nama dhiiga ititaa irraa uume"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Haala Gooftaan kee hunda irra arjaa taâ€™een qara'i"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Isa qalamaan (barreessuu) barsiise sani"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Nama, waan inni hin beekne barsiise"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Dhugumatti, namni daangaa dabra"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Sababa duroomuu of arguutiin"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Deebiin garuma Gooftaa keetiiti"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Sila isa dhorgu san argitee"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Gabricha yeroo inni salaatu (kan dhorgu argitee)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Yoo inni qajeeluma irra jiraate hoo mee naaf himi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Yookiin immoo sodaa Rabbiitti yoo ajaje hoo"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Yoo inni (Abuu Jahal) kijibsiisee irraa gara gale hoo mee naaf himi"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Sila inni Rabbiin arguu hin beekuu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Haa dhorgamu; yoo inni (Abuu Jahal) dhorgamuu baate, sammuu qabna"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Sammuu sobduu, dogoggortuu taate (qabna)"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Garee isaa haa waammatu"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nutis waardiyyaa keenya waammachuuf jirra"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Dhorgami! Isaan hin ajajaminSujuudis gara Rabbiittis dhihaadhu"
        }
    ]
}