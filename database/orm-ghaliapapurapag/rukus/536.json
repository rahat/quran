{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Sila Nuti qoma kee siif hin bal'ifnee"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Baâ€™aa (badii) kee sirraa buufne"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Isa dugda keetti gar malee ulfaate (sirraa buufne)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Maqaa keetis siif ol kaafne"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Rakkinaa wajjin laafinatu jira"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Dhugumatti, rakkina wajjin laafinatu jira"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Yeroo (dalagaa wahii) raawwattes jabaadhu"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Gara Gooftaa keetii qofa kajeeli"
        }
    ]
}