{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yeroo argamtuun (Qiyaamaan) argamte"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Argama isheetiif kijibni hin jirtu"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Gadi qabduu, ol fuutuudha"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yeroo dachiin raafama cimaa raafamte"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "(Yeroo) gaarreenis facaafamiinsa facaafamte"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Dhukkee facaafamtuus taate"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "(Yeroo) Isinis garee sadi taatan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Gareen tokko) warra mirgaati; warri mirgaa akkam (milkaa’oo dha)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Gareen lammaffaan) warra bitaati; warri bitaa akkam (hoonga’oo dha)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(Gareen sadaffaa) Isaan gara toltuutti dursitoota dursoodha"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Isaan (Rabbitti) dhiheeffamoota"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Jannata qananii keessatti ta’u"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Warra duraa irraa baay’eedha"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Warra boodaa irraa immoo xiqqoodha"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Sireewwan dhawamte irratti ta’u"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Fuulaan walitti garagalanii ishee irratti hirkatu"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Ijoollee (ijoollummaan) yeroo hunda jiraatantu isaan irra naanna’a"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Kubbaayyaa, quluufi geeba (xoofoo) farsoo irraa ta’een"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Isii irraa hin bowwaafatan; hin machaa’anis"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Fuduraalee irraa waan isaan filataniin (irra naanna’u)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Foon allaattii irraas waan isaan fedhaniin"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Dubra adaadii ija babal’ootu (isiniif jira)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Akka faaya eegamaa kan fafakkaatan"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Mindaa waan hojjataa turaniif jecha (kana isaaniif goone)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Isii (jannata) keessatti wacaafi wal arrabsuus hin dhaga’an"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "“Nagayaa nagayaa waliin jechuu malee” (hin dhaga’an)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Warri mirgaa; warra mirgaa waa’een isaanii akkam tola"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Isaan) mukkeen qurquraa kan qoree hin qabne keessa jiraatu"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Muuza firiidhaan guutame (keessa jiraatu)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Gaaddisa diriirfamaa (keessa)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Bishaan dhangala’u (keessa)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Fuduraalee baay’ee (keessa)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Kan maddi isaanii hin cinneefi kan irraa hin dhorgamne (keessa)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Firaasholee ol fuudhamtuu (keessa jiraatu)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Dhugumatti, Nuti (dubartoota jannataa) uuminsa isaan uumne"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Dubroota isaan goone"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Jaallattoota (abbaa manaa), kan hiriyoota ta’an (isaan goone)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Warra mirgaatiif (qophaa’an)"
        }
    ]
}