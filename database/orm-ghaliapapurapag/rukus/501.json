{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Waan isin argitaniinis niin kakadha"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Waan isin hin argineenis (niin kakadha)"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Dhugumatti, inni (Qur'aanni) jecha ergamaa kabajamaati"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Inni jecha weellisaatii miti; xiqquma qofa amantu"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Jecha raagaatiis miti; xiqqoo malee hin gorfamtanu"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Gooftaa aalama hundaa irraa buufamaadha"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Odoo garii jechootaa (kan Nuti hin jedhin) Nurratti dubbatee"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Silaa Nuti mirgaan isa qabna turre"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Ergasii hidda onnee isarraa kunna turre"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Isin irraa namni isarraa dhorgu tokkollee hin jiru"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Innis (Qur'aanni) warra Rabbiin sodaataniif gorsa"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Dhugumatti isin irraa namoonni (Qur'aana) sobsiisan akka jiran Nuti ni beekna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Inni (Qur'aanni) kaafiroota irratti gaabbiidha"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Dhugumatti, inni (Qur'aanni) dhugaa mirkanaa’aadha"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Maqaa Gooftaa kee guddaa ta’ee faarsi"
        }
    ]
}