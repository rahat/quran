{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Mee isa gara gale naaf himi"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Kan xiqqoo kennee, ergasii dhoorgate"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Sila beekumsi fagoo isa bira jiraannaan inni (isa) beekee"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Moo wanta barreeffamoota Muusaa keessa jirutu itti hin himaminii"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Kan Ibraahiim isa (ajaja Rabbii) guutees"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "(Isheen sun) lubbuun dilii baadhattu ba’aa lubbuu biraa hin baadhattuudha"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Namaafis waan hojjate malee hintaaneedha"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Hojiin isaas of duratti mul’achuuf jiraadha"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Achii booda mindaa guutuutu isaaf kennamaadha"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Xumurri (waan hundaas) gara Rabbiitti ta’uudha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Inni Isumatu kofalchiise; boochises"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Inni Isumatu ajjeese; jiraachises"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Inni cimdii lama, kormaafi dhalaa, uume"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Bishaan hormaataa irraa (uume), yeroo buufamtu"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Uumuun boodaas Rabbuma irra jira"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Inni Isa duroomseefi hiyyoomseedha"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Inni Isumatu Gooftaa Shi’iraati"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Isumatu Aadii duraa balleesse"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "(Ummata) Samuudis hin hambifne"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ummata Nuuhis sana dura (balleesse)Isaan isaanumatu akkaan miidhaa raawwattootaafi daangaa darbitoota turan"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Ganda gombifamtuus gadi kuffise"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Wanti haguuge ishee haguuge"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Ni’imaa Rabbii keetii irraa kamiin shakkita"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Kun (Muhammad SAW) sodaachisaa sodaachistoota durii irraa ta’eedha"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Dhihaattuun (Qiyaamaa) dhihaatteetti"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Rabbiin ala isheef mul’iftuun hin jirtu"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Sila haasa’a kana (Qur’aana) irraa dinqisiifattanii"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ni kolfituu! Hin boochanuu"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Haala sirbitanuun (irraa garagaltuu)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Allaah’f sujuudaa; (Isuma qofa) gabbaraa"
        }
    ]
}