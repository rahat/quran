{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Isa qormaata sobsiisu argitee"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Sun isa yatiima humnaan deebisu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Hiyyeessa nyaachisuu irrattis (namoota) hin kakaafne"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Warra salaataniif badiin mirkanaa’e"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Isaan sun warra salaata isaanii irraa dagatan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Isaan warra (dalagaa namatti) agarsiifataniif"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Isaan meeshaa ergisaa dhorgataniif (badiin mirkanaa’e)"
        }
    ]
}