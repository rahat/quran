{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tiiniifi Zaytuuniiniin (kakadha)"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Gaara (Tulluu) Siiniinaatiinis"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Biyya nagaa kanaanis (nin kakadha)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Dhugumatti, Nuti nama dhaabbii irra gaarii ta'e keessatti isa uumne"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Ergasii bakka warra jalaa hundarra gadi aanaa ta'etti isa deebifne"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Garuu isaan amananii gaggaarii dalagan isaaniif mindaa addaan hin cinnetu jira"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Kana booda maaltu mindaa [guyyaa Qiyaamaa] si kijibsiisa (kijibsiisutti si kaase)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Sila Rabbiin irra murteessaa warra murteessanuu hin taanee"
        }
    ]
}