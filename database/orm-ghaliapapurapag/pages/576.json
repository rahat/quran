{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "Inni ni xiinxale; ni tilmaames"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Abaarame! Akkamumatti tilmaame"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Ergasiis abaarame! Akkamumatti tilmaame"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Ergasii ni ilaale"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Ergasii fuula guure; akkaan jijjiirame"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Ergasii gara gale; ni boone"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Ni jedhes: \"(Qur'aanni) kun falfala (nama biraa) irraa waraabame qofa"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Kun jecha namaa malee hin taane"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Saqar isa seensisuufin jira"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Saqar maal akka ta'e maaltu si barsiise"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "(Foon) hin hambiftu (lafees) hin dhiiftu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Gogaas gubduudha"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Ishee irra (waardiyyaa) kudha sagaltu jira"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Eegdota Ibiddaas malaykota malee kan biraa hin gooneLakkoofsa isaaniis isaan kafaraniif qormaata, warri kitaaba kennaman akka mirkaneeffatan, warri amanan iimaana akka dabalatanuuf, warri kitaaba kennamaniifi mu'uminoonnis akka hin shakkinee,fi akka warri onnee isaanii keessa dhukkubni jiruufi kaafiroonni ‘Rabbiin (lakkoofsa) kanaan fakkeenya maalii fedhe’ jedhaniif malee kana hin gooneAkka kanatti Rabbiin nama fedhe jallisee nama fedhe immoo qajeelcha(Baay'ina) waraana Gooftaa keetiis Isa malee homtuu hin beekuIsheenis namaaf gorsa malee waan biraatii miti"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Haa dhorgamu! Ji'attin kakadha"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Halkaniinis; yeroo inni deeme"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Bariidhaanis yeroo inni ife"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Dhugumatti, isheen (saqar) tokkoo (rakkoolee) gurgurddooti"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Namaaf akeekkachiiftuudha"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Nama isin irraa (hojii gaariidhaan) fuulduratti deemuu, yookiin duubatti hafuu fedheef (akeekkachiistuu dha)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Lubbuun hundi waan hojjatteen qabamtuudha"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Warra mirgaa malee"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "(Isaan) jannata keessatti wal gaggaafatu"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Yakkamtoota irraa (wal gaafatu)"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Maaltu Saqariin keessa isin seensise?\" (isaanin jedhu)"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Ni jedhan: \"nuti warra salaatan irraa hin turre"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Hiyyeeyyii kan nyaachifnus hin turre"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Warra (hamtuu) keessa seenuu wajjin seenaa turre"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Guyyaa mindaas kan sobsiifnu turre"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hanga (duuti) mirkanaayaan nutti dhufutti (haala kana irra turre)\" (jedhu)"
        }
    ]
}