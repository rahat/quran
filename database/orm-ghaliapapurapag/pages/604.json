{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Jedhi: \"Inni Allaah (gabbaramaa dhugaa) tokkicha"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allaahn hirkoo waa hundaati"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hin dhalle hin dhalannes"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Hiriyaan tokkoos isaaf hin taane.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Jedhi: \"Gooftaa bariittin maganfadha"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Hamtuu waan Inni uumee irraa"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Hamtuu dukkanaa yeroo dukkanaa’e irraas"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Hamtuu guduunfaa keessatti tuftu irraas"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Hamtuu haasidii yeroo inni hassadu (waanyu) irraas (Rabbittin maganfadha).”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Jedhi: \"Gooftaa namaattin maganfadha"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mootii namaatti"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Gabbaramaa namaatti"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Hamtuu waswaasii dhokatuufi mul’atuu irraa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Isa qoma namaa keessa waswaasu irraa"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jinniifi nama irraa kan ta’e (irraan maganfadha).”"
        }
    ]
}