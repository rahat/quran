    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Mindaan isaanii Gooftaa isaanii biratti jannata qubsumaa kan jala ishee laggeen yaatu yeroo hunda ishee keessa jiraatuRabbiin isaan irraa jaalateera; isaanis Isa irraa jaalataniiruKun nama Gooftaa isaa sodaatuuf qofa ta’a"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Yeroo dachiin sochii ishee sosochoote"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Akkasumas, (yeroo) dachiin ba'aa ishee baafte"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Namni \"sila isheen maal taate?\" jedhe"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Guyyaa san (dachiin) oduu ishee ni odeessiti"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "[Kunis] Waan Gooftaan kee isheetti beeksisa godheef"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Guyyaa san namoonni hojii isaanii agarsiifamuuf jecha garee gareen (bakka dhaabbii irraa) deebi’u"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Namni (hanga) madaala atomii toltuu dalage isuma arga"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Namni (hanga) madaala atomii hamtuu dalages isuma arga"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "(Faradoo jihaadaa) harganaa guluftuunin (kakadha)"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Isaan yeroo gulufan kottee isaaniitiin) ibidda qabsiisaniin"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "(Isaan) bariidhaan (diina irratti) weerara adeemsisanitti"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Isaan dhukkee ol kaasanitti"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Isaan (weeraraan) gamtaa diinaa walakkeessaniin [kakadha]"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Dhugumatti, namni (hedduun) Gooftaa isaa mormaadha"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Inni kana irratti ragaadha"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Inni (namni) qabeenya jaalachuutti cimaadha"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Sila inni yeroo wanti qabroota keessa jiran hundi baafaman sana hin beekuu"
        }
    ]
