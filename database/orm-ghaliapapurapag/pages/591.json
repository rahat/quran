{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Samiifi (urjii) halkan dhufaa ta’eenin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Urjii) halkan dhufaa ta’e maaltu si beeksise"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Inni) urjii ifti isaa dukkana uraa ta’eedha"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Lubbuun hundi tiksaan ishee irra jiraatu malee hin hafu"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Namni maal irraa akka uumame haa ilaalu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bishaan utaalaa irraa uumame"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Jidduu dugdaatiifi qomaatii kan ba’u"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Inni isa deebisuu irratti danda’aadha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Guyyaa iccitiiwwan ifa ba'an"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Gaafas) humnas ta’ee tumsaan isaaf hin jiru"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Samii abbaa deddeebiitiinin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Dachii dhodhootuunin (kakadha)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dhugumatti, inni (Qur'aanni) jecha adda baasaadha"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Inni qoosaa hin taane"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Dhugumatti, isaan tooftaa baasu"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Anis tooftaan baasa"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kaafirootaaf yeroo kenniAmmas xinnoo yeroo kenniif"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Maqaa Gooftaa kee olaanaa ta'ee qulqulleessi"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Isa (waan hunda) uumee guute"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Isa murteessee qajeelche"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Isa marga baase"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Goggogaa gurraacha isa taasise"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Si qaraasisuuf jirraa hin dagattu"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Waan Rabbiin fedhe maleeInni mul’ataafi dhokataas ni beeka"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Nuti karaa laaftuu taateef si laaffifna"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Yoo yaadachiisni kan fayyadu ta’e yaadachiisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Namni Rabbiin sodaatu gorfamuuf jira"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Hoonga'aan ishee irraa fagaata"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Inni ibidda guddoo seenu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Ergasii ishee keessatti hin du'u; hin jiraatus"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Dhugumatti, namni of qulqulleeffate milkaa’e"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Maqaa Gooftaa isaa dubbatee kan salaate"
        }
    ]
}