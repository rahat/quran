{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qurayshota barsiisuuf"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Fe’annaa gannaafi bonaa isaan barsiisuuf jecha (warra arbaa balleesse)"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Kanaafuu Gooftaa mana kanaa haa gabbaranu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Isa beela irraa isaan nyachiseefi sodaa irraa isaan tasgabbeesse"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Isa qormaata sobsiisu argitee"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Sun isa yatiima humnaan deebisu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Hiyyeessa nyaachisuu irrattis (namoota) hin kakaafne"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Warra salaataniif badiin mirkanaa’e"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Isaan sun warra salaata isaanii irraa dagatan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Isaan warra (dalagaa namatti) agarsiifataniif"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Isaan meeshaa ergisaa dhorgataniif (badiin mirkanaa’e)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Nuti dhugumatti, tola guddaa siif kennineerra"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Gooftaa keetiif sagadii isaaf qali"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Dhugumatti, kan si jibbu isuma kan qaccee cite (kheyrii hunda irraa cite)"
        }
    ]
}