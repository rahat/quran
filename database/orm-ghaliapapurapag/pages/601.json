{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Yeroonin kakadha"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Dhugumatti, namni hunduu hoongoo keessa jira"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Isaan amanan, gaggaarii dalagan, haqa walii dhaamaniifi obsa walii dhaaman malee (warri hafan hoongoo guddaa keessa jiru)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hamattuufi arrabsituu hundaaf badiin mirkanaa’e"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Isa qabeenya walitti qabee lakkaa'e"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Qabeenyi isaa isa hambisuu kan yaadu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Haa dhorgamu! [Ibidda] caccabsituu keessatti darbamuuf jira"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Caccabsituun maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Isheen) ibidda Rabbii kan qabsiifamtuudha"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ishee onnee irratti (laalaan ishee) mul’attuudha"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Dhugumatti, isheen isaan irratti cufamtuudha"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Utubaalee diriirfamaa keessatti (itti cufama)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Sila Gooftan kee Abbootii Arbaa irratti akkam akka dalage hin arginee"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Sila tooftaa isaanii dhabama hin goonee? (godheera)"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Simbira garee gareedhaan isaanitti erge"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Dhagaalee xuubii (suphee gubamte) irraa taate isaanitti darbatti"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Akka alalaa (okaa) nyaatamee isaan taasise"
        }
    ]
}