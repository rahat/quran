{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "Yeroo wanti qoma keessa jiran (hundi) ifa baafaman (hin beekuu)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Dhugumatti, Gooftan isaanii guyyaa san isaan beekaadha"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qalbii rukuttuun (Qiyaamaan)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Qalbii rukuttuun maali"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Qalbii rukuttuun maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Guyyaa namni akka billaacha faffacaafamee ta’u"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "(Guyyaa) gaarreen akka jirbii iddamee (qaasasame) ta'u"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Namni madaalli (hojii) isaa ulfaatte"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Inni jireenya jaalatamtuu keessa ta'a"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Namni madaalli (hojii) isaa salphatte immoo"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Haati isaa “Haawiyaa[]”dha"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Isheen (Haawiyaan) maal akka taate maaltu si beeksise"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Isheen) ibidda gubduudha"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Baay’ifachuun (ibaadaa irraa) isin jaataneesse (shaagale)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hanga awwaala dhaqxanitti (duutanitti)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Dhorgamaa! Fuula dura beekuuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ergasiis dhorgamaa! Fuula dura beekuuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Dhugumatti, odoo beekumsa mirkanaa’aa beektanii (hin shaagalamtanu ture)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dhugumatti ibidda ni argitu"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ergasii argaa mirkanaa’aa arguuf jirtu"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Ergasii dhugumatti guyyaa san qananii irraa gaafatamuuf jirtu"
        }
    ]
}