{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "Inni cimdii lama, kormaafi dhalaa, uume"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Bishaan hormaataa irraa (uume), yeroo buufamtu"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Uumuun boodaas Rabbuma irra jira"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Inni Isa duroomseefi hiyyoomseedha"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Inni Isumatu Gooftaa Shi’iraati"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Isumatu Aadii duraa balleesse"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "(Ummata) Samuudis hin hambifne"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ummata Nuuhis sana dura (balleesse)Isaan isaanumatu akkaan miidhaa raawwattootaafi daangaa darbitoota turan"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Ganda gombifamtuus gadi kuffise"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Wanti haguuge ishee haguuge"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Ni’imaa Rabbii keetii irraa kamiin shakkita"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Kun (Muhammad SAW) sodaachisaa sodaachistoota durii irraa ta’eedha"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Dhihaattuun (Qiyaamaa) dhihaatteetti"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Rabbiin ala isheef mul’iftuun hin jirtu"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Sila haasa’a kana (Qur’aana) irraa dinqisiifattanii"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ni kolfituu! Hin boochanuu"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Haala sirbitanuun (irraa garagaltuu)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Allaah’f sujuudaa; (Isuma qofa) gabbaraa"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "Qiyaamaan kaloofte; jiinis lamatti addaan dhoohe"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "Yeroo mallattoo wahii argan gara galaniiti “(kun) sihrii cimaadha” jedhu"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "(Qur’aana) sobsiisanii fedha lubbuu isaanii hordofuWanti hunduu galma ga’aadha"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "Dhugumatti, oduu irraa wanti isa keessa dhorgaan jiru isaanitti dhufeera"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "Beekumsi ga’aan (isaanitti dhufeera)Akeekkachiistonni homaa isaan hin fayyadu"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "Isaan irraa garagaliGuyyaa waamaan gara waan rifachiisaa ta’eetti (isaan) waamu"
        }
    ]
