{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "[Warri hin amanin] maal irraa wal gaafatuu"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Oduu guddaa irraa [wal gaafatu]"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Isa isaan isa keessatti wal dhaban [irraayi]"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Haa dhowwamanu! fuula dura beekuuf jiru"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Ammas haa dhowwamanu! fuula dura beekuuf jiru"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Sila Nuti dachii firaasha hin goonee"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Gaarreenis shikaala [hin goonee]"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Cimdiis goonee isin uumne"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Hirriba keessan boqonnaa goone"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Halkan uffata goone"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Guyyaas yeroo jireenyaa (keessa hojjatan) goone"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Gubbaa keessanittis [samii] jajjaboo torba ijaarre"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ifaa boba'aas uumne"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Duumessa irraas bishaan dhangala'aa baay'ee buufne"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Isaan fuduraafi marga baasuuf jecha [buufne]"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ashaakiltii damee wal keessa seensifattes"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Dhugumatti, guyyaan fooyamaa yeroo beellamamaa ta'e"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "[Guyyaan sun] guyyaa Garriin (xurumbaan) afuufamee isinis garee gareen dhuftanuudha"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "[Guyyaa] samiinillee banamtee hulaalee taatuudha"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Gaarreenis deemsifamtee siribii (siribduu) taate"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Dhugumatti jahannam waldaa taateetti"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Warra daangaa dabraniif iddoo deebiiti"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Yeroo dheeraaf ishee keessa turu"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Ishee keessatti qabbanas ta'ee dhugaatii hin dhandhamanu"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Bishaan akkaan danfaafi malaa malee [dhugaatii biraa hin argatanu]"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Mindaa wal madaalu [mindeeffaman]"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Isaan qormaata hin sodaatanu turan"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Keeyyattoota keenyas kijibsiisuu kijibsiisan"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wanta cufaayyuu haala barreeffameen walitti qabne"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "“Dhandhamaa; azaaba malees isiniif hin daballu” [isaaniin jedhama]"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Dhugumatti, warra Rabbiin sodaataniif milkiitu jira"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Ashaakiltiifi inabatu [jira]"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Dubartoota harma guntuttuu hiriyaa ta'antu [jiru]"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Geeba (xoofoo) guutamtuu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Achi keessatti dubbii badaafi wal kijibsiisuu hin dhaga'anu"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Mindaa Gooftaa kee irraa ta’e kennaa ga’aa [mindeeffaman]"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gooftaa samii, dachiifi waan gidduu isaanii jiru, mararfataa kan ta'e [irraa mindeeffaman]Isaa wajjin dubbachuu hin danda'anu"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Guyyaa Jibriiliifi Maleykonni haala hiriiraniin dhaabbatan nama Rabbiin eeyyameefi dhugaa dubbate malee hin haasawanu"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sun guyyaa [dhufaatiin isaa] haqaatiNamni fedhe [hojii gaarii hojjachuun] gara Gooftaa isaatti deebii tolfata"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Nuti adabbii dhihoo kan guyyaa namni waan harki lamaan isaa dabarfatte arguufi kaafirris \"yaa hawwii kiyya! odoon biyyee ta’ee maal qaba\" jedhu san isin sodaachifneerra"
        }
    ]
}