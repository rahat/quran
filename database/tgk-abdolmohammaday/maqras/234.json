{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Рӯйро турш кард ва сар баргардонид"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "чун он нобино ба наздаш омад"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ва ту чӣ донӣ, шояд, ки ӯ покиза шавад"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ё панд гирад ва панди ту фоидааш кунад"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Аммо он ки ӯ тавонгар аст"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ту рӯи худ ба ӯ мекунӣ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "ва агар ҳам пок нагардад, чизе бар зиёни ту нест"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ва аммо он, ки шитобон ба назди ту меояд"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ва метарсад, (аз худо)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ту аз ӯ ба дигаре рӯй, меорӣ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Оре, ин Қуръон пандест"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "пас ҳар кӣ хоҳад, аз он панд гирад"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Дар саҳифаҳое гиромӣ"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "баландқадру покиза"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ба дасти котибоне"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "бузургвору некӯкор"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Марг бар одамӣ бод, ки чӣ носипос (ношукр) аст"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ӯро аз чӣ офаридааст"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Аз нутфае офарид ва ба андоза муайян кард"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "сипас роҳашро осон сохт"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Он гоҳ бимирондаш ва дар гӯр кард"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ва он гоҳ, ки хоҳад, зиндааш созад"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "На ки ҳанӯз он чиро ба ӯ фармон дода буд, ба ҷой наёвардааст"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Пас одамӣ ба таъоми худ бингарад"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Мо боронро фурӯ боридем, бориданӣ"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ва заминро шикофтем, шикофтанӣ"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ва дар он донаҳо рӯёнидем"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ва току сабзавот"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "ва зайтуну нахл (дарахти хурмо)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ва боғҳои пурдарахт"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ва меваву алаф"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "то шумову чорпоёнатон баҳра баред"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Чун бонги қиёмат барояд"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "рӯзе, ки одамӣ аз бародараш мегурезад"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "ва аз модарашу падараш"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "ва аз занашу фарзандонаш"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ҳар касро дар рӯз корест, ки ба худ машғулаш дорад"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Чеҳраҳое дар он рӯз дурахшонанд"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "хандонанду шодонанд"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ва чеҳраҳое дар он рӯз ғуборолуданд"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "дар сиёҳӣ фурӯ рафтаанд"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Инҳо кофирону фоҷиронанд (бадкорон)"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Чун хуршед бефурӯғ (хира) шавад"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "ва чун ситорагон фурӯ резанд"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "ва чун кӯҳҳо аз ҷои худ беҷо бишаванд"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "ва чун шутурони ҳомила ба ҳоли худ гузошта шаванд"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "ва чун ҷонварони ваҳшӣ ҷамъ оварда шаванд"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "ва чун дарёҳо оташ гиранд"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "ва чун рӯҳҳо бо танҳо якҷоя гардганд"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "ва чун аз духтари зинда ба гӯр шуда пурсида шавад, ки"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ба чӣ гуноҳе кушта шудааст"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "ва чун саҳифаҳо боз шаванд"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "ва чун осмон аз ҷои худ канда шавад"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "ва чун ҷаҳаннам афрӯхта гардад"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "ва чун биҳиштро пеш оваранд"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ҳар кас бидонад, ки чӣ ҳозир овардааст"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Савганд ба ситорагони бозгарданда"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "сайркунандаи ғойбшаванда"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "ва савганд ба шаб, чун торик шавад"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "ва савганд ба субҳ чун бидамад"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ки ин сухани расуле бузургвор (Ҷибрил (а)) аст"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "нерӯманд, дар назди Худованди арш соҳибобрӯ"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "он ҷо (дар назди Худо) фармонравои боваринокаст"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ва ҳамсӯҳбати шумо девона нест"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Албатта Ӯро (Ҷибрилро) дар уфуқи равшан дидааст"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ва он чиро аз ғайб ба ӯ гӯянд, дареғ намедорад"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ва он сухан сухани шайтони раҷим нест"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Пас ба куҷо меравед"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ин китоб пандест барои ҷаҳониён"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "барои ҳар кас аз шумо, ки бихоҳад ба роҳи рост афтад"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ва шумо намехоҳед, ғайри он чиро, ки Парвардигори ҷаҳониён хоста бошад"
        }
    ]
}