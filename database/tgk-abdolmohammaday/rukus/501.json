{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Пас савганд ба он чи мебинед"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "ва он чӣ намебинед"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ки ин сухани фиристодае бузургвор аст"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "на сухани шоире. Чӣ андак имон меоварад"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ва низ сухани коҳине (фолбин, ҷодугар) нест. Чӣ андак панд мегиред"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Аз ҷониби Парвардигри ҷаҳониён нозил шуда аст"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Агар паёмбар баъзе суханонро бофта бар мо мебаст, («аз ҷониби худост» — мегуфт)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "бо қудрат ӯро фурӯ мегирифтем"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "сипас раги дилашро пора, мекардем"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "ва ҳеҷ як аз шуморо тавони он набувад, ки монеъ шавад"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ва Қуръон барои парҳезгорон пандест"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Мо медонем, ки аз миёни шумо касоне ҳастанд, ки такзиб мекунанд, (дурӯғ мешумуранд)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ва Қуръон кофиронро мояи пушаймонӣ аст"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ва он сухани ҳақ ва яқин аст"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Пас ба номи Парвардигори бузургат тасбеҳ (бигӯй)"
        }
    ]
}