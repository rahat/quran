{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Савганд ба аспони тездав"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "савганд ба аспоне, ки ба сум аз санг оташ меҷаҳонанд"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ва савганд ба аспоне, ки бомдодон ҳуҷум оваранд"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ва дар он ҷо ғубор бархезонанд"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ва дар он ҷо дар миён дароянд"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ки одамӣ Парвардигори худро шукр намегӯяд"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ва ӯ худ бар ин гувоҳ аст"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ӯ молро сахт дӯст дорад"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Оё намедонад, ки чун он чӣ дар гӯрҳост, зинда гардад"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ва он чӣ дар дилҳо ниҳон аст, ошкор шавад"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Парвардигорашон дар он рӯз аз ҳолашон огоҳ аст"
        }
    ]
}