{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Қасам ба ин шаҳр! (Макка)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ва ту дар ин шаҳр ҳалол хоҳӣ шуд"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ва қасам ба падару фарзандони ӯ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "ки одамиро, дар ранҷу меҳнат биёфаридаем"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Оё мепиндорад, ки кас бар ӯ қодир нагардад"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Мегӯяд: «Моле фаровонро харҷ кардам!»"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Оё мепиндорад, ки касе ӯро надидааст"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Оё барои ӯ ду чашм наёфаридаем"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ва як забону ду лаб"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ва ду роҳ ба ӯ далолат накардем"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ва ӯ дар он гузаргоҳи сахт қадам наниҳод"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ва ту чӣ донӣ, ки гузаргоҳи сахт чист"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Озод кардани банда аст"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ё таъом додан дар рӯзи қаҳтӣ"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "хоса ба ятиме, ки хешованд бошад"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ё ба мискине хокнишин (мӯҳтоҷ)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Баъд, аз он аз касоне бошад, ки имон овардаанд ва якдигарро ба сабр васият кардаанду ба шафқат"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Инҳо аҳли саъодатанд"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ва касоне, ки ба оёти Мо кофиранд, аҳли шақоватанд (бадбахтиянд)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Насиби онҳост оташе, ки аз ҳар сӯ сараш пӯшидааст"
        }
    ]
}