{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Бигӯ: «Ба Парвардигори мардум паноҳ мебарам"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "фармонравои мардум"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Худои мардум"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "аз шарру васвасагари пинҳоншаванда"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "он кӣ дар дилҳои мардум васваса мекунад"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "хоҳ аз ҷинниён бошад ё аз одамиён!»"
        }
    ]
}