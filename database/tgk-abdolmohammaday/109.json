        {
            "chapter": 109,
            "verse": 1,
            "text": "Бигӯ: «Эй кофирон"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ман чизеро, ки шумо мепарастед, намепарастам"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ва шумо низ чизеро, ки ман мепарастам, намепарастед"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ва ман парастандаи чизе, ки шумо мепарастед, нестам"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ва шумо парастандаи чизе, ки ман мепарастам, нестед"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Шуморо дини худ ва маро дини худ!»"
        }
    ]
