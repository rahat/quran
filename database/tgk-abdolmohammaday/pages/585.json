{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Рӯйро турш кард ва сар баргардонид"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "чун он нобино ба наздаш омад"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ва ту чӣ донӣ, шояд, ки ӯ покиза шавад"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ё панд гирад ва панди ту фоидааш кунад"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Аммо он ки ӯ тавонгар аст"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ту рӯи худ ба ӯ мекунӣ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "ва агар ҳам пок нагардад, чизе бар зиёни ту нест"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ва аммо он, ки шитобон ба назди ту меояд"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ва метарсад, (аз худо)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ту аз ӯ ба дигаре рӯй, меорӣ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Оре, ин Қуръон пандест"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "пас ҳар кӣ хоҳад, аз он панд гирад"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Дар саҳифаҳое гиромӣ"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "баландқадру покиза"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ба дасти котибоне"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "бузургвору некӯкор"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Марг бар одамӣ бод, ки чӣ носипос (ношукр) аст"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ӯро аз чӣ офаридааст"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Аз нутфае офарид ва ба андоза муайян кард"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "сипас роҳашро осон сохт"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Он гоҳ бимирондаш ва дар гӯр кард"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ва он гоҳ, ки хоҳад, зиндааш созад"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "На ки ҳанӯз он чиро ба ӯ фармон дода буд, ба ҷой наёвардааст"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Пас одамӣ ба таъоми худ бингарад"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Мо боронро фурӯ боридем, бориданӣ"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ва заминро шикофтем, шикофтанӣ"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ва дар он донаҳо рӯёнидем"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ва току сабзавот"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "ва зайтуну нахл (дарахти хурмо)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ва боғҳои пурдарахт"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ва меваву алаф"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "то шумову чорпоёнатон баҳра баред"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Чун бонги қиёмат барояд"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "рӯзе, ки одамӣ аз бародараш мегурезад"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "ва аз модарашу падараш"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "ва аз занашу фарзандонаш"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ҳар касро дар рӯз корест, ки ба худ машғулаш дорад"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Чеҳраҳое дар он рӯз дурахшонанд"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "хандонанду шодонанд"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ва чеҳраҳое дар он рӯз ғуборолуданд"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "дар сиёҳӣ фурӯ рафтаанд"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Инҳо кофирону фоҷиронанд (бадкорон)"
        }
    ]
}