{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "ва он чӣ дар дилҳо ниҳон аст, ошкор шавад"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Парвардигорашон дар он рӯз аз ҳолашон огоҳ аст"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Қориъаҳ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Чист қориъаҳ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ва ту чӣ донӣ, ки қориъаҳ чист"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Рӯзест, ки мардум чун парвонагон пароканда бошанд"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ва кӯҳҳо чун пашми задашуда"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Аммо ҳар кӣ паллаи некиҳои тарозуяш вазнин бошад"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Пас ӯ дар айши писандида бошад"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "ва аммо ҳар кӣ сабук шуд паллаи некиҳои тарозуяш"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ҷойгоҳаш дар ҳовияҳ аст"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ва ту чӣ донӣ, ки ҳовияҳ чист"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Оташест дар ниҳояти гармӣ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ба ғафлат кашид шуморо фахр кардан ба бисёрии молу фарзанд"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "то ба гӯрҳо расидед"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ҳаққо, ки ба зудӣ хоҳед донист"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Боз ҳам ҳаққо, ки ба зудӣ хоҳед донист"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ҳаққо агар аз рӯи яқин бидонед"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "албатта, ҷаҳаннамро хоҳед дид"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Сипас ба чашми яқинаш хоҳед дид"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "сипас дар он рӯз шуморо аз неъматҳои дунявӣ бозхост мекунанд"
        }
    ]
}