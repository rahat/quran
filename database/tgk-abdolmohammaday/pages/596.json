{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "ҷуз аҳли бадбахтӣ ба он дарнаяфтад"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "он ки дурӯғ баровард ва рӯйгардон шуд"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ва парҳезгортарин мардумро аз он дур доранд"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "он кӣ моли худ мебахшояд ва покӣ меҷӯяд"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ва ҳеҷ касро бар (гардани) ӯ ҳаққе набошад, ки акнун мукофот хоҳад"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ғайри ҷустани хушнудии Парвардигори баландмартабаи худ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ва ба зудӣ хушнуд шавад"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Савганд ба вақти чошт"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ва савганд ба шаб, чун бипӯшад (бо торикии худ атрофро)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ки Парвардигорат туро тарк накарда ва душман ҳам нагирифтааст"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Албатта охират барои ту беҳтар аз дунёст"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ба зуди Парвардигорат туро ато хоҳад дод, то хушнуд шавӣ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Оё туро ятим наёфт ва паноҳат надод"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ва туро гумгашта ёфт ва ҳидоятат кард"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ва туро дарвеш ёфт ва тавонгарат гардонид"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Пас ятимро маёзор"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ва гадоро марон"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ва аз неъмати Парвардигорат сухан бигӯй"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Оё синаатро бароят накушодем"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ва бори гаронатро аз пуштат барнадоштем"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Боре, ки бар пушти ту сангинӣ мекард"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Оё туро баландовоза насохтем"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Пас аз паи душворӣ осонист"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Албатта аз паи душворӣ осонист"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Чун аз кор фориғ шавӣ, ба ибодат кӯш"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ва ба Парвардигорат рағбат намо"
        }
    ]
}