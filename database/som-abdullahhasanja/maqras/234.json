{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Wejiguu)ururshay oo dhabarka jeediyey"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Inuu halkaa ugu yimid indhoolihii"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Oo maxaad ka og tahay waa intaasoo uu daahiriyo naftiisa"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ama waansamo, oo waanadu wax tarto"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ma kan isu arka inuu isagu(kaa) maarmo"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Miyaad adigu xaggiisa u jeedsaneysaa"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Oo eed kaama saarna haddii uusan daahirinba naftiisa"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ma kan se kuu yimid isagoo aad u dadaalaya"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Oo isagu (Alle ka) cabsada"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Miyaad adigu ka mashquuli"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Mayee, hubaal (kani) waa waano"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ee qofkii doona ha ku waano qaato"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Wuxuu ku yaal kutub la sharfay"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Kor ah, la dahiray"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ku jira gacmaha qorayaal (malaa’ig)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Sharaf leh oo baariyaal ah"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Belo ha ku dhacdo aadmigee! Muxuu jazaar yahay"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Wax maxay ah buu ka abuuray"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Mani yar xaggeed, buu ka abuuray markaas qaddaray (isaga)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Markaas uga dhigay sahal marriinka"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Haddana wuxuu sababaa inuu dhinto, oo u yeelaa qabri"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Oo markuu doono wuu soo bixin"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Mayee, ma uusan fulin Wuxuu amray"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ee qofku bal ha fiiriyo cuntadiisa"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Sidee baan u soo shubnaa biyaha soo shubid badan"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Markaas Waxaan jeexnaa arlada jeexdin gaballo ah"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Oo sababnaa inay ka soo baxaan haruur"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Iyo cinabyo iyo naq"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Iyo saytuun iyo geedo timireed"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iyo beero tanaad ah"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Iyo miro iyo caws"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sahay aad leedihiin idinka iyo xoolihiinnuba"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Markase ay qeylada dhego beelisada ah timaaddo"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Maalintaa uu qofku ka carari walaalki"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Iyo hooyadi iyo aabbihi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Iyo xaaskiisa iyo ilmahiisa"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Qof walba oo ka mid ah, Maalintaas waxaa heysan doonta arrin ku filan"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wejiyo (qaar) Malintaas way ifi"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Iyagoo muusoon, ku faraxsan warka san"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Oo wejiyo (kale) Maalintaas waxay ahaan kuwo booreysan"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Mugdi baa dabooli doona iyaga"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Kuwaasi waa kuwa ah gaalada dambiilayaasha ah"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Marka qorraxda la iftiin tiro"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Oo xiddiguhu soo daataan"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Oo buuraha la rujiyo"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Oo geela rimman la caddilo"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Oo dugaagga la soo wada kulmiyo"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Oo badaha la shido (ama soo fatahaan)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Oo nafaha lagu lammaaniyo jirarkooda"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Oo (gabadhii) nolosha lagu xabaalay la weydiiyo"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Denbigee baa loo dilay"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Oo marka kutubta la fidiyo"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Oo marka samada daboolka laga qaado"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Oo marka Naarta la huriyo"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Oo marka Jannada la soo dhoweeyo"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Naf walbaa waxay (markaa) ogaan waxay la goob iyo shir timid"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Mayee se Waxaan ku dhaartay xiddigaha dib gurta"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Ee ku socda rugahooda, ee qarsooma"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Iyo habeenka marka uu tago"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Iyo subaxa marka uu neefsado"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Hubaal kani waa hadalka (Ilaahay) uu keenay Rasuul sharaf badan (Jibriil)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Awood iyo meeqaan sare ku leh (Allaah) agtiis, Rabbiga Carshiga"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "La adeeco, haddana aamin ah kalsooni lagu qabo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Oo Saaxiibkiin (Nabi Muxammad) ma aha (innaba) ku waalan"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Oo xaqiiq horay buu ugu arkay isaga (Jibriil) jihada cad (dhinaca qorrax soo baxa)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Oo ma aha isagu (Muxammad) mid ceshada cilmiga ku saabsan Ghaybka"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Oo (Qur’aanku) ma aha hadalka Shaydaan laga soo ceyrshay (samooyinka)"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ee xaggee baad u socotaan"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Wax kale ma aha aan aheyn Waano ku socota ummadaha"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kii isagu idinka mid ah ee raba inuu qaado jid toosan"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Oo wax ma doontaan aan ahayn waxaa uu Allaah doono, _Rabbiga uunanka"
        }
    ]
}