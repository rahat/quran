        {
            "chapter": 114,
            "verse": 1,
            "text": "Dheh: Waxaan ka magan galay Rabbiga dadka"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Boqorka dadka"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ilaaha dadka"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sharka waswaasyada (sheydaanka) soo noqnoqodka badan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Kaasoo waswaasiya laabaha dadka"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Shaydaankaa) oo jin iyo dadba leh"
        }
    ]
