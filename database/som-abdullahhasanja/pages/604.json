        {
            "chapter": 112,
            "verse": 1,
            "text": "Dheh: Isagu, Allaah waa Mid Keliya"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allaah waa ma baahdaha loo baahdo"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ma dhalin, lamana dhalin"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Oo ma jiro axad u dhigma"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Dheh: Waxaan ka magan galay Rabbiga oogga (dillaacsha)"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Sharka waxa uu abuuray"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Iyo sharka habeenka mugdiga badan marka uu mugdigiisa wax ku daboolo"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Iyo sharka kuwa wax ku tufa guntimaha (naagaha saaxirooyinka ah)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Iyo sharka xaasidka marka uu wax xasdo"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dheh: Waxaan ka magan galay Rabbiga dadka"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Boqorka dadka"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ilaaha dadka"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sharka waswaasyada (sheydaanka) soo noqnoqodka badan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Kaasoo waswaasiya laabaha dadka"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Shaydaankaa) oo jin iyo dadba leh"
        }
