{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Waxaan ku dhaartay waqtiga"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Hubaal Aadmigu wuxuu ku jiraa khasaaro"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Marka laga reebo kuwa (Xaqa) rumeysan oo samaha fala, oo isu dardaraama Xaqa, oo isu dardaarama sabarka"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hoog waxaa leh dure kasta, wax ceebeeya"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Kaasoo maal jamciya oo u heysta sahay ahaan (aanse ka bixin xaqa lagu leeyahay)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Wuxuu moodaa in maalkiisu waarin"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Mayee! Waxaa hubaal lagu tuuri burburisada"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Oo maxaa ku ogeysiiyey waxa ay burburisadu tahay"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Waa Naar Ilaahay oo la huriyey"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oo kor ugu baxda quluubta dushooda"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Hubaal waxaa lagaga xidhi dusha korkooda"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Tiirar la taagay dhexdood"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Mawaadan u fiirsan sida Rabbigaa u galay qoladii maroodiga"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Sow kama uu dhigin duullaankoodii wax jab iyo jah wareer ku dhammaaday"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Oo ku soo dirin shimbiro guutoooyin ah"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ku soo ganeysa dhagaxyo dhoobo la kululeeyey ah"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Markaas ka dhigay sidii bal la daaqay"
        }
    ]
}