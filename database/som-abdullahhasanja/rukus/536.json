{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Miyaanaan kuu waasicinnin laabtaada"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Oo aan kaa dejin culeyskaaga"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ee cusleeyey dhabarkaaga"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Oo aanaan kor yeelin meeqaankaaga"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ee hubaal culeyska waxaa la socda nafis"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Dhibka waxaa hubaal la socda nafis"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ee markaad bogto (howshaada), u istaag cibaado"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Oo xagga Rabbigaa(Keliya) wax ka rajee"
        }
    ]
}