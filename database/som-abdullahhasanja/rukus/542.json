{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Waxaan ku dhaartay ordayaasha neefsada (fardaha) iyagoo qeyliyaya"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Haddana kuwaa dabka kiciya"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Haddana kuwaa qaada duullaanka subaxa hore"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Markaas boorka kiciya"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Markaa dhaqso bartanka ugu gala kulanka (cadowga)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Hubaal! Aadmigu waa ka abaal dhacaa Rabbigi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Oo hubaal isaga baa taasi ka markhaati ah"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Oo hubaal Waa ku daran yahay Jeclaanta maalka"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ma wuusan haddaba ogeyn marka waxa ku jira qabuuraha la soo bixiyo"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Oo waxa ku jira laabaha la caddeeyo"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Hubaal Rabbigood maalintaas waa u Xog-Ogaal si dhan iyaga"
        }
    ]
}