{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Musiibada garaaceysa (qalbiyada)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Maxay tahay Garaacdadu"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Oo maxaa ku ogeysiiyey waxa ay tahay Garaacdadu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Maalinta ay dadku noqon sida baalallay kala firdhay"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Oo buurahu noqon sida cudbi la dhilay"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ee ma kan isagu miisaankiisa (camallada wanaagsan) culeys yahay"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Wuxuu ku noolaan nolol barwaaqo iyo farxad leh"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ma qofka se uu miisaankiisa (camallada wanaagsan) fudeyd yahay"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Hoygiisu waa Haawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Oo maxaa ku ogeysiiyey waxa ay tahay"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Waa) Naar hureysa"
        }
    ]
}