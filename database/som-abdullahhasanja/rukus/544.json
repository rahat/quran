{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Wax badsi doon baa idin jeediya"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ilaa aad gashaan qubuuraha"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Mayee! Waad ogaan doontaan"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Mayee, mayee! Waad ogaan doontaan"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Mayee! haddii aad og tihiin ogaansho dhab ah"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Waxaad hubaal arki doontaan Naarta"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Markaas waxaad hubaal ku arki doontaan il dhab ah"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Oo Maalintaa waxaa hubaal la idin weydiin doonaa Nicmooyinka"
        }
    ]
}