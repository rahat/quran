{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hoog waxaa leh dure kasta, wax ceebeeya"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Kaasoo maal jamciya oo u heysta sahay ahaan (aanse ka bixin xaqa lagu leeyahay)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Wuxuu moodaa in maalkiisu waarin"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Mayee! Waxaa hubaal lagu tuuri burburisada"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Oo maxaa ku ogeysiiyey waxa ay burburisadu tahay"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Waa Naar Ilaahay oo la huriyey"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oo kor ugu baxda quluubta dushooda"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Hubaal waxaa lagaga xidhi dusha korkooda"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Tiirar la taagay dhexdood"
        }
    ]
}