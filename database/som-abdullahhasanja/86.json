{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Waxaan ku dhaartay samada iyo Xiddigga guura"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Oo maxaa ku ogeysiiyey waxa uu xiddiga guura) yahay"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Xiddigga iftiinka badan"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ma jirto naf aanu kor joogin ilaaliye (malag)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ee ha fiiriyo qofka waxa laga abuuray"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Waxaa laga abuuray biyo horay u soo booda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Oo ka yimaada dhabarka iyo feeraha dhexdood"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Hubaal wuu karaa inuu u soo celiyo nolosha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Maalinta waxyaalaha qarsoon iyo wixii sir ahaan loo falay la muujin doono"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ma lahaan doono xoog ama wax u soo gargaara midna"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Waxaan ku dhaartay cirka soo celcelinta badan (ee soo shuba roobka)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Iyo dhulka dil-dillaaca badan (ee soo saara dhirta iyo daaqa)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Hubaal kani waa hadal dhab ah oo cad"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Oo ma aha dheel"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Hubaal waxay qoolaan shirqool"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Oo Anigu (sidoo kale) Waxaan qorsheeyaa qorshe"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ee u sug gaalada: Daa wax yar"
        }
    ]
}