{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Goditja e tmerrshme (Kijameti)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "C’eshte Goditja e tmerrshme"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Kush te tregon ty per Goditjen e tmerrshme"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Njerezit, ate Dite do te jene te shperndare si fluturat"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "kurse malet – si leshi i shprishur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Atij (njeriut), veprat e mira te te cilit rendojne ne peshoje"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ai do te jete ne jeten e kenaqur (ne jeten e perhershme)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "e atij (njeriu), veprat e mira te te cilit nuk rendojne ne peshoje"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "strehimi i tij do te jete gremina e Sketerres"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "E, kush te tregon ty se c’eshte gremina e Sketerres"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Ajo eshte) zjarr i vrullshem"
        }
    ]
}