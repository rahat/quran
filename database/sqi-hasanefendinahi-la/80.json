{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai (Muhammedi) u mrrol e u kthye (me fytyre)"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ngase i erdhi prane i verberi"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E kush te defton ty – ndoshta ai deshiron te pastrohet (nga mekatet)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ose te keshillohet, e t’i sjell dobi atij keshillimi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "e atij qe esthe i pasur (e prezentohet si i panevojshem per te besuar)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "atij po i kthehesh"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "sepse, ti nuk ke pergjegjesi, nese ai nuk don te pastrohet (te besoje)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "e atij qe te vjen ty duke nxituar (per keshillim)"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "dhe ndien droje"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "e ti ate nuk po e merr ne konsiderim"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Mos vepro ashtu! Ato jane keshilla"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "e, kush te doje, le te mesoje"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "ne faqet e (Kur’anit te) cmueshme"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "te lartesuara (te madherueshme), te pastra"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "me duart e shkruesve (engjejve)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "te ndershem dhe te drejte"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mallkuar qofte njeriu! Sa jomirenjohes qe eshte ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Prej ckahjes e krijoi Ai (Perendia) ate (njeriun)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Prej pikes se fares e krijoi dhe pergatiti per te cdo gje te dobishme"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "dhe pastaj ia lehtesoi rrugen (e lindjes)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pas kesaj, (Ai) ia caktoi vdekjen dhe beri qe te varroset"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "dhe, mandej kur te doje Ai, e ringjalle"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ne te vertete! Ai – njeriu, ende nuk i ka plotesuar ato qe i ka urdheruar Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Le te shikoje njeriu ne ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ne kemi derdhur ujera (shira) te bollshme"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pastaj e kemi plasaritur token me carje"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "dhe bejme qe nga ajo te rriten dritherat"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "dhe rrushi e perimet"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "dhe ulliri e palma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "dhe kopshtet e dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "dhe pemet e barishtet"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "per ju dhe bagetine tuaj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "E, kur te arrije zeri i madh shurdhues"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ne Diten kur njeriu iken nga vellau i tij"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "dhe nga nena e babai i tij"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "dhe nga bashkeshortja e femijet e tij"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ate Dite, cdonjeri eshte i preokupuar me gjendjen e vetvetes"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "disa fytyra ate Dite, shendrisin"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "te qeshura, te gezuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "kurse disa fytyra, ate Dite, pluherosen"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "erresira do t’i kaploje"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "keta jane jobesimtaret, te korruptuarit"
        }
    ]
}