        {
            "chapter": 112,
            "verse": 1,
            "text": "Thuaj: “Perendia eshte Nje"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Perendise i drejtohen per cdo gje"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "(Ai) nuk ka lindur prej ndokujt, as nuk ka lindur ke"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "dhe askush nuk i gjason Atij!”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Thuaj: “I lutem dhe mbeshtetem te Zoti i agimit"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "qe te me mbroje nga sherri i te gjitha krijesave"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "dhe prej sherrit te nates kur kaplon terri"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "dhe prej sherrit te falltoreve qe fryejne ne nyje (bejne magji)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "dhe nga sherri i ziliqarit kur e shfaq ziline!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “I lutem dhe i mbeshtetem Zotit te njerezve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundimtarit te njerezve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Perendise se njerezve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "nga sherri i djallit, bezdises"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "i cili fshehurazi hedh dyshime ne shpirterat e njerezve –"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "prej xhindeve dhe prej njerezve!”"
        }
    ]
