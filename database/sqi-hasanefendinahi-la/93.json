{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Betohem ne mengjesin"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "dhe naten kur terrohet e qetesohet –"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Zoti yt, nuk te ka braktisur e as nuk te ka urrejtur"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Me te vertete, jeta e ardhshme per ty eshte me e mire se kjo jete"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "e, me te vertete, Zoti yt do te japi ty, e ti do te jesh i kenaqur"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk ishe ti bonja, e Ai te mori ne strehim"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "dhe te gjeti te humbur (te paudhezuar), e te udhezoi me Shpalljen e Kur’anit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "dhe ishe i varfer e te beri te pasur"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Mos e nencmo bonjakun"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "e lypsin mos e refuzo me ashpersi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "dhe dhuntite e Zotit tend, shprehi"
        }
    ]
}