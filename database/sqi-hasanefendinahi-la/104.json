{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Mjer per cdo perqeshes – shpifes"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "qe grumbullon pasuri dhe e ruan, (per ardhmerine e enderruar)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "dhe mendon qe ajo pasuri e tij, do ta beje te pavdekshem"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jo, jo! Mashtrohet! Ai, me te vertete, do te hidhet (ne) Hutameh (Sketerre), i habitur"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "E kush te tregon ty se c’eshte el-Hutameh"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ai eshte zjarri i ndezur i Zotit"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "i cili do te depertoje deri ne zemer"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ai (zjarri) do te jete i mbyllur mbi ta (si sac)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "ne shtylla te gjata flakeruese"
        }
    ]
}