{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Noi nu racori vostri caracter"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Noi descarca vostri sarcina pacat"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Una ala însarcina vostri spinare"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Noi exalted tu onorabil pozitie"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Cu durea acolo exista câstiga"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Într-adevar cu durea acolo exista câstiga"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Când posibil tu tinde"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Cauta doar vostri Domnitor"
        }
    ]
}