{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Glorifica nume vostri Domnitor a Multi(multe) Înaltime"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "El crea modela"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "El desena ghid"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "El produce pasune"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Atunci rasuci el usor împrejmuire"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Noi recita tu; nu uita"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Everything exista în acordan\u0004a cu dumnezeu vei El sti care declara ce camufla"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Noi îndruma tu easiest poteca"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Therefore tu aminti; probabil aluzie beneficia"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Respectuos vointa lua atentie"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Hain vointa ocoli el"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Consequently el suferi mare Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Wherein el niciodata deceda nor sedere alive"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Prosper într-adevar exista una compensa his suflet"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "By aminti nume his Domnitor observa atingere rugaciune (Salat)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Într-adevar tu preocupa acesta 1 trai"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Seara desi Hereafter exista departe bine everlasting"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Acesta înregistra devreme teachings"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "teachings ai Abraham Moses"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Fi tu constient a a Coplesi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Fa\u0004a ala zi rusina"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Trudi si exhausted"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Suferi arde Hellfire"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "bea de flaming primavara"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Ei nu avea aliment nefolositor varietate"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "El niciodata nutri nor satisface flamânzi"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Altul fa\u0004a zi vei exista plin a bucura"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Satisface their munca"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "În exalted Paradis"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "În el nu absurditate asculta"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "În el tâsni flows"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "În el acolo exista luxos furnishings"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Bea produce disponibil"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ulcior în vâsli"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Si covor rotund"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Why ei nu reflecta camila si cum ei crea"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Si cer si cum el raised"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Si munte si cum ei construi"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Si earth si cum el cladire"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Tu aminti vostri misiune exista livra acesta aluzie"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Tu nu avea power ele"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "As for ala întoarcere disbelieve"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "DUMNEZEU vei COMITE ELE MARE retribution"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Catre us exista their ultim destin"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Atunci noi chemare ele cont"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "By zori"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "noapte"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "By însera a impar"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "By noapte el trece"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Adânc juramânt 1 cine poseda inteligenta"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Tu însemna ce vostri Domnitor avea did catre ‘Aad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Erum; oras înalt cladire"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "acolo exista nimic el oriunde"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Altminteri Thamoud cine taia stânca their vale"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Pharaoh poseda putere"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ei toate încalca uscat"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Ei întinde evil rotund"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Consequently vostri Domnitor curge ele biciuire retribution"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Vostri Domnitor exista ever vigilent"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "When uman exista proba his Domnitor binecuvântare bucura el spune Meu Domnitor exista generos catre mie"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "El proba him micsorare aprovizionare el spune Meu Domnitor umili mie"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "gresit Exista tu aduce el însati nu privi orfan"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Si nu advoca caritate sarac"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "consuma mostenire neajutorat orfan"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Si iubi bani prea mult"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Într-adevar when earth strivi utterly strivi"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Vostri Domnitor veni împreuna înger în vâsli când vâsli"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ala zi Gehenna aduce departe. Ala zi uman exista aminti - Dar care memorie - El exista prea întîrziat"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "El spune Oh Eu dorinta eu pregatit meu (eternal) trai"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ala zi nu retribution exista urât decât His retribution"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Nu închidere exista as effective as His închidere"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "As for tu O multumit suflet"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Întoarcere catre vostri Domnitor multumit multumi"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "întîmpinat în Meu servitor"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "întîmpinat în Meu Paradis"
        }
    ]
