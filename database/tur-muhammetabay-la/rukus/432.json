{
    "rukus": [
        {
            "chapter": 44,
            "verse": 30,
            "text": "velekad necceyna beni israile mine-l`azabi-lmuhin"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "min fir`avn. innehu kane `aliyem mine-lmusrifin"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "velekadi-hternahum `ala `ilmin `ale-l`alemin"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "veateynahum mine-l'ayati ma fihi belaum mubin"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "inne haulai leyekulun"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "in hiye illa mevtetune-l'ula vema nahnu bimunserin"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "fe'tu biabaina in kuntum sadikin"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "ehum hayrun em kavmu tubbe`iv vellezine min kablihim. ehleknahum. innehum kanu mucrimin"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "vema halakne-ssemavati vel'arda vema beynehuma la`ibin"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "ma halaknahuma illa bilhakki velakinne ekserahum la ya`lemun"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "inne yevme-lfasli mikatuhum ecme`in"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "yevme la yugni mevlen `am mevlen sey'ev vela hum yunsarun"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "illa mer rahime-llah. innehu huve-l`azizu-rrahim"
        }
    ]
}