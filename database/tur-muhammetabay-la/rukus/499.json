{
    "rukus": [
        {
            "chapter": 68,
            "verse": 34,
            "text": "inne lilmuttekine `inde rabbihim cennati-nne`im"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "efenec`alu-lmuslimine kelmucrimin"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "ma lekum. keyfe tahkumun"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "em lekum kitabun fihi tedrusun"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "inne lekum fihi lema tehayyerun"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "em lekum eymanun `aleyna baligatun ila yevmi-lkiyameti inne lekum lema tahkumun"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "selhum eyyuhum bizalike za`im"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "em lehum suraka'. felye'tu bisurakaihim in kanu sadikin"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "yevme yuksefu `an sakiv veyud`avne ile-ssucudi fela yesteti`un"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "hasi`aten ebsaruhum terhekuhum zilleh. vekad kanu yud`avne ile-ssucudi vehum salimun"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "fezerni vemey yukezzibu bihaze-lhadis. senestedricuhum min haysu la ya`lemun"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "veumli lehum. inne keydi metin"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "em tes'eluhum ecran fehum mim magramim muskalun"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "em `indehumu-lgaybu fehum yektubun"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "fasbir lihukmi rabbike vela tekun kesahibi-lhut. iz nada vehuve mekzum"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "levla en tedarakehu ni`metum mir rabbihi lenubize bil`arai vehuve mezmum"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "fectebahu rabbuhu fece`alehu mine-ssalihin"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "veiy yekadu-llezine keferu leyuzlikuneke biebsarihim lemma semi`u-zzikra veyekulune innehu lemecnun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "vema huve illa zikrul lil`alemin"
        }
    ]
}