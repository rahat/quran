{
    "rukus": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "ya eyyuhe-lmuzzemmil"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "kumi-lleyle illa kalila"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "nisfehu evi-nkus minhu kalila"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "ev zid `aleyhi verattili-lkur'ane tertila"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "inna senulki `aleyke kavlen sekila"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "inne nasiete-lleyli hiye eseddu vat'ev veakvemu kila"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "inne leke fi-nnehari sebhan tavila"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "vezkuri-sme rabbike vetebettel ileyhi tebtila"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "rabbu-lmesriki velmagribi la ilahe illa huve fettehizhu vekila"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "vasbir `ala ma yekulune vehcurhum hecran cemila"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "vezerni velmukezzibine uli-nna`meti vemehhilhum kalila"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "inne ledeyna enkalev vecehima"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "veta`amen za gussativ ve`azaben elima"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "yevme tercufu-l'ardu velcibalu vekaneti-lcibalu kesibem mehila"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "inna erselna ileykum rasulen sahiden `aleykum kema erselna ila fir`avne rasula"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "fe`asa fir`avnu-rrasule feehaznahu ahzev vebila"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "fekeyfe tettekune in kefertum yevmey yec`alu-lvildane siba"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "essemau munfetirum bih. kane va`duhu mef`ula"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "inne hazihi tezkirah. femen sae-ttehaze ila rabbihi sebila"
        }
    ]
}