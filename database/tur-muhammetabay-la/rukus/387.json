{
    "rukus": [
        {
            "chapter": 37,
            "verse": 22,
            "text": "uhsuru-llezine zalemu veezvacehum vema kanu ya`budun"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "min duni-llahi fehduhum ila sirati-lcehim"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "vekifuhum innehum mes'ulun"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "ma lekum la tenasarun"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "bel humu-lyevme musteslimun"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "veakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "kalu innekum kuntum te'tunena `ani-lyemin"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "kalu bel lem tekunu mu'minin"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "vema kane lena `aleykum min sultan. bel kuntum kavmen tagin"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "fehakka `aleyna kavlu rabbina. inna lezaikun"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "feagveynakum inna kunna gavin"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "feinnehum yevmeizin fi-l`azabi musterikun"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "inna kezalike nef`alu bilmucrimin"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "innehum kanu iza kile lehum la ilahe ille-llahu yestekbirun"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "veyekulune einna letariku alihetina lisa`irim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "bel cae bilhakki vesaddeka-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "innekum lezaiku-l`azabi-l'elim"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "vema tuczevne illa ma kuntum ta`melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "ulaike lehum rizkum ma`lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "fevakih. vehum mukramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "fi cennati-nne`im"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "ala sururim mutekabilin"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "yutafu `aleyhim bike'sim mim me`in"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "beydae lezzetil lissaribin"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "la fiha gavluv vela hum `anha yunzefun"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "ve`indehum kasiratu-ttarfi `in"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "keennehunne beydum meknun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "feakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "kale kailum minhum inni kane li karin"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "yekulu einneke lemine-lmusaddikin"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "eiza mitna vekunna turabev ve`izamen einna lemedinun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "kale hel entum muttali`un"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "fettale`a feraahu fi sevai-lcehim"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "kale tellahi in kitte leturdin"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "velevla ni`metu rabbi lekuntu mine-lmuhdarin"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "efema nahnu bimeyyitin"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "illa mevtetene-l'ula vema nahnu bimu`azzebin"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "inne haza lehuve-lfevzu-l`azim"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "limisli haza felya`meli-l`amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "ezalike hayrun nuzulen em seceratu-zzekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "inna ce`alnaha fitnetel lizzalimin"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "inneha seceratun tahrucu fi asli-lcehim"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "tal`uha keennehu ruusu-sseyatin"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "feinnehum leakilune minha femaliune minhe-lbutun"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "summe inne lehum `aleyha lesevbem min hamim"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "summe inne merci`ahum leile-lcehim"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "innehum elfev abaehum dallin"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "fehum `ala asarihim yuhra`un"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "velekad dalle kablehum ekseru-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "velekad erselna fihim munzirin"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "fenzur keyfe kane `akibetu-lmunzerin"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "illa `ibade-llahi-lmuhlesin"
        }
    ]
}