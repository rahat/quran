{
    "rukus": [
        {
            "chapter": 36,
            "verse": 51,
            "text": "venufiha fi-ssuri feiza hum mine-l'ecdasi ila rabbihim yensilun"
        },
        {
            "chapter": 36,
            "verse": 52,
            "text": "kalu ya veylena mem be`asena mim merkadina. haza ma ve`ade-rrahmanu vesadeka-lmurselun"
        },
        {
            "chapter": 36,
            "verse": 53,
            "text": "in kanet illa sayhatev vahideten feiza hum cemi`ul ledeyna muhdarun"
        },
        {
            "chapter": 36,
            "verse": 54,
            "text": "felyevme la tuzlemu nefsun sey'ev vela tuczevne illa ma kuntum ta`melun"
        },
        {
            "chapter": 36,
            "verse": 55,
            "text": "inne ashabe-lcennehi-lyevme fi sugulin fakihun"
        },
        {
            "chapter": 36,
            "verse": 56,
            "text": "hum veezvacuhum fi zilalin `ale-l'eraiki muttekiun"
        },
        {
            "chapter": 36,
            "verse": 57,
            "text": "lehum fiha fakihetuv velehum ma yedde`un"
        },
        {
            "chapter": 36,
            "verse": 58,
            "text": "selamun kavlem mir rabbir rahim"
        },
        {
            "chapter": 36,
            "verse": 59,
            "text": "vemtazu-lyevme eyyuhe-lmucrimun"
        },
        {
            "chapter": 36,
            "verse": 60,
            "text": "elem a`hed ileykum ya beni ademe el la ta`budu-sseytan. innehu lekum `aduvvum mubin"
        },
        {
            "chapter": 36,
            "verse": 61,
            "text": "veeni-`buduni. haza siratum mustekim"
        },
        {
            "chapter": 36,
            "verse": 62,
            "text": "velekad edalle minkum cibillen kesira. efelem tekunu ta`kilun"
        },
        {
            "chapter": 36,
            "verse": 63,
            "text": "hazihi cehennemu-lleti kuntum tu`adun"
        },
        {
            "chapter": 36,
            "verse": 64,
            "text": "islevhe-lyevme bima kuntum tekfurun"
        },
        {
            "chapter": 36,
            "verse": 65,
            "text": "elyevme nahtimu `ala efvahihim vetukellimuna eydihim veteshedu erculuhum bima kanu yeksibun"
        },
        {
            "chapter": 36,
            "verse": 66,
            "text": "velev nesau letamesna `ala a`yunihim festebeku-ssirata feenna yubsirun"
        },
        {
            "chapter": 36,
            "verse": 67,
            "text": "velev nesau lemesahnahum `ala mekanetihim feme-steta`u mudiyyev vela yerci`un"
        }
    ]
}