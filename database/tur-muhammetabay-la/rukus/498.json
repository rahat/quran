{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "nun. velkalemi vema yesturun"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ma ente bini`meti rabbike bimecnun"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "veinne leke leecran gayra memnun"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "veinneke le`ala hulukin `azim"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "fesetubsiru veyubsirun"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "bieyyikumu-lmeftun"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "inne rabbeke huve a`lemu bimen dalle `an sebilih. vehuve a`lemu bilmuhtedin"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "fela tuti`i-lmukezzibin"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "veddu lev tudhinu feyudhinun"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "vela tuti` kulle hallafim mehin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "hemmazim messaim binemim"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "menna`il lilhayri mu`tedin esim"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "utullim ba`de zalike zenim"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "en kane za maliv vebenin"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "iza tutla `aleyhi ayatuna kale esatiru-l'evvelin"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "senesimuhu `ale-lhurtum"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "inna belevnahum kema belevna ashabe-lcenneh. iz aksemu leyasrimunneha musbihin"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "vela yestesnun"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "fetafe `aleyha taifum mir rabbike vehum naimun"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "feasbehat kessarim"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "fetenadev musbihin"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "eni-gdu `ala harsikum in kuntum sarimin"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "fentaleku vehum yetehafetun"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "el la yedhulennehe-lyevme `aleykum miskin"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "vegadev `ala hardin kadirin"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "felemma raevha kalu inna ledallun"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "bel nahnu mahrumun"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "kale evsetuhum elem ekul lekum levla tusebbihun"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "kalu subhane rabbina inna kunna zalimin"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "feakbele ba`duhum `ala ba`diy yetelavemun"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "kalu ya veylena inna kunna tagin"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "asa rabbuna ey yubdilena hayram minha inna ila rabbina ragibun"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "kezalike-l`azab. vele`azabu-l'ahirati ekber. lev kanu ya`lemun"
        }
    ]
}