{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "ikra' bismi rabbike-llezi halek"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "haleka-l'insane min `alak"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ikra' verabbuke-l'ekram"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ellezi `alleme bilkalem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "alleme-l'insane ma lem ya`lem"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "kella inne-l'insane leyatga"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "er raahu-stagna"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "inne ila rabbike-rruc`a"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "era'eyte-llezi yenha"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "abden iza salla"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "era'eyte in kane `ale-lhuda"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ev emera bittakva"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "era'eyte in kezzebe vetevella"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "elem ya`lem bienne-llahe yera"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "kella leil lem yentehi lenesfe`am binnasiyeh"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "nasiyetin kazibetin hatieh"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "felyed`u nadiyeh"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "sened`u-zzebaniyeh"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "kella. la tuti`hu vescud vakterib"
        }
    ]
}