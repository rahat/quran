{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "velekad cae ale fir`avne-nnuzur"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "kezzebu biayatina kulliha feehaznahum ahze `azizim muktedir"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "ekuffarukum hayrum min ulaikum em lekum beraetun fi-zzubur"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "em yekulune nahnu cemi`um muntesir"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "seyuhzemu-lcem`u veyuvellune-ddubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "beli-ssa`atu mev`iduhum vessa`atu edha veemerr"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "inne-lmucrimine fi dalaliv vesu`ur"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "yevme yushabune fi-nnari `ala vucuhihim. zuku messe sekara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "inna kulle sey'in halaknahu bikader"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "vema emruna illa vahidetun kelemhim bilbesar"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "velekad ehlekna esya`akum fehel mim muddekir"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "vekullu sey'in fe`aluhu fi-zzubur"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "vekullu sagiriv vekebirim mustetar"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "inne-lmuttekine fi cennativ veneher"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "fi mak`adi sidkin `inde melikim muktedir"
        }
    ]
}