{
    "rukus": [
        {
            "chapter": 54,
            "verse": 23,
            "text": "kezzebet semudu binnuzur"
        },
        {
            "chapter": 54,
            "verse": 24,
            "text": "fekalu ebeseram minna vahiden nettebi`uhu inna izel lefi dalaliv vesu`ur"
        },
        {
            "chapter": 54,
            "verse": 25,
            "text": "eulkiye-zzikru `aleyhi mim beynina bel huve kezzabun esir"
        },
        {
            "chapter": 54,
            "verse": 26,
            "text": "seya`lemune gadem meni-lkezzabu-l'esir"
        },
        {
            "chapter": 54,
            "verse": 27,
            "text": "inna mursilu-nnakati fitnetel lehum fertekibhum vastabir"
        },
        {
            "chapter": 54,
            "verse": 28,
            "text": "venebbi'hum enne-lmae kismetum beynehum. kullu sirbim muhtedar"
        },
        {
            "chapter": 54,
            "verse": 29,
            "text": "fenadev sahibehum fete`ata fe`akara"
        },
        {
            "chapter": 54,
            "verse": 30,
            "text": "fekeyfe kane `azabi venuzur"
        },
        {
            "chapter": 54,
            "verse": 31,
            "text": "inna erselna `aleyhim sayhatev vahideten fekanu kehesimi-lmuhtezir"
        },
        {
            "chapter": 54,
            "verse": 32,
            "text": "velekad yesserne-lkur'ane lizzikri fehel mim muddekir"
        },
        {
            "chapter": 54,
            "verse": 33,
            "text": "kezzebet kavmu lutim binnuzur"
        },
        {
            "chapter": 54,
            "verse": 34,
            "text": "inna erselna `aleyhim hasiben illa ale lut. necceynahum bisehar"
        },
        {
            "chapter": 54,
            "verse": 35,
            "text": "ni`metem min `indina. kezalike neczi men sekera"
        },
        {
            "chapter": 54,
            "verse": 36,
            "text": "velekad enzerahum batsetena fetemarav binnuzur"
        },
        {
            "chapter": 54,
            "verse": 37,
            "text": "velekad raveduhu `an dayfihi fetamesna a`yunehum fezuku `azabi venuzur"
        },
        {
            "chapter": 54,
            "verse": 38,
            "text": "velekad sabbehahum bukraten `azabum mustekirr"
        },
        {
            "chapter": 54,
            "verse": 39,
            "text": "fezuku `azabi venuzur"
        },
