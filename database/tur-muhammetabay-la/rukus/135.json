{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "kale-lmeleu min kavmi fir`avne inne haza lesahirun `alim"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "yuridu ey yuhricekum min ardikum. femaza te'murun"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "kalu ercih veehahu veersil fi-lmedaini hasirin"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "ye'tuke bikulli sahirin `alim"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "vecae-sseharatu fir`avne kalu inne lena leecran in kunna nahnu-lgalibin"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "kale ne`am veinnekum lemine-lmukarrabin"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "kalu ya musa imma en tulkiye veimma en nekune nahnu-lmulkin"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "kale elku. felemma elkav seharu a`yune-nnasi vesterhebuhum vecau bisihrin `azim"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "veevhayna ila musa en elki `asak. feiza hiye telkafu ma ye'fikun"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "feveka`a-lhakku vebetale ma kanu ya`melun"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "fegulibu hunalike venkalebu sagirin"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "veulkiye-sseharatu sacidin"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "kalu amenna birabbi-l`alemin"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "rabbi musa veharun"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "kale fir`avnu amentum bihi kable en azene lekum. inne haza lemekrum mekertumuhu fi-lmedineti lituhricu minha ehleha. fesevfe ta`lemun"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "leukatti`anne eydiyekum veerculekum min hilafin summe leusallibennekum ecme`in"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "kalu inna ila rabbina munkalibun"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "vema tenkimu minna illa en amenna biayati rabbina lemma caetna. rabbena efrig `aleyna sabrav veteveffena muslimin"
        }
    ]
}