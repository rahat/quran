{
    "rukus": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "ha-mim"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "velkitabi-lmubin"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "inna enzelnahu fi leyletim mubaraketin inna kunna munzirin"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "fiha yufraku kullu emrin hakim"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "emram min `indina. inna kunna mursilin"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "rahmetem mir rabbik. innehu huve-ssemi`u-l`alim"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "rabbi-ssemavati vel'ardi vema beynehuma. in kuntum mukinin"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "la ilahe illa huve yuhyi veyumit. rabbukum verabbu abaikumu-l'evvelin"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "bel hum fi sekkiy yel`abun"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "fertekib yevme te'ti-ssemau biduhanim mubin"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "yagse-nnas. haza `azabun elim"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "rabbene-ksif `anne-l`azabe inna mu'minun"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "enna lehumu-zzikra vekad caehum rasulum mubin"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "summe tevellev `anhu vekalu mu`allemum mecnun"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "inna kasifu-l`azabi kalilen innekum `aidun"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "yevme nebtisu-lbatsete-lkubra. inna muntekimun"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "velekad fetenna kablehum kavme fir`avne vecaehum rasulun kerim"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "en eddu ileyye `ibade-llah. inni lekum rasulun emin"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "veel la ta`lu `ale-llah. inni atikum bisultanim mubin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "veinni `uztu birabbi verabbikum en tercumun"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "veil lem tu'minu li fa`tezilun"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "fede`a rabbehu enne haulai kavmum mucrimun"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "feesri bi`ibadi leylen innekum muttebe`un"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "vetruki-lbahra rahva. innehum cundum mugrakun"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "kem teraku min cennativ ve`uyun"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "vezuru`iv vemekamin kerim"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "vena`metin kanu fiha fakihin"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "kezalik. veevrasnaha kavmen aharin"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "fema beket `aleyhimu-ssemau vel'ardu vema kanu munzarin"
        }
    ]
}