{
    "rukus": [
        {
            "chapter": 23,
            "verse": 78,
            "text": "vehuve-llezi ensee lekumu-ssem`a vel'ebsara vel'ef'ideh. kalilem ma teskurun"
        },
        {
            "chapter": 23,
            "verse": 79,
            "text": "vehuve-llezi zera'ekum fi-l'ardi veileyhi tuhserun"
        },
        {
            "chapter": 23,
            "verse": 80,
            "text": "vehuve-llezi yuhyi veyumitu velehu-htilafu-lleyli vennehar. efela ta`kilun"
        },
        {
            "chapter": 23,
            "verse": 81,
            "text": "bel kalu misle ma kale-l'evvelun"
        },
        {
            "chapter": 23,
            "verse": 82,
            "text": "kalu eiza mitna vekunna turabev ve`izamen einna lemeb`usun"
        },
        {
            "chapter": 23,
            "verse": 83,
            "text": "lekad vu`idna nahnu veabauna haza min kablu in haza illa esatiru-l'evvelin"
        },
        {
            "chapter": 23,
            "verse": 84,
            "text": "kul limeni-l'ardu vemen fiha in kuntum ta`lemun"
        },
        {
            "chapter": 23,
            "verse": 85,
            "text": "seyekulune lillah. kul efela tezekkerun"
        },
        {
            "chapter": 23,
            "verse": 86,
            "text": "kul mer rabbu-ssemavati-sseb`i verabbu-l`arsi-l`azim"
        },
        {
            "chapter": 23,
            "verse": 87,
            "text": "seyekulune lillah. kul efela tettekun"
        },
        {
            "chapter": 23,
            "verse": 88,
            "text": "kul mem biyedihi melekutu kulli sey'iv vehuve yuciru vela yucaru `aleyhi in kuntum ta`lemun"
        },
        {
            "chapter": 23,
            "verse": 89,
            "text": "seyekulune lillah. kul feenna tusharun"
        },
        {
            "chapter": 23,
            "verse": 90,
            "text": "bel eteynahum bilhakki veinnehum lekazibun"
        },
        {
            "chapter": 23,
            "verse": 91,
            "text": "me-ttehaze-llahu miv velediv vema kane me`ahu min ilahin izel lezehebe kullu ilahim bima haleka vele`ala ba`duhum `ala ba`d. subhane-llahi `amma yesifun"
        },
        {
            "chapter": 23,
            "verse": 92,
            "text": "alimi-lgaybi vessehadeti fete`ala `amma yusrikun"
        }
    ]
}