{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "velekad menenna `ala musa veharun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "venecceynahuma vekavmehuma mine-lkerbi-l`azim"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "venesarnahum fekanu humu-lgalibin"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "veateynahume-lkitabe-lmustebin"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "vehedeynahume-ssirata-lmustekim"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "veterakna `aleyhima fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selamun `ala musa veharun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innehuma min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "veinne ilyase lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "iz kale likavmihi ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "eted`une ba`lev vetezerune ahsene-lhalikin"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "allahe rabbekum verabbe abaikumu-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "fekezzebuhu feinnehum lemuhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "veterakna `aleyhi fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selamun `ala ilyasin"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innehu min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "veinne lutal lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "iz necceynahu veehlehu ecme`in"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illa `acuzen fi-lgabirin"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "summe demmerne-l'aharin"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "veinnekum letemurrune `aleyhim musbihin"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vebilleyl. efela ta`kilun"
        }
    ]
}