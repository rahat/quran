{
    "rukus": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "kad efleha-lmu'minun"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "ellezine hum fi salatihim hasi`un"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "vellezine hum `ani-llagvi mu`ridun"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "vellezine hum lilzekati fa`ilun"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "vellezine hum lifurucihim hafizun"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "illa `ala ezvacihim ev ma meleket eymanuhum feinnehum gayru melumin"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "femeni-btega verae zalike feulaike humu-l`adun"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "vellezine hum liemanatihim ve`ahdihim ra`un"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "vellezine hum `ala salevatihim yuhafizun"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "ulaike humu-lvarisun"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "ellezine yerisune-lfirdevs. hum fiha halidun"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "velekad halakne-l'insane min sulaletim min tin"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "summe ce`alnahu nutfeten fi kararim mekin"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "summe halakne-nnutfete `alekaten fehalakne-l`alekate mudgaten fehalakne-lmudgate `izamen fekesevne-l`izame lahma. summe ense'nahu halkan ahar. fetebarake-llahu ahsenu-lhalikin"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "summe innekum ba`de zalike lemeyyitun"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "summe innekum yevme-lkiyameti tub`asun"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "velekad halakna fevkakum seb`a taraik. vema kunna `ani-lhalki gafilin"
        },
        {
            "chapter": 23,
            "verse": 18,
            "text": "veenzelna mine-ssemai maem bikaderin feeskennahu fi-l'ard. veinna `ala zehabim bihi lekadirun"
        },
        {
            "chapter": 23,
            "verse": 19,
            "text": "feense'na lekum bihi cennatim min nehiliv vea`nab. lekum fiha fevakihu kesiratuv veminha te'kulun"
        },
        {
            "chapter": 23,
            "verse": 20,
            "text": "veseceraten tahrucu min turi seynae tembutu bidduhni vesibgil lil'akilin"
        },
        {
            "chapter": 23,
            "verse": 21,
            "text": "veinne lekum fi-l'en`ami le`ibrah. nuskikum mimma fi butuniha velekum fiha menafi`u kesiratuv veminha te'kulun"
        },
        {
            "chapter": 23,
            "verse": 22,
            "text": "ve`aleyha ve`ale-lfulki tuhmelun"
        }
    ]
}