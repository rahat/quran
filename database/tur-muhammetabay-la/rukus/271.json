{
    "rukus": [
        {
            "chapter": 20,
            "verse": 25,
            "text": "kale rabbi-srah li sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "veyessir li emri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "vahlul `ukdetem mil lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yefkahu kavli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "vec`al li veziram min ehli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "harune ehi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "usdud bihi ezri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "veesrikhu fi emri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "key nusebbihake kesira"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "venezkurake kesira"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "inneke kunte bina besira"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "kale kad utite su'leke ya musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "velekad menenna `aleyke merraten uhra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "iz evhayna ila ummike ma yuha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "eni-kzifihi fi-ttabuti fakzifihi fi-lyemmi felyulkihi-lyemmu bissahili ye'huzhu `aduvvul li ve`aduvvul leh. veelkaytu `aleyke mehabbetem minni. velitusne`a `ala `ayni"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "iz temsi uhtuke fetekulu hel edullukum `ala mey yekfuluh. feraca`nake ila ummike key tekarra `aynuha vela tahzen. vekatelte nefsen fenecceynake mine-lgammi vefetennake futuna. felebiste sinine fi ehli medyene summe ci'te `ala kaderiy ya musa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "vastana`tuke linefsi"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "izheb ente veehuke biayati vela teniya fi zikri"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "izheba ila fir`avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "fekula lehu kavlel leyyinel le`allehu yetezekkeru ev yahsa"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "kala rabbena innena nehafu ey yefruta `aleyna ev ey yatga"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "kale la tehafa inneni me`akuma esme`u veera"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "fe'tiyahu fekula inna rasula rabbike feersil me`ana beni israile vela tu`azzibhum. kad ci'nake biayetim mir rabbik. vesselamu `ala meni-ttebe`a-lhuda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "inna kad uhiye ileyna enne-l`azabe `ala men kezzebe vetevella"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "kale femer rabbukuma ya musa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "kale rabbune-llezi a`ta kulle sey'in halkahu summe heda"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "kale fema balu-lkuruni-l'ula"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "kale `ilmuha `inde rabbi fi kitab. la yedillu rabbi vela yensa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "ellezi ce`ale lekumu-l'arda mehdev veseleke lekum fiha subulev veenzele mine-ssemai maa. feahracna bihi ezvacem min nebetin setta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "kulu ver`av en`amekum. inne fi zalike leayatil liuli-nnuha"
        }
    ]
}