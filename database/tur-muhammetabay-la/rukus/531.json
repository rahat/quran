{
    "rukus": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "velfecr"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "veleyalin `asr"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "vessef`i velvetr"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "velleyli iza yesr"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hel fi zalike kasemul lizi hicr"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "elem tera keyfe fe`ale rabbuke bi`ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irame zati-l`imad"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "elleti lem yuhlak misluha fi-lbilad"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "vesemude-llezine cabu-ssahra bilvad"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "vefir`avne zi-l'evtad"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ellezine tagav fi-lbilad"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "feekseru fihe-lfesad"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "fesabbe `aleyhim rabbuke sevta `azab"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inne rabbeke lebilmirsad"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "feemme-l'insanu iza me-btelahu rabbuhu feekramehu vene``amehu feyekulu rabbi ekramen"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "veemma iza me-btelahu fekadera `aleyhi rizkahu feyekulu rabbi ehanen"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kella bel la tukrimune-lyetim"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "vela tehaddune `ala ta`ami-lmiskin"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "vete'kulune-tturase eklel lemma"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "vetuhibbune-lmale hubben cemma"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kella iza dukketi-l'ardu dekken dekka"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "vecae rabbuke velmeleku saffen saffa"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "vecie yevmeizim bicehenneme yevmeiziy yetezekkeru-l'insanu veenna lehu-zzikra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yekulu ya leyteni kaddemtu lihayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "feyevmeizil la yu`azzibu `azabehu ehad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "vela yusiku vesakahu ehad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "ya eyyetuhe-nnefsu-lmutmeinneh"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "irci`i ila rabbiki radiyetem merdiyyeh"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fedhuli fi `ibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "vedhuli cenneti"
        }
    ]
}