{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "eentum eseddu halkan emi-ssemau. benaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafe`a semkeha fesevvaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "veagtase leyleha veahrace duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "vel'arda ba`de zalike dehaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "ahrace minha maeha vemer`aha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "velcibale ersaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "meta`al lekum velien`amikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "feiza caeti-ttammetu-lkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yevme yetezekkeru-l'insanu ma se`a"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "veburrizeti-lcehimu limey yera"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "feemma men taga"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "veasera-lhayate-ddunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "feinne-lcehime hiye-lme'va"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "veemma men hafe mekame rabbihi venehe-nnefse `ani-lheva"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "feinne-lcennete hiye-lme'va"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yes'eluneke `ani-ssa`ati eyyane mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fime ente min zikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ila rabbike muntehaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innema ente munziru mey yahsaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "keennehum yevme yeravneha lem yelbesu illa `asiyyeten ev duhaha"
        }
    ]
