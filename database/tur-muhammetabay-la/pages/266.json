{
    "pages": [
        {
            "chapter": 15,
            "verse": 71,
            "text": "kale haulai benati in kuntum fa`ilin"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "le`amruke innehum lefi sekratihim ya`mehun"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "feehazethumu-ssayhatu musrikin"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fece`alna `aliyeha safileha veemtarna `aleyhim hicaratem min siccil"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inne fi zalike leayatil lilmutevessimin"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "veinneha lebisebilim mukim"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inne fi zalike leayetel lilmu'minin"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "vein kane ashabu-l'eyketi lezalimin"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fentekamna minhum. veinnehuma lebiimamim mubin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "velekad kezzebe ashabu-lhicri-lmurselin"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "veateynahum ayatina fekanu `anha mu`ridin"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "vekanu yenhitune mine-lcibali buyuten aminin"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "feehazethumu-ssayhatu musbihin"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "fema agna `anhum ma kanu yeksibun"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "vema halakne-ssemavati vel'arda vema beynehuma illa bilhakki. veinne-ssa`ate leatiyetun fasfehi-ssafha-lcemil"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "inne rabbeke huve-lhallaku-l`alim"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "velekad ateynake seb`am mine-lmesani velkur'ane-l`azim"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "la temuddenne `ayneyke ila ma metta`na bihi ezvacem minhum vela tahzen `aleyhim vahfid cenahake lilmu'minin"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "vekul inni ene-nneziru-lmubin"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "kema enzelna `ale-lmuktesimin"
        }
    ]
}