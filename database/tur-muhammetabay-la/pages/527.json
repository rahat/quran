{
    "pages": [
        {
            "chapter": 53,
            "verse": 27,
            "text": "inne-llezine la yu'minune bil'ahirati leyusemmune-lmelaikete tesmiyete-l'unsa"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "vema lehum bihi min `ilm. iy yettebi`une ille-zzanne. veinne-zzanne la yugni mine-lhakki sey'a"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fea`rid `am men tevella `an zikrina velem yurid ille-lhayate-ddunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "zalike mebleguhum mine-l`ilm. inne rabbeke huve a`lemu bimen dalle `an sebilihi vehuve a`lemu bimeni-hteda"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "velillahi ma fi-ssemavati vema fi-l'ardi liyecziye-llezine esau bima `amilu veyecziye-llezine ahsenu bilhusna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "ellezine yectenibune kebaira-l'ismi velfevahise ille-llemem. inne rabbeke vasi`u-lmagfirah. huve a`lemu bikum iz enseekum mine-l'ardi veiz entum ecinnetun fi butuni ummehatikum. fela tuzekku enfusekum. huve a`lemu bimeni-tteka"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "eferaeyte-llezi tevella"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "vea`ta kalilev veekda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "e`indehu `ilmu-lgaybi fehuve yera"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "em lem yunebbe' bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "veibrahime-llezi veffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ella teziru vaziratuv vizra uhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "veel leyse lil'insani illa ma se`a"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "veenne sa`yehu sevfe yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "summe yuczahu-lcezae-l'evfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "veenne ila rabbike-lmunteha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "veennehu huve adhake veebka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "veennehu huve emate veahya"
        }
    ]
}