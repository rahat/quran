    "pages": [
        {
            "chapter": 44,
            "verse": 40,
            "text": "inne yevme-lfasli mikatuhum ecme`in"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "yevme la yugni mevlen `am mevlen sey'ev vela hum yunsarun"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "illa mer rahime-llah. innehu huve-l`azizu-rrahim"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "inne secerate-zzekkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "ta`amu-l'esim"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "kelmuhl. yagli fi-lbutun"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "kegalyi-lhamim"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "huzuhu fa`tiluhu ila sevai-lcehim"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "summe subbu fevka ra'sihi min `azabi-lhamim"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "zuk. inneke ente-l`azizu-lkerim"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "inne haza ma kuntum bihi temterun"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "inne-lmuttekine fi mekamin emin"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "fi cennativ ve`uyun"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "yelbesune min sundusiv veistebrakim mutekabilin"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "kezalik. vezevvecnahum bihurin `in"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "yed`une fiha bikulli fakihetin aminin"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "la yezukune fihe-lmevte ille-lmevtete-l'ula. vevekahum `azabe-lcehim"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "fadlem mir rabbik. zalike huve-lfevzu-l`azim"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "feinnema yessernahu bilisanike le`allehum yetezekkerun"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "fertekib innehum murtekibun"
        }
