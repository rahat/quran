{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "eferaeyte-llezi kefera biayatina vekale leuteyenne malev veveleda"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "ettale`a-lgaybe emi-ttehaze `inde-rrahmani `ahda"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "kella. senektubu ma yekulu venemuddu lehu mine-l`azabi medda"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "venerisuhu ma yekulu veye'tina ferda"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "vettehazu min duni-llahi alihetel liyekunu lehum `izza"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "kella. seyekfurune bi`ibadetihim veyekunune `aleyhim didda"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "elem tera enna erselne-sseyatine `ale-lkafirine teuzzuhum ezza"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "fela ta`cel `aleyhim. innema ne`uddu lehum `adda"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "yevme nahsuru-lmuttekine ile-rrahmani vefda"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "venesuku-lmucrimine ila cehenneme virda"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "la yemlikune-ssefa`ate illa meni-ttehaze `inde-rrahmani `ahda"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "vekalu-ttehaze-rrahmanu veleda"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "lekad ci'tum sey'en idda"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "tekadu-ssemavatu yetefettarne minhu vetensekku-l'ardu vetehirru-lcibalu hedda"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "en de`av lirrahmani veleda"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "vema yembegi lirrahmani ey yettehize veleda"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "in kullu men fi-ssemavati vel'ardi illa ati-rrahmani `abda"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "lekad ahsahum ve`addehum `adda"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "vekulluhum atihi yevme-lkiyameti ferda"
        }
    ]
}