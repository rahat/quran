        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "veyekulune einna letariku alihetina lisa`irim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "bel cae bilhakki vesaddeka-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "innekum lezaiku-l`azabi-l'elim"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "vema tuczevne illa ma kuntum ta`melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "ulaike lehum rizkum ma`lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "fevakih. vehum mukramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "fi cennati-nne`im"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "ala sururim mutekabilin"
        },
        {
            "chapter": 37,
            "verse": 45,
