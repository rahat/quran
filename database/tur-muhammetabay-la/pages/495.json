{
    "pages": [
        {
            "chapter": 43,
            "verse": 74,
            "text": "inne-lmucrimine fi `azabi cehenneme halidun"
        },
        {
            "chapter": 43,
            "verse": 75,
            "text": "la yufetteru `anhum vehum fihi mublisun"
        },
        {
            "chapter": 43,
            "verse": 76,
            "text": "vema zalemnahum velakin kanu humu-zzalimin"
        },
        {
            "chapter": 43,
            "verse": 77,
            "text": "venadev ya maliku liyakdi `aleyna rabbuk. kale innekum makisun"
        },
        {
            "chapter": 43,
            "verse": 78,
            "text": "lekad ci'nakum bilhakki velakinne ekserakum lilhakki karihun"
        },
        {
            "chapter": 43,
            "verse": 79,
            "text": "em ebramu emran feinna mubrimun"
        },
        {
            "chapter": 43,
            "verse": 80,
            "text": "em yahsebune enna la nesme`u sirrahum venecvahum. bela verusuluna ledeyhim yektubun"
        },
        {
            "chapter": 43,
            "verse": 81,
            "text": "kul in kane lirrahmani veled. feena evvelu-l`abidin"
        },
        {
            "chapter": 43,
            "verse": 82,
            "text": "subhane rabbi-ssemavati vel'ardi rabbi-l`arsi `amma yesifun"
        },
        {
            "chapter": 43,
            "verse": 83,
            "text": "fezerhum yehudu veyel`abu hatta yulaku yevmehumu-llezi yu`adun"
        },
        {
            "chapter": 43,
            "verse": 84,
            "text": "vehuve-llezi fi-ssemai ilahuv vefi-l'ardi ilahun. vehuve-lhakimu-l`alim"
        },
        {
            "chapter": 43,
            "verse": 85,
            "text": "vetebarake-llezi lehu mulku-ssemavati vel'ardi vema beynehuma. ve`indehu `ilmu-ssa`ah. veileyhi turce`un"
        },
        {
            "chapter": 43,
            "verse": 86,
            "text": "vela yemliku-llezine yed`une min dunihi-ssefa`ate illa men sehide bilhakki vehum ya`lemun"
        },
        {
            "chapter": 43,
            "verse": 87,
            "text": "velein seeltehum men halekahum leyekulunne-llahu feenna yu'fekun"
        },
        {
            "chapter": 43,
            "verse": 88,
            "text": "vekilihi ya rabbi inne haulai kavmul la yu'minun"
        },
        {
            "chapter": 43,
            "verse": 89,
            "text": "fasfah `anhum vekul selam. fesevfe ya`lemun"
        }
    ]
}