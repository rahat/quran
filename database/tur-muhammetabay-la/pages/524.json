{
    "pages": [
        {
            "chapter": 52,
            "verse": 15,
            "text": "efesihrun haza em entum la tubsirun"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "islevha fasbiru ev la tasbiru. sevaun `aleykum. innema tuczevne ma kuntum ta`melun"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "inne-lmuttekine fi cennativ vene`im"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "fakihine bima atahum rabbuhum. vevekahum rabbuhum `azabe-lcehim"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "kulu vesrabu heniem bima kuntum ta`melun"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "muttekiine `ala sururim masfufeh. vezevvecnahum bihurin `in"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "vellezine amenu vettebe`athum zurriyyetuhum biimanin elhakna bihim zurriyyetehum vema eletnahum min `amelihim min sey'. kullu-mriim bima kesebe rahin"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "veemdednahum bifakihetiv velahmim mimma yestehun"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "yetenaza`une fiha ke'sel la lagvun fiha vela te'sim"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "veyetufu `aleyhim gilmanul lehum keennehum lu'luum meknun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "veakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "kalu inna kunna kablu fi ehlina musfikin"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "femenne-llahu `aleyna vevekana `azabe-ssemum"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "inna kunna min kablu ned`uh. innehu huve-lberru-rrahim"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "fezekkir fema ente bini`meti rabbike bikahiniv vela mecnun"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "em yekulune sa`irun neterabbesu bihi raybe-lmenun"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "kul terabbesu feinni me`akum mine-lmuterabbisin"
        }
    ]
}