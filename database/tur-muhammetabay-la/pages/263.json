{
    "pages": [
        {
            "chapter": 15,
            "verse": 16,
            "text": "velekad ce`alna fi-ssemai burucev vezeyyennaha linnazirin"
        },
        {
            "chapter": 15,
            "verse": 17,
            "text": "vehafiznaha min kulli seytanir racim"
        },
        {
            "chapter": 15,
            "verse": 18,
            "text": "illa meni-steraka-ssem`a feetbe`ahu sihabum mubin"
        },
        {
            "chapter": 15,
            "verse": 19,
            "text": "vel'arda medednaha veelkayna fiha ravasiye veembetna fiha min kulli sey'im mevzun"
        },
        {
            "chapter": 15,
            "verse": 20,
            "text": "vece`alna lekum fiha me`ayise vemel lestum lehu birazikin"
        },
        {
            "chapter": 15,
            "verse": 21,
            "text": "veim min sey'in illa `indena hazainuh. vema nunezziluhu illa bikaderim ma`lum"
        },
        {
            "chapter": 15,
            "verse": 22,
            "text": "veerselne-rriyaha levakiha feenzelna mine-ssemai maen feeskaynakumuh. vema entum lehu bihazinin"
        },
        {
            "chapter": 15,
            "verse": 23,
            "text": "veinna lenahnu nuhyi venumitu venahnu-lvarisun"
        },
        {
            "chapter": 15,
            "verse": 24,
            "text": "velekad `alimne-lmustakdimine minkum velekad `alimne-lmuste'hirin"
        },
        {
            "chapter": 15,
            "verse": 25,
            "text": "veinne rabbeke huve yahsuruhum. innehu hakimun `alim"
        },
        {
            "chapter": 15,
            "verse": 26,
            "text": "velekad halakne-l'insane min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "velcanne halaknahu min kablu min nari-ssemum"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "veiz kale rabbuke lilmelaiketi inni halikum beseram min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "feiza sevveytuhu venefahtu fihi mir ruhi feka`u lehu sacidin"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "fesecede-lmelaiketu kulluhum ecme`un"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "illa iblis. eba ey yekune me`a-ssacidin"
        }
    ]
}