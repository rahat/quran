{
    "pages": [
        {
            "chapter": 44,
            "verse": 19,
            "text": "veel la ta`lu `ale-llah. inni atikum bisultanim mubin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "veinni `uztu birabbi verabbikum en tercumun"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "veil lem tu'minu li fa`tezilun"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "fede`a rabbehu enne haulai kavmum mucrimun"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "feesri bi`ibadi leylen innekum muttebe`un"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "vetruki-lbahra rahva. innehum cundum mugrakun"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "kem teraku min cennativ ve`uyun"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "vezuru`iv vemekamin kerim"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "vena`metin kanu fiha fakihin"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "kezalik. veevrasnaha kavmen aharin"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "fema beket `aleyhimu-ssemau vel'ardu vema kanu munzarin"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "velekad necceyna beni israile mine-l`azabi-lmuhin"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "min fir`avn. innehu kane `aliyem mine-lmusrifin"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "velekadi-hternahum `ala `ilmin `ale-l`alemin"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "veateynahum mine-l'ayati ma fihi belaum mubin"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "inne haulai leyekulun"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "in hiye illa mevtetune-l'ula vema nahnu bimunserin"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "fe'tu biabaina in kuntum sadikin"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "ehum hayrun em kavmu tubbe`iv vellezine min kablihim. ehleknahum. innehum kanu mucrimin"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "vema halakne-ssemavati vel'arda vema beynehuma la`ibin"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "ma halaknahuma illa bilhakki velakinne ekserahum la ya`lemun"
        }
    ]
}