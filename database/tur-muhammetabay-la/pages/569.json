{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "yubessarunehum. yeveddu-lmucrimu lev yeftedi min `azabi yevmiizim bibenih"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "vesahibetihi veehih"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "vefesiletihi-lleti tu'vih"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "vemen fi-l'ardi cemi`an summe yuncih"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "kella. inneha leza"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "nezza`atel lisseva"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "ted`u men edbera vetevella"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "veceme`a feev`a"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "inne-l'insane hulika helu`a"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "iza messehu-sserru cezu`a"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "veiza messehu-lhayru menu`a"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "ille-lmusallin"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "ellezine hum `ala salatihim daimun"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "vellezine fi emvalihim hakkum ma`lum"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "lissaili velmahrum"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "vellezine yusaddikune biyevmi-ddin"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "vellezine hum min `azabi rabbihim musfikun"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "inne `azabe rabbihim gayru me'mun"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "vellezine hum lifurucihim hafizun"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "illa `ala ezvacihim ev ma meleket eymanuhum feinnehum gayru melumin"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "femeni-btega verae zalike feulaike humu-l`adun"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "vellezine hum liemanatihim ve`ahdihim ra`un"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "vellezine hum bisehadetihim kaimun"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "vellezine hum `ala salatihim yuhafizun"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "ulaike fi cennatim mukramun"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "fema lillezine keferu kibeleke muhti`in"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "ani-lyemini ve`ani-ssimali `izin"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "eyatme`u kullu-mriim minhum ey yudhale cennete ne`im"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "kella. inna halaknahum mimma ya`lemun"
        }
    ]
