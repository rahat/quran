{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "fekezzebuhu feinnehum lemuhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "veterakna `aleyhi fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selamun `ala ilyasin"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innehu min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "veinne lutal lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "iz necceynahu veehlehu ecme`in"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illa `acuzen fi-lgabirin"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "summe demmerne-l'aharin"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "veinnekum letemurrune `aleyhim musbihin"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vebilleyl. efela ta`kilun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "veinne yunuse lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "iz ebeka ile-lfulki-lmeshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "fesaheme fekane mine-lmudhadin"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "feltekamehu-lhutu vehuve mulim"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "felevla ennehu kane mine-lmusebbihin"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "lelebise fi batnih ila yevmi yub`asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "fenebeznahu bil`arai vehuve sekim"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "veembetna `aleyhi seceratem miy yaktin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "veerselnahu ila mieti elfin ev yezidun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "feamenu femetta`nahum ila hin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "festeftihim elirabbike-lbenatu velehumu-lbenun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "em halakne-lmelaikete inasev vehum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "ela innehum min ifkihim leyekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "velede-llahu veinnehum lekazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "astafe-lbenati `ale-lbenin"
        }
    ]
}