{
    "pages": [
        {
            "chapter": 23,
            "verse": 90,
            "text": "bel eteynahum bilhakki veinnehum lekazibun"
        },
        {
            "chapter": 23,
            "verse": 91,
            "text": "me-ttehaze-llahu miv velediv vema kane me`ahu min ilahin izel lezehebe kullu ilahim bima haleka vele`ala ba`duhum `ala ba`d. subhane-llahi `amma yesifun"
        },
        {
            "chapter": 23,
            "verse": 92,
            "text": "alimi-lgaybi vessehadeti fete`ala `amma yusrikun"
        },
        {
            "chapter": 23,
            "verse": 93,
            "text": "kur rabbi imma turiyenni ma yu`adun"
        },
        {
            "chapter": 23,
            "verse": 94,
            "text": "rabbi fela tec`alni fi-lkavmi-zzalimin"
        },
        {
            "chapter": 23,
            "verse": 95,
            "text": "veinna `ala en nuriyeke ma ne`iduhum lekadirun"
        },
        {
            "chapter": 23,
            "verse": 96,
            "text": "idfa` billeti hiye ahsenu-sseyyieh. nahnu a`lemu bima yesifun"
        },
        {
            "chapter": 23,
            "verse": 97,
            "text": "vekur rabbi e`uzu bike min hemezati-sseyatin"
        },
        {
            "chapter": 23,
            "verse": 98,
            "text": "vee`uzu bike rabbi ey yahdurun"
        },
        {
            "chapter": 23,
            "verse": 99,
            "text": "hatta iza cae ehadehumu-lmevtu kale rabbi-rci`un"
        },
        {
            "chapter": 23,
            "verse": 100,
            "text": "le`alli a`melu salihan fima teraktu kella. inneha kelimetun huve kailuha. vemiv veraihim berzehun ila yevmi yub`asun"
        },
        {
            "chapter": 23,
            "verse": 101,
            "text": "feiza nufiha fi-ssuri fela ensabe beynehum yevmeiziv vela yetesaelun"
        },
        {
            "chapter": 23,
            "verse": 102,
            "text": "femen sekulet mevazinuhu feulaike humu-lmuflihun"
        },
        {
            "chapter": 23,
            "verse": 103,
            "text": "vemen haffet mevazinuhu feulaike-llezine hasiru enfusehum fi cehenneme halidun"
        },
        {
            "chapter": 23,
            "verse": 104,
            "text": "telfehu vucuhehumu-nnaru vehum fiha kalihun"
        }
    ]
