{
    "pages": [
        {
            "chapter": 26,
            "verse": 61,
            "text": "felemma terae-lcem`ani kale ashabu musa inna lemudrakun"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "kale kella. inne me`iye rabbi seyehdin"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "feevhayna ila musa eni-drib bi`asake-lbahr. fenfeleka fekane kullu firkin kettavdi-l`azim"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "veezlefna semme-l'aharin"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "veenceyna musa vemem me`ahu ecme`in"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "summe agrakne-l'aharin"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "vetlu `aleyhim nebee ibrahim"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "iz kale liebihi vekavmihi ma ta`budun"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "kalu na`budu asnamen fenezallu leha `akifin"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "kale hel yesme`unekum iz ted`un"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "ev yenfe`unekum ev yedurrun"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "kalu bel vecedna abaena kezalike yef`alun"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "kale eferaeytum ma kuntum ta`budun"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "entum veabaukumu-l'akdemun"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "feinnehum `aduvvul li illa rabbe-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "ellezi halekani fehuve yehdin"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "vellezi huve yut`imuni veyeskin"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "veiza meridtu fehuve yesfin"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "vellezi yumituni summe yuhyin"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "vellezi atme`u ey yagfira li hatieti yevme-ddin"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi heb li hukmev veelhikni bissalihin"
        }
    ]
}