{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "veene-htertuke festemi` lima yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "inneni ene-llahu la ilahe illa ene fa`budni veekimi-ssalate lizikri"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "inne-ssa`ate atiyetun ekadu uhfiha litucza kullu nefsim bima tes`a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "fela yesuddenneke `anha mel la yu'minu biha vettebe`a hevahu feterda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "vema tilke biyeminike ya musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "kale hiye `asay. etevekkeu `aleyha veehussu biha `ala ganemi veliye fiha mearibu uhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "kale elkiha ya musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "feelkaha feiza hiye hayyetun tes`a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "kale huzha vela tehaf. senu`iduha siratehe-l'ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "vadmum yedeke ila cenahike tahruc beydae min gayri suin ayeten uhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "linuriyeke min ayatine-lkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "izheb ila fir`avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "kale rabbi-srah li sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "veyessir li emri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "vahlul `ukdetem mil lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yefkahu kavli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "vec`al li veziram min ehli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "harune ehi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "usdud bihi ezri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "veesrikhu fi emri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "key nusebbihake kesira"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "venezkurake kesira"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "inneke kunte bina besira"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "kale kad utite su'leke ya musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "velekad menenna `aleyke merraten uhra"
        }
    ]
}