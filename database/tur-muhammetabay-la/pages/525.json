{
    "pages": [
        {
            "chapter": 52,
            "verse": 32,
            "text": "em te'muruhum ahlamuhum bihaza em hum kavmun tagun"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "em yekulune tekavveleh. bel la yu'minun"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "felye'tu bihadisim mislihi in kanu sadikin"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "em huliku min gayri sey'in em humu-lhalikun"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "em haleku-ssemavati vel'ard. bel la yukinun"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "em `indehum hazainu rabbike em humu-lmusaytirun"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "em lehum sullemuy yestemi`une fih. felye'ti mustemi`uhum bisultanim mubin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "em lehu-lbenatu velekumu-lbenun"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "em tes'eluhum ecran fehum mim magramim muskalun"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "em `indehumu-lgaybu fehum yektubun"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "em yuridune keyda. fellezine keferu humu-lmekidun"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "em lehum ilahun gayru-llah. subhane-llahi `amma yusrikun"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "veiy yerav kisfem mine-ssemai sakitay yekulu sehabum merkum"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "fezerhum hatta yulaku yevmehumu-llezi fihi yus`akun"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yevme la yugni `anhum keyduhum sey'ev vela hum yunsarun"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "veinne lillezine zalemu `azaben dune zalike velakinne ekserahum la ya`lemun"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "vasbir lihukmi rabbike feinneke bia`yunina vesebbih bihamdi rabbike hine tekum"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "vemine-lleyli fesebbihhu veidbara-nnucum"
        }
    ]
}