{
    "chapter": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "vessemai zati-lburuc"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "velyevmi-lmev`ud"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "vesahidiv vemeshud"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kutile ashabu-l'uhdud"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ennari zati-lvekud"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "iz hum `aleyha ku`ud"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "vehum `ala ma yef`alune bilmu'minine suhud"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "vema nekamu minhum illa ey yu'minu billahi-l`azizi-lhamid"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "ellezi lehu mulku-ssemavati vel'ard. vellahu `ala kulli sey'in sehid"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inne-llezine fetenu-lmu'minine velmu'minati summe lem yetubu felehum `azabu cehenneme velehum `azabu-lharik"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inne-llezine amenu ve`amilu-ssalihati lehum cennatun tecri min tahtihe-l'enhar. zalike-lfevzu-lkebir"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "inne batse rabbike lesedid"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "innehu huve yubdiu veyu`id"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "vehuve-lgafuru-lvedud"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "zu-l`arsi-lmecid"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "fa``alul lima yurid"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "hel etake hadisu-lcunud"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "fir`avne vesemud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "beli-llezine keferu fi tekzib"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "vellahu miv veraihim muhit"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "bel huve kur'anum mecid"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "fi levhim mahfuz"
        }
    ]
}