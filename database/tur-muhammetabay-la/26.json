{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "ta-sin-mim"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "tilke ayatu-lkitabi-lmubin"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "le`alleke bahi`un nefseke ella yekunu mu'minin"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "in nese' nunezzil `aleyhim mine-ssemai ayeten fezallet a`nakuhum leha hadi`in"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "vema ye'tihim min zikrim mine-rrahmani muhdesin illa kanu `anhu mu`ridin"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "fekad kezzebu feseye'tihim embau ma kanu bihi yestehziun"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "evelem yerav ile-l'ardi kem embetna fiha min kulli zevcin kerim"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "veiz nada rabbuke musa eni-'ti-lkavme-zzalimin"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "kavme fir`avn. ela yettekun"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "kale rabbi inni ehafu ey yukezzibun"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "veyediku sadri vela yentaliku lisani feersil ila harun"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "velehum `aleyye zembun feehafu ey yaktulun"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "kale kella. fezheba biayatina inna me`akum mustemi`un"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "fe'tiya fir`avne fekula inna rasulu rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "en ersil me`ana beni israil"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "kale elem nurabbike fina velidev velebiste fina min `umurike sinin"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "vefe`alte fa`leteke-lleti fe`alte veente mine-lkafirin"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "kale fe`altuha izev veena mine-ddallin"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "feferartu minkum lemma hiftukum fevehebe li rabbi hukmev vece`aleni mine-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "vetilke ni`metun temunnuha `aleyye en `abbette beni israil"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "kale fir`avnu vema rabbu-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "kale rabbu-ssemavati vel'ardi vema beynehuma. in kuntum mukinin"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "kale limen havlehu ela testemi`un"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "kale rabbukum verabbu abaikumu-l'evvelin"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "kale inne rasulekumu-llezi ursile ileykum lemecnun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "kale rabbu-lmesriki velmagribi vema beynehuma. in kuntum ta`kilun"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "kale leini-ttehazte ilahen gayri leec`alenneke mine-lmescunin"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "kale evelev ci'tuke bisey'im mubin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "kale fe'ti bihi in kunte mine-ssadikin"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "feelka `asahu feiza hiye su`banum mubin"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "veneza`a yedehu feiza hiye beydau linnazirin"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "kale lilmelei havlehu inne haza lesahirun `alim"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "yuridu ey yuhricekum min ardikum bisihrih. femaza te'murun"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "kalu ercih veehahu veb`as fi-lmedaini hasirin"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "ye'tuke bikulli sehharin `alim"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "fecumi`a-sseharatu limikati yevmim ma`lum"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "vekile linnasi hel entum muctemi`un"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "le`allena nettebi`u-sseharate in kanu humu-lgalibin"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "felemma cae-sseharatu kalu lifir`avne einne lena leecran in kunna nahnu-lgalibin"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "kale ne`am veinnekum izel lemine-lmukarrabin"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "kale lehum musa elku ma entum mulkun"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "feelkav hibalehum ve`isiyyehum vekalu bi`izzeti fir`avne inna lenahnu-lgalibun"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "feelka musa `asahu feiza hiye telkafu ma ye'fikun"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "feulkiye-sseharatu sacidin"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "kalu amenna birabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "rabbi musa veharun"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "kale amentum lehu kable en azene lekum. innehu lekebirukumu-llezi `allemekumu-ssihr. felesevfe ta`lemun. leukatti`anne eydiyekum veerculekum min hilafiv veleusallibennekum ecme`in"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "kalu la dayr. inna ila rabbina munkalibun"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "inna natme`u ey yagfira lena rabbuna hatayana en kunna evvele-lmu'minin"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "veevhayna ila musa en esri bi`ibadi innekum muttebe`un"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "feersele fir`avnu fi-lmedaini hasirin"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inne haulai lesirzimetun kalilun"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "veinnehum lena legaizun"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "veinna lecemi`un hazirun"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "feahracnahum min cennativ ve`uyun"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "vekunuziv vemekamin kerim"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "kezalik. veevrasnaha beni israil"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "feetbe`uhum musrikin"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "felemma terae-lcem`ani kale ashabu musa inna lemudrakun"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "kale kella. inne me`iye rabbi seyehdin"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "feevhayna ila musa eni-drib bi`asake-lbahr. fenfeleka fekane kullu firkin kettavdi-l`azim"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "veezlefna semme-l'aharin"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "veenceyna musa vemem me`ahu ecme`in"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "summe agrakne-l'aharin"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "vetlu `aleyhim nebee ibrahim"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "iz kale liebihi vekavmihi ma ta`budun"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "kalu na`budu asnamen fenezallu leha `akifin"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "kale hel yesme`unekum iz ted`un"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "ev yenfe`unekum ev yedurrun"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "kalu bel vecedna abaena kezalike yef`alun"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "kale eferaeytum ma kuntum ta`budun"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "entum veabaukumu-l'akdemun"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "feinnehum `aduvvul li illa rabbe-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "ellezi halekani fehuve yehdin"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "vellezi huve yut`imuni veyeskin"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "veiza meridtu fehuve yesfin"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "vellezi yumituni summe yuhyin"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "vellezi atme`u ey yagfira li hatieti yevme-ddin"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi heb li hukmev veelhikni bissalihin"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "vec`al li lisane sidkin fi-l'ahirin"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "vec`alni miv veraseti cenneti-nne`im"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "vagfir liebi innehu kane mine-ddallin"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "vela tuhzini yevme yub`asun"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "yevme la yenfe`u maluv vela benun"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "illa men ete-llahe bikalbin selim"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "veuzlifeti-lcennetu lilmuttekin"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "veburrizeti-lcehimu lilgavin"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "vekile lehum eyne ma kuntum ta`budun"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "min duni-llah. hel yensurunekum ev yentesirun"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "fekubkibu fiha hum velgavun"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "vecunudu iblise ecme`un"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "kalu vehum fiha yahtesimun"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "tellahi in kunna lefi dalalim mubin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "iz nusevvikum birabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "vema edallena ille-lmucrimun"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "fema lena min safi`in"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "vela sadikin hamim"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "felev enne lena kerraten fenekune mine-lmu'minin"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "kezzebet kavmu nuhin-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "iz kale lehum ehuhum nuhun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "inni lekum rasulun emin"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "vema es'elukum `aleyhi min ecr. in ecriye illa `ala rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "kalu enu'minu leke vettebe`ake-l'erzelun"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "kale vema `ilmi bima kanu ya`melun"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "in hisabuhum illa `ala rabbi lev tes`urun"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "vema ene bitaridi-lmu'minin"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "in ene illa nezirum mubin"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "kalu leil lem tentehi ya nuhu letekunenne mine-lmercumin"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "kale rabbi inne kavmi kezzebun"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "feftah beyni vebeynehum fethav veneccini vemem me`iye mine-lmu'minin"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "feenceynahu vemem me`ahu fi-lfulki-lmeshun"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "summe agrakna ba`du-lbakin"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "kezzebet `adun-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "iz kale lehum ehuhum hudun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "inni lekum rasulun emin"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "vema es'elukum `aleyhi min ecr. in ecriye illa `ala rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "etebnune bikulli ri`in ayeten ta`besun"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "vetettehizune mesani`a le`allekum tahludun"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "veiza betastum betastum cebbarin"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "vetteku-llezi emeddekum bima ta`lemun"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "emeddekum bien`amiv vebenin"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "vecennativ ve`uyun"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "inni ehafu `aleykum `azabe yevmin `azim"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "kalu sevaun `aleyna eve`azte em lem tekum mine-lva`izin"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "in haza illa huluku-l'evvelin"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "vema nahnu bimu`azzebin"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "fekezzebuhu feehleknahum. inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "kezzebet semudu-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "iz kale lehum ehuhum salihun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "inni lekum rasulun emin"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "vema es'elukum `aleyhi min ecr. in ecriye illa `ala rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "etutrakune fi ma hahuna aminin"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "fi cennativ ve`uyun"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "vezuru`iv venahlin tal`uha hedim"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "vetenhitune mine-lcibali buyuten farihin"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "vela tuti`u emra-lmusrifin"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "ellezine yufsidune fi-l'ardi vela yuslihun"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "kalu innema ente mine-lmusehharin"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "ma ente illa beserum misluna. fe'ti biayetin in kunte mine-ssadikin"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "kale hazihi nakatul leha sirbuv velekum sirbu yevmim ma`lum"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "vela temessuha bisuin feye'huzekum `azabu yevmin `azim"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "fe`akaruha feasbehu nadimin"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "feehazehumu-l`azab. inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "kezzebet kavmu lutini-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "iz kale lehum ehuhum lutun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "inni lekum rasulun emin"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "vema es'elukum `aleyhi min ecr. in ecriye illa `ala rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "ete'tune-zzukrane mine-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "vetezerune ma haleka lekum rabbukum min ezvacikum. bel entum kavmun `adun"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "kalu leil lem tentehi ya lutu letekunenne mine-lmuhracin"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "kale inni li`amelikum mine-lkalin"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "rabbi neccini veehli mimma ya`melun"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "fenecceynahu veehlehu ecme`in"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "illa `acuzen fi-lgabirin"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "summe demmerne-l'aharin"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "veemtarna `aleyhim metara. fesae metaru-lmunzerin"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "kezzebe ashabu-l'eyketi-lmurselin"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "iz kale lehum su`aybun ela tettekun"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "inni lekum rasulun emin"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "fetteku-llahe veeti`un"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "vema es'elukum `aleyhi min ecr. in ecriye illa `ala rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "evfu-lkeyle vela tekunu mine-lmuhsirin"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "vezinu bilkistasi-lmustekim"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "vela tebhasu-nnase esyaehum vela ta`sev fi-l'ardi mufsidin"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "vetteku-llezi halekakum velcibillete-l'evvelin"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "kalu innema ente mine-lmusehharin"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "vema ente illa beser misluna vein nezunnuke lemine-lkazibin"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "feeskit `aleyna kisefem mine-ssemai in kunte mine-ssadikin"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "kale rabbi a`lemu bima ta`melun"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fekezzebuhu feehazehum `azabu yevmi-zzulleh. innehu kane `azabe yevmin `azim"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inne fi zalike leayeh. vema kane ekseruhum mu'minin"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "veinne rabbeke lehuve-l`azizu-rrahim"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "veinnehu letenzilu rabbi-l`alemin"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nezele bihi-rruhu-l'emin"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ala kalbike litekune mine-lmunzirin"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisanin `arabiyyim mubin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "veinnehu lefi zuburi-l'evvelin"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "evelem yekul lehum ayeten ey ya`lemehu `ulemau beni israil"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "velev nezzelnahu `ala ba`di-l'a`cemin"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "fekara'ehu `aleyhim ma kanu bihi mu'minin"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "kezalike seleknahu fi kulubi-lmucrimin"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "la yu'minune bihi hatta yeravu-l`azabe-l'elim"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "feye'tiyehum bagtetev vehum la yes`urun"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "feyekulu hel nahnu munzarun"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "efebi`azabina yesta`cilun"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "eferaeyte im metta`nahum sinin"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "summe caehum ma kanu yu`adun"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "ma agna `anhum ma kanu yumette`un"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "vema ehlekna min karyetin illa leha munzirun"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "zikra. vema kunna zalimin"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "vema tenezzelet bihi-sseyatin"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "vema yembegi lehum vema yesteti`un"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innehum `ani-ssem`i lema`zulun"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "fela ted`u me`a-llahi ilahen ahara fetekune mine-lmu`azzebin"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "veenzir `asirateke-l'akrabin"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "vahfid cenahake limeni-ttebe`ake mine-lmu'minin"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fein `asavke fekul inni berium mimma ta`melun"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "vetevekkel `ale-l`azizi-rrahim"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "ellezi yerake hine tekum"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "vetekallubeke fi-ssacidin"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innehu huve-ssemi`u-l`alim"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hel unebbiukum `ala men tenezzelu-sseyatin"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tenezzelu `ala kulli effakin esim"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yulkune-ssem`a veekseruhum kazibun"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "vessu`arau yettebi`uhumu-lgavun"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "elem tera ennehum fi kulli vadiy yehimun"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "veennehum yekulune ma la yef`alun"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "ille-llezine amenu ve`amilu-ssalihati vezekeru-llahe kesirav ventesaru mim ba`di ma zulimu. veseya`lemu-llezine zalemu eyye munkalebiy yenkalibun"
        }
    ]
}