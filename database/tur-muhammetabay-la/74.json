{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "ya eyyuhe-lmuddessir"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "kum feenzir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "verabbeke fekebbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "vesiyabeke fetahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "verrucze fehcur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "vela temnun testeksir"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "velirabbike fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "feiza nukira fi-nnakur"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "fezalike yevmeiziy yevmun `asir"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "ale-lkafirine gayru yesir"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "zerni vemen halaktu vehida"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "vece`altu lehu malem memduda"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "vebenine suhuda"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "vemehhettu lehu temhida"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "summe yatme`u en ezid"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "kella. innehu kane liayatina `anida"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "seurhikuhu sa`uda"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "innehu fekkera vekaddera"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "fekutile keyfe kaddera"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "summe kutile keyfe kaddera"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "summe nezara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "summe `abese vebesera"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "summe edbera vestekbera"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "fekale in haza illa sihruy yu'ser"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "in haza illa kavlu-lbeser"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "seuslihi sekara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "vema edrake ma sekar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "la tubki vela tezer"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "levvahatul lilbeser"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "aleyha tis`ate `aser"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "vema ce`alna ashabe-nnari illa melaikeh. vema ce`alna `iddetehum illa fitnetel lillezine keferu liyesteykine-llezine utu-lkitabe veyezdade-llezine amenu imanev vela yertabe-llezine utu-lkitabe velmu'minune veliyekule-llezine fi kulubihim meraduv velkafirune maza erade-llahu bihaza mesela. kezalike yudillu-llahu mey yesau veyehdi mey yesa'. vema ya`lemu cunude rabbike illa hu. vema hiye illa zikra lilbeser"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "kella velkamer"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "velleyli iz edbera"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "vessubhi iza esfera"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "inneha leihde-lkuber"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "neziral lilbeser"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "limen sae minkum ey yetekaddeme ev yeteehhar"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "kullu nefsim bima kesebet rahineh"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "illa ashabe-lyemin"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "fi cennatin. yetesaelun"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ani-lmucrimin"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ma selekekum fi sekara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "kalu lem neku mine-lmusallin"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "velem neku nut`imu-lmiskin"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "vekunna nehudu me`a-lhaidin"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "vekunna nukezzibu biyevmi-ddin"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "hatta etane-lyekin"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "fema tenfe`uhum sefa`atu-ssafi`in"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "fema lehum `ani-ttezkirati mu`ridin"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "keennehum humurum mustenfirah"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "ferrat min kasverah"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "bel yuridu kullu-mriim minhum ey yu'ta suhufem munesserah"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "kella. bel la yehafune-l'ahirah"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "kella innehu tezkirah"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "femen sae zekerah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "vema yezkurune illa ey yesae-llah. huve ehlu-ttakva veehlu-lmagfirah"
        }
    ]
}