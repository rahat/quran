{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "ize-ssemau-nfetarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "veize-lkevakibu-nteserat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "veize-lbiharu fuccirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "veize-lkuburu bu`sirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "alimet nefsum ma kaddemet veehharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "ya eyyuhe-l'insanu ma garrake birabbike-lkerim"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "ellezi halekake fesevvake fe`adelek"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "fi eyyi suratim ma sae rakkebek"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "kella bel tukezzibune biddin"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "veinne `aleykum lehafizin"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "kiramen katibin"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "ya`lemune ma tef`alun"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "inne-l'ebrara lefi ne`im"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "veinne-lfuccara lefi cehim"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "yaslevneha yevme-ddin"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "vema hum `anha bigaibin"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "vema edrake ma yevmu-ddin"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "summe ma edrake ma yevmu-ddin"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "yevme la temliku nefsul linefsin sey'a. vel'emru yevmeizil lillah"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "veylul lilmutaffifin"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "ellezine ize-ktalu `ale-nnasi yestevfun"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "veiza kaluhum ev vezenuhum yuhsirun"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "ela yezunnu ulaike ennehum meb`usun"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "liyevmin `azim"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "yevme yekumu-nnasu lirabbi-l`alemin"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "kella inne kitabe-lfuccari lefi siccin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "vema edrake ma siccin"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "kitabum merkum"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "veyluy yevmeizil lilmukezzibin"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "ellezine yukezzibune biyevmi-ddin"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "vema yukezzibu bihi illa kullu mu`tedin esim"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "iza tutla `aleyhi ayatuna kale esatiru-l'evvelin"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "kella bel rane `ala kulubihim ma kanu yeksibun"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "kella innehum `ar rabbihim yevmeizil lemahcubun"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "summe innehum lesalu-lcehim"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "summe yukalu haze-llezi kuntum bihi tukezzibun"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "kella inne kitabe-l'ebrari lefi `illiyyin"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "vema edrake ma `illiyyun"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "kitabum merkum"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "yesheduhu-lmukarrabun"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "inne-l'ebrara lefi ne`im"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "ale-l'eraiki yenzurun"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "ta`rifu fi vucuhihim nadrate-nne`im"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "yuskavne mir rahikim mahtum"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "hitamuhu misk. vefi zalike felyetenafesi-lmutenafisun"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "vemizacuhu min tesnim"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "ayney yesrabu bihe-lmukarrabun"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "inne-llezine ecramu kanu mine-llezine amenu yadhakun"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "veiza merru bihim yetegamezun"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "veize-nkalebu ila ehlihimu-nkalebu fekihin"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "veiza raevhum kalu inne haulai ledallun"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "vema ursilu `aleyhim hafizin"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "felyevme-llezine amenu mine-lkuffari yadhakun"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "ale-l'eraiki yenzurun"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "hel suvvibe-lkuffaru ma kanu yef`alun"
        }
    ]
}