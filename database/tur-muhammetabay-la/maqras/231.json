{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "la uksimu biyevmi-lkiyameh"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "vela uksimu binnefsi-llevvameh"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "eyahsebu-l'insanu ellen necme`a `izameh"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "bela kadirine `ala en nusevviye benaneh"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "bel yuridu-l'insanu liyefcura emameh"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "yes'elu eyyane yevmu-lkiyameh"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "feiza berika-lbesar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "vehasefe-lkamer"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "vecumi`a-ssemsu velkamer"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "yekulu-l'insanu yevmeizin eyne-lmeferr"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "kella la vezer"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "ila rabbike yevmeizin-lmustekarr"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "yunebbeu-l'insanu yevmeizim bima kaddeme veehhar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "beli-l'insanu `ala nefsihi besirah"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "velev elka me`azirah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "la tuharrik bihi lisaneke lita`cele bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "inne `aleyna cem`ahu vekur'aneh"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "feiza kara'nahu fettebi` kur'aneh"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "summe inne `aleyna beyaneh"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "kella bel tuhibbune-l`acileh"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "vetezerune-l'ahirah"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "vucuhuy yevmeizin nadirah"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "ila rabbiha nazirah"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "vevucuhuy yevmeizim basirah"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "tezunnu ey yuf`ale biha fakirah"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "kella iza belegati-tterakiy"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "vekile men rak"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "vezanne ennehu-lfirak"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "velteffeti-ssaku bissak"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "ila rabbike yevmeizin-lmesak"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "fela saddeka vela salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "velakin kezzebe vetevella"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "summe zehebe ila ehlihi yetemetta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "evla leke feevla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "summe evla leke feevla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "eyahsebu-l'insanu ey yutrake suda"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "elem yeku nutfetem mim meniyyiy yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "summe kane `alekaten fehaleka fesevva"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "fece`ale minhu-zzevceyni-zzekera vel'unsa"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "eleyse zalike bikadirin `ala ey yuhyiye-lmevta"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "hel eta `ale-l'insani hinum mine-ddehri lem yekun sey'em mezkura"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "inna halakne-l'insane min nutfetin emsac. nebtelihi fece`alnahu semi`am besira"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "inna hedeynahu-ssebile imma sakirav veimma kefura"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "inna a`tedna lilkafirine selasile veaglalev vese`ira"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "inne-l'ebrara yesrabune min ke'sin kane mizacuha kafura"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "ayney yesrabu biha `ibadu-llahi yufecciruneha tefcira"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "yufune binnezri veyehafune yevmen kane serruhu mustetira"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "veyut`imune-tta`ame `ala hubbihi miskinev veyetimev veesira"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "innema nut`imukum livechi-llahi la nuridu minkum cezaev vela sukura"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "inna nehafu mir rabbina yevmen `abusen kamtarira"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "fevekahumu-llahu serra zalike-lyevmi velekkahum nadratev vesurura"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "vecezahum bima saberu cennetev veharira"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "muttekiine fiha `ale-l'eraik. la yeravne fiha semsev vela zemherira"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "vedaniyeten `aleyhim zilaluha vezullilet kutufuha tezlila"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "veyutafu `aleyhim bianiyetim min fiddativ veekvabin kanet kavarira"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "kavarirae min fiddatin kadderuha takdira"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "veyuskavne fiha ke'sen kane mizacuha zencebila"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "aynen fiha tusemma selsebila"
        }
    ]
}