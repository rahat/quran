{
    "maqras": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "ta-ha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "ma enzelna `aleyke-lkur'ane liteska"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "illa tezkiratel limey yahsa"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "tenzilem mimmen haleka-l'arda vessemavati-l`ula"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "errahmanu `ale-l`arsi-steva"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "lehu ma fi-ssemavati vema fi-l'ardi vema beynehuma vema tahte-ssera"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "vein techer bilkavli feinnehu ya`lemu-ssirra veahfa"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "allahu la ilahe illa hu. lehu-l'esmau-lhusna"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "vehel etake hadisu musa"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "iz raa naran fekale liehlihi-mkusu inni anestu naral le`alli atikum minha bikabesin ev ecidu `ale-nnari huda"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "felemma etaha nudiye ya musa"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "inni ene rabbuke fahla` na`leyk. inneke bilvadi-lmukaddesi tuva"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "veene-htertuke festemi` lima yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "inneni ene-llahu la ilahe illa ene fa`budni veekimi-ssalate lizikri"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "inne-ssa`ate atiyetun ekadu uhfiha litucza kullu nefsim bima tes`a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "fela yesuddenneke `anha mel la yu'minu biha vettebe`a hevahu feterda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "vema tilke biyeminike ya musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "kale hiye `asay. etevekkeu `aleyha veehussu biha `ala ganemi veliye fiha mearibu uhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "kale elkiha ya musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "feelkaha feiza hiye hayyetun tes`a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "kale huzha vela tehaf. senu`iduha siratehe-l'ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "vadmum yedeke ila cenahike tahruc beydae min gayri suin ayeten uhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "linuriyeke min ayatine-lkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "izheb ila fir`avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "kale rabbi-srah li sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "veyessir li emri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "vahlul `ukdetem mil lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yefkahu kavli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "vec`al li veziram min ehli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "harune ehi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "usdud bihi ezri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "veesrikhu fi emri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "key nusebbihake kesira"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "venezkurake kesira"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "inneke kunte bina besira"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "kale kad utite su'leke ya musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "velekad menenna `aleyke merraten uhra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "iz evhayna ila ummike ma yuha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "eni-kzifihi fi-ttabuti fakzifihi fi-lyemmi felyulkihi-lyemmu bissahili ye'huzhu `aduvvul li ve`aduvvul leh. veelkaytu `aleyke mehabbetem minni. velitusne`a `ala `ayni"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "iz temsi uhtuke fetekulu hel edullukum `ala mey yekfuluh. feraca`nake ila ummike key tekarra `aynuha vela tahzen. vekatelte nefsen fenecceynake mine-lgammi vefetennake futuna. felebiste sinine fi ehli medyene summe ci'te `ala kaderiy ya musa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "vastana`tuke linefsi"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "izheb ente veehuke biayati vela teniya fi zikri"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "izheba ila fir`avne innehu taga"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "fekula lehu kavlel leyyinel le`allehu yetezekkeru ev yahsa"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "kala rabbena innena nehafu ey yefruta `aleyna ev ey yatga"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "kale la tehafa inneni me`akuma esme`u veera"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "fe'tiyahu fekula inna rasula rabbike feersil me`ana beni israile vela tu`azzibhum. kad ci'nake biayetim mir rabbik. vesselamu `ala meni-ttebe`a-lhuda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "inna kad uhiye ileyna enne-l`azabe `ala men kezzebe vetevella"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "kale femer rabbukuma ya musa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "kale rabbune-llezi a`ta kulle sey'in halkahu summe heda"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "kale fema balu-lkuruni-l'ula"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "kale `ilmuha `inde rabbi fi kitab. la yedillu rabbi vela yensa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "ellezi ce`ale lekumu-l'arda mehdev veseleke lekum fiha subulev veenzele mine-ssemai maa. feahracna bihi ezvacem min nebetin setta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "kulu ver`av en`amekum. inne fi zalike leayatil liuli-nnuha"
        }
    ]
}