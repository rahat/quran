{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "ize-ssemau-nsekkat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "veezinet lirabbiha vehukkat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "veize-l'ardu muddet"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "veelkat ma fiha vetehallet"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "veezinet lirabbiha vehukkat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "ya eyyuhe-l'insanu inneke kadihun ila rabbike kedhan femulakih"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "feemma men utiye kitabehu biyeminih"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "fesevfe yuhasebu hisabey yesira"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "veyenkalibu ila ehlihi mesrura"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "veemma men utiye kitabehu verae zahrih"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "fesevfe yed`u subura"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "veyasla se`ira"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "innehu kane fi ehlihi mesrura"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "innehu zanne el ley yehur"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "bela. inne rabbehu kane bihi besira"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "fela uksimu bissefek"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "velleyli vema veseka"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "velkameri ize-tteseka"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "leterkebunne tabekan `an tabak"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "fema lehum la yu'minun"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "veiza kurie `aleyhimu-lkur'anu la yescudun"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "beli-llezine keferu yukezzibun"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "vellahu a`lemu bima yu`un"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "febessirhum bi`azabin elim"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "ille-llezine amenu ve`amilu-ssalihati lehum ecrun gayru memnun"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "vessemai zati-lburuc"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "velyevmi-lmev`ud"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "vesahidiv vemeshud"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kutile ashabu-l'uhdud"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ennari zati-lvekud"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "iz hum `aleyha ku`ud"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "vehum `ala ma yef`alune bilmu'minine suhud"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "vema nekamu minhum illa ey yu'minu billahi-l`azizi-lhamid"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "ellezi lehu mulku-ssemavati vel'ard. vellahu `ala kulli sey'in sehid"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inne-llezine fetenu-lmu'minine velmu'minati summe lem yetubu felehum `azabu cehenneme velehum `azabu-lharik"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inne-llezine amenu ve`amilu-ssalihati lehum cennatun tecri min tahtihe-l'enhar. zalike-lfevzu-lkebir"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "inne batse rabbike lesedid"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "innehu huve yubdiu veyu`id"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "vehuve-lgafuru-lvedud"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "zu-l`arsi-lmecid"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "fa``alul lima yurid"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "hel etake hadisu-lcunud"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "fir`avne vesemud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "beli-llezine keferu fi tekzib"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "vellahu miv veraihim muhit"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "bel huve kur'anum mecid"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "fi levhim mahfuz"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "vessemai vettarik"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "vema edrake me-ttarik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ennecmu-ssakib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "in kullu nefsil lemma `aleyha hafiz"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "felyenzuri-l'insanu mimme hulik"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "hulika mim main dafik"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yahrucu mim beyni-ssulbi vetteraib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "innehu `ala rac`ihi lekadir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "yevme tuble-sserair"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "fema lehu min kuvvetiv vela nasir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "vessemai zati-rrac"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "vel'ardi zati-ssad"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "innehu lekavlun fasl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "vema huve bilhezl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "innehum yekidune keyda"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "veekidu keyda"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "femehhili-lkafirine emhilhum ruveyda"
        }
    ]
}