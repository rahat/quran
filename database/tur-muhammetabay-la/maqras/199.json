{
    "maqras": [
        {
            "chapter": 44,
            "verse": 17,
            "text": "velekad fetenna kablehum kavme fir`avne vecaehum rasulun kerim"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "en eddu ileyye `ibade-llah. inni lekum rasulun emin"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "veel la ta`lu `ale-llah. inni atikum bisultanim mubin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "veinni `uztu birabbi verabbikum en tercumun"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "veil lem tu'minu li fa`tezilun"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "fede`a rabbehu enne haulai kavmum mucrimun"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "feesri bi`ibadi leylen innekum muttebe`un"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "vetruki-lbahra rahva. innehum cundum mugrakun"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "kem teraku min cennativ ve`uyun"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "vezuru`iv vemekamin kerim"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "vena`metin kanu fiha fakihin"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "kezalik. veevrasnaha kavmen aharin"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "fema beket `aleyhimu-ssemau vel'ardu vema kanu munzarin"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "velekad necceyna beni israile mine-l`azabi-lmuhin"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "min fir`avn. innehu kane `aliyem mine-lmusrifin"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "velekadi-hternahum `ala `ilmin `ale-l`alemin"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "veateynahum mine-l'ayati ma fihi belaum mubin"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "inne haulai leyekulun"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "in hiye illa mevtetune-l'ula vema nahnu bimunserin"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "fe'tu biabaina in kuntum sadikin"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "ehum hayrun em kavmu tubbe`iv vellezine min kablihim. ehleknahum. innehum kanu mucrimin"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "vema halakne-ssemavati vel'arda vema beynehuma la`ibin"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "ma halaknahuma illa bilhakki velakinne ekserahum la ya`lemun"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "inne yevme-lfasli mikatuhum ecme`in"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "yevme la yugni mevlen `am mevlen sey'ev vela hum yunsarun"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "illa mer rahime-llah. innehu huve-l`azizu-rrahim"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "inne secerate-zzekkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "ta`amu-l'esim"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "kelmuhl. yagli fi-lbutun"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "kegalyi-lhamim"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "huzuhu fa`tiluhu ila sevai-lcehim"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "summe subbu fevka ra'sihi min `azabi-lhamim"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "zuk. inneke ente-l`azizu-lkerim"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "inne haza ma kuntum bihi temterun"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "inne-lmuttekine fi mekamin emin"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "fi cennativ ve`uyun"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "yelbesune min sundusiv veistebrakim mutekabilin"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "kezalik. vezevvecnahum bihurin `in"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "yed`une fiha bikulli fakihetin aminin"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "la yezukune fihe-lmevte ille-lmevtete-l'ula. vevekahum `azabe-lcehim"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "fadlem mir rabbik. zalike huve-lfevzu-l`azim"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "feinnema yessernahu bilisanike le`allehum yetezekkerun"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "fertekib innehum murtekibun"
        },
        {
            "chapter": 45,
            "verse": 1,
            "text": "ha-mim"
        },
        {
            "chapter": 45,
            "verse": 2,
            "text": "tenzilu-lkitabi mine-llahi-l`azizi-lhakim"
        },
        {
            "chapter": 45,
            "verse": 3,
            "text": "inne fi-ssemavati vel'ardi leayatil lilmu'minin"
        },
        {
            "chapter": 45,
            "verse": 4,
            "text": "vefi halkikum vema yebussu min dabbetin ayatul likavmiy yukinun"
        },
        {
            "chapter": 45,
            "verse": 5,
            "text": "vahtilafi-lleyli vennehari vema enzele-llahu mine-ssemai mir rizkin feahya bihi-l'arda ba`de mevtiha vetasrifi-rriyahi ayatul likavmiy ya`kilun"
        },
        {
            "chapter": 45,
            "verse": 6,
            "text": "tilke ayatu-llahi netluha `aleyke bilhakk. febieyyi hadisim ba`de-llahi veayatihi yu'minun"
        },
        {
            "chapter": 45,
            "verse": 7,
            "text": "veylul likulli effakin esim"
        },
        {
            "chapter": 45,
            "verse": 8,
            "text": "yesme`u ayati-llahi tutla `aleyhi summe yusirru mustekbiran keel lem yesma`ha. febessirhu bi`azabin elim"
        },
        {
            "chapter": 45,
            "verse": 9,
            "text": "veiza `alime min ayatina sey'en-ttehazeha huzuva. ulaike lehum `azabum muhin"
        },
        {
            "chapter": 45,
            "verse": 10,
            "text": "miv veraihim cehennem. vela yugni `anhum ma kesebu sey'ev vela me-ttehazu min duni-llahi evliya'. velehum `azabun `azim"
        },
        {
            "chapter": 45,
            "verse": 11,
            "text": "haza huda. vellezine keferu biayati rabbihim lehum `azabum mir riczin elim"
        }
    ]
}