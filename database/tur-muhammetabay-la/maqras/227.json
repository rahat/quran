{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "elhakkah"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "me-lhakkah"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "vema edrake me-lhakkah"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "kezzebet semudu ve`adum bilkari`ah"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "feemma semudu feuhliku bittagiyeh"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "veemma `adun feuhliku birihin sarsarin `atiyeh"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "sehharaha `aleyhim seb`a leyaliv vesemaniyete eyyamin husumen fetere-lkavme fiha sar`a keennehum a`cazu nahlin haviyeh"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "fehel tera lehum mim bakiyeh"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "vecae fir`avnu vemen kablehu velmu'tefikatu bilhatieh"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "fe`asav rasule rabbihim feehazehum ahzeter rabiyeta"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "inna lemma taga-lmau hamelnakum fi-lcariyeh"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "linec`aleha lekum tezkiratev vete`iyeha uzunuv va`iyeh"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "feiza nufiha fi-ssuri nefhatuv vahideh"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "vehumileti-l'ardu velcibalu fedukketa dekketev vahideta"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "feyevmeiziv veka`ati-lvaki`ah"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "vensekkati-ssemau fehiye yevmeiziv vahiyeh"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "velmeleku `ala ercaiha. veyahmilu `arse rabbike fevkahum yevmeizin semaniyeh"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "yevmeizin tu`radune la tahfa minkum hafiyeh"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "feemma men utiye kitabehu biyeminihi feyekulu haumu-krau kitabiyeh"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "inni zanentu enni mulakin hisabiyeh"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "fehuve fi `isetir radiyeh"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "fi cennetin `aliyeh"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "kutufuha daniyeh"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "kulu vesrabu heniem bima esleftum fi-l'eyyami-lhaliyeh"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "veemma men utiye kitabehu bisimalihi feyekulu ya leyteni lem ute kitabiyeh"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "velem edri ma hisabiyeh"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "ya leyteha kaneti-lkadiyeh"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "ma agna `anni maliyeh"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "heleke `anni sultaniyeh"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "huzuhu fegulluh"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "summe-lcehime salluh"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "summe fi silsiletin zer`uha seb`une zira`an feslukuh"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "innehu kane la yu'minu billahi-l`azim"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "vela yehuddu `ala ta`ami-lmiskin"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "feleyse lehu-lyevme hahuna hamim"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "vela ta`amun illa min gislin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "la ye'kuluhu ille-lhatiun"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "fela uksimu bima tubsirun"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "vema la tubsirun"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "innehu lekavlu rasulin kerim"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "vema huve bikavli sa`ir. kalilem ma tu'minun"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "vela bikavli kahin. kalilem ma tezekkerun"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "tenzilum mir rabbi-l`alemin"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "velev tekavvele `aleyna ba`da-l'ekavil"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "leehazna minhu bilyemin"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "summe lekata`na minhu-lvetin"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "fema minkum min ehadin `anhu hacizin"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "veinnehu letezkiratul lilmuttekin"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "veinna lena`lemu enne minkum mukezzibin"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "veinnehu lehasratun `ale-lkafirin"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "veinnehu lehakku-lyekin"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "fesebbih bismi rabbike-l`azim"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "seele sailum bi`azabiv vaki"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "lilkafirine leyse lehu dafi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "mine-llahi zi-lme`aric"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "ta`rucu-lmelaiketu verruhu ileyhi fi yevmin kane mikdaruhu hamsine elfe seneh"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "fasbir sabran cemila"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "innehum yeravnehu be`ida"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "venerahu kariba"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "yevme tekunu-ssemau kelmuhl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "vetekunu-lcibalu kel`ihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "vela yes'elu hamimun hamima"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "yubessarunehum. yeveddu-lmucrimu lev yeftedi min `azabi yevmiizim bibenih"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "vesahibetihi veehih"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "vefesiletihi-lleti tu'vih"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "vemen fi-l'ardi cemi`an summe yuncih"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "kella. inneha leza"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "nezza`atel lisseva"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "ted`u men edbera vetevella"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "veceme`a feev`a"
        }
    ]
}