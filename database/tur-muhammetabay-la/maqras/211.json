{
    "maqras": [
        {
            "chapter": 53,
            "verse": 26,
            "text": "vekem mim melekin fi-ssemavati la tugni sefa`atuhum sey'en illa mim ba`di ey ye'zene-llahu limey yesau veyerda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "inne-llezine la yu'minune bil'ahirati leyusemmune-lmelaikete tesmiyete-l'unsa"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "vema lehum bihi min `ilm. iy yettebi`une ille-zzanne. veinne-zzanne la yugni mine-lhakki sey'a"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fea`rid `am men tevella `an zikrina velem yurid ille-lhayate-ddunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "zalike mebleguhum mine-l`ilm. inne rabbeke huve a`lemu bimen dalle `an sebilihi vehuve a`lemu bimeni-hteda"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "velillahi ma fi-ssemavati vema fi-l'ardi liyecziye-llezine esau bima `amilu veyecziye-llezine ahsenu bilhusna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "ellezine yectenibune kebaira-l'ismi velfevahise ille-llemem. inne rabbeke vasi`u-lmagfirah. huve a`lemu bikum iz enseekum mine-l'ardi veiz entum ecinnetun fi butuni ummehatikum. fela tuzekku enfusekum. huve a`lemu bimeni-tteka"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "eferaeyte-llezi tevella"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "vea`ta kalilev veekda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "e`indehu `ilmu-lgaybi fehuve yera"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "em lem yunebbe' bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "veibrahime-llezi veffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ella teziru vaziratuv vizra uhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "veel leyse lil'insani illa ma se`a"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "veenne sa`yehu sevfe yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "summe yuczahu-lcezae-l'evfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "veenne ila rabbike-lmunteha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "veennehu huve adhake veebka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "veennehu huve emate veahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "veennehu haleka-zzevceyni-zzekera vel'unsa"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nutfetin iza tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "veenne `aleyhi-nnes'ete-l'uhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "veennehu huve agna veakna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "veennehu huve rabbu-ssi`ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "veennehu ehleke `aden-l'ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "vesemude fema ebka"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "vekavme nuhim min kabl. innehum kanu hum azleme veatga"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "velmu'tefikete ehva"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "fegassaha ma gassa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "febieyyi alai rabbike tetemara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "haza nezirum mine-nnuzuri-l'ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "ezifeti-l'azifeh"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "leyse leha min duni-llahi kasifeh"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "efemin haze-lhadisi ta`cebun"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "vetadhakune vela tebkun"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "veentum samidun"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fescudu lillahi va`budu"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "ikterabeti-ssa`atu vensekka-lkamer"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "veiy yerav ayetey yu`ridu veyekulu sihrum mustemirr"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "vekezzebu vettebe`u ehvaehum vekullu emrim mustekirr"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "velekad caehum mine-l'embai ma fihi muzdecer"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "hikmetum baligatun fema tugni-nnuzur"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "fetevelle `anhum. yevme yed`u-dda`i ila sey'in nukur"
        },
        {
            "chapter": 54,
            "verse": 7,
            "text": "husse`an ebsaruhum yahrucune mine-l'ecdasi keennehum ceradum muntesir"
        },
        {
            "chapter": 54,
            "verse": 8,
            "text": "muhti`ine ile-dda`. yekulu-lkafirune haza yevmun `asir"
        }
    ]
}