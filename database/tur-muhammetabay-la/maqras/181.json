{
    "maqras": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "fenebeznahu bil`arai vehuve sekim"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "veembetna `aleyhi seceratem miy yaktin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "veerselnahu ila mieti elfin ev yezidun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "feamenu femetta`nahum ila hin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "festeftihim elirabbike-lbenatu velehumu-lbenun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "em halakne-lmelaikete inasev vehum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "ela innehum min ifkihim leyekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "velede-llahu veinnehum lekazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "astafe-lbenati `ale-lbenin"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "ma lekum. keyfe tahkumun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "efela tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "em lekum sultanum mubin"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "fe'tu bikitabikum in kuntum sadikin"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "vece`alu beynehu vebeyne-lcinneti neseba. velekad `alimeti-lcinnetu innehum lemuhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "subhane-llahi `amma yesifun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "feinnekum vema ta`budun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "ma entum `aleyhi bifatinin"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "illa men huve sali-lcehim"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "vema minna illa lehu mekamum ma`lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "veinna lenahnu-ssaffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "veinna lenahnu-lmusebbihun"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "vein kanu leyekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "lev enne `indena zikram mine-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "lekunna `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "fekeferu bih. fesevfe ya`lemun"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "velekad sebekat kelimetuna li`ibadine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "innehum lehumu-lmensurun"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "veinne cundena lehumu-lgalibun"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "fetevelle `anhum hatta hin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "veebsirhum fesevfe yubsirun"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "efebi`azabina yesta`cilun"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "feiza nezele bisahatihim fesae sabahu-lmunzerin"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "vetevelle `anhum hatta hin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "veebsir fesevfe yubsirun"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "subhane rabbike rabbi-l`izzeti `amma yesifun"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "veselamun `ale-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "velhamdu lillahi rabbi-l`alemin"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "sad. velkur'ani zi-zzikr"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "beli-llezine keferu fi `izzetiv vesikak"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "kem ehlekna min kablihim min karnin fenadev velate hine menas"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "ve`acibu en caehum munzirum minhum. vekale-lkafirune haza sahirun kezzab"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "ece`ale-l'alihete ilahev vahida. inne haza lesey'un `ucab"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "ventaleka-lmeleu minhum eni-msu vasbiru `ala alihetikum. inne haza lesey'uy yurad"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "ma semi`na bihaza fi-lmilleti-l'ahirah. in haza ille-htilak"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "eunzile `aleyhi-zzikru mim beynina. bel hum fi sekkim min zikri. bel lemma yezuku `azab"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "em `indehum hazainu rahmeti rabbike-l`azizi-lvehhab"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "em lehum mulku-ssemavati vel'ardi vema beynehuma. felyerteku fi-l'esbab"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "cundum ma hunalike mehzumum mine-l'ahzab"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "kezzebet kablehum kavmu nuhiv ve`aduv vefir`avnu zu-l'evtad"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "vesemudu vekavmu lutiv veashabu-l'eykeh. ulaike-l'ahzab"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "in kullun illa kezzebe-rrusule fehakka `ikab"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "vema yenzuru haulai illa sayhatev vahidetem ma leha min fevak"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "vekalu rabbena `accil lena kittana kable yevmi-lhisab"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "isbir `ala ma yekulune vezkur `abdena davude ze-l'eyd. innehu evvab"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "inna sehharne-lcibale me`ahu yusebbihne bil`asiyyi vel'israk"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "vettayra mahsurah. kullul lehu evvab"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "vesededna mulkehu veateynahu-lhikmete vefasle-lhitab"
        }
    ]
}