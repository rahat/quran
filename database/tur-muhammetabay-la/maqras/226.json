{
    "maqras": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "nun. velkalemi vema yesturun"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ma ente bini`meti rabbike bimecnun"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "veinne leke leecran gayra memnun"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "veinneke le`ala hulukin `azim"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "fesetubsiru veyubsirun"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "bieyyikumu-lmeftun"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "inne rabbeke huve a`lemu bimen dalle `an sebilih. vehuve a`lemu bilmuhtedin"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "fela tuti`i-lmukezzibin"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "veddu lev tudhinu feyudhinun"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "vela tuti` kulle hallafim mehin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "hemmazim messaim binemim"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "menna`il lilhayri mu`tedin esim"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "utullim ba`de zalike zenim"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "en kane za maliv vebenin"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "iza tutla `aleyhi ayatuna kale esatiru-l'evvelin"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "senesimuhu `ale-lhurtum"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "inna belevnahum kema belevna ashabe-lcenneh. iz aksemu leyasrimunneha musbihin"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "vela yestesnun"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "fetafe `aleyha taifum mir rabbike vehum naimun"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "feasbehat kessarim"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "fetenadev musbihin"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "eni-gdu `ala harsikum in kuntum sarimin"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "fentaleku vehum yetehafetun"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "el la yedhulennehe-lyevme `aleykum miskin"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "vegadev `ala hardin kadirin"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "felemma raevha kalu inna ledallun"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "bel nahnu mahrumun"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "kale evsetuhum elem ekul lekum levla tusebbihun"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "kalu subhane rabbina inna kunna zalimin"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "feakbele ba`duhum `ala ba`diy yetelavemun"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "kalu ya veylena inna kunna tagin"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "asa rabbuna ey yubdilena hayram minha inna ila rabbina ragibun"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "kezalike-l`azab. vele`azabu-l'ahirati ekber. lev kanu ya`lemun"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "inne lilmuttekine `inde rabbihim cennati-nne`im"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "efenec`alu-lmuslimine kelmucrimin"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "ma lekum. keyfe tahkumun"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "em lekum kitabun fihi tedrusun"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "inne lekum fihi lema tehayyerun"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "em lekum eymanun `aleyna baligatun ila yevmi-lkiyameti inne lekum lema tahkumun"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "selhum eyyuhum bizalike za`im"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "em lehum suraka'. felye'tu bisurakaihim in kanu sadikin"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "yevme yuksefu `an sakiv veyud`avne ile-ssucudi fela yesteti`un"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "hasi`aten ebsaruhum terhekuhum zilleh. vekad kanu yud`avne ile-ssucudi vehum salimun"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "fezerni vemey yukezzibu bihaze-lhadis. senestedricuhum min haysu la ya`lemun"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "veumli lehum. inne keydi metin"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "em tes'eluhum ecran fehum mim magramim muskalun"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "em `indehumu-lgaybu fehum yektubun"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "fasbir lihukmi rabbike vela tekun kesahibi-lhut. iz nada vehuve mekzum"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "levla en tedarakehu ni`metum mir rabbihi lenubize bil`arai vehuve mezmum"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "fectebahu rabbuhu fece`alehu mine-ssalihin"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "veiy yekadu-llezine keferu leyuzlikuneke biebsarihim lemma semi`u-zzikra veyekulune innehu lemecnun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "vema huve illa zikrul lil`alemin"
        }
    ]
}