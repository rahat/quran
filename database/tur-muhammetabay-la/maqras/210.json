{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "veyetufu `aleyhim gilmanul lehum keennehum lu'luum meknun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "veakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "kalu inna kunna kablu fi ehlina musfikin"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "femenne-llahu `aleyna vevekana `azabe-ssemum"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "inna kunna min kablu ned`uh. innehu huve-lberru-rrahim"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "fezekkir fema ente bini`meti rabbike bikahiniv vela mecnun"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "em yekulune sa`irun neterabbesu bihi raybe-lmenun"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "kul terabbesu feinni me`akum mine-lmuterabbisin"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "em te'muruhum ahlamuhum bihaza em hum kavmun tagun"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "em yekulune tekavveleh. bel la yu'minun"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "felye'tu bihadisim mislihi in kanu sadikin"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "em huliku min gayri sey'in em humu-lhalikun"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "em haleku-ssemavati vel'ard. bel la yukinun"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "em `indehum hazainu rabbike em humu-lmusaytirun"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "em lehum sullemuy yestemi`une fih. felye'ti mustemi`uhum bisultanim mubin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "em lehu-lbenatu velekumu-lbenun"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "em tes'eluhum ecran fehum mim magramim muskalun"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "em `indehumu-lgaybu fehum yektubun"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "em yuridune keyda. fellezine keferu humu-lmekidun"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "em lehum ilahun gayru-llah. subhane-llahi `amma yusrikun"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "veiy yerav kisfem mine-ssemai sakitay yekulu sehabum merkum"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "fezerhum hatta yulaku yevmehumu-llezi fihi yus`akun"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yevme la yugni `anhum keyduhum sey'ev vela hum yunsarun"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "veinne lillezine zalemu `azaben dune zalike velakinne ekserahum la ya`lemun"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "vasbir lihukmi rabbike feinneke bia`yunina vesebbih bihamdi rabbike hine tekum"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "vemine-lleyli fesebbihhu veidbara-nnucum"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "vennecmi iza heva"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ma dalle sahibukum vema gava"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "vema yentiku `ani-lheva"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huve illa vahyuy yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "allemehu sedidu-lkuva"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "zu mirrah. festeva"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "vehuve bil'ufuki-l'a`la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "summe dena fetedella"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fekane kabe kavseyni ev edna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "feevha ila `abdihi ma evha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "ma kezebe-lfuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "efetumarunehu `ala ma yera"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "velekad raahu nezleten uhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "inde sidrati-lmunteha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "indeha cennetu-lme'va"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "iz yagse-ssidrate ma yagsa"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "ma zaga-lbesaru vema taga"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "lekad raa min ayati rabbihi-lkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "eferaeytumu-llate vel`uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "vemenate-ssalisete-l'uhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "elekumu-zzekeru velehu-l'unsa"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "tilke izen kismetun diza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiye illa esmaun semmeytumuha entum veabaukum ma enzele-llahu biha min sultan. iy yettebi`une ille-zzanne vema tehve-l'enfus. velekad caehum mir rabbihimu-lhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "em lil'insani ma temenna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "felillahi-l'ahiratu vel'ula"
        }
    ]
}