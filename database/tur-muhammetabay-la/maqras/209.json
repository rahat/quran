{
    "maqras": [
        {
            "chapter": 51,
            "verse": 31,
            "text": "kale fema hatbukum eyyuhe-lmurselun"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "kalu inna ursilna ila kavmim mucrimin"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "linursile `aleyhim hicaratem min tin"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "musevvemeten `inde rabbike lilmusrifin"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "feahracna men kane fiha mine-lmu'minin"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "fema vecedna fiha gayra beytim mine-lmuslimin"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "veterakna fiha ayetel lillezine yehafune-l`azabe-l'elim"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "vefi musa iz erselnahu ila fir`avne bisultanim mubin"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "fetevella biruknihi vekale sahirun ev mecnun"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "feehaznahu vecunudehu fenebeznahum fi-lyemmi vehuve mulim"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "vefi `adin iz erselna `aleyhimu-rriha-l`akim"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "ma tezeru min sey'in etet `aleyhi illa ce`alethu kelramim"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "vefi semude iz kile lehum temette`u hatta hin"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "fe`atev `an emri rabbihim feehazethumu-ssa`ikatu vehum yenzurun"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "feme-steta`u min kiyamiv vema kanu muntesirin"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "vekavme nuhim min kabl. innehum kanu kavmen fasikin"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "vessemae beneynaha bieydiv veinna lemusi`un"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "vel'arda ferasnaha feni`me-lmahidun"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "vemin kulli sey'in halakna zevceyni le`allekum tezekkerun"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "fefirru ile-llah. inni lekum minhu nezirum mubin"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "vela tec`alu me`a-llahi ilahen ahar. inni lekum minhu nezirum mubin"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "kezalike ma ete-llezine min kablihim mir rasulin illa kalu sahirun ev mecnun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "etevasav bih. bel hum kavmun tagun"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "fetevelle `anhum fema ente bimelum"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "vezekkir feinne-zzikra tenfe`u-lmu'minin"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "vema halaktu-lcinne vel'inse illa liya`budun"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ma uridu minhum mir rizkiv vema uridu ey yut`imun"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "inne-llahe huve-rrazzaku zu-lkuvveti-lmetin"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "feinne lillezine zalemu zenubem misle zenubi ashabihim fela yesta`cilun"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "feveylul lillezine keferu miy yevmihimu-llezi yu`adun"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "vettur"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "vekitabim mestur"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "fi rakkim mensur"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "velbeyti-lma`mur"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "vessakfi-lmerfu"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "velbahri-lmescur"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "inne `azabe rabbike levaki"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "ma lehu min dafi"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "yevme temuru-ssemau mevra"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "vetesiru-lcibalu seyra"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "feveyluy yevmeizil lilmukezzibin"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "ellezine hum fi havdiy yel`abun"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "yevme yude``une ila nari cehenneme da``a"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "hazihi-nnaru-lleti kuntum biha tukezzibun"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "efesihrun haza em entum la tubsirun"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "islevha fasbiru ev la tasbiru. sevaun `aleykum. innema tuczevne ma kuntum ta`melun"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "inne-lmuttekine fi cennativ vene`im"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "fakihine bima atahum rabbuhum. vevekahum rabbuhum `azabe-lcehim"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "kulu vesrabu heniem bima kuntum ta`melun"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "muttekiine `ala sururim masfufeh. vezevvecnahum bihurin `in"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "vellezine amenu vettebe`athum zurriyyetuhum biimanin elhakna bihim zurriyyetehum vema eletnahum min `amelihim min sey'. kullu-mriim bima kesebe rahin"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "veemdednahum bifakihetiv velahmim mimma yestehun"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "yetenaza`une fiha ke'sel la lagvun fiha vela te'sim"
        }
    ]
}