{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "abese vetevella"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "en caehu-l'a`ma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "vema yudrike le`allehu yezzekka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ev yezzekkeru fetenfe`ahu-zzikra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "emma meni-stagna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "feente lehu tesadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "vema `aleyke ella yezzekka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "veemma men caeke yes`a"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "vehuve yahsa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "feente `anhu telehha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "kella inneha tezkirah"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "femen sae zekerah"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "fi suhufim mukerrameh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "merfu`atim mutahherah"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bieydi seferah"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kiramim berarah"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "kutile-l'insanu ma ekferah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "min eyyi sey'in halekah"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "min nutfeh. halekahu fekadderah"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "summe-ssebile yesserah"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "summe ematehu feakberah"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "summe iza sae enserah"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "kella lemma yakdi ma emerah"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "felyenzuri-l'insanu ila ta`amih"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "enna sabebne-lmae sabba"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "summe sakakne-l'arda sekka"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "feembetna fiha habba"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ve`inebev vekadba"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "vezeytunev venahla"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "vehadaika gulba"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "vefakihetev veebba"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "meta`al lekum velien`amikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "feiza caeti-ssahhah"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "yevme yefirru-lmeru min ehih"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "veummihi veebih"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "vesahibetihi vebenih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "likulli-mriim minhum yevmeizin se'nuy yugnih"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "vucuhuy yevmeizim musfirah"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "dahiketum mustebsirah"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "vevucuhuy yevmeizin `aleyha gaberah"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "terhekuha katerah"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ulaike humu-lkeferatu-lfecerah"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "ize-ssemsu kuvvirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "veize-nnucumu-nkederat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "veize-lcibalu suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "veize-l`isaru `uttilet"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "veize-lvuhusu husirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "veize-lbiharu succirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "veize-nnufusu zuvvicet"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "veize-lmev'udetu suilet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bieyyi zembin kutilet"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "veize-ssuhufu nusirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "veize-ssemau kusitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "veize-lcehimu su``irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "veize-lcennetu uzlifet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "alimet nefsum ma ahdarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "fela uksimu bilhunnes"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "elcevari-lkunnes"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "velleyli iza `as`as"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "vessubhi iza teneffes"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "innehu lekavlu rasulin kerim"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "zi kuvvetin `inde zi-l`arsi mekin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "muta`in semme emin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "vema sahibukum bimecnun"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "velekad raahu bil'ufuki-lmubin"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "vema huve `ale-lgaybi bidanin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "vema huve bikavli seytanir racim"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "feeyne tezhebun"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "in huve illa zikrul lil`alemin"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "limen sae minkum ey yestekim"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "vema tesaune illa ey yesae-llahu rabbu-l`alemin"
        }
    ]
}