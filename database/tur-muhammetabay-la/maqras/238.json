{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "la uksimu bihaze-lbeled"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "veente hillum bihaze-lbeled"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "vevalidiv vema veled"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "lekad halakne-l'insane fi kebed"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "eyahsebu el ley yakdira `aleyhi ehad"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "yekulu ehlektu malel lubeda"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "eyahsebu el lem yerahu ehad"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "elem nec`al lehu `ayneyn"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "velisanev vesefeteyn"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "vehedeynahu-nnecdeyn"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "fele-ktehame-l`akabeh"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "vema edrake me-l`akabeh"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "fekku rakabeh"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ev it`amun fi yevmin zi mesgabeh"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yetimen za makrabeh"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ev miskinen za metrabeh"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "summe kane mine-llezine amenu vetevasav bissabri vetevasav bilmerhameh"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ulaike ashabu-lmeymeneh"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "vellezine keferu biayatina hum ashabu-lmes'emeh"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "aleyhim narum mu'sadeh"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "vessemsi veduhaha"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "velkameri iza telaha"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "vennehari iza cellaha"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "velleyli iza yagsaha"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "vessemai vema benaha"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "vel'ardi vema tahaha"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "venefsiv vema sevvaha"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "feelhemeha fucuraha vetakvaha"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "kad efleha men zekkaha"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "vekad habe men dessaha"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "kezzebet semudu bitagvaha"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "izi-mbe`ase eskaha"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "fekale lehum rasulu-llahi nakate-llahi vesukyaha"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "fekezzebuhu fe`akaruha. fedemdeme `aleyhim rabbuhum bizembihim fesevvaha"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "vela yehafu `ukbaha"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "velleyli iza yagsa"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "vennehari iza tecella"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "vema haleka-zzekera vel'unsa"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "inne sa`yekum lesetta"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "feemma men a`ta vetteka"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "vesaddeka bilhusna"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "fesenuyessiruhu lilyusra"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "veemma mem behile vestagna"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "vekezzebe bilhusna"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "fesenuyessiruhu lil`usra"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "vema yugni `anhu maluhu iza teradda"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "inne `aleyna lelhuda"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "veinne lena lel'ahirate vel'ula"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "feenzertukum naran telezza"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "la yaslaha ille-l'eska"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ellezi kezzebe vetevella"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "veseyucennebuhe-l'etka"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ellezi yu'ti malehu yetezekka"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "vema liehadin `indehu min ni`metin tucza"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "ille-btigae vechi rabbihi-l'a`la"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "velesevfe yerda"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "vedduha"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "velleyli iza seca"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ma vedde`ake rabbuke vema kala"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "velel'ahiratu hayrul leke mine-l'ula"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "velesevfe yu`tike rabbuke feterda"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "elem yecidke yetimen feava"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "vevecedeke dallen feheda"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "vevecedeke `ailen feagna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "feemme-lyetime fela takher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "veemme-ssaile fela tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "veemma bini`meti rabbike fehaddis"
        }
    ]
}