{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sebbihi-sme rabbike-l'a`la"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ellezi haleka fesevva"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vellezi kaddera feheda"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "vellezi ahrace-lmer`a"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fece`alehu gusaen ahva"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "senukriuke fela tensa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illa ma sae-llah. innehu ya`lemu-lcehra vema yahfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "venuyessiruke lilyusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "fezekkir in nefe`ati-zzikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "seyezzekkeru mey yahsa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "veyetecennebuhe-l'eska"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ellezi yasle-nnara-lkubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "summe la yemutu fiha vela yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "kad efleha men tezekka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "vezekera-sme rabbihi fesalla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bel tu'sirune-lhayate-ddunya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "vel'ahiratu hayruv veebka"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inne haza lefi-ssuhufi-l'ula"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "suhufi ibrahime vemusa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "hel etake hadisu-lgasiyeh"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "vucuhuy yevmeizin hasi`ah"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "amiletun nasibeh"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tasla naran hamiyeh"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tuska min `aynin aniyeh"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "leyse lehum ta`amun illa min dari"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "la yusminu vela yugni min cu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "vucuhuy yevmeizin na`imeh"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lisa`yiha radiyeh"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fi cennetin `aliyeh"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "la tesme`u fiha lagiyeh"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "fiha `aynun cariyeh"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fiha sururum merfu`ah"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "veekvabum mevdu`ah"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "venemariku masfufeh"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "vezerabiyyu mebsuseh"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "efela yenzurune ile-l'ibili keyfe hulikat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "veile-ssemai keyfe rufi`at"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "veile-lcibali keyfe nusibet"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "veile-l'ardi keyfe sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "fezekkir innema ente muzekkir"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "leste `aleyhim bimusaytir"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "illa men tevella vekefera"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "feyu`azzibuhu-llahu-l`azabe-l'ekber"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "inne ileyna iyabehum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "summe inne `aleyna hisabehum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "velfecr"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "veleyalin `asr"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "vessef`i velvetr"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "velleyli iza yesr"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hel fi zalike kasemul lizi hicr"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "elem tera keyfe fe`ale rabbuke bi`ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irame zati-l`imad"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "elleti lem yuhlak misluha fi-lbilad"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "vesemude-llezine cabu-ssahra bilvad"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "vefir`avne zi-l'evtad"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ellezine tagav fi-lbilad"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "feekseru fihe-lfesad"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "fesabbe `aleyhim rabbuke sevta `azab"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inne rabbeke lebilmirsad"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "feemme-l'insanu iza me-btelahu rabbuhu feekramehu vene``amehu feyekulu rabbi ekramen"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "veemma iza me-btelahu fekadera `aleyhi rizkahu feyekulu rabbi ehanen"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kella bel la tukrimune-lyetim"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "vela tehaddune `ala ta`ami-lmiskin"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "vete'kulune-tturase eklel lemma"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "vetuhibbune-lmale hubben cemma"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kella iza dukketi-l'ardu dekken dekka"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "vecae rabbuke velmeleku saffen saffa"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "vecie yevmeizim bicehenneme yevmeiziy yetezekkeru-l'insanu veenna lehu-zzikra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yekulu ya leyteni kaddemtu lihayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "feyevmeizil la yu`azzibu `azabehu ehad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "vela yusiku vesakahu ehad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "ya eyyetuhe-nnefsu-lmutmeinneh"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "irci`i ila rabbiki radiyetem merdiyyeh"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fedhuli fi `ibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "vedhuli cenneti"
        }
    ]
}