{
    "maqras": [
        {
            "chapter": 50,
            "verse": 27,
            "text": "kale karinuhu rabbena ma atgaytuhu velakin kane fi dalalim be`id"
        },
        {
            "chapter": 50,
            "verse": 28,
            "text": "kale la tahtesimu ledeyye vekad kaddemtu ileykum bilve`id"
        },
        {
            "chapter": 50,
            "verse": 29,
            "text": "ma yubeddelu-lkavlu ledeyye vema ene bizallamil lil`abid"
        },
        {
            "chapter": 50,
            "verse": 30,
            "text": "yevme nekulu licehenneme heli-mtele'ti vetekulu hel mim mezid"
        },
        {
            "chapter": 50,
            "verse": 31,
            "text": "veuzlifeti-lcennetu lilmuttekine gayra be`id"
        },
        {
            "chapter": 50,
            "verse": 32,
            "text": "haza ma tu`adune likulli evvabin hafiz"
        },
        {
            "chapter": 50,
            "verse": 33,
            "text": "men hasiye-rrahmane bilgaybi vecae bikalbim munib"
        },
        {
            "chapter": 50,
            "verse": 34,
            "text": "udhuluha biselam. zalike yevmu-lhulud"
        },
        {
            "chapter": 50,
            "verse": 35,
            "text": "lehum ma yesaune fiha veledeyna mezid"
        },
        {
            "chapter": 50,
            "verse": 36,
            "text": "vekem ehlekna kablehum min karnin hum eseddu minhum batsen fenekkabu fi-lbilad. hel mim mehis"
        },
        {
            "chapter": 50,
            "verse": 37,
            "text": "inne fi zalike lezikra limen kane lehu kalbun ev elka-ssem`a vehuve sehid"
        },
        {
            "chapter": 50,
            "verse": 38,
            "text": "velekad halakne-ssemavati vel'arda vema beynehuma fi sitteti eyyam. vema messena mil lugub"
        },
        {
            "chapter": 50,
            "verse": 39,
            "text": "fasbir `ala ma yekulune vesebbih bihamdi rabbike kable tulu`i-ssemsi vekable-lgurub"
        },
        {
            "chapter": 50,
            "verse": 40,
            "text": "vemine-lleyli fesebbihhu veedbara-ssucud"
        },
        {
            "chapter": 50,
            "verse": 41,
            "text": "vestemi` yevme yunadi-lmunadi mim mekanin karib"
        },
        {
            "chapter": 50,
            "verse": 42,
            "text": "yevme yesme`une-ssayhate bilhakk. zalike yevmu-lhuruc"
        },
        {
            "chapter": 50,
            "verse": 43,
            "text": "inna nahnu nuhyi venumitu veileyne-lmesir"
        },
        {
            "chapter": 50,
            "verse": 44,
            "text": "yevme tesekkaku-l'ardu `anhum sira`a. zalike hasrun `aleyna yesir"
        },
        {
            "chapter": 50,
            "verse": 45,
            "text": "nahnu a`lemu bima yekulune vema ente `aleyhim bicebbarin fezekkir bilkur'ani mey yehafu ve`id"
        },
        {
            "chapter": 51,
            "verse": 1,
            "text": "vezzariyati zerva"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "felhamilati vikra"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "felcariyati yusra"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "felmukassimati emra"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "innema tu`adune lesadik"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "veinne-ddine levaki"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "vessemai zati-lhubuk"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "innekum lefi kavlim muhtelif"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "yu'feku `anhu men ufik"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "kutile-lharrasun"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "ellezine hum fi gamratin sahun"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "yes'elune eyyane yevmu-ddin"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "yevme hum `ale-nnari yuftenun"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "zuku fitnetekum. haze-llezi kuntum bihi testa`cilun"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "inne-lmuttekine fi cennativ ve`uyun"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ahizine ma atahum rabbuhum. innehum kanu kable zalike muhsinin"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "kanu kalilem mine-lleyli ma yehce`un"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "vebil'eshari hum yestagfirun"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "vefi emvalihim hakkul lissaili velmahrum"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "vefi-l'ardi ayatul lilmukinin"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "vefi enfusikum. efela tubsirun"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "vefi-ssemai rizkukum vema tu`adun"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "feverabbi-ssemai vel'ardi innehu lehakkum misle ma ennekum tentikun"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "hel etake hadisu dayfi ibrahime-lmukramin"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "iz dehalu `aleyhi fekalu selama. kale selam. kavmum munkerun"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "feraga ila ehlihi fecae bi`iclin semin"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "fekarrabehu ileyhim kale ela te'kulun"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "feevcese minhum hifeh. kalu la tehaf. vebesseruhu bigulamin `alim"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "feakbeleti-mraetuhu fi sarratin fesakket vecheha vekalet `acuzun `akim"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "kalu kezaliki kale rabbuk. innehu huve-lhakimu-l`alim"
        }
    ]
}