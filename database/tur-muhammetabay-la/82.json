{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "ize-ssemau-nfetarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "veize-lkevakibu-nteserat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "veize-lbiharu fuccirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "veize-lkuburu bu`sirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "alimet nefsum ma kaddemet veehharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "ya eyyuhe-l'insanu ma garrake birabbike-lkerim"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "ellezi halekake fesevvake fe`adelek"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "fi eyyi suratim ma sae rakkebek"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "kella bel tukezzibune biddin"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "veinne `aleykum lehafizin"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "kiramen katibin"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "ya`lemune ma tef`alun"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "inne-l'ebrara lefi ne`im"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "veinne-lfuccara lefi cehim"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "yaslevneha yevme-ddin"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "vema hum `anha bigaibin"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "vema edrake ma yevmu-ddin"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "summe ma edrake ma yevmu-ddin"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "yevme la temliku nefsul linefsin sey'a. vel'emru yevmeizil lillah"
        }
    ]
}