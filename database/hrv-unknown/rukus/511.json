{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Nikako! Tako mi Mjeseca"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "I noći kad mine"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "I jutra kad zasjaji"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Zaista je on jedan najveći"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Upozorenje smrtnicima"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Onom od vas ko hoće da napreduje ili zaostane"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Svaka duša je zalog onog što je zaslužila"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Osim drugova zdesna"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "U baščama će se raspitivati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "O zločincima"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Šta vas je u sekar dovelo?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Reći će: “Nismo od musallija bili"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Niti smo siromaha hranili"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "I dangubili smo s dangubama"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "I poricali Dan sudnji"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Dok nam nije došlo zagarantovano.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Zato im neće koristiti posredovanje posrednika"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa šta im je?, od Opomene su odvraćeni"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kao da su oni magarci preplašeni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Pobjegli od lava"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Šta više, svaki čovjek od njih želi da mu se daju listovi rašireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikako! Naprotiv! Ne plaše se Ahireta"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Nikako! Zaista je on Pouka"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Pa ko htjedne imaće ga na umu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "A poučit će se samo ako htjedne Allah. On je Vlasnik bogobojaznosti i Vlasnik oprosta"
        }
    ]
}