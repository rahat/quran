{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji čupaju žestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeći"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad Tresući zatrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijediće ga naredno"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca će Tog dana uzdrhtala biti"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi biće oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: “Hoćemo li mi uistinu u prvobitno stanje vraćeni biti"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: “To je onda povratak poguban.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa to će samo krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je došao hadis o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kad ga zovnu Gospodar njegov u dolini svetoj Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Idi faraonu, zaista je on prevršio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Pa reci: ’Da li bi ti da se očistiš?’"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“I uputiću te Gospodaru tvom, pa da se bojiš.”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Tad mu je pokazao znak veliki"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Pa - porekao je i nije poslušao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Zatim se okrenuo žureći"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Pa sakupio (ljude), te povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Pa rekao: “Ja sam gospodar vaš najuzvišeniji.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Zato ga je Allah dograbio primjernom kaznom drugog i prvog"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Uistinu, u tome ibret za onog ko se boji"
        }
    ]
}