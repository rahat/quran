{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milostivi"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Poučava Kur’anu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Stvara čovjeka"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Uči ga izražavanju"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Sunce i Mjesec su po proračunu"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "I zvijezda i drvo čini sedždu"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I nebo! Uzdigao ga je, i postavio mjerilo"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Da ne prelazite mjeru"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "I uspostavljajte težinu po pravdi i ne zakidajte na vagi"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I Zemlja! Postavio je nju za stvorenja"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Na njoj je voće i palme sa čaškama"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "I zrno sa ljuskom i mirisna biljka"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Stvorio je čovjeka od gline zvečeće kao grnčarija"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "I stvorio je džinna od plamena vatre"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Gospodar dvaju istoka i Gospodar dvaju zapada"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Pustio je mora dva - susreću se"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Između njih je berzeh - ne prelaze"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Izlaze iz njih biser i merdžan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "I Njegova su plovila - naprave u moru kao brda"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Pa koju od blagodati Gospodara svoga oba poričete"
        }
    ]
}