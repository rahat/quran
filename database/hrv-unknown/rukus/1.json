{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Hvala Allahu, Gospodaru svjetova"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Milostivom, Milosrdnom"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Vladaru Dana sudnjeg"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Tebe obožavamo i od Tebe pomoć tražimo"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Uputi nas na put pravi"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Put onih kojima si blagodat darovao"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Ne onih na kojima je srdžba, niti zalutalih"
        }
    ]
}