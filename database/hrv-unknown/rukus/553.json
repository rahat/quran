{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Propale ruke Ebu Lehebove - i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Neće mu koristiti bogatstvo njegovo i šta je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Pržiće se vatrom rasplamsalom"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "I žena njegova, nosačica drva"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Na vratu njenom biće konop od palmina lika"
        }
    ]
}