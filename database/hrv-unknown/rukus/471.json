{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Pa ne! Kunem se položajima zvijezda"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "A uistinu, je to zakletva - kad biste znali - velika"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Uistinu, on je Kur’an plemeniti"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "U Knjizi skrivenoj"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Koju ne dotiču, izuzev očišćeni"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Objava od Gospodara svjetova"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Pa da li vi ovaj hadis prezirete"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "I činite opskrbom svojom da vi poričete"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Pa zašto ne - kad stigne grlu"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "I vi tad budete gledali"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "A Mi smo mu bliže od vas, ali ne vidite"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Pa zašto ne - ako ste nedužni"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Vratite nju, ako ste istiniti"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Pa, ako bude od približenih"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Onda je rewh i miomiris, i Džennet zadovoljstva"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A ako od drugova desne strane bude"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Tad: “Selam tebi” od drugova desne strane"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A ako od poricatelja zabludjelih bude"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Tad je ugošćenje tekućinom vrijućom"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "I prženje džehimom"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Uistinu! Ovo je ta Istina sigurna"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Zato slavi ime Gospodara svog Veličanstvenog"
        }
    ]
}