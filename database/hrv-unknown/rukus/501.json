{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Pa ne! Kunem se onim što vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "I onim što ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Uistinu je on govor Poslanika plemenitog"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "I nije on govor pjesnika. Malo što vjerujete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Niti je govor proroka. Malo čega se sjećate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Objava je od Gospodara svjetova"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A da je slagao na Nas neke riječi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Doista bismo ga zgrabili za desnicu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Potom doista presjekli njegovu aortu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Pa ne biste bili njegovi branitelji, nijedan od vas"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "I uistinu, on je bogobojaznima Opomena"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "A uistinu, Mi znamo da su između vas poricatelji"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "I uistinu, on je žalost za nevjernike"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "I uistinu, on je Istina sigurna"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Zato slavi ime Gospodara svog, Veličanstvenog"
        }
    ]
}