{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kad se događaj dogodi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Neće biti poricanja događanja njegovog"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ponižavajući, uzdižući"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se Zemlja potresom zatrese"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "I smrve brda drobljenjem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "I budu prašina rasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "I budete vrste tri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Pa drugovi desne strane - šta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I drugovi lijeve strane - šta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A prednjaci će biti prednjaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Takvi će približeni biti"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "U baščama uživanja"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnoštvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "A malo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na sofama ukrašenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Naslonjeni na njima, naspramni"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obilaziće oko njih mladići vječni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "S peharima i ibricima i čašom s izvorišta"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Neće dobiti glavobolju od njega, niti se opiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I voćem od čega izaberu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I mesom ptičjim od čega zažele"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I hurije lijepih krupnih očiju"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kao primjerci bisera skrivenog"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Plaća za ono šta su radili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Neće čuti u njemu besmislicu, niti griješnost"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Izuzev govora: “Selam! Selam!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A drugovi desne strane - šta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U lotosovom drveću bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I bananama nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I hladu rasprostrtom"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I vodi tekućoj"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I voću mnogobrojnom"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne odsječenim, niti zabranjenim"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Uistinu! Mi smo ih stvorili stvaranjem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I djevicama ih učinili"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Umilnim vršnjakinjama"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Za drugove desne strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Mnoštvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "I mnoštvo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A drugovi lijeve strane - šta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "U pržećem vjetru i vrijućoj vodi će biti"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "I sjenci dima crnog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ni hladnoj, ni oplemenjujućoj"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Uistinu, oni su prije toga raskošnici bili"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I ustrajavali na prekršaju strašnom"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I govorili: “Zar ćemo kad pomremo i prah i kosti budemo, uistinu mi podignuti biti"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ili preci naši raniji?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: “Uistinu, raniji i kasniji.”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Doista će biti sakupljeni za određeno vrijeme Dana poznatog"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Zatim ćete uistinu vi, o zabludjeli poricatelji"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Sigurno jesti od drveta zekkuma"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Pa ćete njime trbuhe puniti"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Te piti na njega vrijuću vodu"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Ta pićete pijenjem silno žednih"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Ovo će biti ugošćenje njihovo na Dan sudnji"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mi smo vas stvorili, pa zašto ne povjerujete"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Pa jeste li vidjeli šta izbacujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Da li vi to stvarate, ili smo Mi Stvoritelji"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mi smo među vama smrt odredili, i nećemo Mi biti preteknuti"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Da izmijenimo likove vaše i oblikujemo vas u ono šta ne znate"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "A doista znate stvaranje prvo. Pa zašto se ne poučite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Pa vidite li šta sijete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Da li vi dajete da to uzraste, ili smo Mi Uzgajatelji"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Da hoćemo, sigurno bismo ga učiniti kršom, tad biste se počeli čuditi"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Uistinu, mi smo oglobljeni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Šta više, mi smo lišeni.”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Pa vidite li vodu koju pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Da li je vi spuštate iz oblaka, ili smo Mi Spuštaoci"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Da hoćemo, učinili bismo je slanom; pa zašto ne zahvaljujete"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Pa vidite li vatru koju palite"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Da li vi stvarate drvo njeno, ili smo Mi Stvoritelji"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mi smo je učinili podsjetnikom i korišću za stanovnike divljine"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Zato slavi ime Gospodara svog Veličanstvenog"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Pa ne! Kunem se položajima zvijezda"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "A uistinu, je to zakletva - kad biste znali - velika"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Uistinu, on je Kur’an plemeniti"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "U Knjizi skrivenoj"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Koju ne dotiču, izuzev očišćeni"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Objava od Gospodara svjetova"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Pa da li vi ovaj hadis prezirete"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "I činite opskrbom svojom da vi poričete"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Pa zašto ne - kad stigne grlu"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "I vi tad budete gledali"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "A Mi smo mu bliže od vas, ali ne vidite"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Pa zašto ne - ako ste nedužni"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Vratite nju, ako ste istiniti"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Pa, ako bude od približenih"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Onda je rewh i miomiris, i Džennet zadovoljstva"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A ako od drugova desne strane bude"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Tad: “Selam tebi” od drugova desne strane"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A ako od poricatelja zabludjelih bude"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Tad je ugošćenje tekućinom vrijućom"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "I prženje džehimom"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Uistinu! Ovo je ta Istina sigurna"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Zato slavi ime Gospodara svog Veličanstvenog"
        }
    ]
}