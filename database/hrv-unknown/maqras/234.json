{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Namrštio se i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Jer mu je slijepac došao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A šta znaš ti - možda se on čisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ili podsjeća, pa mu koristi Opomena"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A što se tiče onog koji se smatra neovisnim"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ta ti se njime baviš"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "A nije do tebe ako se ne očisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A onaj ko ti dođe žureći"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "I on strahuje"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Pa ti se na njega ne osvrćeš"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Nikako! Uistinu, on je Opomena"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Pa ko hoće imaće ga na umu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Na listovima časnim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Uzvišenim, pročišćenim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "U rukama pisara"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Plemenitih, čestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet bio čovjek! Kako je on nezahvalan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od koje stvari ga stvori"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvara, pa mu razmjer da"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Zatim mu put olakša"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potom ga usmrti, pa ga sahrani"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Zatim, kad htjedne, proživiće ga"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Nikako! Još nije izvršio šta mu je naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pa nek pogleda čovjek hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Da mi lijemo vodu izlivom"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Zatim iscjepkamo zemlju pucanjem"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Pa damo da iznikne iz nje zrno"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "I grožđe i jestivo bilje"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "I masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "I bašče bujne"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "I voće i ispašu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Uživanje za vas i stoku vašu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pa kad dođe glas zaglušujući"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Na Dan kad bude bježao čovjek od brata svog"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "I majke svoje i oca svog"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "I žene svoje i sinova svojih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Svaki čovjek između njih će Tog dana imati stvar koja će ga zaokupiti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Lica će Tog dana biti svijetla"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "I biće lica Tog dana - na njima prašina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Pokriće ih tama"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "To će oni - nevjernici, razvratnici biti"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada se Sunce smota"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "I kad zvijezde kolapsiraju"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "I kad se brda pokrenu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "I kad se steone kamile napuste"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "I kad se zvijeri sakupe"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "I kad se mora potpale"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "I kad se duše spare"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "I kad živo zakopana bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Zbog kojeg grijeha je ubijena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "I kad se listovi rašire"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "I kad se nebo otkrije"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "I kad se džehim raspali"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "I kad se Džennet približi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Znaće duša šta je pripremila"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Pa ne! Kunem se onima koje uzmiču (odstupaju)"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Koje se kreću, koje se prikrivaju (nestaju)"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "I noću, kad odlazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "I jutrom, kad diše"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Uistinu, to je riječ izaslanika plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Posjednika moći, kod Vlasnika Arša cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Slušanog, tamo pouzdanog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I nije drug vaš lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "A uistinu, vidio ga je na horizontu jasnom"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I nije on nad nevidljivim škrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Niti je on govor šejtana prokletog"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Pa kamo idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Zaista je on samo Opomena svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Za onog od vas ko hoće da ide pravo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "A nećete, izuzev šta hoće Allah, Gospodar svjetova"
        }
    ]
}