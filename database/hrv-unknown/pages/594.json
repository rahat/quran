{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Govorit će: “O da sam (što) unaprijed poslao za život svoj!”"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Pa Tog dana neće kazniti kaznom Njegovom niko"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Niti će vezati vezom Njegovom iko"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O dušo smirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Vrati se Gospodaru svom zadovoljna, zadovoljavajuća"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Pa uđi među robove Moje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "I uđi u Džennet Moj"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A ti si slobodan u gradu ovom"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditeljem i šta je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Uistinu, stvorili smo čovjeka (da) bude u trpnji"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Računa li da moć nad njim nema niko"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govori: “Uništio sam bogatstvo mnogo.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Računa li da ga ne vidi niko"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo načinili oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "I jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "I uputili ga stazama dvjema"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa nije pokušao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A šta znaš ti šta je uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Oslobođenje roba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ili hranjenje u danu gladi"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Siročeta bliskog"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ili siromaha u prašini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Zatim biti od onih koji vjeruju i preporučuju jedni drugima sabr, i preporučuju jedni drugima merhamet"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Takvi će biti drugovi desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete Naše, oni će biti drugovi lijeve strane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad njima će vatra biti zatvorena"
        }
    ]
}