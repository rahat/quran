{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obilaziće oko njih mladići vječni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "S peharima i ibricima i čašom s izvorišta"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Neće dobiti glavobolju od njega, niti se opiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I voćem od čega izaberu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I mesom ptičjim od čega zažele"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I hurije lijepih krupnih očiju"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kao primjerci bisera skrivenog"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Plaća za ono šta su radili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Neće čuti u njemu besmislicu, niti griješnost"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Izuzev govora: “Selam! Selam!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A drugovi desne strane - šta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U lotosovom drveću bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I bananama nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I hladu rasprostrtom"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I vodi tekućoj"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I voću mnogobrojnom"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne odsječenim, niti zabranjenim"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Uistinu! Mi smo ih stvorili stvaranjem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I djevicama ih učinili"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Umilnim vršnjakinjama"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Za drugove desne strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Mnoštvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "I mnoštvo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A drugovi lijeve strane - šta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "U pržećem vjetru i vrijućoj vodi će biti"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "I sjenci dima crnog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ni hladnoj, ni oplemenjujućoj"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Uistinu, oni su prije toga raskošnici bili"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I ustrajavali na prekršaju strašnom"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I govorili: “Zar ćemo kad pomremo i prah i kosti budemo, uistinu mi podignuti biti"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ili preci naši raniji?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: “Uistinu, raniji i kasniji.”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Doista će biti sakupljeni za određeno vrijeme Dana poznatog"
        }
    ]
}