{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "A Ja se kunem onim sto vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i onim sto ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "on – Kur’an doista je govor objavljen plemenitom Poslaniku"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "a nije govor nikakva pjesnika – kako vi malo vjerujete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "I nisu rijeci nikakva proroka – kako vi malo razmisljate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Objava je on od Gospodara svjetova"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A da je on o Nama kojekakve rijeci iznosio"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Mi bismo ga desnicom dohvatili"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a onda mu zilu kucavicu presjekli"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "i niko ga između vas ne bi mogao od toga odbraniti"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Zaista je on opomena bogobojaznima"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "a Mi, sigurno, znamo da ce među vama poricatelja biti"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "I on je, zbilja, jad nevjernicima"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "a on je, doista, Istina sama"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Zato ti slavi i velicaj ime Gospodara svoga, Velicanstvenoga"
        }
    ]
}