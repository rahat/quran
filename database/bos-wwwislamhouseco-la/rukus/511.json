{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "I tako Mi Mjeseca"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i noci kada mine"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zore kada svane"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "to je, zaista, jedna od najvecih nevolja"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ljudima je opomena"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "onome između vas koji hoce naprijed ici ili zaostati"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Svaka dusa zalog je onoga sto je stekla"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osim onih na desnoj strani"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "oni ce se u dzennetskim bascama raspitivati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o prestupnicima"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Sta vas je u Sekar dovelo?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "“Nismo”, reci ce, “bili od onih koji su klanjali"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "niti smo siromahe hranili"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "I u besposlice smo se sa besposlenjacima upustali"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i Sudnji dan smo poricali"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "sve dok nam smrt nije dosla.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Njima posredovanje posrednika nece koristiti"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa zasto se oni okrecu od opomene"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kao da su divlji magarci preplaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "pobjegli od lavova"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Naprotiv, svaki bi covjek među njima htio da mu se daju listovi rasireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikada, jer oni se ahireta ne plase"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Uistinu! Kur’an je opomena"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "i ko hoce, na umu ce ga imati"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "a na umu ce ga imati samo ako Allah bude htio, On je jedini dostojan da Ga se boje i dostojan je da oprasta"
        }
    ]
}