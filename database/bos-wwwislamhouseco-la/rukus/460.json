{
    "rukus": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Tako Mi zvijezde kad zapada"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "vas drug nije s Pravog puta skrenuo i nije zalutao"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "On ne govori po hiru svome"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "To je samo Objava koja mu se obznanjuje"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Uci ga jedan velikih moci"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "snazni, koji se uspravio u liku svome"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "na obzorju najvisem"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Zatim se priblizio, pa nadnio"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "blizu koliko dva luka ili blize"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "i objavio robu Njegovu ono sto je objavio"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Srce nije poreklo ono sto je vidio"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Pa zasto se prepirete s njim o onom sto je vidio"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "On ga je i drugi put vidio"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Kod Sidretu-l-Muntehaa"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "gdje je dzennetsko utociste"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Kad je Sidru prekrilo ono sto ju je prekrilo"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "pogled mu nije skrenuo i nije prekoracio"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Vidio je najvelicanstvenije znakove svoga Gospodara"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Vidjeste li Lata i Uzzaa"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "I Menata, treceg, posljednjeg od njih"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Zar za vas da su sinovi, a za Njega kceri"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "To bi tada bila podjela nepravedna"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "To su samo imena koja ste im vi i preci vasi nadjenuli; Allah o njima"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "I zar da covjek sve sto pozeli dobije"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Pa, Allahu pripada i prvi i drugi svijet"
        }
    ]
}