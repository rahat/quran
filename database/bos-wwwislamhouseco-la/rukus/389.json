{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "I Musau i Harunu milost smo ukazali…"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "pa i njih i narod njihov nevolje velike spasili…"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "i pomogli im da pobijede…"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "i Knjigu im jasnu dali…"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "i obojicu na Pravi put uputili…"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "i u narastajima im kasnijim spomen sacuvali"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Nek su u miru Musa i Harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Eto tako Mi nagrađujemo one koji dobra djela cine"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "a njih dvojica su, uistinu, bili robovi Nasi, vjernici"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "I Il’jas je bio poslanik"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Kad on rece narodu svom: “Zar se ne bojite?…"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Sto se Ba'lu molite, a najljepseg Stvoritelja ostavljate…"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha, Gospodara svog i Gospodara vasih predaka drevnih?”…"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "oni ga lascem nazvase i zato ce, sigurno, u vatru svi oni biti baceni…"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "A Allahovi iskreni robovi…"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "i u narastajima mu kasnijim spomen sacuvali…"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Neka je u miru Il’jas"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Eto tako Mi nagrađujemo one koji dobra djela cine"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "On je bio rob Nas, vjernik…"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "I Lut je bio poslanik"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Mi smo njega i citavu njegovu porodicu spasili…"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "osim starice, ona je nastradala s onima koji su nastradali"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "A ostale smo unistili"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "I vi pored nastambi njihovih prolazite i ujutro…"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "i nocu, pa zasto se ne urazumite"
        }
    ]
}