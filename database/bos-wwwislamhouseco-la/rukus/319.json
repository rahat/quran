{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "I faraon posla po gradovima sakupljace…"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "“Ovih je zaista malo…"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "i rasrdili su nas…"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "a mi smo svi budni!”"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "I Mi ih izvedosmo iz vrtova i rijeka…"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "iz riznica i dvoraca divnih"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Eto tako je bilo, i Mi dadosmo da to naslijede sinovi Israilovi"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "I oni ih, kad se sunce rađalo, sustigose"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Pa kad jedni druge ugledase, drugovi Musaovi povikase: “Samo sto nas nisu stigli!”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "“Nece!”, rece on, “Gospodar moj je sa mnom, On ce mi put pokazati.”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "I Mi objavismo Musau: “Udari stapom svojim po moru!”, i ono se rastavi i svaki bok njegov bijase kao veliko brdo…"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "i Mi onda tamo one druge priblizismo…"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "a Musaa i sve one koji bijahu s njime spasismo…"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "i one druge potopismo"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "To je doista dokaz, ali vecina njih ne vjeruje"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "A Gospodar je tvoj zaista silan i milostiv"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "I kazi im vijest o Ibrahimu…"
        }
    ]
}