{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O cemu oni jedni druge pitaju"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O Vijesti velikoj"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "o kojoj se oni razilaze"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Tako ne treba, oni ce saznati sigurno"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "I opet, tako ne treba, oni ce saznati sigurno"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Zar Zemlju posteljom nismo ucinili"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "i planine kao klinove"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "i vas kao parove stvorili"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i vas san smirajem ucinili"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "i noc odjecom ucinili"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "i dan za privređivanje odredili"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "i iznad vas sedam cvrstih sazdali"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "i svjetiljku plamtecu postavili"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Mi iz kisnih oblaka vodu obilnu spustamo"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "da njome zito i bilje izvedemo"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i basce guste"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Dan suda je, zaista, vec određen"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Dan kada ce u rog biti puhnuto, pa cete vi, sve skupina po skupina,dolaziti"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "i nebo ce biti otvoreno i mnogo ce kapija imati"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "i planine ce pokrenute biti i priviđenje ce postati"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Dzehennem ce, doista, zasjeda biti"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "obijesnima mjesto povratka"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "u kojem ce zauvijek ostati"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "u njemu nikakve svjezine nece osjetiti, ni pica okusiti"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "osim vrele vode i kapljevine"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "kazne prikladne"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Zaista se oni nisu nadali polaganju racuna"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i potpuno laznim smatrali su Nase ajete i znakove"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "a Mi smo sve pobrojali i zapisali"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "“Pa zato kusajte, a Mi cemo vam patnju samo povecati!”"
        }
    ]
}