{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako Mi onih koji cupaju grubo"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "i onih koji vade blago"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "i onih koji plove brzo"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "i onih koji bas prednjace"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i onih koji odredbe uređuju"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "na Dan kada ono sto potresa potrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "za kojim ce doci sljedeci"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "srca taj dan bit ce uplasena"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a pogledi njihovi ponizni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore oni: “Zar cemo, zaista, u kaburovima biti vraceni u zivot"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad truhle kosti postanemo?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "I jos kazu: “E, to je, dakle, stetan povratak.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "A bit ce to samo naredba jedna"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "a oni vec na povrsini Zemlje"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li je doprla do tebe vijest o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "kad ga je Gospodar njegov u blagoslovljenoj, cistoj dolini Tuva zovnuo"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Idi faraonu, on se, doista, osilio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "i reci: ‘Da li bi ti da se ocistis"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "da te Gospodaru tvome uputim, pa da Ga se bojis?’”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "I onda mu je najveci znak pokazao"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "ali je on porekao i nije poslusao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "vec se okrenuo i potrudio"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "i sabrao i povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "“Ja sam gospodar vas najveci!”, on je rekao"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "I Allah ga je na drugom, a i na ovom svijetu kaznio"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "U tome ima pouka za svakoga koji se bude Allaha bojao"
        }
    ]
}