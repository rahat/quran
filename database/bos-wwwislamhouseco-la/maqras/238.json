{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "a ti si u ovom gradu oslobođen ogranicenja"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i roditeljem i onim koga je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Covjeka smo stvorili da je u naporu"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li on da mu niko nista ne moze"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "“Utrosio sam imetak nagomilani!”, govori"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Zar misli da ga niko vidio nije"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo dali oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i dobro i zlo mu objasnili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa zasto nije savladao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A sta ti mislis: sta je to uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Roba ropstva osloboditi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili, u vrijeme gladi, nahraniti"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "siroce bliska roda"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili ubogoga nevoljnika"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "A potom da je bio od onih koji vjeruju, koji jedni drugima strpljivost"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "oni ce biti – oni s desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete i dokaze Nase, oni ce biti – oni s lijeve"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "iznad njih ce vatra zatvorena biti"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i Mjeseca kada ga prati"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dana kada ga vidljivim ucini"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noci kada ga zakloni"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i neba i Onoga Koji ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i Zemlje i Onoga Koji je rasprostrije"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i duse i Onoga Koji je ispravnom stvori"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "pa je nadahnu da zna sta je razvrat, a sta bogobojaznost"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "uspio je onaj ko je ocisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a izgubio je onaj ko je zapostavi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Narod Semud je zbog obijesti svoje poricao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kad se jedan nesretnik između njih podigao"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahov poslanik im je doviknuo: “Pazite na Allahovu devu i njen red"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ali su ga u laz utjerali, pa su je zaklali – i Gospodar njihov ih je, zbog"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ne bojeci se kakve ce posljedice"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noci kada sve zakrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dana kad svane"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i Onoga Koji musko i zensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vas trud je, zaista, razlicit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "A onome koji udjeljuje i bogobojazan je"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i ono najljepse smatra istinitim"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "njemu cemo ono sto je lahko dostupnim uciniti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "a onome koji tvrdici i osjeca se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i ono najljepse smatra laznim"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "njemu cemo ono sto je tegobno dostupnim uciniti"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i imetak njegov mu, kad se strovali, nece koristiti"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mi smo duzni ukazati na Pravi put"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "i jedino Nama pripada i ahiret i ovaj svijet"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Zato vas upozoravam na razbuktalu vatru"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "u koju ce uci samo najveci nesretnik"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "onaj koji bude poricao i okretao se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "a od nje ce odstranjen biti onaj najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "onaj koji bude dio imetka svoga udjeljivao da bi se ocistio"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ne ocekujuci da mu se zahvalnoscu uzvrati"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "vec jedino zudeci za Licem Gospodara svoga Svevisnjeg"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "i on ce, zbilja, zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noci kad se utisa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret je, zaista, bolji za tebe od ovog svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj ce tebi, sigurno, dati, pa ces zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te sirocetom ne nađe, pa ti utociste dade"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i zalutalim te nađe, pa te uputi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromahom te nađe, pa te bogatim ucini"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato, siroce ne okahari"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "i onoga ko trazi ne odbij"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}