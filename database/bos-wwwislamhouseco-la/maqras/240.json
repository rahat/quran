{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "A zar on ne zna da ce, kada kaburovi budu prevrnuti s onim sto je u njima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i kada iziđe na vidjelo ono sto je u srcima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Gospodar njihov Dan taj, sigurno, sve o njima znati"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar zastrasujuci"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Sta je Udar zastrasujuci"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I sta ti znas sta je Udar zastrasujuci"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Dan taj ljudi ce biti kao leptiri rastrkani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a planine kao sarena vuna iscupana"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onaj cija dobra djela budu teska"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "u ugodnom zivotu ce zivjeti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onaj cija dobra djela budu lahka"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "boraviste ce mu bezdan biti"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A znas li ti sta ce to biti"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Vatra uzarena"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas zgrtanje imovine"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok grobove ne naselite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne treba tako, saznat cete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I jos jednom, ne treba tako! Saznat cete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, kad biste vi znali pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Oganj cete, zaista, vidjeti"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I jos jednom, doista cete ga vidjeti sasvim jasno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim cete Dan taj o blagodatima biti pitani sigurno"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Tako mi vremena"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "doista je covjek na gubitku"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "samo nisu oni koji vjeruju i dobra djela cine, i koji jedni drugima Istinu"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Tesko svakom klevetniku podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koji blago gomila i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i misli da ce ga blago njegovo besmrtnim uciniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A ne treba tako! On ce, sigurno, biti bacen u Hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znas li ti sta je Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova razbuktala"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koja ce do srca dopirati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ona ce nad njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "plamenim izduzenim stubovima"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Zar nisi cuo sta je s vlasnicima slona Gospodar tvoj uradio"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Zar lukavstvo njihovo nije upropastio"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "i na njih jata ptica poslao"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "koje su na njih grumenje od gline pecene bacale"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "pa ih kao izjedeno lisce ucinio"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog navike Kurejsija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "navike njihove da zimi i ljeti putuju"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "neka oni Gospodaru ove Kuce ibadet cine"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Koji ih gladne hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Sta mislis o onome koji Sud porice"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroce"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah – ne podstice"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa tesko klanjacima"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onima koji su nemarni prema svome namazu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo ljudima pokazuju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i cak potrepstine koje se na posudbu daju oni ne daju"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mi smo ti, uistinu, Kevser dali"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "pa klanjaj Gospodaru svome i kurban kolji"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "onaj koji tebe mrzi – sigurno ce on bez spomena ostati"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: “O vi nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ja ne obozavam ono sto vi obozavate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "niti ste vi obozavaoci Onoga Koga ja obozavam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "niti sam ja obozavalac onoga sto vi obozavate"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "niti ste vi obozavaoci Onoga Koga ja obozavam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "vama vasa vjera, a meni moja"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kada Allahova pomoc i pobjeda dođu"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "i vidis ljude kako u skupinama u Allahovu vjeru ulaze"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ti slavi i velicaj Gospodara svoga hvaleci Ga i moli Ga da ti oprosti, On"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Neka propadnu ruke Ebu Lehebove, i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nece mu biti od koristi imetak njegov, a ni ono sto je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "uci ce on, sigurno, u vatru rasplamsalu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zena njegova sto drvlje nosi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "o vratu njenu bit ce uze od licine usukane"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Reci: “On je Allah – Jedan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah je Es-Samed"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nije rodio i rođen nije"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i niko Mu ravan nije!”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: “Utjecem se Gospodaru svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "od zla onoga sto On stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i od zla mrkle noci kada razastre tmine"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i od zla onih koje u cvorove pusu"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i od zla zavidnika kad zavidi"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: “Utjecem se Gospodaru ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladaru ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogu ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zla sejtana – koji nanosi zle misli pa se skrije"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koji zle misli unosi svijetu u grudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "od dzina i od ljudi"
        }
    ]
}