{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Namrstio se i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "zato sto je slijepi covjek njemu prisao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A sta ti znas, mozda ce se on ocistiti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili opomenuti, pa da mu opomena koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A sto se tice onoga koji je bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti se njemu okreces"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a ti nisi odgovoran ako se on ne ocisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A sto se tice onoga koji ti je dosao zureci"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a on se boji"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti njega zanemarujes"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne cini tako! Zbilja je ovo opomena"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pa ko hoce, imat ce Ga na umu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "na listovima je cijenjenim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uzvisenim, cistim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "u rukama izaslanika"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "casnih, cestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet neka je covjek! Koliko je samo njegovo poricanje"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od cega ga On stvara"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvara, pa mu određuje"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "I put mu dostupnim ucini"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "zatim mu zivot oduzme i ucini da bude sahranjen"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "i poslije ce ga, kada On bude htio, ozivjeti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Uistinu! On jos nije ispunio ono sto mu je On naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka covjek pogleda u hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mi obilnu kisu prolijevamo"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "zatim zemlju pukotinama cijepamo"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i cinimo da iz nje zito izrasta"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grozđe i povrce"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i basce guste"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i voce i trave"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na uzivanje vama i stoci vasoj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kad dođe glas zaglusujuci"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "na Dan kada ce covjek od brata svoga pobjeci"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i od majke svoje, i od oca svoga"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i od druge svoje i sinova svojih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Dan taj svaki covjek ce se samo o sebi brinuti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "neka lica Dan taj bit ce blistava"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a na nekim licima Dan taj bit ce prasina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tama ce ih prekrivati"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to ce, zbilja, nevjernici, razvratnici biti"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada Sunce bude skupljeno i izgubi sjaj"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kada zvijezde rastrkane popadaju"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kada planine budu pokrenute i smrvljene"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kada steone kamile budu ostavljene"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kada divlje zivotinje budu sabrane"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kada mora budu raspaljena"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kada duse budu sparene"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kada djevojcica koja je ziva zakopana bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "zbog kojeg je grijeha ubijena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kada listovi budu rasireni"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kada nebo bude uklonjeno"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "kada Oganj bude razbuktan"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kada Dzennet bude priblizen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "svako ce saznati ono sto je pripremio"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "I ne! Kunem se onim sto se skriva"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "sto se krece i sklanja"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i noci kad dolazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i zorom kad dise"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "On – Kur’an je, zaista, kazivanje Izaslanika Plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Mocnog, kod Gospodara Arsa, cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Kome se drugi tamo pokoravaju, povjerljivog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "A drug vas nije lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "on ga je na obzorju jasnom vidio"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i, kada je u pitanju ono sto je culima nedokucivo, on nije skrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "i on nije prokletog sejtana govor"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "pa kuda onda idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "On je samo opomena svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "onome od vas koji hoce da je na Pravom putu"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "a vi ne mozete nista htjeti ako to Allah, Gospodar svjetova, nece"
        }
    ]
}