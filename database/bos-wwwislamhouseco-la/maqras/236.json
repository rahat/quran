{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Kada se nebo rascijepi"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "i poslusa Gospodara svoga, a ono ce to duzno biti"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "i kada Zemlja bude rastegnuta"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "i izbaci ono sto je u njoj, i potpuno se isprazni"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "i poslusa Gospodara svoga, a ona ce to duzno biti"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O covjece, ti se trudis iduci svome Gospodaru, pa ces svoj trud naci"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Onaj kome bude knjiga njegova u desnu ruku njegovu data"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "na lahak obracun naici ce"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "i svojima ce se radostan vratiti"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "a onaj kome bude knjiga njegova iza leđa njegovih data"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "propast ce prizivati"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "i u Ognju ce gorjeti"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Zaista je on sa porodicom svojom radostan bio"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "i mislio da se nikada nece vratiti"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "a hoce! Gospodar njegov o njemu, zaista, sve zna"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Ali ne! Kunem se rumenilom vecernjim"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "i noci, i onim sto ona sakuplja"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "i Mjesecom kada se ispuni"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "vi cete, sigurno, iz stanja u stanje prelaziti"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Pa sta im je, da ne vjeruju"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "i kad im se Kur’an uci na tlo licem ne padaju"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Naprotiv, oni koji ne vjeruju poricu"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "a Allah najbolje zna sta oni u sebi kriju"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "zato im navijesti patnju bolnu"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "A oni koji budu vjerovali i dobra djela cinili, imat ce nagradu neprekidnu"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Tako mi neba sa zvijezdama velikim"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "i Dana vec obecanog"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "i svjedoka i posvjedocenog"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "prokleti bili vlasnici rovova"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "i vatre s gorivom"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "kada su oko nje sjedili"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "i bili svjedoci onoga sto su vjernicima radili"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "A svetili su im se samo zato sto su u Allaha, Silnoga i Hvaljenoga"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Cija je vlast i na nebesima i na Zemlji, a Allah je svemu Svjedok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Zaista, oni koji su stavljali na kusnju vjernike i vjernice, a zatim se nisu"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Zaista, onima koji su vjerovali i radili dobra djela pripadaju basce"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Kazne Gospodara tvoga zaista su zestoke"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "On ni iz cega stvara, i ponovo ce to uciniti"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On je Oprostitelj grijeha, Onaj Koji puno voli"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Gospodar Arsa, Velicanstveni"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "On radi sta hoce"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Da li je do tebe doprla vijest o vojskama"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o faraonu i Semudu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Ali, ovi koji ne vjeruju stalno poricu"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "a Allah je iza njih, okruzuje ih"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Naprotiv! Ovo je Kur’an velicanstveni"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "na Ploci pomno cuvanoj"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i nocnog posjetioca"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znas li ti sta je to nocni posjetilac"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodiruca"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema covjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek’ covjek pogleda od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine koja izbija u mlazu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kicme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On ga je, zaista, kadar ponovno stvoriti"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onaj dan kada tajne budu otkrivene"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada covjek ni snage ni pomagaca nece imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba punog kise koja se vraca"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i Zemlje pune raspuknuca"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur’an je, doista, govor koji rastavlja Istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni, zbilja, prave zamke"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "pa i Ja njima zamke pravim"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima jos vremena daj, jos koji trenutak ih ostavi"
        }
    ]
}