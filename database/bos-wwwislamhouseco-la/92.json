{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noci kada sve zakrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dana kad svane"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i Onoga Koji musko i zensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vas trud je, zaista, razlicit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "A onome koji udjeljuje i bogobojazan je"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i ono najljepse smatra istinitim"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "njemu cemo ono sto je lahko dostupnim uciniti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "a onome koji tvrdici i osjeca se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i ono najljepse smatra laznim"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "njemu cemo ono sto je tegobno dostupnim uciniti"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i imetak njegov mu, kad se strovali, nece koristiti"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mi smo duzni ukazati na Pravi put"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "i jedino Nama pripada i ahiret i ovaj svijet"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Zato vas upozoravam na razbuktalu vatru"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "u koju ce uci samo najveci nesretnik"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "onaj koji bude poricao i okretao se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "a od nje ce odstranjen biti onaj najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "onaj koji bude dio imetka svoga udjeljivao da bi se ocistio"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ne ocekujuci da mu se zahvalnoscu uzvrati"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "vec jedino zudeci za Licem Gospodara svoga Svevisnjeg"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "i on ce, zbilja, zadovoljan biti"
        }
    ]
}