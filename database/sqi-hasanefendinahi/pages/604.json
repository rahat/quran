{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Thuaj: “Perëndia është Një"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Perëndisë i drejtohen për çdo gjë"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "(Ai) nuk ka lindur prej ndokujt, as nuk ka lindur kë"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "dhe askush nuk i gjason Atij!”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Thuaj: “I lutem dhe mbështetem te Zoti i agimit"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "që të më mbrojë nga sherri i të gjitha krijesave"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "dhe prej sherrit të natës kur kaplon terri"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "dhe prej sherrit të falltorëve që fryejnë në nyje (bëjnë magji)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "dhe nga sherri i ziliqarit kur e shfaq zilinë!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “I lutem dhe i mbështetem Zotit të njerëzve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundimtarit të njerëzve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Perëndisë së njerëzve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "nga sherri i djallit, bezdisës"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "i cili fshehurazi hedh dyshime në shpirtërat e njerëzve –"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "prej xhindëve dhe prej njerëzve!”"
        }
    ]
}