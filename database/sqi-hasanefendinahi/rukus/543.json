{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Goditja e tmerrshme (Kijameti)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ç’është Goditja e tmerrshme"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Kush të tregon ty për Goditjen e tmerrshme"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Njerëzit, atë Ditë do të jenë të shpërndarë si fluturat"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "kurse malet – si leshi i shprishur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Atij (njeriut), veprat e mira të të cilit rëndojnë në peshojë"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ai do të jetë në jetën e kënaqur (në jetën e përhershme)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "e atij (njeriu), veprat e mira të të cilit nuk rëndojnë në peshojë"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "strehimi i tij do të jetë gremina e Skëterrës"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "E, kush të tregon ty se ç’është gremina e Skëterrës"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Ajo është) zjarr i vrullshëm"
        }
    ]
}