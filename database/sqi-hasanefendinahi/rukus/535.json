{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Betohem në mëngjesin"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "dhe natën kur terrohet e qetësohet –"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Zoti yt, nuk të ka braktisur e as nuk të ka urrejtur"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Me të vërtetë, jeta e ardhshme për ty është më e mirë se kjo jetë"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "e, me të vërtetë, Zoti yt do të japi ty, e ti do të jesh i kënaqur"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk ishe ti bonja, e Ai të mori në strehim"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "dhe të gjeti të humbur (të paudhëzuar), e të udhëzoi me Shpalljen e Kur’anit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "dhe ishe i varfër e të bëri të pasur"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Mos e nënçmo bonjakun"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "e lypsin mos e refuzo me ashpërsi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "dhe dhuntitë e Zotit tënd, shprehi"
        }
    ]
}