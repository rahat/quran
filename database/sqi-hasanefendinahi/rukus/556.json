{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “I lutem dhe i mbështetem Zotit të njerëzve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundimtarit të njerëzve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Perëndisë së njerëzve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "nga sherri i djallit, bezdisës"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "i cili fshehurazi hedh dyshime në shpirtërat e njerëzve –"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "prej xhindëve dhe prej njerëzve!”"
        }
    ]
}