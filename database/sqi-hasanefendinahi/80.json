{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai (Muhammedi) u mrrol e u kthye (me fytyrë)"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ngase i erdhi pranë i verbëri"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E kush të dëfton ty – ndoshta ai dëshiron të pastrohet (nga mëkatet)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ose të këshillohet, e t’i sjell dobi atij këshillimi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "e atij që ësthë i pasur (e prezentohet si i panevojshëm për të besuar)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "atij po i kthehesh"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "sepse, ti nuk ke përgjegjësi, nëse ai nuk don të pastrohet (të besojë)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "e atij që të vjen ty duke nxituar (për këshillim)"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "dhe ndien drojë"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "e ti atë nuk po e merr në konsiderim"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Mos vepro ashtu! Ato janë këshilla"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "e, kush të dojë, le të mësojë"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "në faqet e (Kur’anit të) çmueshme"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "të lartësuara (të madhërueshme), të pastra"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "me duart e shkruesve (engjëjve)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "të ndershëm dhe të drejtë"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mallkuar qoftë njeriu! Sa jomirënjohës që është ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Prej çkahjes e krijoi Ai (Perëndia) atë (njeriun)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Prej pikës së farës e krijoi dhe përgatiti për të çdo gjë të dobishme"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "dhe pastaj ia lehtësoi rrugën (e lindjes)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pas kësaj, (Ai) ia caktoi vdekjen dhe bëri që të varroset"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "dhe, mandej kur të dojë Ai, e ringjallë"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Në të vërtetë! Ai – njeriu, ende nuk i ka plotësuar ato që i ka urdhëruar Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Le të shikojë njeriu në ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ne kemi derdhur ujëra (shira) të bollshme"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pastaj e kemi plasaritur tokën me çarje"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "dhe bëjmë që nga ajo të rriten drithërat"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "dhe rrushi e perimet"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "dhe ulliri e palma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "dhe kopshtet e dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "dhe pëmët e barishtet"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "për ju dhe bagëtinë tuaj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "E, kur të arrijë zëri i madh shurdhues"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "në Ditën kur njeriu ikën nga vëllau i tij"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "dhe nga nëna e babai i tij"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "dhe nga bashkëshortja e fëmijët e tij"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "atë Ditë, çdonjëri është i preokupuar me gjendjen e vetvetes"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "disa fytyra atë Ditë, shëndrisin"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "të qeshura, të gëzuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "kurse disa fytyra, atë Ditë, pluhërosen"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "errësira do t’i kaplojë"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "këta janë jobesimtarët, të korruptuarit"
        }
    ]
}