{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "(Naħilfu) fuq iz-zmien"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "lil-bniedem tabilħaqq jinsab f'telfien"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ħlief dawk li jemmnu u jagħmlu l-gid, u jwissu lil-xulxin għas-sewwa, u jwissu lil xulxin għas-sabarl"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Kastig kbir għal kulmin iħobb iqasqas u jaqla' fuq ħaddieħor"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "(tu għal) dak li jigma' l-gid f'materjali) u jgħoddu (izda ma jonfqux fil-karita)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Huwa jaħseb li gidu se jzommu ħaj għal dejjem"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Le (mhux hekk)I Tabilħaqq se jintefa' fin-Nar qerried"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "U x'inhu li jgħarrfek (Muħammad) x'inhu n-Nar qerried"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nar Alla, imkebbes"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "lijasal (bil-ħruq tiegħu) sal-qlub"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Huwa tabilħaqq jagħlaq fuqhom (ur ma jħalli xejn jgħaddi minnu)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(nirien) f'kolonni twal"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Mela ma rajtx (Muħammad) x'għamel Sidek ma' nies l- iljunfant (li rħewlha lejn Mekka biex jaħbtu għaliha)"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Mela ma gabx fix-xejn in-nasba tagħhom"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "ubagħat fuqhom tajr fi qtajja' kbar"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "iwaddbulhom gebel ta' fuħħar iebes"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "u (Sidek) għamilhom bħal tiben mikul"
        }
    ]
}