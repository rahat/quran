{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Majinxtewiex fih għajr l-agħar bniedem"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "(ligiddeb fis-sewwa) u ma tax kas"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "U se jitwarrab minnu l-aktar gust"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "lijagħti gidu (biex) jissaffa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ulejn ħadd ma kien obbligat ta' xi pjacir li jistħoqq jitħallas"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "ħliefi jixtieq Wicc Sidu, l-Ogħla"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Huwa tabilħaqq jithenna (fil-Gennajl)"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "(Naħilfur) fuq id-dawl ta' l-għodwa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ufuq il-lejl ħiemed u mudlam"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Matelqekx Sidek (Muħammad), u (lilek) ma bagħadx"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Tabilħaqq li l-Aħħar (ħajja) hija aħjar għalik mill-ewwel"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "uSidek tabilħaqq se jagħtik, u inti tithenna"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "(Jaqaw ma sabekx iltim u ta/k) kenn"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "usabek mitluf u wriek it-triq"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "usabek fil-ħtiega u ta(k) il-gid"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Melalill-iltim la taħqarx"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ulit-tallab la ccanfarx u la tkeccix"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ul-grazzja ta' Sidek xandarhal"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Jaqaw ma wessajnilekx sidrek (Muħammad, bit-twemmin ti bit-tmexxija t-tajba)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "u warrabna minnek it-toqol tiegħek (iera ħfirnielek in- nuqqasijiet tiegħek)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "litaqqal dahrek"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "u għollejnielek gieħek"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Tabilħaqq li mas-saram jigi l-farag"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "tassew, mas-saram jigi l-faragt"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "U meta ttemm (it-tixrid tal-kelma), ħabrek (fil-qima ta' Alla)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ugħal Sidek ixxennaq"
        }
    ]
}