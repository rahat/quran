        {
            "chapter": 112,
            "verse": 1,
            "text": "Għid (Muħammad): Huwa Alla wieħed"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allalilejh kulħadd idur fil-ħtiega"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "lajiledu lanqas jitwieled"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ubħalu ma għandu lil-ħaddi"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Għid (Muħammad): Nistkenn f'Sid is-sebħ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "mill-ħazen ta' dak li ħalaq"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mill-ħazen ta' dlam il-lejl meta jinfirex"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mill-ħazen ta' dawk (is-sħaħar) li jonfħu fl-għoqod"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "umill-ħazen ta' l-għajjur meta jgħir"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Għid (Muħammad): Nistkenn f'Sid in-nies"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sultan in-nies"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Alla tan-nies"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "mill-ħazen tal-fesfies li (wara li jfes/es) dlonk jinħeba"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "lijfesfes fi qlub in-nies"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(tfesfis gej) mill-ginn u min-nies (qarrieqa)"
        }
