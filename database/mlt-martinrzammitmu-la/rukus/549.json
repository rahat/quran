{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Rajt (Muħammad) lil dak li jgiddeb il-Gudizzju"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Huwa dak li jaħqar lill-iltim"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "uma jħeggigx (lill-oħrajn) biex jitimgħu l-fqir"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Mela kastig kbir għal dawk (ta' wicc b'ieħor) li (taparsi) jitolbu"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "illi ma jagħtux kas tat-talb tagħhom"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "dawklijħobbu jidhru (u jagħmlu kollox għal għajnejn in- nies)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "jicħdu l-karita"
        }
    ]
}