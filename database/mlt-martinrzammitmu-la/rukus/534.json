{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "(Naħilfu) fuq il-lejl meta jgħatti (id-dawl bid-dalma tiegħu)"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "ufuq in-nhar meta jinkixef (u jxerred id-dawl tiegħu)"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "ufuq Min ħalaq lir-ragel u lill-mara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "lir-tħabrik tagħkom huwa tabilħaqq ta' bosta għamliet"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Melamin jagħti (minn gidu) u jibza' minn Alla"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "ujemmen fl-aħjar (ħlas fil-Genna)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "senħaffulu (il-mixja) lejn triq il-gid (kotran fil-Genna)"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Izda min jixxaħħaħ u jaħseb li ma għandux bzonn /li/ Alla"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "ujgiddeb l-aħjar (ħlas fil-Genna)"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Aħna senħaffulu (il-mixja) lejn it-triq l-iebsa"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Majiswilux gidu meta jinqered (fl-/ 7/e 77i)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Tabilħaqq f'idejna t-tmexxija t-tajba"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "u tagħna, tabilħaqq, l-Aħħar (Ħajja) u l-ewwel"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Għalhekk wissejtkom dwar Nar iħeggeg"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Majinxtewiex fih għajr l-agħar bniedem"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "(ligiddeb fis-sewwa) u ma tax kas"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "U se jitwarrab minnu l-aktar gust"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "lijagħti gidu (biex) jissaffa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ulejn ħadd ma kien obbligat ta' xi pjacir li jistħoqq jitħallas"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "ħliefi jixtieq Wicc Sidu, l-Ogħla"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Huwa tabilħaqq jithenna (fil-Gennajl)"
        }
    ]
}