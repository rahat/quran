{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Meta l-art tithezzez bit-thezziza (qawwija) tagħha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "u l-art tferra' t-toqol tagħha (minn gewwa fiha)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "il-bniedem jgħid: 'X'għandha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Dakinhar hija tħabbar l-aħbar tagħha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "għaliex Sidek nebbaħha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Dakinhar in-nies joħorgu, imxerrdin, biex jintwerew (lilhom) l-għemejjel tagħhom (u jieħdu l-ħlas mistħoqq)"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "U min jagħmel b'toqol ta' atomu gid, jarah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "umin jagħmel b'toqol ta' atomu deni, jarahl"
        }
    ]
}