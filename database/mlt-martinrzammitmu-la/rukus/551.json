{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Għid (Muħammad lill-pagani): 'Intom li ma temmnux"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "(kummafis li) jiena ma nqimx dak li tqimu"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ulanqasintom matqimu dak li nqim"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "La nqim dak li tqimu"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ulanqas ma tqimu dak li nqim"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Intom għandkom id-din tagħkom, u jiena għandi d-din tiegħi"
        }
    ]
}