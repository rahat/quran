{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ħalli jinqerdu idejn 'Abii Lahabl Huwa nqered"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ma swilux gidu u dak li kiseb"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Se jinxtewa f'Nar bl-ilsna jħeggu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "U martu li ggorr il-ħatab"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "f'għongha (għandha) ħabel mibrum mill-weraq tal-palm"
        }
    ]
