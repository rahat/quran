{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "A horkanva száguldókra"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "A patájukkal szikrát csiholókra"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "A reggel támadásra lendülökre"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "És eközben port kavarókra"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "A középre (egyként) hatolókra"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Az ember bizony hálátlan az ő Urához"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "És erre ő maga a tanú"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "És erős benne a javak iránti szeretet"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Vajon nem tudja, mikor felfordíttatnak a sírok (és az emberek feltámasztatnak)"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "És világosságra kerül az, ami a keblekben van"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Azon a Napon, bizony az ő Uruk ismerni fogja őket"
        }
    ]
}