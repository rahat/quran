        {
            "chapter": 94,
            "verse": 1,
            "text": "Vajon nem tártuk-e ki kebled (Muhammad- Allah áldja meg és adjon Neki örök üdvösséget)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "És nem vettük-e le rólad a terhed"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Amely a hátadat béklyózta meg"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "És felemeltük híred"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Bizony (minden) bajjal (követve azt) jön a jó"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Bizony (minden) bajjal (követve azt) jön a jó"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "És ha befejezted, utána újra tovább fáradozz"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "És csak Urad után vágyódj"
        }
    ]
