    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Mondd: Ő, Allah az Egyedüli, az Egyetlen"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, as-Samadu (az Úr, Akihez minden teremtmény fordul)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nem nemzett és nem nemzetet"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "És senki és semmi nem hasonló Őhozzá"
        }
    ]
}