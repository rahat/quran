{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Mondd: , Az emberek Urához menekülök"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Az emberek Királyához"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Az emberek Istenéhez"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "A titokban sugalmazók gonoszsága elől"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Aki bűnös gondolatokat sugall az emberek kebleibe"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Akár dzsinn, akár ember"
        }
    ]
}