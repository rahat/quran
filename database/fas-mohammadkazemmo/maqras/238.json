{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "سوگند نیارم بدین شهر"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "و توئی جای گزین در این شهر"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "و پدری و آنچه فرزند نهاد"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "که آفریدیم انسان را در رنج‌"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "آیا پندارد که توانائی ندارد هرگز بر او کسی‌"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "گوید تباه کردم مالی انبوه را"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "آیا پندارد که ندیدش کسی‌"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "آیا ننهادیم برایش دیدگانی‌"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "و زبانی و لبانی‌"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "و رهبریش کردیم بدان دو پشته‌"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "پس برنیامد (یا نگذشت) بر آن پشته‌"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "و چه دانستت چیست آن پشته‌"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "آزادکردن یک بنده‌"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "یا خورانیدنی در روز پریشانی (یا روزی دارنده گرسنگی)"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "به یتیمی خویشاوند"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "یا بینوائی خاکسار (یا نیازمند)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "سپس گشت از آنان که ایمان آوردند و سفارش کردند به شکیبائی و سفارش کردند به مهربانی کردن‌"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "آنانند یاران راست‌"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "و آنان که کفر ورزیدند به آیتهای ما آنانند یاران چپ (یا شومی)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "بر ایشان است آتش افروخته (یا انبوه یا پوشیده)"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "سوگند به خورشید و پرتوش (یا آفتابش)"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "و به ماه گاهی که از پیش آید"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "و به روز گاهی که تابناکش سازد"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "و به شب گاهی که بپوشدش‌"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "و به آسمان و آنچه (یا آنکه) بساختش‌"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "و به زمین و آنچه گستردش‌"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "و به روان و آنچه راست آوردش‌"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "پس سروشش فرستاد گنهکاریش و پرهیزش‌"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "همانا رستگار شد آنکه پاک ساختش‌"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "و همانا آیان کرد آنکه بفریفتش (یا نهان داشتش)"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "تکذیب کرد ثمود به سرکشیش‌"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "گاهی که برانگیخت بدنهادتر (یا تیربخت‌ترش)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "پس گفت بدیشان پیمبر خدا اشتر خدا را و آبشخورش‌"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "پس تکذیبش کردندش پس پی کردند پس برآشفت بر ایشان پروردگارشان به گناهشان پس یکسانش کرد"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "و نهراسد از فرجامش‌"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "سوگند به شب گاهی که فراگیرد"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "و روز گاهی که برتابد"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "و آنچه (یا آنکه) آفرید نر و ماده را"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "که کوشش شما است همانا گوناگون‌"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "پس اما آنکه بخشید و پرهیزکاری کرد"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "تصدیق به نکوکاری کرد"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "زود است رهنمونش شویم بسوی گشایش‌"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "و اما آنکه بخل ورزید و بی‌نیازی گرفت‌"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "و تکذیب به نیکوکاری کرد"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "پس زود است برانیمش بسوی دشواری‌"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "و بی‌نیاز نکند از او مالش گاهی که تباه شود"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "همانا بر ما است هدایت‌"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "و همانا برای ما است بازپسین و نخستین‌"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "پس ترسانیدم شما را از آتش که زبانه کشد"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "نچشدش جز بدبخت تری‌"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "که تکذیب کند و پشت کند"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "و زود است دور شودش پرهیزکارتری‌"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "که دهد مالش را پاکی جوید"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "و نیست کسی را نزدش نعمتی که پاداش داده شود"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "جز در پی روی پروردگار برترش‌"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "و همانا بزودی خوشنود شود"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "سوگند به چاشتگاه (یا روز)"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "و به شب گاهی که تیرگی افکند"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "بدرودت نگفت پروردگارت و نه خشمگین شد"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "و همانا انجام بهتر است تو را از آغاز"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "و هر آینه زود است دهدت پروردگارت تا خشنود شوی‌"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "آیا نیافتت یتیمی پس جای داد"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "و یافتت سر به گمی پس رهبری کرد"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "و یافتت گرانباری (عیالمند) پس بی‌نیاز کرد"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "اما یتیم را پس خشم نیار"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "و اما دریوزه را پس نران (یا نهیب نزن)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "و اما به نعمت پروردگار خویش پس زبان گشای‌"
        }
    ]
}