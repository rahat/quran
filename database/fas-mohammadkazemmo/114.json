{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "بگو پناه به پروردگار مردم‌"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "پادشاه مردم‌"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "خدای مردم‌"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "از بدی وسوسه‌کننده نهان شونده‌"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "آنکه وسوسه کند در سینه‌های مردم‌"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "از پریان و مردم‌"
        }
    ]
}