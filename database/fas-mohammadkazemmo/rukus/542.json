{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "سوگند به دوندگان به ستوه‌آمدن‌"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "پس آتش افروزندگان به نواختن سمها بر زمین‌"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "پس تاخت آرندگان به بامداد"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "پس برانگیزند بدان گردی‌"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "پس درآیند بدان در میان گروهی‌"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "همانا انسان است به پروردگار خویش ناسپاس‌"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "و همانا او است بر این گواه‌"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "و همانا او است در دوستی خواسته (یا اسبان) سرسخت‌"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "پس آیا نداند گاهی که انگیخته شود آنچه در گورها است‌"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "و فراهم شود آنچه در سینه‌ها است‌"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "که پروردگار ایشان است بدیشان آگاه‌"
        }
    ]
}