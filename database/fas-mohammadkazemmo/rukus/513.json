{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "پس نه تصدیق کرد و نه نماز گزارد"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "و لیکن تکذیب کرد و روی برتافت‌"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "پس روان شد بسوی خاندان خویش خرامان (خمیازه‌کشان)"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "تو را سزد پس تو را سزد"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "سپس تو را سزد پس تو را سزد"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "آیا پندارد انسان که گذارده شود رها"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "آیا نبود چکّه‌ای از منی که ریخته شود"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "پس گردید خونی بسته پس بیافرید پس بیاراست‌"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "پس گردانید از او دو جفت را نر و ماده‌"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "آیا نیست آن توانا بر آنکه زنده سازد مرده‌گان را"
        }
    ]
}