{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "آیا دیدی آن را که روی برتافت‌"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "و بخشید اندکی و خودداری کرد"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "آیا نزد او است علم ناپیدا پس او می‌بیند"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "یا آگهی داده نشد بدانچه در نامه‌های موسی است‌"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "و ابراهیم که وفا کردار است‌"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "که برندارد هیچ گنهباری بار دگری را"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "و آنکه نیست انسان را جز آنچه بکوشد (یا بدود)"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "و آنکه کوشش او زود است دیده شود"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "پس پاداش داده شودش پاداش رساتر"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "و آنکه بسوی پروردگار تو است پایان‌"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "و آنکه او است که بخندانید و بگریانید"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "و آنکه او است که میرانید و زنده ساخت‌"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "و آنکه او آفرید دو جفت را نر و ماده‌"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "از چکّه آبی گاهی که ریخته گردد"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "و آنکه بر او است پدیدآوردن دیگر"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "و آنکه او بی‌نیاز کرد و نگاه داشت‌"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "و آنکه او است پروردگار شعری (ستاره است)"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "و آنکه او نابود ساخت عاد نخستین را"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "و ثمود را بجای نگذارد"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "و قوم نوح را از پیش که ایشان بودند ستمگرتر و سرکش‌تر"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "و باژگون‌شدگان را فرود آورد"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "پس پوشانیدش آنچه پوشانید"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "پس به کدام نعمتهای پروردگار خویش می‌ستیزی‌"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "این است بیم‌دهنده از بیم‌دهندگان پیشین‌"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "نزدیک گشت نزدیک شونده‌"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "نیستش جز خدا گشاینده‌"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "آیا از این داستان شگفت مانید"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "و خندید و نگریید"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "و شمائید ناآگهان‌"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "پس سجده کنید برای خدا و ستایش کنید"
        }
    ]
}