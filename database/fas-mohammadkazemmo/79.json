{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "سوگند به برکنندگان به قوّت‌"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "و به کشندگان کشیدنی (یا جهندگاه جهشی)"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "و به شناوری‌کنان شناوریی‌"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "پس پیشی‌گیرندگان پیشی‌گرفتنی‌"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "پس کارپردازان کاری‌"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "روزی که بلرزد لرزنده‌"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "از پیش آید پی درآینده‌"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "دلهائی است در آن روز هراسان‌"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "دیدگان آنها است سرافکنده‌"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "گویند آیا مائیم بازگردانیده در گور (یا در حال نخستین)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "آیا گاهی که گردیدیم استخوانهائی پوسیده‌"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "گویند آن است آن هنگام بازگشتی زیانمند"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "پس جز این نیست که خروشی است یگانه‌"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "ناگهان ایشانند به روی زمین‌"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "آیا بیامدت داستان موسی‌"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "گاهی که بانگ دادش پروردگارش بر درّه مقدّس طوی‌"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "برو بسوی فرعون که او سرکشی کرد"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "پس بگو آیا تو را است بسوی آنکه پاکی جوئی‌"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "و رهبریت کنم بسوی پروردگارت پس بترسی‌"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "پس نمایاندش آیت بزرگ را"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "پس تکذیب کرد و سرپیچید"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "سپس پشت کرد می‌دوید"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "پس گردآورد پس برخواند"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "پس گفت منم پروردگار بزرگتر شما"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "پس گرفتش خدا به کیفر انجام و آغاز"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "همانا در این است عبرتی برای آنکه بترسد"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "آیا شما سخت‌ترید در آفرینش یا آسمان که ساختش‌"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "برافراشت پوشش را پس بیاراستش‌"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "و تاریک ساخت شبش را و برآورد روزش‌"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "و زمین را از آن پس گسترانیدش‌"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "برون آورد از آن آبش را و چراگاهش‌"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "و کوه‌ها را لنگر گردانیدش‌"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "بهره‌مندیی برای شما و برای دامهای شما"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "پس گاهی که آید فرودآینده‌ای بزرگتر"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "روزی که یاد آورد انسان آنچه را کوشیده است‌"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "و آشکار گردد دوزخ برای هر که بیند"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "پس اما آنکه سرپیچید"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "و زندگانی دنیا را برگزید"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "همانا دوزخ است جایگاهش‌"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "و امّا آنکه بترسد جایگاه پروردگار خویش را و بازدارد خویشتن را از هوسها"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "همانا بهشت است جایگاهش‌"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "پرسندت از ساعت کی است لنگرگاهش‌"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "در چیستی تو از یادآوردنش‌"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "بسوی پروردگار تو است پایانش‌"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "جز این نیست که توئی ترساننده آنکه بترسدش‌"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "گوئیا ایشان روزی که بینندش نماندند جز شبی یا روزش‌"
        }
    ]
}