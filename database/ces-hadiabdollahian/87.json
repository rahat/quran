{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Oslavovat uvést svuj Magnát nejvetší Nejvyšší"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "On tvorit obrábet"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "On model ovlivnovat"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "On vytáhnout pastva"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pak zapnout to osvetlit seno"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My prednášet ty; ne nepamatovat si"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Všechno byl proto buh prosit On módní co prohlásit co ukrýt"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My poslat ty lehký dráha"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Proto ty upomenout; asi pripomínka beneficní"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ctihodný prosit pokládat pozornost"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Prostopášný prosit zabránit to"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Proto on snést prima Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Wherein on nikdy poslední nor zastávka ijící"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Úspešný vskutku byl jednotka splnit svém duše"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Na pamatovat uvést svém Magnát slavit spojení prosba (Salat)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Vskutku ty zaujmout napred tento dríve doivotní"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Práve trebae Dále jsem druhý zcela nekonecný"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Tento zaznamenat dríve nauka"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Nauka z Abraham Moses"
        }
    ]
}