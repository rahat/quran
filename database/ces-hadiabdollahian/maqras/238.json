{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Já solemnly prísahat tento mesto"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Mesto ty ivý"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "zplodit a zplodit"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "My tvorit lidská bytost jsem spravovat strnulý splnit"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "On myšlen onen nikdo zpusobit vubec prijít jemu odpovídat se"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "On chvála já vycerpat aby skoro peníze"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "On myšlen nikdo zpusobit provést jemu"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "My ne odevzdat jemu 2 otvor"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Špice 2 líbat"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "My ne beet jemu 2 dráha"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "On chtít teký dráha"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Který jednotka jsem teký dráha"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "ucinit prístupným pomocný"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "dát najíst cas útrapa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Sirotek vypravovat"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "chatrný jsem za zrovna"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Byl 1 z ty domnívat, se, varovat 1 jiný jsem pevný varovat 1 jiný jsem hodný"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Tyto zaslouilý štestí"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ohledne Ceho ty pochybovat náš zjevení oni privodit si bída"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Oni omezit Hellfire"
        },
        {
