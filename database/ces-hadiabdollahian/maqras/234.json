{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On (Muhammad) hledet nevlídne a povaha"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "When slepecký vzmuit se podlehnout jemu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Jak ty módní? On cistit"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "On absorbovat pozornost uitek pred poslání"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ohledne Ceho drahý mu"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ty odevzdat jemu svuj ošetrení"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Práve trebae ty ne rucit svém spása"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Jednotka podlehnout ty eagerly"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "A jsem skutecne ctihodný"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ty neznat jemu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Vskutku tento jsem pripomínka"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Whoever prosit pokládat pozornost"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Od honorable bible"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Povýšit nesmíšený"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Styl) písmo hlasatel"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Který jsem honorable spravedlivý"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Hore s lidská bytost jsem on jsem e unappreciative"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "co On tvorit jemu podle"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Podle malý napsat On tvorit jemu model jemu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Pak On zpracovat bodcem dráha jemu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Pak On pripojit jemu smrt do váný"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "When On prosit On vzkrísit jemu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "On udrovat Svém prikázání"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pripustit lidský povaovat svém jídlo"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "My proudit rozvodnit generously"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Pak my rozdelení pošpinit otevrít"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "My dospelý to jádro"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "zrnko vína pastva"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Oliva podmazat"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Druh ovocný sad"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ovoce rostlinný"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "S postarat se doivotní snášet ty svuj zvíre"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pak when rána podlehnout proplout"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Onen byl cas when jednotka prchal svém kamarád"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Podle svém ujmout se jako matka a predek"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Podle svém snoubenec detský"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kadý jednotka z ti u onen cas trápit u svých uznat urcení"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Trochu celo onen cas jsem nejšastnejší"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Smavý veselý"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Jinak celo onen cas pokrývka bída"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Zaplavit hryzení"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tyto jsem prostopášný disbelievers"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "When slunit se balit"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Vystupovat zrícení do kadý jinam"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Hora otírat out"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Znovuzplození zastavit"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Zvíre predvolat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Oceán urcit v plamenech"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Duše uzdravit jejich lod"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Holka pohrbít ijící poadovat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Pro který zlocin ena znemonit"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Zápis útocit módní"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Nebe odstranující"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Peklo zapálit"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ráj podat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Naprostý duše módní všechno to privést"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Já solemnly prísahat mlécná dráha"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Presne tekoucí jejich dráha"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na vecer to dostat"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "A morn to dýchat"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Tento jsem projev honorable hlasatel"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Zmocnit Vlastník Dosadit celkem snést"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "On poslouchat sverit"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Svuj stoupenec (Rashad) ne jsem senzacní"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "On prijmout jemu výška obzor"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "On byl vezení zpet kadý zpráva"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "To ne jsem povídat vyradit souit"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "okamite pak tam ty odchod"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tento jsem poslání úplne lidé"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ty ádat odchod rovný"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Kadý ty ucinil to jsem proto prosit BUH Magnát vesmír"
        }
    ]
}