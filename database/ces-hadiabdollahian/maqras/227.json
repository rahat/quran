{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "Nesporný (prípad)"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Který nesporný (prípad)"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "To jsem pravý nesporný"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Thamoud ‘Aad pochybovat Ostuda"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Ohledne Ceho Thamoud oni zrušit zpustošit (trást) (se)"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Ohledne Ceho ‘Aad oni zrušit trvalý prudký boure"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "On pustit z remene to ti 7 vecer 8 cas violently Ty prijmout lidé los pribline jako rozpad podmazat trup"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Ty rozhodnutí kadý nacrtnout z ti"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "(Pharaoh others jemu hríšník Sodom) jsem prostopášný"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Oni neuposlechnout hlasatel jejich Magnát. Proto On oplatit ti zpustošit náhrada"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Proud znicit my prodávat ty pojízdný (archa)"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "My prevod to lekce ty kadý naslouchat záves porozumení"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "When roh kvet dríve"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Zahrabat hora prodávat z rozmackat; zcela rozmackat"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Byl cas when nevyhnutelný prípad podlehnout proplout"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Nebe rána podzim stranou"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Andel byl Svuj Magnát navláda pak zvládnout 8 (vesmír)"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Onen cas ty ukázat nic z ty ukrýt"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Ohledne Ceho 1 který dostat svém zaznamenat svém vzprímit písmo on odríkávat Podlehnout zjistit muj zápis"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Já ucinil verit já zastavit odpovedný"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "On zaslouilý nejšastnejší doivotní"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Od povýšit Ráj"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Svuj ovoce jsem duverný sahat"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Strávit nápoj šastne operadlo svuj spravovat cas po"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Ohledne Ceho jemu ustoupit svém zaznamenat svém opustit písmo on odríkávat Ohio Já prát si já nikdy prijatý muj zápis"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Já prát si já nikdy módní muj vyrídit"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Já prát si muj smrt jsem vecný"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Muj peníze ne porce mne"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Úplne muj schopnost jsem odjezd"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "absorbovat jemu prekáet jemu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Pálit jemu od Peklo"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Od retez byl 70 rameno velký kravaty jemu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Pro on ne domnívat se za BUH Veliciny"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Nor ucinil on obhajovat zásobovat z chatrný"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Proto on ne mel stoupenec sem"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Nor kadý potrava a na ostrý druh"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "potrava pro hríšník"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Já prísahat co ty prijmout"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "A co ty ne prijmout"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Tento jsem projev honorable hlasatel"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ne projev básník; zrídka ucinil ty verit"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Nor projev veštec; zrídka ucinil ty pokládat pozornost"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Zjevení podle Magnát vesmír"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Mel on vyjádrit kadý jiný nauka"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "My potrestat jemu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "My konec zjevení jemu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ádný o ty porce jemu"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Tento jsem pripomínka pro a spravedlivý"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "My módní; trochu o ty jsem rejectors"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "ono jsem bolest disbelievers"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "To jsem prostý vernost"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Proto ty oslavovat uvést svuj Magnát Veliciny"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Tazatel pochybovat nevyhnutelný odškodnení"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Pro disbelievers ádný konec to"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Podle BUH; Vlastník výška Výšina"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Andel jejich lícit vylézt s Jemu od cas spravedlivý 50000 roky"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Proto ty prícina blahosklonný trpelivost"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Pro oni prijmout to nejvzdálenejší stranou"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "i kdy my prijmout to samý blízko"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Cas podlehnout when lye jsem ulitý otrást"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Hora jsem chmýrovitý vlnený oblek"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Nikoliv stoupenec pecovat svém konec stoupenec"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "When oni prijmout ti vinný prosit ádat on odevzdat svých uznat detský protoe vykoupit ušetrit jemu odškodnení onen cas"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Také svém snoubenec svém kamarád"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ješte svém soubor kmen sbírat jemu"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Práve úplne lidé zahrabat jestlie to uchovat jemu"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Nikoliv; ono jsem v plamenech"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "netrpelivý svítit"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "To hovor ochoten ty povaha"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ty zásoba pocet"
        }
    ]
}