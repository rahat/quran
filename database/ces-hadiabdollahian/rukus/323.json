{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "Thamoud pochybovat hlasatel"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Jejich kamarád Saaleh ríct podle ti ty ne jsem spravedlivý"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Já jsem cestný hlasatel ty"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Ty váit si BUH poslouchat mne"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Já ne poadovat ty kadý vést; muj vést podlehnout jen Magnát vesmír"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Ty verit ty odkázat stále uzavrít za tohleto stát"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Ty trávit zahradní skákal"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "osázet datovat podmazat výtecný ovoce"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Ty vyrezávat hora bujný príbytek"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Ty váit si BUH poslouchat mne"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Ne poslouchat prestupník"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Který predloit odporný ne poslušný spravovat"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Oni odríkávat Ty okouzlit"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ty jsem nikoliv jinam than lidský nás. Vytáhnout zázrak jestlie ty jsem pravý"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "On odríkávat Sem byl velbloud nápoj jen cas pridelit jí; cas byl jinak svuj specifikovat cas z nápoj"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Vyloucit co z ceho ne nakazit jí kadý poškození lest ty privodit si odškodnení desivý cas"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Oni poráka jí tím privodit si bolest"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Odškodnení zaplavit ti. Tento jsem lekce mnoho lidé ne jsem verící"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "mnoství jiste svuj Magnát jsem Všemohoucí Milosrdný"
        }
    ]
}