{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Na slunit se svuj jas"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Mesíc následující to"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Cas odhalit"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Vecer pokrývka"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Lye Jemu stavet ono"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Zahrabat Jemu tvrdit ono"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Duše Jemu tvorit ono"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Pak beet to co jsem odporný který jsem poslušný"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Úspešný byl jednotka splnit to"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Chyba jsem jednotka promeškat to"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamoud's nevera zpusobit ti zhrešit"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Oni následující škoda among ti"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Buh hlasatel ríct podle ti Tento jsem buh velbloud; pripustit jí pít"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Oni pochybovat jemu poráka jí! Jejich Magnát pak odmenit se ti pro jejich hrích zrušit ti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "však ty který podlehnout pozdeji ti trvat nepozorný"
        }
    ]
}