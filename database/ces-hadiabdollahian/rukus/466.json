{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Nejvetší Blahosklonný"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Ucitel Quran"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Tvurce lidská bytost existence"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "On ucit ti jak vyznamenat"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Slunit Se mesíc zcela vypocítávat"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Vystupovat strom premoct"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "On postavit lye pevne platný predpis"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ty ne zhrešit predpis"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Ty dosadit oprávnenost ne rozrušit predpis"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "On tvorit zahrabat úplne tvor"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Od to v tom jsem ovoce datovat podmazat jejich význam ovoce"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Také jádro a korení"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(O lidský jinns) z svuj Magnát asnout ty poprít"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On tvorit lidský starý jíl procházet sem a tam jíl"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "A tvorit jinns podle razit zapálit"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "(O lidský jinns) z svuj Magnát asnout ty poprít"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Magnát z 2 východ 2 vecerní"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Který z svuj Magnát asnout ty poprít"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "On rozloucit 2 more kde? oni vhodný"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Ohrada umístení ti prekazit ti podle zhrešit"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Který z svuj Magnát asnout ty poprít"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Out o oba ti ty dosáhnout delat kroupy a korálový"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Který z svuj Magnát asnout ty poprít"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "On odevzdat ty nakládání onen chodit more jako umdlévat"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Který z svuj Magnát asnout ty poprít"
        }
    ]
}