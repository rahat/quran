{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Spravedlivý prosit trávit chránit skákal"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "A ovoce onen oni poadovaný"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Strávit nápoj šastne operadlo svuj spravovat"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "My tím odmenit a pusobivý"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Strávit trávit docasne; ty jsem vinný"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "When oni zjistit Sehnout shodit oni ne sklánet shodit"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Který Hadith tento oni udrovat"
        }
