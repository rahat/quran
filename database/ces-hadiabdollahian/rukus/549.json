{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ty módní který zpusobit skutecne vyradit vernost"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Onen byl jednotka týrat sirotek"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ne obhajovat dát najíst chatrný"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "A hore s ty slavit spojení prosba (Salat)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Který jsem zcela nepozorný z jejich prosba"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Oni jen revue"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Oni odporný milodar"
        }
    ]
}