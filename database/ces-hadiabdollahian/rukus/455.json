{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "oddechnout si ovinout"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Loisko déš"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "nosit zajištení"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Provozovat ti moný príkaz"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Co slibný ty prece podlehnout proplout"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Cas Soud jsem nevyhnutelný"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Despite zcela tvorit lye"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Ty zustat debatovat vernost"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Zmenit Smer Ceho proto jsem deviators"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Hore s padelatel"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Od jejich chyba oni jsem zcela nepozorný"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Oni pochybovat Cas Soud"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Cas oni podat zapálit"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Chutnat odškodnení; tento jsem co ty uívaný upírat"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Spravedlivý zaslouilý zahradní skákal"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Oni dostat jejich Magnát odmenit oni jednat jsem oddaný"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Zrídka ucinil oni poskytnout nocleh soubor vecer"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "O záblesk, oni ádat odpuštení"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Cást jejich peníze byl spolecnost stranou pro ebrák potrebný"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Zahrabat jsem plne o známka ty jsem nejaký"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "v dome ty prijmout"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Od nebe jsem svuj zajištení všechno slibný ty"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Na Magnát z nebe zahrabat tento byl jako správný ackoliv onen ty mluvit"
        }
    ]
}