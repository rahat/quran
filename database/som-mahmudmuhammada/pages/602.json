    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Caadaysigii Qureesheed"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Ay ka caadayeesheen Safarka Xagaaga iyo Jilaalka darteed"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ha u caabudeen Eebeha Beydkan (Kacbada)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Eebahi ka quudiyay gaajo, kana aamingaliyay cabsi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ka warrama kan beenin Abaalmarinta (Qiyaamada)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Kaas xooga Agoonta (xageeda)"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Aan kuna boorrinaynin quudinta miskiinka"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Waxaa halaag u sugnaaday kuwa tukan (waa Munaafiqiinta)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ee haddana halmaansan Salaaddooda"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Dadkana is tusiya"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Una diida alaabta (Laysugu kaalmeeyo)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Annagaa ku Sinnay Nabiyow Waadi Janno (iyo khary badan)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ee tuko Eebaha dartiis, waxna u gawrac"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ruuxii ku necebna waa mid cidhib beelay"
