{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Abaalkooda ay Eebe agtiisa kii leeyihiin waa Jannooyin Nagaadi, dhexdeedana ay dureeri Wabiyaal ay kuna waari dhexdeeda waligood, Eebaa ka raalli noqday iyana way ka raali noqdeen, arrintaasna waxay u sugnaatay Ruusii Eeba ka yaaba"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Marka DHulka la gilgilo"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Dhulkuna soo bixiyo culaykiisii (wixii ku aasnaa)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Dadkuna (Gaaladuna) dhahaan maxay leedahay"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Maalintaas waxay ka sheekayn warkeeda"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Maxaayeelay Eebaa u waxyooday"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Maalintaas Dadku way soo bixi iyagoo kala tagsan (xagga Diinta iyo Camalkaba) si ay u arkaaan camalkooda"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Ruuxiise wax yaroo khayra fala wuu arki"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Kiise wax yar oo shar fala wuu arki"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Eebe wuxuu ku dhaartay Fardaha Jahaadka u orda iyagoo qaylyi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "iyo kuwa dabka Qoobkooda ku ifiya (guuraha)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Iyo kuwa Cadawga weerara Subaxdii"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ee Boodhka kiciya"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Cadawgana dhexgala"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ee Dadku Eebohood waa diideen"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Arrintaasna Dadku wuu ogyahay"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Dadkuna Xoola jacaylka waa ku daranyahay"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Miyuuna ogayn Marka Xabaalaha waxa ku jira la soo bixiyo"
