{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Eebe wuxuu ku dhaartay Samada iyo Xiddigga guura"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ma taqaan Xiddigga guura"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Waa Xiddig ifa"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ee Nafwalba waxaa kor jooga ilaaliye (Malag)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ee Dadku ha dayo waxa laga abuuray"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Waxaa laga abuuray Biyo booda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Kana soobaxa Dhabar iyo laab dhexdood"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Eebana waa karaa soo celinta Dadka (qiyaamada)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Maalinta la imtixaani waxa (Laabta) ku qarsoon"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Dadkuna Xoog ma leh (Maalintaas) iyo wax u gargaaara midna"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Eebana wuxuu ku dhaartay Samada Roobka leh"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Iyo Dhulka jeexma (waxna ka soo baxo)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ee Quraanku waa hadal wax kala bixiya (wanaagga iyo Xumaanta)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ciyaarra ma aha (ee waa dhab)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Gaaladu waxbay dhakri"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Annaguna waanu kaydi (Abaalmarin)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ee u sug Gaalada wax yar"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Waynee Magaca Eebe ee Sarreeya"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Eebaha abuuray khalqiga ekeeyayna"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Eebaha wax walba qaddaray Jidkana tusiyey"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Eebaha soo bixiya Daaqa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Kana dhiga markaas ka dib caws ingagagan oo Madow"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Waxaan kuu akhriyaynaa Quraanka halmaaminamaysid"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Wuxuu Eebe doono mooyee, Eebana waa ogyahay waxa muuqda iyo waxa qarsoonba"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Waxaana kuu fududaynhaynaa Waddo fiican"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ee wax waani (Nabiyow) markay wax tari waanadu"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Waxaa waantoobi Ruuxa Eebe ka yaabee"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Waxaana ka fogaan waanada Ruuxa xune"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ee ah mid gali Naar wayn"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Oon kuna dhimanayn dhexdeeda kuna noolanayn"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Waxaana liibaanay Ruuxii Daahir noqda"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "magaca Eebana Xusa, Salaaddana tukada"
        }
    ]
}