    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Saas ma'aha ee Dadku wuxuu doortay nolosha Adduunyo ee dhaw"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Aakhiraase khayr badan baaqina ah (hadhi)"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Arrintaasuna waxay ku sugantahay Kutubtii hore"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Kutubtii (Nabi) Ibraahim iyo (Nabi) Muuse"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ma ku soo gadhay Qiyaamada warkeedii"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Wajiyaal Maalintaas way dullaysnaan"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Waxayna qaban Shaqo dhib ah"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Waxayna gali Naar kulul"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Waxaana laga waraabin il kulayl daran"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Wax cunto ahna kama helaan oon ahayn cunno xun (oo wax dhibta)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Taasoon waxna cayilinhaynin Gaajana wax ka taraynin"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Wajiyaalna maalintaas way Nicmaysnaan (Waa mu'miniinta)"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Waxayna ka raalli noqotay camalkeedii"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Janno sarana way gali"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Kumana maqasho Jannada hadal macna darro"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Waxaana ah dhexdeed il socota"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "iyo Sariiro la koryeelay"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Iyo koobab la hor dhigay (ehlu Jannaha)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Iyo Barkimooyin la safay"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Iyo gogol la fidiyay"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Miyayna fiirinaynin sida Deela loo abuuray"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Iyo sida Samada loo koryeelay"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Iyo sida Ruuraha loo taagay"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Iyo sida dhulka loo fidiyay"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ee Waani dadka waxaad tahay waaniyee (Nabiyow)"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Mana tihid mid xoog ku leh (qasab)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Hase yeeshee Ruuxii xaqa ka jeedsada oo Gaalooba (ku soo celi xaqa)"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Eebana wuu caddibi Caddibaadda u wayn"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Xagganagayna u soo noqon (khalqigu)"
        },
        {
