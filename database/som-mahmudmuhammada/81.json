{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Marka qorraxda la duubo (Qiyaamada)"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Xiddiguhuna daataan (iyagoo Madow)"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Buurahana la kexeeyo (La rujiyo)"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Geelii rimmanaana la halleeyo"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Dugaaggana la kulmiyo (Qisaas darteed)"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Badahana la kululeeyo"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Naf walbana Jidhkeedii lammaaneeyo"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Gabdhihii la xabaalnololiyayna la warsado"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Maxaa dambi ah oo loo dilay"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kutubta (camalkana) la fidiyo"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Samadana la rujiyo"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Naarta Jaxiimana la huriyo"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Jannadana la dhaweeyo"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Waxay ogaan nafu waxay camal fashay"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Waxaan ku dhaartay «huu Eebe yidhi» Xiddigaha Qarsooma (Maalintii)"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Ee socda iftiinka Qorraxdana ku qarsooma"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Iyo Habecnku markuu jeedsado"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Iyo subaxu markuu soo qaabilo"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ee Quraanku waa hadalka Eebe, Soona raray Malag sharaf lah"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Xoogna leh Eeba agtiisana Daraja ku leh"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Laguna addeeco halkaas aaminna ah"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Saaxiibkiinna (Nabiguna) ma aha ku waalan"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Horaybuuna ugn arkay Jibriil Jahadii Caddayd (Bariga Maka)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Nabiguna ma aha Ruux lugu shakiyo waxa Maqan"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Qaraankuna ma aha hadal shaydaan la fogeeyay"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ee xaggeed Gaalooy aadaysaan"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Quraankuna waxaan waanada caalamka ahayn ina aha (waa waano)"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ruuxii doona inuu toosnaado (yuuna u yahay)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Waxna ma dooni kartaan in Eebaha Caalamku doona mooyec"
        }
    ]
}