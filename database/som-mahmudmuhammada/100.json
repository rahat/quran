    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Eebe wuxuu ku dhaartay Fardaha Jahaadka u orda iyagoo qaylyi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "iyo kuwa dabka Qoobkooda ku ifiya (guuraha)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Iyo kuwa Cadawga weerara Subaxdii"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ee Boodhka kiciya"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Cadawgana dhexgala"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ee Dadku Eebohood waa diideen"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Arrintaasna Dadku wuu ogyahay"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Dadkuna Xoola jacaylka waa ku daranyahay"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Miyuuna ogayn Marka Xabaalaha waxa ku jira la soo bixiyo"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Laabaha waxa ku qarsoonna la muujiyo"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "In Eebohood Maalintaas ogyahay"
        }
    ]
