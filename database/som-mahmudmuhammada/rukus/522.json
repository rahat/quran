{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Wajiguu u uruuriyay oo jeedsaday (Nabigu)"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Inuu u yimid kii Indhaha la'aa"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Miyaad ogtahay inay u dhawdahay inuu hanuuno oo Daahir noqdo"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ama uu xusuusto oy xusuustu anfacdo"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ruuxiise is deeqtoonaysiiya (islana waynaada)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ma; Adaa u bambixi"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Maxaa kaa saaran hadduusan hanuunin"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ruuxiise kuu yimaadda isagoo dagdagi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Oo Eeba ka yaabi"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Miyaad ka shuqloomi"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Saas ma; aha ee Aayadahani waa waano"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ruuxii doonana wuu ku waana qaadan"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Waxay ku sugantahay (waanadu) Kutub Sharaf leh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Oo la kor yeelay lana Daahiriyay"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Oy Gacanta ku hayaan Malaa'ig (wax qorta)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Sharafna leh oo baarri ah"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Gaalo halla lacnado Gaalnima badanaa"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Muxuu Eebe ka abuuray kuwaas"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Dhibicbuu ka abuuray wuuna ekeeyay"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Jidkana wuu u fudaydiyay"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Markaasuu dilaa Qabrina ku Asturaa"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Markuu doonana wuu soo bixin"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Saas ma; aha (ee Dadku) wixii la faray ma yeelin"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Dadku ha fiiriyo Cuntadiisa"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Inaan Roob ku soo shubno"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Dhulkana markaas aan jeexno"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Kana soo bixinno Midho"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Iyo cinab iyo Daaqa (Xoolaha)"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Iyo Zeytuun iyo Timir"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iyo Beerow waawayn"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Iyo Faakiho (khudaar) iyo Daaq"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Si aad ugu intifaacdaan idinka iyo xoolihiinnu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Markay Qayladii darrayd timaaddo (Suurka)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Waa maalinta Ruuxu ka carari Walaalkiis"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Iyo Hooyadiis Iyo Aabihiis"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Iyo Haweeneydiisii iyo Carruurtiisii"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ruux walbana Maalintaas waxaa shuqlin xaalkiisa"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wajiyaal Maalintaas way Nuuri (Farxi)"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Wayna Qosli oy bishaaraysan"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wajiyaalna Maalintaas waxaa ka muuqan Dulli"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Waxaana dabooli Madow (iyo naxdin)"
        },
        {
