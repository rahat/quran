{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Eebe wuxuu ku dhaartay Habeenku markuu wax qariyo"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Iyo markay Maalintu Muuqato (ifto)"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Iyo Abuuridda Dadka Lab iyo dhaddigba"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "In Camalkiinnu (dadow) kala duwanyahay"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ruuxiise bixiya (xuquuqda) Eebana ka dhawrsada"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Rumeeyana tii wanaaga badnayd (ciwal)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Waxaanni u fududaynaa wanaagga (iyo Jidka fiican)"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ruuxiise ku bakhayla Xoolaha isna deeqtoonaysiiya «iskibriya»"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Reeniyana Tii wanaaga badnayd (ciwal)"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Waxaan u fududaynaa Jidka xun"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Xoolihiisuna wax uma taraan markuu halaagsamo"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Annagaa wax hanuuninna"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Aakhariyo Addunbana Annagaa iska leh"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Waxana idiinka digaynaa Naar kulayl daran"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Oosan galaynin Ruux xun mooyeen"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Oo xaqa beeniyay jeedsadayna"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Waxaana ka fogaada (galidda naarta) Ruuxii Eeba ka dhawrsada"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ee bixiya Xoolihiisa si uu u daahirnoqdo"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ruux wax ugu nicmeeyayna oo abaalmarinna ma uusanjirin"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ee Wuxuu u bixin dar Eebaha sareeya"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Wuuna raalli noqondoonaa Ruuxaasi"
        }
    ]
}