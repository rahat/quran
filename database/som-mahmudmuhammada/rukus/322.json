{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Waxay Beeniyeen Caad Rasuulladii"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Markuu ku Yidhi Walaalkood Huud Miyeydaan Dhawrsanayn"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Anigu waxaan idiin ahay Rasuul Aamin ah"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Ee ka Dhawrsada Eebe Ina Adeeca"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Idinkamana Warsanaayo Xaqa ujuuro, Ajrigaygana Eebaha Caalamkaan ka Sugi"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Miyaad ka Dhisaysaan Meel Sare oo Dhan Calaamad (Daar) Idinkoo Ciyaari"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Ood Yeelanaysaan Masaanic (Dhismo) inaad Waartaan Darteed"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Haddaad wax Qabataan Ciqaabtaan una Ciqaabaysaan si Daran"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "ee Eebe ka Dhawrsada Ina Adeeca"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Kana Dhawrsada Eebaha idin Siiyey waxaad Ogtihiin"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Wuxuu idin Siiyey Xoolo iyo Caruur"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Iyo Beero iyo Ilo"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Waxaana Idiinka Digi Cadab Maalin Wayn"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Waxay Dheheen waxaa Isu kaanna Mid ah Waanisayaa ama Maadan Waanininaa"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "Kanna ma aha waxaan Dabeecaddii «Diintii» Dadkii hore ahayn"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Nalamana Cadaabayo"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Wayna Beeniyeen, markasaan Halaagnay. Taasina waxaa ku Sugan Calaamad, mana aha Badankoodu kuwo Rumayn"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Eebahaana waa Adkaade Naxariista"
        }
    ]
}