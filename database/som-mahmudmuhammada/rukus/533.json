{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Eebe wuxuu ku dhaartay Qorraxda iyo Barqinkeeda"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Iyo Dayuxu markuu Qorraxda raaco"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Iyo Maalintu markay ifiso"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Iyo Habeenku markuu daboolo (madoobaado)"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Iyo Samada iyo say u dhisantahay"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Iyo Dhulka iyo suu u fidsanyahay"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Iyo Nafta iyo sida Eebe u ekeeyay"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Una tusiyay Jidka xun iyo ka Eebe kayaabiddaba"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ee waxaa liibaanay Ruxii Naftiisa Daarhiriya"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Waxaana khasaaray Ruuxii Naftiisa xumaan dhex galiya"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamuud waxay beeniyeen (xaqii) kibir dartiis"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Marku tagay koodi xumaa (si uu hasha u dilo)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Oo Rasuulkii Eebana ku yidhi, iska daaya Hasha Eebe iyo Cabbideeda"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Wayna beeniyeen Rasuulkii Hashiina way Dileen markaasaa Eebe si daran u caddibay Dambigooda dartiis"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Isagoon Eebe ka yaabaynin cidhib (iyo wax yeello)"
        }
    ]
}