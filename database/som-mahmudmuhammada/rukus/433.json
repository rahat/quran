{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Geedka Zaquumka ah"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "waa cunnada dambi badanaha"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "waana Laami oo kale kuna kari Caloosha"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "sida Xamiimka (Kulayl daran) oo kale"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "(waxaana lagu dhihi) qabta Gaalka una Jiida Jaximo dhexdeeda"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "markaas ku shuba Madaxiisa korkiisa cadaab kulul"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "(waxaana lagu dhihi) dhadhami (Cadaabka) waxaad tahay adkaade sharaflehe"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "kaasina waa kaad ahaydeen kuwo shakiya (Cadaabka)"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "kuwa dhawrsadana waxay kusugnaan meel aamin ah"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Jannooyin iyo Ilo (yey kusugnaan)"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "waxayna xidhan Xariir jilicsan iyo mid adag (oo dhalaali) wayna is qaabili"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "saasoo kale waxaa usugnaaday Haween Indho waaweyn"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "waxayna uyeedhan Janada dhexdeeda Khudaar kasa iyagoo aamin ah"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "mana ku dhadhamiyaan Jannada dhexdeeda Geeri, Geeridii hore mooyee wuxuuna ka dhawray (Eebe) Cadaabka Jaxiimo"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "waana fadliga Eebahaa, taasina waa liibaanta wayn"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "waxaan ugu fududaynay Quraanka Carrabkaaga inay waantoobaan"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "ee sug iyaguna way sugiye"
        }
    ]
}