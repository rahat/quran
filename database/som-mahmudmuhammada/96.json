{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Wax ku akhri Nabiyow Magaca Eebe ee abuuray (khaliqiga)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Dadkana ka abuuray Calaq"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Akhri Eebana waa kan Sharafta lehe"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ee Dadka baray sida Qalinka wax loogu qoro"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Dadkana baray waxayna aqoonin"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Saas ma aha ee Dadku (Gaaladu) waa gudbeen"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Markuu isku ogaaday Hodontinnimo"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Eebaha xaggiisaase loo noqon"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ka warrama midka ka reebaya"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Addoon (Eebe) oo tukan Salaadda"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Bal ka warrama hadduu ka laga reebi hanuunsanyahay (waa Nabiga)"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ama uu fari Eeba ka yaabid"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Balse ka warrama hadduu midka wax ka reebi uu beeniyay (xaqa) kana jeedsaday (waa AbuuJahal iyo wixii la mid ahe)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Miyuusan ogayn in Eebe arko"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Saas ma aha ee hadduusan joogin waxaannu qaban Foodda"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Food beenaleey ah ghafna badan"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Ha u yeedho gargaarihiisa"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Waxaanu u yeedhi Malaa'ig darane"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Saas ma aha ee warkiisa ha maqlin Sujuudna una dhawow (Eebe)"
        }
    ]
}