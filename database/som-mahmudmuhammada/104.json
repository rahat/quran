{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Halaag Wuxuu u Sugnaaday xanle wax ceebeya oo Dhan"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oo Xoolaha kulmiya (xaqana ku bixinin)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ma wuxuu malayn in xoolihiisu waarin"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Saas ma aha ee waxaa lagu tuuri (Naarta) Xudhama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ma taqaannaa Xudhama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Waa Naarta Eebe ee la huriyay"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Gubaysana Quluubta (Kulayl dartiis)"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Waana lagu dabooli Naartaas Gaalada"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Tiraar la fidiyay yayna ahaan dhexdeeda"
        }
    ]
}