{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "śuṁ tamē (tēnē paṇa) jōyō jē badalānā divasanē juṭhalāvē chē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "ā ja tē chē jē anāthanē dhakkā mārē chē"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "anē garībanē khavaḍāvavānuṁ prōtsāhana nathī āpatō"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "tē namājhī'ō māṭē khēda chē (anē vaila nāmanī jahannamanī jagyā) chē"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "jē pōtānī namājhathī bēparavāha chē"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "jē dēkhāḍō karē chē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "anē sāmān'ya jarūriyātathī rōkē chē"
        }
    ]
}