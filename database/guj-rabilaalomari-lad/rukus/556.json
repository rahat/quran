{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "tamē kahī dō! Kē huṁ lōkōnā pālanahāranī śaraṇamāṁ āvuṁ chuṁ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "lōkōnā mālikanā (anē)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "lōkōnā ma'abūdanā (śaraṇamāṁ)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "śaṅkā'ō nākhanāra, pāchā haṭī janāranī burā'ithī"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jē lōkōnā hr̥dayōmāṁ śaṅkā'ō nākhē chē"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(pachī) tē jinnātōmāṁ thī hōya athavā tō manuṣyōmāṁ thī"
        }
    ]
}