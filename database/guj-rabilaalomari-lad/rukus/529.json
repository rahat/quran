{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "pōtānā sarvōcca pālanahāranā nāmanī pavitratā bayāna kara"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jēṇē sarjana karyu anē ṭhīka-ṭhāka karyō"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "anē jēṇē (ṭhīka-ṭhāka) bhāgya banāvyuṁ anē pachī mārga batāvyō"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "anē jēṇē tājī vanaspati'ō upajāvī"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "pachī tēṇē tēnē (sukāvīnē) kāḷō kacarō banāvī dīdhō"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "amē tamanē paḍhāvīśu pachī tamē nahīṁ bhulō"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "sivāya jē kaṁi allāha icchē, tē jāhēra anē chupī (vātōnē paṇa) jāṇē chē"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Amē tamārā māṭē saraḷatā pēdā karī da'iśuṁ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "tō tamē śikhāmaṇa āpatā rahō, jō śikhāmaṇa lābhadāyaka hōya"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "ḍaravāvāḷō tō śikhāmaṇa grahaṇa karaśē"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "(hā) durbhāgī tēnāthī dūra rahēśē"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "jē mōṭī āgamāṁ jaśē"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "tyāṁ na tō tē mr̥tyu pāmaśē na tō jīvaśē. (Jāna-kanīnī avasthāmāṁ haśē)"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "kharēkhara tēṇē saphaḷatā prāpta karī jē pavitra tha'i gayō"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "anē jēṇē pōtānā pālanahāranā nāmanuṁ smaraṇa karyu, anē namājha paḍhatō rahyō"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "parantu tamē tō duniyāvī jīvananē prāthamikatā āpō chō"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "anē ākhirata sarvōttama anē avināśī chē"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ā vātō prathama granthōmāṁ paṇa chē"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "(ēṭalē kē) ibrāhīma anē mūsānā granthōmāṁ"
        }
    ]
}