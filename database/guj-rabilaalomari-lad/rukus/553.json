{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "abū lahaba nā bannē hātha tuṭī gayā anē tē (pōtē) barabāda tha'i gayō"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "na tō tēnuṁ dhana tēnā māṭē kāma āvyuṁ anē na tō tēnī kamāṇī"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "tē najīkamāṁ bhaḍakāvēlī āgamāṁ jaśē"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "anē tēnī patni paṇa (jaśē) jē lākaḍī'ō uṭhāvanārī chē"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "tēnā gaḷāmāṁ kāthīnuṁ dōraḍuṁ haśē"
        }
    ]
}