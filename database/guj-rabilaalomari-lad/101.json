{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "khaṭakhaṭāvī nākhanāra"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "śuṁ chē tē khaṭakhaṭāvī nākhanāra"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "tanē śuṁ khabara tē khaṭakhaṭāvī nākhanāra śuṁ chē"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "jē divasē mānavī vikharāyēlā pataṅgiyānī māphaka tha'i jaśē"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "anē parvatō pin̄jāyēlā raṅgīna ūna jēvā tha'i jaśē"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "pachī jēnā pallā bhārē haśē"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "tō tē'ō manapasanda ēśa'ārāma māṁ haśē"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "anē jēnā pallā'ō halkā haśē"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tēmanuṁ ṭhēkāṇuṁ hāviyaha chē"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "tanē śuṁ khabara kē tē śuṁ chē"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "bhaḍakē baḷatī āga (chē)"
        }
    ]
}