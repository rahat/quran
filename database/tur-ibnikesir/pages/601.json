    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asr´a andolsun ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Muhakkak insan, hüsrandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edenler ve salih amel işleyenler müstesnadır. Bir de birbirlerine hakkı tavsiye edenler ve sabrı tavsiye edenler"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Diliyle çekiştirip yüzünde de alay edenin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ki o, mal toplayıp onu sayar"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının kendisini ebedi kılacağını sanır"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır; andolsun ki o, Hutame´ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame´nin ne olduğunu bilir misin sen"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "O, Allah´ın tutuşturulmuş bir ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ki tırmanıp yüreklerin içine işler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Bu, onların üzerine kapatılmıştır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmış sütunlar arasında"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbının; fil sahiplerine ne ettiğini görmedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "O; bunların düzenlerini boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "O; bunların üzerine sürülerle kuşlar gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ki, onların üzerine pişkin tuğladan taşlar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayet onları, yenik ekin yaprağı gibi yapıverdi"
        }
    ]
}