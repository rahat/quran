{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun; incire ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve şu emin kente"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Doğrusu Biz; insanı en güzel biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu, aşağıların aşağısına döndürdük"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız iman edip salih amel işleyenler müstesna. Onlara kesintisiz mükafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Öyleyse bundan sonra hangi şey sana dini yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah; hükmedenlerin en iyi hükmedeni değil mi"
        }
    ]
}