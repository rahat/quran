{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Tanyerini ağartan Rabba sığınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yaratıkların şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Bastırdığı zaman, karanlığın şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Düğümlere üfürenlerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Hased ettiğinde, hased edenlerin şerrinden"
        }
    ]
}