{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: İnsanların Rabbına sığınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların malikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların Tanrısına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi şeytanın şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların kalblerine hep vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinnlerden, gerek insanlardan"
        }
    ]
}