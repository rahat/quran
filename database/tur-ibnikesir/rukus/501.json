{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Görebildiğinize yemin ederim ki"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve göremediklerinize de"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Muhakkak o; şerefli bir elçinin kat´i sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve o, bir şair sözü değildir. Ne de az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kahin sözü de değildir. Ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbından indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer o; bazı sözleri Bize karşı buna katmış olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette Biz; onu, kuvvetle yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da, hiç şüphesiz onun şah damarını koparırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "O zaman sizden hiç biriniz de buna engel olamazdınız"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Doğrusu o; müttakiler için bir öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "İçinizde yalanlayanlar bulunduğunu Biz de bilmekteyiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve muhakkak ki o; kafirler için bir üzüntüdür"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Hiç şüphesiz ki o; kesin gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Öyleyse Rabbını, o büyük adıyla tesbih et"
        }
    ]
}