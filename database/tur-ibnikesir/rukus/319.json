{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Bunun üzerine Firavun şehirlere toplayıcılar gönderdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Şüphesiz ki bunlar; döküntü azınlıklarıdır"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve gerçekten bize de büyük bir öfke beslemektedirler"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Doğrusu biz, topluca tedbirli olmalıyız"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fakat Biz, onları bahçelerden ve pınar başlarından çıkardık"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Hazinelerden ve şerefli makamlardan"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Böylece onlara İsrailoğullarını mirasçı kıldık"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Güneş üzerlerine doğarken onları izlediler"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "İki topluluk karşı karşıya geldiğinde, Musa´nın arkadaşları dediler ki: Gerçekten biz, yakalandık"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Hayır, dedi. Muhakkak ki Rabbım benimledir. Bana doğru yolu gösterecektir"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Bunun üzerine Musa´ya vahyettik ki: Asanı denize vur. O, hemen yarıldı ve her parçası yüce bir dağ gibi oldu"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Sonra diğerlerini oraya yaklaştırdık"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa´yı ve beraberindekileri yopluca kurtardık"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sonra diğerlerini suda boğduk"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Şüphesiz ki bunda, bir ayet vardır. Ama onların çoğu inananlar değildi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Muhakkak ki Rabbın, elbette o; Aziz´dir, Rahim´dir"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Onlara İbrahim´in haberini oku"
        }
    ]
}