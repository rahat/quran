{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "کہو، وہ اللہ ہے، یکتا"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اللہ سب سے بے نیاز ہے اور سب اس کے محتاج ہیں"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نہ اس کی کوئی اولاد ہے اور نہ وہ کسی کی اولاد"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "اور کوئی اس کا ہمسر نہیں ہے"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "کہو، میں پناہ مانگتا ہوں صبح کے رب کی"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ہر اُس چیز کے شر سے جو اُس نے پیدا کی ہے"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "اور رات کی تاریکی کے شر سے جب کہ وہ چھا جائے"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "اور گرہوں میں پھونکنے والوں (یا والیوں) کے شر سے"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "اور حاسد کے شر سے جب کہ وہ حسد کرے"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "کہو، میں پناہ مانگتا ہوں انسانوں کے رب"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "انسانوں کے بادشاہ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "انسانوں کے حقیقی معبود کی"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "اُس وسوسہ ڈالنے والے کے شر سے جو بار بار پلٹ کر آتا ہے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "جو لوگوں کے دلوں میں وسوسے ڈالتا ہے"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "خواہ وہ جنوں میں سے ہو یا انسانوں میں سے"
