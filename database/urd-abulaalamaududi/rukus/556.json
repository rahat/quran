{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "کہو، میں پناہ مانگتا ہوں انسانوں کے رب"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "انسانوں کے بادشاہ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "انسانوں کے حقیقی معبود کی"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "اُس وسوسہ ڈالنے والے کے شر سے جو بار بار پلٹ کر آتا ہے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "جو لوگوں کے دلوں میں وسوسے ڈالتا ہے"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "خواہ وہ جنوں میں سے ہو یا انسانوں میں سے"
        }
    ]
}