{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "عظیم حادثہ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "کیا ہے وہ عظیم حادثہ؟"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "تم کیا جانو کہ وہ عظیم حادثہ کیا ہے؟"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "وہ دن جب لوگ بکھرے ہوئے پروانوں کی طرح"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "اور پہاڑ رنگ برنگ کے دھنکے ہوئے اون کی طرح ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "پھر جس کے پلڑے بھاری ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "وہ دل پسند عیش میں ہوگا"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "اور جس کے پلڑے ہلکے ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "اس کی جائے قرار گہری کھائی ہوگی"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "اور تمہیں کیا خبر کہ وہ کیا چیز ہے؟"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "بھڑکتی ہوئی آگ"
        }
    ]
}