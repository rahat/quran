{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "La course aux richesses vous distrait"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "jusqu’à ce que vous visitiez les tombes"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Mais non ! Vous saurez bientôt"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "(Encore une fois) ! Vous saurez bientôt"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Sûrement ! Si vous saviez de science certaine"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Vous verrez, certes, la Fournaise"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Puis, vous la verrez certes, avec l’œil de certitude"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Puis, assurément, vous serez interrogés, ce jour-là, sur les délices"
        }
    ]
}