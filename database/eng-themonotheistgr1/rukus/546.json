{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Woe to every backbiter, slanderer"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Who gathered his wealth and counted it"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "He thinks that his wealth will make him immortal"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Never! He will be thrown into the Destroyer"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "And do you know what is the Destroyer"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "The kindled Fire of God"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Which reaches the inside of the hearts"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "It will confine them therein"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "In extended columns"
        }
    ]
}