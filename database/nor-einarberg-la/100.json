{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ved dem som pruster i galopp"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "som slar gnister"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "som stormer pa ved morgengry"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "og virvler opp støv under dette"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "og derved trenger inn i flokkens midte"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Mennesket er sannelig utakknemlig mot Herren"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "og han er selv vitne til det"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Han er sterk i sitt begjær etter livets goder"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Vet han da ikke nar det som er i gravene, tømmes ut"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "og det som bor i hjertene, bringes frem"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "pa denne dag er Herren vel underrettet om dem"
        }
    ]
}