{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ved solen og dens formiddagsglød"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ved manen, nar den følger den"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ved dagen, nar den lar den lyse"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Ved natten, nar den hyller den inn"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Ved himmelen, og Han som bygget den"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Ved jorden, og Han som bredte den ut"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ved mennesket, og Han som formet det"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "og utstyrte det med syndighet og gudsfrykt"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ham gar det godt, som holder seg ren"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ham gar det galt, som lar seg forkomme"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamod fornektet i sin oppsetsighet"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "da den laveste blant dem stod frem"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Guds sendebud sa til dem: «Guds kamelhoppe! La den drikke!»"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Men de holdt ham for løgner og lemlestet den, og sa kom Herren over dem for deres synd, og jevnet dem med jorden"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Han var ikke redd for følgene"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ved natten nar den dekker til"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ved dagen nar den straler opp"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ved Han som skapte mann og kvinne"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Deres innsats er visselig av mange slag"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Hva ham angar, som gir gaver og viser gudsfrykt"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "og tror pa det beste"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "ham vil Vi lette det for hen til lettelsen"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Men hva ham angar, som er gjerrig og seg selv nok"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "og fornekter det beste"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "ham vil Vi lette det for hen til det tunge"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "og hans rikdom kan ikke hjelpe ham nar han gar i fortapelsen"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ledelsen er visselig Var sak"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "og Vart er det siste, og det første"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sa advarer Jeg dere da mot en flammende Ild"
        }
    ]
}