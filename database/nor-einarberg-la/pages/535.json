{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "mens evig unge gutter gar rundt blant dem"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "med skaler og krus og beger med den klareste drikk"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "som man ikke far vondt i hodet eller mister vettet av"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "med slike frukter som de velger seg ut"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "og kjøtt og fugl som de ønsker seg"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Og storøyde kvinner er der"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "lik vel forvarte perler"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Som belønning for deres innsats"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Der skal de ikke høre tomt snakk, og ingen bebreidelser"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "bare ordene: «Fred, fred!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Og de til høyre, hva med dem til høyre"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Blant tornefrie lotustrær"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "og tette akasier"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "utstrakt skygge"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "og sprudlende vann"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "frukt i overflod"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "alltid for handen og fri til a ta av"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "pa opphøyde leier"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Fullkomne har Vi skapt dem, paradisets kvinner"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "som plettfrie jomfruer"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ømme og kjærlige, jevngamle"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "med dem til høyre"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "– en stor flokk fra tidligere tider"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "og en stor flokk fra de senere"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Og de til venstre, hva med dem til venstre"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "I het, kvelende vind og skaldende vann"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i skygge av kullsvart røyk"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "som ikke svaler, og ikke er til behag"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Tidligere levde de et liv i vellevnet"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "og turet frem i stor synd"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "og pleide a si: «Nar vi er døde og blitt til støv og tørre ben, skal vi da oppsta"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Og henfarne fedre ogsa?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Si: «Bade de første og de siste"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "alle vil bli samlet, til fastsatt tid en bestemt dag.»"
        }
    ]
}