{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ved himmelen og morgenstjernen"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Vet du hva morgenstjernen er"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Den tindrende stjerne"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Over hvert menneske er det en som passer pa"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "La mennesket betenke hvorav han ble skapt"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Han ble skapt av en frembrytende væske"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "som kommer fra mellom lendene og brystbenet"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Han er visselig i stand til a bringe ham tilbake"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "den dag da alle hemmeligheter prøves"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "og han har verken makt eller hjelper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ved himmelen med det tilbakevendende regn"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "og jorden, som apner sitt skjød"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dette er avgjørende ord"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "det er ingen spøk"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "De anvender knep"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men ogsa Jeg bruker list"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sa ta det med ro med de vantro, ta det med ro med dem for en tid"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Pris Herrens navn, den Høyeste"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "som skapte og formet"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "bestemte og ledet"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "som frembrakte beitemarker"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "og lot dem bli tørre stra"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Vi skal la deg forkynne (Koran), og glem sa ikke"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "unntatt det Gud vil. Han kjenner til det som lyder høyt, og det som holdes skjult"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Vi skal lette det for deg hen til lettelsen"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Sa forman, der formaning nytter"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Den som frykter, vil vise ettertanke"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "men den usle vil unnga den"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "han som skal havne i den store Ild"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "og der kan han verken dø eller leve"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ham gar det godt, som har renset seg"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "kommer Herrens navn i hu, og forretter bønnen"
        }
    ]
}