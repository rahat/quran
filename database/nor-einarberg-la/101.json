{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Hva er det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Hvordan kan du vite hva det knusende slag er"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den dag blir menneskene som flagrende mygg"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "og fjellene som kardet ull"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Den, hvis vektskaler er tunge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "vil ga til et godt liv"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Men den, hvis vektskaler er lette"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avgrunnen har omsorg for ham"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Og hvordan kan du vite hva det er"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Det er het ild"
        }
    ]
}