{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Vet han da ikke nar det som er i gravene, tømmes ut"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "og det som bor i hjertene, bringes frem"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "pa denne dag er Herren vel underrettet om dem"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Hva er det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Hvordan kan du vite hva det knusende slag er"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den dag blir menneskene som flagrende mygg"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "og fjellene som kardet ull"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Den, hvis vektskaler er tunge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "vil ga til et godt liv"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Men den, hvis vektskaler er lette"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avgrunnen har omsorg for ham"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Og hvordan kan du vite hva det er"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Det er het ild"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Eie-mest mentaliteten opptar dere"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "til dere gar til gravene"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nei, sannelig, dere skal fa a vite"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sa sannelig, dere skal fa a vite"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nei, sannelig, om dere bare visste med visshets kunnskap"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dere skal visselig se helvete"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ja, sannelig skal dere se det med sikkert blikk"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Og sa, pa den dag, skal dere visselig fa svare for velstanden"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ved tidens makt"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Mennesket kommer visselig til kort"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Unntatt er de som tror og lever rettskaffent, som legger hverandre sannheten pa hjertet, og standhaftighet"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ve over hver ivrig baktaler"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "som samler rikdom, og teller og teller"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "og tror at hans rikdom har gjort ham udødelig"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nei, sa sannelig, han skal bli kastet i al-Hotama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hvordan kan du vite hva al-Hotama er"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Det er Guds opptente Ild"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "som gar oppover hjertene"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Den hvelver seg over dem"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "i langstrakte søyler"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Du har vel sett hva Herren gjorde med elefantfolkene"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Lot Han ikke deres list sla feil"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Han sendte over dem fugleflokker"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "som kastet pa dem steiner av brent leire"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "og lot dem bli som avspist vegetasjon"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "For Quraysh-stammens samordning"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "samordning av vinter- og sommerkaravanen"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "sa la dem dyrke Herren over dette Hus (Ka’aba)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Han som gir dem mat mot sult, og trygghet mot frykt"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Du har vel sett ham som fornekter dommen"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Det er han som støter bort den foreldreløse"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "og ikke oppfordrer til a gi den fattige mat"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ve dem som holder bønn"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "men som ikke samler seg om den"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "som gjør det bare for syns skyld"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "som nekter a gi en handsrekning"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Vi har visselig gitt deg i overflod"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sa be til Herren, og bring offer"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Sannelig, han som har lagt deg for hat, han har intet etter seg"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Si: «Dere vantro"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ikke kommer jeg til a tilbe det dere tilber"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "og ikke kommer dere til a tilbe det jeg tilber"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ikke tilber jeg det som dere tilber"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ikke tilber dere det jeg tilber"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Dere har deres religion, og jeg har min!»"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Nar Guds hjelp kommer, og seieren"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "og du ser menneskene tre inn i Guds religion flokkevi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "lovpris da Herren, og be Ham om tilgivelse! Han er full av miskunn"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "I undergangen med Abu Lahabs verk, i undergangen med ham"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Hans rikdom og det han har tjent, hjelper ham ikke"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Han skal havne i flammende ild"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "mens hans hustru, som bærer brennefang"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "har en snor av palmefibre rundt halsen"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Si: «Han er Gud, En"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Gud, den Evigvarende"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ikke har Han avlet noen, ei heller er Han selv blitt avlet"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ingen er Hans like.»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Si: «Jeg søker tilflukt hos morgengryets Herre"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "mot ondt fra det Han har skapt"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mot ondt fra mørket nar det bryter inn"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mot ondt fra dem som blaser pa knuter og utøver magi"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "mot ondt fra en misunnelig, nar han misunner (kaster det onde øye)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Jeg søker tilflukt hos menneskenes Herre"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "menneskenes Konge"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "menneskenes Gud"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "mot ondt fra den smygende frister"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "som hvisker i menneskenes hjerter"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "av dsjinner eller mennesker"
        }
    ]
}