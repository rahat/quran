{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Nar himmelen revner"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "adlyder Herren, og er under Hans bud"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Nar jorden jevnes ut"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "og kaster frem det som er i den og tømmer seg"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "adlyder Herren, og er under Hans bud"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Menneske, du strever med kraft henimot din Herre, og du skal møte Ham"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Hva den angar som far sin bok levert i høyre hand"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "han far en lett avregning"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "og vender glad om til sine"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Men hva den angar som far levert sin bok fra bak ryggen"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "han vil rope pa tilintetgjørelse"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "og han havner i helvetes ild"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Han levde glad og fornøyd blant sine"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "og trodde visselig at han aldri skulle vende tilbake"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Men nei, Herren hadde ham for øyet"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Nei, jeg sverger ved aftenrøden"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "ved natten og alt den bærer i seg"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "og ved manen nar den er full"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Dere skal sannelig ga fra tilstand til tilstand"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Hva er det med dem, at de ikke tror"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Og nar Koranen fremleses for dem, at de ikke faller ned for Gud"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Nei, de som er vantro holder sannhet for løgn"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "og Gud vet best hva de har i seg"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Bebud for dem en smertelig straff"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Unntatt er de som tror og lever rettskaffent. De skal fa en velfortjent lønn"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Ved himmelen med sitt stjernemylder"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Ved den lovede dag"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ved vitnet og vitnesbyrdet"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Død over branngrav-mennene"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "med den brenselsforsynte ild"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "som satt omkring den"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "og overvar hva man gjorde med de troende"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "De hevnet seg pa dem bare fordi de trodde pa Gud, den Mektige, den Prisede"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ham, som herredømmet over himlene og jorden tilkommer. Gud bevitner alle ting"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "De som forfølger troende menn og kvinner, og sa ikke vender om, de har i vente helvetes straff, de har i vente Ildens straff"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "De som tror og lever rettskaffent, har i vente paradisets haver, hvor bekker sildrer. Dette er den store seier"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Visselig, Herren slar hardt til"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Han bringer frem, og gjør det pa nytt"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Han er den Ettergivende, den Velvillige"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "tronens Herre, den Ærerike"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Den som gjennomfører det Han vil"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Har historien om hærskarene nadd deg"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "om Farao og Thamod"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Sannelig, de vantro turer frem i fornektelse"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Men Gud er bak dem og innringer"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Ja, dette er en herlig Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "pa en vel forvart himmelsk tavle"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ved himmelen og morgenstjernen"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Vet du hva morgenstjernen er"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Den tindrende stjerne"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Over hvert menneske er det en som passer pa"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "La mennesket betenke hvorav han ble skapt"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Han ble skapt av en frembrytende væske"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "som kommer fra mellom lendene og brystbenet"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Han er visselig i stand til a bringe ham tilbake"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "den dag da alle hemmeligheter prøves"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "og han har verken makt eller hjelper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ved himmelen med det tilbakevendende regn"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "og jorden, som apner sitt skjød"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dette er avgjørende ord"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "det er ingen spøk"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "De anvender knep"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men ogsa Jeg bruker list"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sa ta det med ro med de vantro, ta det med ro med dem for en tid"
        }
    ]
}