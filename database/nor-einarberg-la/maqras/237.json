{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Pris Herrens navn, den Høyeste"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "som skapte og formet"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "bestemte og ledet"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "som frembrakte beitemarker"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "og lot dem bli tørre stra"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Vi skal la deg forkynne (Koran), og glem sa ikke"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "unntatt det Gud vil. Han kjenner til det som lyder høyt, og det som holdes skjult"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Vi skal lette det for deg hen til lettelsen"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Sa forman, der formaning nytter"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Den som frykter, vil vise ettertanke"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "men den usle vil unnga den"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "han som skal havne i den store Ild"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "og der kan han verken dø eller leve"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ham gar det godt, som har renset seg"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "kommer Herrens navn i hu, og forretter bønnen"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Nei, dere foretrekker jordelivet"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "men det hinsidige er bedre, og mer varig"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Dette star visselig i de gamle skrifter"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "pa Abrahams og Mose blader"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Har du fatt beretningen om det overdekkende"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Noen vil pa denne dag være ydmykede"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "anstrengte, utslitte"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "idet de er i den hete Ild"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "far drikke fra en kokhet kilde"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "og bare har kaktus til mat"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "som ikke gir næring og ikke stiller hungeren"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Andre vil pa denne dag være glade"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tilfredse med sitt strev"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "idet de er i en frittliggende have"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "hvor de ikke hører tomt prat"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Det er en sprudlende kilde"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "behagelige benker"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "fremsatte glass"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "puter pa rekke og rad"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "og tepper bredt ut"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ser de da ikke pa kamelen, pa hvordan den er skapt"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Pa himmelen, hvordan den hvelves høyt oppe"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Pa fjellene, hvordan de er reist opp"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Pa jorden, hvordan den er strukket ut"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Sa minn dem pa! Du er bare en formaner"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Du har ingen makt over dem"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Den som vender seg bort i vantro"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "vil Gud hjemsøke med den største straff"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Til Oss skal de vende tilbake"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "og sa skal Vi ordne med deres regnskap"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ved morgengryet"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ved ti netter"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Ved det like og det ulike"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Ved natten nar den svinner hen"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Er det ikke i dette en ed for den som har innsikt"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Du har vel sett hva Herren gjorde med folket Ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Med det søylerike Iram"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "hvis like ikke er skapt i verden"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Og med folket Thamod, som hugget seg inn i fjellsiden i dalen"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Og Farao, han med pyramidene"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "De som vandret i overmot pa jorden"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "og spredte ufred og fordervelse"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Men Herren lot tuktens svepe ramme dem"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Herren er pa vakt"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Nar Herren setter mennesket pa prøve ved ære og velstand, sier det: «Herren har hedret meg!»"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Men hvis Han setter det pa prøve ved a utmale knapt hans levemate, sa sier det: «Herren ringeakter meg!»"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Nei, sannelig, men dere hedrer ikke den faderløse"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "og ansporer ikke til a gi den fattige mat"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "dere sluker gradig hans lille arv"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Dere elsker rikdom over all mate"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Nei, sannelig! Nar jorden jevnes til støv"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "nar Herren kommer, og englene, rekke etter rekke"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "nar helvete bringes frem, pa denne dag vil mennesket saktens erindre. Men hva tjener det da til med ettertanke"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Og det vil si: «Om jeg bare hadde sendt i forveien noe godt for mitt liv!»"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ingen kan straffe som Han gjør pa denne dag"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ingen lenker slik som Han"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Du sjel, som har ro og harmoni"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "vend tilbake til din Herre tilfreds og godtatt"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Ga inn blant Mine tjenere"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Ga inn i Mitt paradis"
        }
    ]
}