{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Hva mener du om den som vender ryggen til"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "og gir lite, og bare motvillig"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Kjenner han vel det skjulte, sa han kan se"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Er han ikke underrettet om hva som star pa Mose skriftblader"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Eller Abrahams, han som"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "fullt ut oppfylte sin plikt? At ingen kan bære en annens byrde"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "at intet tilkommer et menneske uten det han selv har strevet for, og"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "at hans strev skal komme for dagen"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Sa vil han fa sin lønn fullt ut"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "At hos din Herre er slutten"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "At Han er Den som fremkaller latter og grat"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "At Han er Den som fremkaller død og liv"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "At Han skaper de to kjønn, hankjønn og hunkjønn"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "av en drape som utstøtes"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "At Han star for den annen skapelse"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "At Han gir rikdom og velstand"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "At Han er Sirius’ Herre"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "At Han tilintetgjorde det gamle folket Ad"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "og av Thamod sparte Han ingen"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Og Noas folk tidligere, de var særlig onde og oppsetsige"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Den sammenstyrtede by har Han latt falle over ende"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "og det som dekker den, dekker den"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Hvilke av Guds velgjerninger vil du bestride"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Dette er en advarsel i trad med tidligere advarsler"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Det overhengende er overhengende"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Bare Gud kan bringe det frem"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Er dere forundret over dette budskap"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ler dere, og grater ikke"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "I ubekymret lek"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fall ned for Gud, og tilbe Ham"
        }
    ]
}