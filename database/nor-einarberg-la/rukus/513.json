{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ikke trodde han, og ikke bad han"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "men han fornektet og snudde seg bort"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "og gikk sa hjem til sine med stolte steg"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Ve deg, ve"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Ja, ve deg, ve"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Tror mennesket at det overlates til seg selv"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Var det ikke en drape utstøtt sæd"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sa ble det det minste foster. Og Han skapte og formet"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "og laget det til to sorter, mann og kvinne"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Skulle ikke Han være i stand til a gi de døde liv"
        }
    ]
}