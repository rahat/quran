{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "Vi viste ogsa godhet mot Moses og Aron"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "og Vi reddet dem og deres folk fra den store nød"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "og hjalp dem, sa de vant seier"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Vi gav dem den tydelige skriften"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "og ledet dem pa rett vei"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Vi etterlot om dem hos senere slektledd disse ord"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "«Fred over Moses og Aron!»"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Slik lønner Vi dem som handler vel"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "De var av Vare troende tjenere"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Elias var ogsa et av sendebudene"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "da han sa til sitt folk: «Vil dere ikke vise gudsfrykt"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Vil dere pakalle Bal, og oppgi den beste skaper"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Gud, deres Herre, og deres henfarne fedres Herre?»"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Men de holdt ham for løgner, sa de vil matte møte for dommen"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Unntatt er Guds oppriktige tjenere"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Vi etterlot om ham hos senere slektledd disse ord"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "«Fred over Elias!»"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Slik lønner Vi dem som handler vel"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Han var en av Vare troende tjenere"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Lot var ogsa et av sendebudene"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "da Vi reddet ham og alle hans"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "unntatt en gammel kone som ble igjen"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Sa utslettet Vi de andre"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Dere ferdes forbi dem"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "sent og tidlig. Vil dere da ikke forsta"
        }
    ]
}