{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Helvetestreet Zaqqum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "er synderens mat"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "som flytende metall vil det koke i maven"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "som kokende vann koker"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "«Ta ham, og slep ham inn midt i helvete"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "og pøs over hans hode det kokende vannets straff"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Bare smak! Du var jo sa mektig og høyt i ære"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Dette er det dere tvilte pa.»"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Men de gudfryktige er i et trygt bosted"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "blant haver og kilder"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "kledd i silke og brokade, plassert overfor hverandre"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Slik er det. Og vi fører dem sammen med mørkøyde paradisjomfruer"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Der kan de be om all slags frukt i trygghet"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Der smaker de ikke døden etter den første død. Han har bevart dem fra helvetes straff"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "ved Herrens nade. Dette er den store seier"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Vi har gjort Koranen lett tilgjengelig ved ditt eget tungemal, sa de ma komme til ettertanke"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Sa vær pa vakt! De er ogsa pa vakt"
        }
    ]
}