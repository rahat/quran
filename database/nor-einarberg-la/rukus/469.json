{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Nar begivenheten inntreffer"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "kan det ikke benektes at den inntreffer"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Den fornedrer og opphøyer"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Nar jorden rystes i en veldig skaking"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "nar fjellene smuldres til smuler"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "og blir som oppvirvlet støv"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "da blir dere av tre slag"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "De til høyre, hva med dem til høyre"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "De til venstre, hva med dem til venstre"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Og de som ligger først i løpet? De som ligger først i løpet"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "– disse er det som kommer i Herrens nærhet"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "i lykksalighetens haver"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "En stor flokk fra tidligere tider"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "men fa fra de senere"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Pa pyntede benker hviler de"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "overfor hverandre"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "mens evig unge gutter gar rundt blant dem"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "med skaler og krus og beger med den klareste drikk"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "som man ikke far vondt i hodet eller mister vettet av"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "med slike frukter som de velger seg ut"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "og kjøtt og fugl som de ønsker seg"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Og storøyde kvinner er der"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "lik vel forvarte perler"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Som belønning for deres innsats"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Der skal de ikke høre tomt snakk, og ingen bebreidelser"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "bare ordene: «Fred, fred!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Og de til høyre, hva med dem til høyre"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Blant tornefrie lotustrær"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "og tette akasier"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "utstrakt skygge"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "og sprudlende vann"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "frukt i overflod"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "alltid for handen og fri til a ta av"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "pa opphøyde leier"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Fullkomne har Vi skapt dem, paradisets kvinner"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "som plettfrie jomfruer"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ømme og kjærlige, jevngamle"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "med dem til høyre"
        }
    ]
}