{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Nei, sannelig, jeg sverger ved denne byen"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "og du bor jo i denne byen"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "og ved en far og hans avkom"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Vi har visselig skapt mennesket til møye"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Tror han ikke at noen har makt over ham"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Han sier: «Jeg har satt over styr rikdom i massevis.»"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Tror han at ingen har sett ham"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Har Vi ikke gitt ham to øyne"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "tunge, og to lepper"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Har Vi ikke vist ham de to veier"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Men han gav seg ikke i kast med den trange og bratte"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Vet du da hva den trange vei er"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Det er a skaffe en slave frihet"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "og pa hungerens dag a gi mat"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "til en foreldreløs slektning"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "eller til en fattig stakkar"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "og sa være av dem som tror, og som anbefaler hverandre standhaftighet, og rader hverandre til medfølelse"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Dette er høyre hands folk"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Men de som fornekter Vart ord, de er venstre hands folk"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Over dem hvelver seg Ild"
        }
    ]
}